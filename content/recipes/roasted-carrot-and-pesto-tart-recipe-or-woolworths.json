  "recipeIngredient": [
    "400g carrots, peeled, trimmed and quartered",
    "1 tbs olive oil",
    "1 tsp thyme, dried",
    "320g puff pastry, ready rolled",
    "150g ricotta",
    "1 lemon, zest",
    "1 tbs pesto",
    "1 egg, beaten"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced. Toss the carrots with the oil and thyme and roast for 25 minutes, turning halfway through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Roll the pastry out into a rectangle and cut a 2cm border around the edge. Spread the ricotta on top and scatter the carrots, lemon zest and pesto all over."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Brush the edges with beaten egg and bake for 12 minutes, until the pastry is golden and puffed."
    }
  ],
