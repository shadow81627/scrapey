  "keywords": "Roast vegetable",
  "name": "Oven Roasted Capsicums Recipe | Woolworths",
  "prepTime": "PT25M",
  "recipeCategory": "Side dishes",
  "recipeCuisine": "Australian",
  "recipeId": "a0K2y0000012a9NEAQ",
  "recipeIngredient": [
    "2 large capsicums",
    "2 tbs olive oil",
    "1 tsp oregano",
    "1 tsp salt",
    "pinch chilli flakes"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat the oven 250 Celsius."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Prepare a tray with baking paper. Set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Wash and dry capsicums."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut in half and lay on tray flat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place in oven and bake on 250 Celsius until the skin turns a bit black."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from oven and transfer to a plate and cover with cling film."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Let stand for 15-20 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove all the skin and seeds from the capsicums and slice into strips."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place in a bowl and season with oregano, salt, chilli flakes and olive oil. Enjoy as a side dish with your favourite meal or just on it's own with fresh bread."
    }
  ],
