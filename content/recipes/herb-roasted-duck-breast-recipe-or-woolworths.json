  "recipeId": "RGWW338319",
  "recipeIngredient": [
    "100g butter",
    "4 medium duck breasts",
    "1 tbs parsley, chopped",
    "4 cloves garlic, peeled and mashed",
    "2 medium shallots, finely diced",
    "1 tbs rosemary, chopped",
    "2 tbs lemon juice",
    "1 tbs thyme"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan to a medium heat and add 20g of the butter."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pan fry the duck breasts for 4 minutes on each side and then remove from the pan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients to the pan and fry together for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the duck breasts back into the pan and cook while basting with the sauce continuously for 4 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and serve with a choice of condiments and drizzling the sauce leftover over the duck breasts."
    }
  ],
