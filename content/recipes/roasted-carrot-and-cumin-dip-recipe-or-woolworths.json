  "recipeIngredient": [
    "1kg carrots, peeled and chopped",
    "1 tsp cumin",
    "1 tsp ground coriander",
    "1 onion, peeled and sliced",
    "3 cloves garlic, peeled",
    "0.3 cup olive oil, save 2 tsp",
    "2 tbs honey",
    "250g creme fraiche"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat the oven to 180°C/160°C fan-forced and toss the carrots in a roasting tin with the spices, onion, garlic and olive oil. Cover with foil and roast for 45 minutes. Remove the foil and roast for 45 minutes more."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Tip the mix into a food processor with honey and creme fraiche and blitz to form a smooth dip."
    }
