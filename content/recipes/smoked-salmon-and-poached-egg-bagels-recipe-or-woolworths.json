    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Season with Salt & Pepper to taste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sandwich with bagel tops and serve."
    }
