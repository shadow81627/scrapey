  "recipeIngredient": [
    "1 cup cream cheese",
    "1/2 red bell pepper (chopped)",
    "1 celery stalk (chopped)",
    "1 carrot (grated)",
    "1/2 tsp garlic powder",
    "salt (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place cream cheese in a mixing bowl and let it come to room temperature (about 30 minutes)."
    },
    {
      "@type": "HowToStep",
      "text": "While your cream cheese is warming to room temperature, start chopping your veggies."
    },
    {
      "@type": "HowToStep",
      "text": "Chop celery and peppers into small cubes."
    },
    {
      "@type": "HowToStep",
      "text": "Wash and grate carrots."
    },
    {
      "@type": "HowToStep",
      "text": "Add all of your chopped vegetables to mixing bowl with the cream cheese."
    },
    {
      "@type": "HowToStep",
      "text": "Start mixing by folding your vegetables into your cream cheese with a spoon. Mix and fold veggie medley into your cream cheese until the vegetables are spread evenly throughout the cream cheese spread."
    }
  ],
