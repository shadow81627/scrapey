  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Use a board or platter to serve"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut the eggplant to size and serve with semi-dried tomatoes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice the sourdough and layer with grilled capsicum, fresh rosemary and salami slices."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice the cucumber from the end to give circular cuts then layer on top of the sourdough slices and salami slices. Skewer with a toothpick for stability."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Finish with a crack of black pepper over the entire board."
    }
  ],
