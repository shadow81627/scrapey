  "recipeIngredient": [
    "12 slice soppressa salami",
    "1 capsicum, grilled",
    "1 Lebanese cucumber",
    "0.5 eggplant, chargrilled",
    "2 semi dried tomatoes",
    "1 pinch black pepper, cracked",
    "2 slice sourdough",
    "1 sprig rosemary"
