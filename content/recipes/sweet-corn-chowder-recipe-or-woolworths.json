  "recipeIngredient": [
    "2 tbs cornflour",
    "2 carrots, diced",
    "2 stick celery, chopped",
    "2 cup salt-reduced chicken stock",
    "1 onion, chopped",
    "1 cup milk",
    "2 rasher streaky bacon, chopped",
    "1 tbs olive oil",
    "2 tbs chives, snipped",
    "1 garlic clove, crushed",
    "400g potatoes, cut into 1cm cubes",
    "250g frozen corn kernels"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add oil, bacon, onion and garlic to the slow cooker and cook on high setting for 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add corn, carrot, celery and potato. Mix cornflour with a little water to make a smooth paste. Add stock and milk. Pour into cooker. Season with pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook on high setting for 2 1/2 - 3 hours (or reduce to low setting and cook for 6 hours)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon into serving bowls, garnish with chives and serve."
    }
  ],
