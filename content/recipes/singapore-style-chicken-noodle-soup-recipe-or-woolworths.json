  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil in a large deep saucepan over medium heat. Add onion, carrot, celery and garlic and cook for 8 minutes or until tender. Add stock and 3 cups of water. Stir until well combined and mixture comes to the boil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add chicken and noodles. Cook for 2 minutes or until heated through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Ladle into bowls, top with parsley and serve immediately."
    }
