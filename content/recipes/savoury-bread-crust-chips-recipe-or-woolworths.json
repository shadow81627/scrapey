  "recipeIngredient": [
    "1 tsp dried garlic",
    "1 tsp smoked paprika",
    "1 tsp dried thyme",
    "2 bread crusts, sliced to 1.5cm strips",
    "2 tbs olive oil",
    "0.25 tsp salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 200oC and line a baking tray with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a medium bowl, toss together the bread strips with the remaining ingredients."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread onto prepared tray and bake for 10 minutes or until golden and crisp."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with dips."
    }
  ],
