  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook pasta as per packet instructions. Drain and rinse. Transfer to a large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drain tuna, reserving 2 tbs of oil. Flake tuna and add to pasta with tomato, olives, onion, capers and parsley."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk lemon juice with reserved oil and garlic. Season with pepper. Toss through pasta mixture. Serve sprinkled with feta."
    }
  ],
