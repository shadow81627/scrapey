  "recipeIngredient": [
    "1 clove garlic, crushed",
    "200g grape tomatoes, halved",
    "0.5 small red onion, thinly sliced",
    "0.25 cup flat-leaf parsley, chopped",
    "200g feta, crumbled",
    "2 tbs capers",
    "425g can tuna in olive oil",
    "300g fusilli pasta",
    "12 kalamata olives, pitted",
    "0.25 cup lemon juice"
