  "recipeIngredient": [
    "1kg beef blade steak, large dice",
    "800g tomatoes, diced",
    "4 tbs tomato paste",
    "250ml red wine",
    "3 dash worcestershire sauce",
    "4 beef stock cubes",
    "200ml hot water",
    "1 tbs thyme",
    "1 tbs basil",
    "1 tbs oregano",
    "salt (to taste)",
    "peper (to taste)",
    "3 tbs garlic, minced",
    "2 red onions, diced",
    "1 tbs olive oil",
    "1 chilli, diced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/i8x49z7su4aame6wmrad?wid=3024&hei=3931&fmt=png-alpha",
      "text": "Heat olive oil in pan on a medium-high heat on stove. Quickly brown off diced beef in batches and add to slow cooker."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/g8tdwpx45kxny2rla5ga?wid=3024&hei=3931&fmt=png-alpha",
      "text": "Add all remaining ingredients and set slow cooker to 'low' setting for 8 hours, stirring occasionally. At 8 hour point, beef should be falling apart. Leave in longer for an even richer sauce. Serve with pasta of your choosing and fresh parmigiana cheese."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/vtn14mpnomytxr1rvy3u?wid=3024&hei=3931&fmt=png-alpha",
      "text": "In same pot, fry off the onion, garlic and chilli. Add to slow cooker. Add hot water and stock cubes to dissolve"
    }
  ],
