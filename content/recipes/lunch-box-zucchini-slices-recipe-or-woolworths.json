      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/uzyfef13ezaasgr5e42r?wid=2656&hei=1812&fmt=png-alpha",
      "text": "Bake at 170 degrees for 20 minutes. Enjoy!"
    }
