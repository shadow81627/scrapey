  "recipeIngredient": [
    "1 medium zucchini, grated",
    "1 carrot, grated",
    "1 small onion, finely diced",
    "2 cloves of garlic, minced",
    "5 eggs",
    "0.25 cup oil",
    "2 rashers bacon",
    "0.25 tsp salt and pepper (to taste)",
    "1 cup self raising flour",
    "0.5 cup grated cheese"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
