  "recipeIngredient": [
    "4 beetroot, cooked",
    "160g halloumi cheese, cut into small cubes",
    "2 tbs canola oil",
    "1 white onion, peeled and diced",
    "2 cloves garlic, peeled and diced",
    "300g mixed salad leaves, washed and drained",
    "2 tbs olive oil",
    "2 tbs balsamic vinegar",
    "1 tbs Dijon mustard"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Dice the beetroot into small cubes and toss in a baking dish with the halloumi, canola oil, onion and garlic."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in the oven for 30 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Now combine your dressing ingredients and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once the halloumi and beetroot is ready, you can remove from the oven and toss with the salad leave and drizzle with dressing before serving."
    }
  ],
