  "recipeIngredient": [
    "1 prepared pizza crust",
    "1 tsp olive oil",
    "4 cremini mushrooms (sliced)",
    "1/2 cup prepared pesto",
    "3/4 cup Italian blend or Mozzarella cheese",
    "1/3 cup roasted red peppers (sliced)",
    "4 asparagus stalks (tips cut off and sliced into ribbons with a vegetable peeler)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat oven to temperature indicated on pizza crust. Place pizza crust on baking sheet."
    },
    {
      "@type": "HowToStep",
      "text": "Heat olive oil in a small skillet over medium heat. Add mushrooms and cook about 5 minutes, or until softened."
    },
    {
      "@type": "HowToStep",
      "text": "Spread pesto evenly onto pizza crust. Top with cheese, mushrooms, red peppers, and asparagus tips and ribbons. Bake until asparagus has softened and cheese has melted, about 10 minutes."
    }
