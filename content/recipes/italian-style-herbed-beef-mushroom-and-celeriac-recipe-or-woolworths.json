  "recipeIngredient": [
    "500 g heart smart beef, diced",
    "200 g carrots, peeled and diced",
    "200 g brown onions, peeled and diced",
    "400g celeriac, peeled and diced",
    "375g field mushrooms, thickly sliced",
    "1 medium yellow capsicum, deseeded and diced",
    "1 tbs fresh oregano, chopped",
    "2 clove garlic, peeled and minced",
    "1 tsp dried tarragon",
    "2 tbs basil, chopped",
    "0.5 tsp dried thyme",
    "400g can diced tomatoes",
    "1 cup shiraz",
    "500ml beef stock",
    "pinch salt and black pepper",
    "1 tsp rice bran oil",
    "3 bay leaves"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/rhzwtcmli2if8arwvzbz?wid=3024&hei=2326&fmt=png-alpha",
      "text": "Add the oil to a cast iron Dutch oven or thick based pot, brown the onions carrots and beef."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/oync7xewt5wlqkem9i81?wid=3024&hei=2326&fmt=png-alpha",
      "text": "Add the capsicum, mushrooms and stock, simmer with lid on for 15 minutes on low heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add tomatoes, garlic, wine and herbs and continue to cook on low heat for a further 3 hours and 15 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/hv4wmeiowsokjg5yum6f?wid=4032&hei=3024&fmt=png-alpha",
      "text": "Add diced celeriac, seasoning and mix well replace lid and cook for a further 30 minutes or until celeriac is tender and sauce reduced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/wdddg1pbjpi6acaxpuue?wid=3024&hei=4032&fmt=png-alpha",
      "text": "Adjust seasoning and enjoy."
    }
  ],
