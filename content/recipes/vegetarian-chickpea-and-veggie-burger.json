  "recipeIngredient": [
    "265 g 1/2 lb canned chickpeas, drained or fresh",
    "2 eggs",
    "1 cooked carrot (cubed)",
    "1/2 tsp sea salt",
    "1 handful parsley (chopped)",
    "2 green onions (finely chopped)",
    "1 1/4 tsp lemon zest",
    "1 cup panko bread crumbs",
    "2 tsp olive oil",
    "TO SERVE",
    "4 hamburger buns",
    "mayo",
    "lettuce leaves",
    "1/3 finely chopped red onion",
    "sliced ​​tomatoes",
    "pickles"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Put the chickpeas, eggs, carrots, salt and pepper in a blender and grind until the mixture is smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Transfer to large bowl and add parsley, green onions, lemon zest, and bread crumbs. Mix well and reserve in the fridge for 5–10 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Wet your hands with water and shape the mixture into 4 patties."
    },
    {
      "@type": "HowToStep",
      "text": "Heat the oil in a skillet over medium-high heat. Cook the veggie patties for 3–4 minutes on each side."
    },
    {
      "@type": "HowToStep",
      "text": "Serve chickpea burgers in breads spread with mayo, lettuce, red onion, tomato, and pickles."
    }
  ],
