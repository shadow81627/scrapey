  "recipeIngredient": [
    "600g red cabbage, finely sliced",
    "3 large apples, cored and sliced",
    "250g beetroot, peeled and grated",
    "1 medium red onion, peeled and finely sliced",
    "2 tbs apple cider vinegar",
    "1 tbs lemon juice",
    "3 tbs olive oil",
    "50g pomegranate seeds",
    "3 tbs pumpkin seeds",
    "1 pinch cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the sliced cabbage, apples, beetroot and red onion in a large bowl and mix."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a small bowl, whisk the apple cider vinegar, lemon juice and olive oil together, then pour over the cabbage mix."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Season with cracked black pepper, add the pomegranate and pumpkin seeds, mix and serve."
    }
  ],
