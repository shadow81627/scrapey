  "recipeIngredient": [
    "1 to 2 tablespoons vegetable oil",
    "1 8 ounce package tempeh, (broken into 1 to 2 inch chunks)",
    "1/2 cup panko breadcrumbs",
    "1/4 cup coarsely chopped onion",
    "2 garlic cloves, (minced)",
    "2 tablespoons unflavored soy or almond milk",
    "1 tablespoon soy sauce",
    "1/2 teaspoon liquid smoke",
    "1/4 cup soy sauce",
    "3 tablespoons maple syrup",
    "1 tablespoon rice vinegar",
    "1 tablespoon sake, (optinal, but very nice)",
    "1 teaspoon freshly grated ginger",
    "dash white pepper",
    "6 tablespoons cold water, (divided)",
    "2 teaspoons cornstarch",
    "2 scallions, (chopped)",
    "toasted sesame seeds"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat oven to 400Â° and lightly oil a baking sheet."
    },
    {
      "@type": "HowToStep",
      "text": "Place all ingredients into a food processor bowl and pulse, just until ingredients are chopped and well mixed. Don't overdo it."
    },
    {
      "@type": "HowToStep",
      "text": "Roll the mixture into 1 inch balls and arrange on the prepared baking sheet. Bake for 15 minutes, or until the meatballs begin to brown, flipping once or twice during baking."
    }
  ],
