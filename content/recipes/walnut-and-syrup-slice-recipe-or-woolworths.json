  "recipeIngredient": [
    "250g butter, chopped (save 100g butter for later)",
    "0.5 tsp ground cinnamon",
    "2 tbs maple syrup",
    "1 cup dark choc bits",
    "1 cup brown sugar",
    "150g walnuts",
    "2 cup gluten-free plain flour"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180Â°C. Line a 17x27cm slice pan with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place flour, sugar, cinnamon and butter into a food processor and process until the mixture starts to clump together. Transfer to a pan. Press firmly and evenly over the base."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Break walnuts over the base. Melt brown sugar and butter in a small saucepan over a low heat, stirring until smooth. Add maple syrup and boil for 2 minutes. Pour over the base and nuts. Bake for 20 minutes until golden."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the oven and scatter chocolate bits evenly over the surface while still hot. Cool. Cut into bars."
    }
  ],
