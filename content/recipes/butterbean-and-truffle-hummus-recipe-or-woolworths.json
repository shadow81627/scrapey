  "recipeIngredient": [
    "400g butterbeans, drained",
    "2 cloves garlic, peeled and minced",
    "4 tbs olive oil, divided",
    "1 lemon, juiced",
    "2 tbs truffle oil",
    "1 pinch black pepper",
    "1 pinch salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat a tablespoon of oil in a small pan and gently cook the garlic for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour the butterbeans, garlic, olive oil and lemon juice into a blender and season well. Blitz until smooth, adding cold water if necessary."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Scoop the hummus into a serving bowl, drizzle with truffle oil and serve."
    }
  ],
