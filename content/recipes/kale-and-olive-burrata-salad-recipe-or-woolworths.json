  "recipeIngredient": [
    "400g kale, washed, drained and stems removed",
    "240g burrata, drained and sliced",
    "4 medium heirloom tomato, sliced",
    "80g green olives, pitted and halved",
    "80g croutons",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "2 tbs olive oil",
    "1 tsp garlic powder",
    "2 tbs balsamic vinegar",
    "1 tbs basil, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a small bowl, combine the olive oil with the salt, pepper, garlic, balsamic and basil and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the kale leaves on your serving plates and then equally place the sliced burrata and tomatoes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sprinkle the croutons and olives over the salad plates."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drizzle with the sauce before serving."
    }
  ],
