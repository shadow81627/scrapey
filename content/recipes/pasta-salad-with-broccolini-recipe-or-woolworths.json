  "recipeIngredient": [
    "500g pasta (of your choice)",
    "2 bunches broccollini florets, steamed",
    "1 large carrot, shredded",
    "1 onion, thinly sliced",
    "420g kidney beans, drained & rinsed",
    "0.25 cup fresh parsley, chopped",
    "1 tsp dried oregano",
    "1 clove garlic, minced",
    "1.5 cups tomatoes, deseeded & cubed",
    "0.25 cup olive oil",
    "0.25 cup red wine vinegar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook pasta as per packet instructions (al dente). Rinse under cold water and set aside. Drizzle a little oil so pasta doesn't stick together."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stem broccoli florets for approximately 1-2 mins (do not over steam, must still have a crunch)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a bowl combine pasta, beans, tomatoes, broccolini, carrots, onions, and parsley."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a jar combine olive oil, red wine vinegar, garlic, oregano, salt and pepper to taste and shake vigorously."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drizzle dressing over the pasta salad and mix until evenly coated."
    }
  ],
