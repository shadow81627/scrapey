  "recipeIngredient": [
    "0.5 can condensed milk",
    "50g butter",
    "1 block malteser chocolate",
    "1 cup mini marshmallows",
    "1 cup raspberry lollies",
    "1 cup crispy m&ms",
    "0.5 pkt nice biscuits, crushed"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Melt chocolate and butter in the microwave"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add crushed biscuits"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add condensed milk and mix together"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add 3/4 raspberries, marshmallows and M&Ms and mix"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Line a baking tray with baking paper and pour in mixture. Flatten out evenly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/bkjswisoabryj5ayidfc?$foodhub-method-images$&fmt=png-alpha",
      "text": "Decorate the top with the rest of the raspberries, marshmallows, M&Ms"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place in fridge overnight. Slice and enjoy!"
    }
  ],
