  "recipeIngredient": [
    "150ml spiced rum",
    "1 lemon, juiced",
    "1L tomato juice",
    "1 tbs Woolworths Worcestershire sauce",
    "1 tbs Tabasco",
    "1 pinch mixed spice",
    "6 drained mild pickled chilli peppers",
    "18 pitted Sicilian olives",
    "1 bunch rosemary, sprigs picked"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine rum, lemon juice, tomato juice, Worcestershire sauce, Tabasco and mixed spice in a large jug."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Thread 1 chilli pepper and 3 olives onto 6 cocktail skewers. Fill 6 x 1-cup capacity glasses with ice. Divide rum mixture among glasses, then garnish with rosemary sprigs and olive skewers. Serve."
    }
  ],
