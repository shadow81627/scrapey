  "recipeId": "RGWW337891",
  "recipeIngredient": [
    "1 medium zucchini, sliced into 8-12 slices",
    "0.4 cup marinara sauce",
    "100g chorizo, diced",
    "60g cheddar cheese, grated",
    "2 cloves garlic, peeled and diced",
    "1 small white onion, peeled and diced",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "1 tsp mixed herbs"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a non-stick pan to a medium heat and fry the zucchini slices for 2 minutes on each side and then place on a baking tray."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Now fry the chorizo, garlic and onion in the pan for 4 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the marinara and cook for an additional 3 minutes to reduce until thickened."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and place the mix on top of each zucchini slice."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top the mixtures with the grated cheese and top with seasoning and herbs."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place under the grill to melt the cheese for 3 minutes before serving."
    }
  ],
