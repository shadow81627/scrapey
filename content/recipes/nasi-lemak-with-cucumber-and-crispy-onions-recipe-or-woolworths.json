  "recipeIngredient": [
    "400g jasmine rice, rinsed",
    "0.8 cup coconut milk",
    "2 stalks lemongrass, sliced lengthways",
    "1 small cucumber, sliced",
    "100g crispy onions",
    "1 small carrot, peeled and thinly sliced",
    "4 tbs soy sauce"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the rice as per packet instructions with the lemongrass and then drain the excess water."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the coconut milk to the pot and cook for 8 minutes while constantly stirring so the rice does not stick."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once the rice is cooked, remove from the heat and serve with crispy onions to garnish and the freshly sliced veg and soy."
    }
  ],
