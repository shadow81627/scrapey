  "name": "Raw Summer Avocado Gazpacho",
  "prepTime": "PT5M",
  "recipeCategory": "Soup",
  "recipeCuisine": "raw",
  "recipeIngredient": [
    "3 large tomatoes",
    "1 red pepper",
    "handful parsley",
    "1/2 avocado",
    "2 garlic cloves",
    "1 cucumber",
    "1/2 onion",
    "2 tablespoons apple cider vinegar",
    "1 teaspoon paprika",
    "1 teaspoon sea salt",
    "1/4 cup extra virgin olive oil",
    "1/2 avocado (chopped)",
    "1/4 cup fresh cilantro"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Add all the ingredients into the blender and process until creamy."
    },
    {
      "@type": "HowToStep",
      "text": "Refrigerate for 30 minutes then serve."
    }
  ],
