  "recipeIngredient": [
    "1 cup non-dairy milk, (I use macadamia nut milk)",
    "1/4 cup chia seeds",
    "2 tbsp. peanut flour",
    "2 tsp. maple syrup",
    "1 tsp. vanilla extract",
    "1/2 tsp. ground cinnamon",
    "2 tbsp. cacao nibs, (optional)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "In a small mixing bowl, whisk together all the ingredients."
    },
    {
      "@type": "HowToStep",
      "text": "Pour chia pudding into (2) 6 oz mason jars. They should be about half full. Cover and place in the refrigerator for 2 hours to let the pudding set up."
    },
    {
      "@type": "HowToStep",
      "text": "Once pudding is thick, you can serve immediately. Store any uneaten chia pudding in the refrigerator for up to 3 days."
    }
  ],
