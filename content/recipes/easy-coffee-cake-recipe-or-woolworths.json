  "recipeIngredient": [
    "1.8 cups plain flour",
    "1 pinch salt",
    "180g butter",
    "0.5 cup sugar",
    "0.8 cup milk",
    "2 eggs",
    "4 tsp instant coffee granules",
    "1 tbs hot water"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/hej12rvynqbmml3k3k5p?wid=2322&hei=4128&fmt=png-alpha",
      "text": "Dissolve coffee in hot water. Line loaf pan with baking paper. Preheat oven to 180'C"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/rwhewmplibgz3k4bmpbn?wid=2322&hei=4128&fmt=png-alpha",
      "text": "Cream butter and sugar with mixer."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/lsbabqv85zfzp6pqwgdn?wid=2322&hei=4128&fmt=png-alpha",
      "text": "Add eggs and mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/rvsvzdgxsckqihmq2qzw?wid=2322&hei=4128&fmt=png-alpha",
      "text": "Add coffee, milk, flour and salt and mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/vvxdxssttyn95xnkhlmu?wid=2322&hei=4128&fmt=png-alpha",
      "text": "Spoon mixture into lined loaf pan and bake for between 50 - 60 minutes. (Oven times vary so keep an eye on it towards to end of the cooking time.) Cake is cooked if a skewer inserted into the deepest part comes out clean."
    }
  ],
