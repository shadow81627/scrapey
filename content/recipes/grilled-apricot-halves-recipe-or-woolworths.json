  "recipeIngredient": [
    "20g butter",
    "0.25 cup brown sugar",
    "0.25 cup slivered almonds",
    "820g tin apricot halves, drained"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat a grill on high. Line a tray with paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place apricots, cutside up, onto tray."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine sugar and almonds in a small bowl, add butter and rub in until combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sprinkle over each apricot. Grill for 5 minutes until golden. Serve."
    }
  ],
