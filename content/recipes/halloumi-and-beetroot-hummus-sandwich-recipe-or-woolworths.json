  "recipeIngredient": [
    "200g beetroot, cooked, drained",
    "300g hummus",
    "1 tbs lemon juice",
    "200g halloumi cheese, sliced",
    "2 tsp olive oil",
    "60g rocket, crumbled",
    "8 slices sourdough bread",
    "1 pinch black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the hummus and beetroot in a food processor with lemon juice and blitz until smooth."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a non-stick pan on medium high heat and cook the halloumi slices for 2 minutes on each side, until golden."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toast the bread, spread the hummus on 4 of the slices, top each with halloumi, rocket and cracked black pepper, add the top slice and serve."
    }
  ],
