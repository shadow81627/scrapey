  "recipeIngredient": [
    "1 tbs mild tomato salsa",
    "40g red capsicum, sliced",
    "4 strawberries (for snack)",
    "1 wholegrain mini tortilla",
    "20g lite cheese block, grated",
    "1 tbs lite cream cheese (for snack)",
    "100g Greek style yoghurt (for snack)",
    "2 tsp sultanas (for snack)",
    "1 stick celery (for snack)",
    "50g leg ham, shaved"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place tortilla on a clean work surface. Spread salsa down middle of tortilla. Top with ham, cheese and capsicum. Firmly roll to enclose."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat sandwich press. Place tortilla in sandwich press and grill for 2 minutes or until golden and cheese has melted. Set aside to cool completely. Cover and refrigerate until required. Snack 1: Celery filled with cream cheese and sultanas. Snack 2: Greek yoghurt served with strawberries to dip."
    }
  ],
