  "recipeIngredient": [
    "1 cup self-raising flour",
    "2 tsp mixed spice",
    "380g Top’n’Fill caramel",
    "1 cup milk",
    "2 tbs icing sugar (for dusting)",
    "4 scoops vanilla ice cream (to serve)",
    "75g unsalted butter, melted",
    "1 tsp vanilla essence",
    "3 carrots, coarsely grated",
    "3 free range eggs",
    "1 cup brown sugar",
    "1 cup walnuts"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 160°C. Lightly grease 4 x 2-cup capacity shallow ovenproof dishes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place flour, sugar, walnuts, mixed spice, milk, butter, eggs, vanilla and 1/3 cup caramel in a blender. Blend for 30 seconds or until smooth. Add carrot and pulse until just combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide mixture evenly between dishes. Place on a baking tray and bake for 25-30 minutes or until firm to touch and just set."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, heat remaining caramel in a saucepan over medium heat. Dust puddings with icing sugar, drizzle over warm caramel, and serve topped with ice-cream and extra walnuts."
    }
  ],
