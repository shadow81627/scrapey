  "recipeIngredient": [
    "8 crackers",
    "0.5 ripe avocado",
    "1 lemon, 1 tsp juiced",
    "1 pinch pepper (to taste)",
    "0.5 punnet berry truss tomatoes",
    "1 pinch pink salt ground (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut avocado in half, remove the pit. Spook the avocado flesh into a small bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the lemon juice, and mash roughly with a fork. Season with salt and pepper to taste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice Sweet Berry Truss Tomatoes in halves or thirds."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon the avocado evenly over the crackers. Top with tomato slices, and serve immediately."
    }
  ],
