  "recipeIngredient": [
    "1 cup flour",
    "1 cup milk",
    "1 cup egg",
    "tsp salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sift flour and salt into large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Make a indent into center of flour."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour milk into the center."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Break egg open into the center of the bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/uf2ojw4ezird8rlhknkm?wid=1600&hei=1200&fmt=png-alpha",
      "text": "Mix together well until there are no lumps."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/w927qpl0klgfdo8msv4k?wid=1379&hei=1060&fmt=png-alpha",
      "text": "Pour 1/4 cup of mixture onto sandwich press and press the lid down firmly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/s7j73jgkwmzfwle5inhs?wid=627&hei=482&fmt=png-alpha",
      "text": "Leave for 10 seconds than check, cook for up to 10 seconds longer, (depending on the colour desired)."
    }
  ],
