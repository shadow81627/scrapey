  "recipeIngredient": [
    "1 (12 ounce) bag fresh cranberries",
    "1/2 cup dry red wine",
    "1/2 cup maple syrup",
    "1/4 cup apple cider",
    "1 teaspoon ground cinnamon",
    "1/4 teaspoon ground cloves",
    "1/4 teaspoon ground cardamom",
    "2 star anise"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Stir all of the ingredients together in a small saucepan and place it over high heat."
    },
    {
      "@type": "HowToStep",
      "text": "Bring the liquid to a boil, lower the heat, and allow the mixture to simmer for 10 minutes, stirring occasionally, until most of the berries have burst."
    },
    {
      "@type": "HowToStep",
      "text": "Remove the pot from the heat and allow the sauce to cool. Remove the star anise."
    },
    {
      "@type": "HowToStep",
      "text": "Serve."
    }
  ],
