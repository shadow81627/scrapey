  "recipeIngredient": [
    "1 teaspoon olive oil",
    "1/4 cup diced onion",
    "1 clove garlic, minced",
    "1 (8 oz) package tempeh, crumbled",
    "1 (15 oz) can chickpeas, drained and rinsed",
    "1 cup marinara sauce",
    "1 tablespoon maple syrup",
    "1 tablespoon ketchup",
    "2 teaspoons apple cider vinegar",
    "1 teaspoons tamari",
    "1 teaspoon chili powder",
    "3/4 teaspoons smoked paprika",
    "1/2 teaspoon mustard powder",
    "1/2 teaspoon ground cumin",
    "1/4 teaspoon salt",
    "1/8 teaspoon black pepper",
    "6 hamburger buns",
    "Coleslaw for topping, optional"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Heat a large pan over medium heat and add olive oil and onion. Cook onion for 2 minutes and then add garlic and cook for 30 seconds."
    },
    {
      "@type": "HowToStep",
      "text": "Add in crumbled tempeh and cook until browned, 3-4 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Next add in all the remaining ingredients, including the chickpeas, stir until combined and cook until everything is heated through, about 10 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Scoop sloppy Joe mixture onto buns and top with coleslaw, if desired."
    }
  ],
