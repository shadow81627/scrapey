  "recipeIngredient": [
    "625g soba noodles, cooked",
    "2 carrots, finely julienned",
    "6 spring onions, finely shredded",
    "1 red capsicum, finely sliced",
    "4 tbs dark soy sauce",
    "3 tbs sesame oil",
    "1 tbs mirin",
    "1 tbs caster sugar",
    "0.5 tsp chilli oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the soba noodles in a wide bowl with the carrots, spring onions and capsicum."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a separate bowl, mix together the soy sauce, sesame oil, mirin, sugar and chilli oil, then pour over the noodle mixture."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toss to mix well and serve chilled or at room temperature."
    }
  ],
