  "recipeIngredient": [
    "50g mushrooms, sliced",
    "3 tbs capers",
    "5 tbs extra-virgin olive oil",
    "1 lemon, juiced",
    "4 slices ham",
    "450g crusty cobb loaf",
    "100g pastrami",
    "250g low fat ricotta",
    "125g chargrilled capsicum",
    "125g chargrilled eggplant",
    "125g semi sun dried tomatoes",
    "0.5 cup mozzarella, grated",
    "200g plain black kalamata olives",
    "2 tsp thyme, chopped",
    "45g anchovies",
    "1 clove garlic, crushed"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut top from Crusty Cob Loaf and set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove bread from inside, leaving a shell 2.5cm thick."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Using a spoon, spread tapenade over insides and underside of lid."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lay 4 slices of ham over the base, top with chargrilled eggplant, semi sun-dried tomatoes, low-fat ricotta, sliced mushrooms, chargrilled capsicum, pastrami and finish with the grated mozzarella, layering each ingredient on top of each other."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Replace bread lid and wrap tightly in plastic wrap."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Refrigerate overnight."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut into wedges to serve."
    }
  ],
