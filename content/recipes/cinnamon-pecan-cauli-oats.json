  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Combine the rolled oats, riced cauliflower, butter, cinnamon, salt, milk, and water in a bowl. Stir to combine."
    },
    {
      "@type": "HowToStep",
      "text": "Microwave for 3 minutes, stirring every minute or as often as needed to avoid the oats from boiling over (you'll need to stir more often toward the end)."
    },
    {
      "@type": "HowToStep",
      "text": "Stir in the brown sugar and pecans just before serving."
    }
