  "recipeIngredient": [
    "160g baby potatoes, cooked and lightly mashed",
    "1 medium white onion, peeled and diced",
    "1 tsp fresh ginger, chopped",
    "1 pinch cracked black pepper",
    "2 tbs sesame oil",
    "1 tbs curry powder",
    "1 tsp cumin",
    "1 tsp mustard seeds",
    "12 bao buns"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan to a medium heat and fry the cooked potatoes and vegetables with the sesame oil, seeds and spices for 5 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Warm up the bao buns in the microwave for 15 seconds."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Put the ingredients in the bao buns and serve with your choice of condiments."
    }
  ],
