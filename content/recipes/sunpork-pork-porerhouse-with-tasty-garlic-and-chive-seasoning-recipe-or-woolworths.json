  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat a fry pan or grill plate and lightly oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place pork porterhouse in heated pan or grill pan, cook for 6 minutes on one side. Turn and cook for a further 4 minutes or until cooked through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a bowl, combine chickpeas, cherry tomatoes, parsley, red wine vinegar and olive oil. Season to taste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread salad on plates and top with thinly sliced porterhouse for a light fresh meal."
    }
