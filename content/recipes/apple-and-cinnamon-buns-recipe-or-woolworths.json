  "recipeCategory": "Breakfast",
  "recipeCuisine": "Australian",
  "recipeId": "a0K2y0000012db5EAA",
  "recipeIngredient": [
    "1 tbs dried yeast",
    "2 tbs raw sugar",
    "1 cup lukewarm water",
    "4 tsp cinnamon",
    "0.5 tsp mixed spice",
    "4 cups plain flour",
    "70g salted butter",
    "0.5 cup whole milk",
    "2 Granny smith apples, peeled & grated",
    "1 tbs apricot jam",
    "1 tbs water",
    "1 packet dried sultanas"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a jug mix lukewarm water, sugar and yeast."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Allow to rest until frothy. Then add melted butter and milk, stir well. Add to flour."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine cinnamon, mixed spice and flour in a stainless steel bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Using a mixer and dough hook, knead for 1 minute on low speed."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add grated apple and dried sultanas, knead for a further 3 minutes on medium speed."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cover bowl with a tea towel and set aside for 30 minutes to rise."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide the dough into 12 pieces and knead into balls, place on greased lined tray and cover with tea towel. Allow to rise for 20 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven 180 Celsius."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake at 180 Celsius for 20 minutes or until golden brown."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from oven and leave on tray."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a small saucepan warm up apricot jam and water and glaze top of buns."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Delicious served with butter and they freeze really well. Enjoy!"
    }
  ],
