  "recipeIngredient": [
    "2 cup milk, save 2 tsp for later",
    "120g dark chocolate, chopped",
    "3.2 cups coffee, freshly brewed",
    "3 tsp peppermint extract",
    "2 tbs whipped cream (to serve)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the milk, chocolate and coffee to a saucepan on medium heat and simmer for 2 minutes, stirring, until the chocolate has melted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and stir in the peppermint extract."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour into 4 mugs, top with whipped cream and serve."
    }
  ],
