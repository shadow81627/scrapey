  "recipeIngredient": [
    "1 clove garlic, crushed",
    "30g butter, melted",
    "0.25 cup light sour cream",
    "1 tbs parsley, chopped",
    "3 spring onions, chopped",
    "1kg sweet potatoes, peeled and diced",
    "1 pinch pepper (for seasoning)",
    "1 tbs olive oil",
    "0.25 cup pepitas",
    "0.33 cup tasty cheese",
    "0.5 cup fresh breadcrumbs",
    "1 pinch salt (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 200°C."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine sweet potato, oil, garlic and spring onion in a 4-cup capacity ovenproof dish. Toss to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake for 30 minutes, turning occasionally, until just tender. Meanwhile, combine cheese, breadcrumbs, pepitas, parsley and butter in a medium bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Reduce oven to 180°C and remove baking dish. Roughly mash sweet potato using a fork. Stir through sour cream. Season to taste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sprinkle crumb mixture evenly over. Bake for 15 minutes until topping is golden."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with roast meat."
    }
  ],
