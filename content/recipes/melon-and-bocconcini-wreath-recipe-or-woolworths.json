  "recipeIngredient": [
    "1 tbs extra virgin olive oil",
    "2 tsp balsamic vinegar",
    "0.5 rockmelon, deseeded",
    "0.25 small watermelon",
    "250g cherry tomatoes, quartered",
    "220g Woolworths cherry bocconcini, drained",
    "0.5 bunch radish, thinly sliced",
    "0.25 bunch basil, leaves picked"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place oil in a small bowl and gradually whisk in vinegar until combined. Season with pepper. Set aside until required."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Using a melon baller, scoop melon flesh into balls."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Arrange melon balls, tomato, bocconcini, radish and basil in a wreath shape on a 24cm round rimmed plate. Serve drizzled with dressing."
    }
  ],
