  "recipeIngredient": [
    "500g turkey mince",
    "1 tbs fish sauce",
    "2 tsp honey",
    "1 tbs soy sauce",
    "3 tsp sesame oil",
    "1 carrot, grated",
    "4 spring onions, sliced",
    "2 heads little gem lettuce, leaves separated",
    "3 tbs coriander, fresh, chopped",
    "1 tbs sesame seeds"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat a pan on high and cook the turkey mince, fish sauce, honey, soy sauce and sesame oil for 45 minutes until browned, add the carrot and spring onion and cook for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon the turkey mix into little gem leaves, top with coriander and sesame seeds and serve."
    }
  ],
