  "recipeIngredient": [
    "0.25 cup Sunbeam raisins, roughly chopped",
    "1 green apple, shredded",
    "1 red apple, shredded",
    "0.25 savoy cabbage, finely shredded",
    "0.25 cup mint leaves, chopped",
    "2 spring onions, finely sliced",
    "0.25 cup Sunbeam pine nuts",
    "0.25 cup Sunbeam premium seed mix",
    "1 tbs Sunbeam sesame seeds",
    "3 tsp apple cider vinegar",
    "1 tbs olive oil spray"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place pine nuts and sesame seeds in a small frying pan and cook for 5 minutes until golden and toasted. Set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine the cabbage, apples, spring onions, raisins and mint in a large mixing bowl and toss well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk together the oil and vinegar. Season to taste. Pour over apple mixture and toss well. Spoon onto serving platter and sprinkle with toasted seeds and nuts and some additional mint leaves."
    }
  ],
