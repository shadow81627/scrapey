  "recipeIngredient": [
    "3.2 cup coconut milk, reduced fat",
    "250g vegetable stir fry mix",
    "3 tbs Thai red curry paste",
    "2 tbs fish sauce",
    "2 large limes, juice and zest",
    "0.5 bunch coriander, fresh, chopped",
    "200g rice noodles",
    "1 red chilli, slice",
    "1 tbs canola oil",
    "400g king prawns, cooked, peeled, deveined"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the rice noodles according to pack instructions, drain and set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a large, deep pan and fry the vegetables for 2 minutes, stir in the curry paste and cook for 1 minute. Stir in the coconut milk and fish sauce and the zest and juice of 1 lime and bring to a simmer."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir in the prawns, sliced chilli, coriander stalks and rice noodles and simmer for 3 minutes. Serve with lime wedges and fresh coriander on top."
    }
  ],
