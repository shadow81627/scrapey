  "recipeIngredient": [
    "75g walnuts, chopped",
    "2 tbs instant espresso powder",
    "1.2 cup double cream",
    "175g condensed milk"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat a dry pan on medium heat and toast the walnut pieces for 2-3 minutes until fragrant. Set aside to cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk the instant espresso, cream and condensed milk in a stand mixer until soft peaks form. Fold in the walnuts and transfer the mix to a freezer safe container. Freeze for 6 hours."
    }
  ],
