  "recipeIngredient": [
    "2 cup SR flour",
    "0.5 tsp salt",
    "1 tsp baking powder",
    "0.5 cup grated cheese",
    "0.5 cup diced ham",
    "0.75 cup milk",
    "50g butter"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 200°C or 180°C fan forced, grease a baking tray and line with baking paper"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sift flour and baking powder into a mixing bowl and add salt, butter and rub mixture with fingertips to resemble fine breadcrumbs"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add cheese and ham then mix. Make a well in middle and add egg and milk. Combine to a soft dough"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Turn dough out on floured board and knead lightly, press out to form a 2cm thick. Cut using a floured round cutter or a glass"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place on baking tray and glaze with a little milk. Bake for 15 mins, cool on a wire rack"
    }
  ],
