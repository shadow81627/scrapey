  "recipeIngredient": [
    "1 tsp worcestershire sauce",
    "2 pork chops",
    "1 tsp cider vinegar",
    "1 clove small garlic, chopped",
    "3 brussel sprouts",
    "0.25 onion, chopped",
    "1 tbs real ale",
    "2 mushrooms, washed and chopped",
    "1 tbs tomato paste"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Switch the oven on to warm up to 200 degrees c. Place the tomato paste, Worcestershire sauce, apple cider vinegar, garlic and beer in a bowl and mix together. Pour the mixture over the uncooked pork chops and leave for 30 mins to marinate."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer into a baking tin (with high sides) and add onions, brussel sprouts and mushrooms."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook in a hot oven for 15 mins."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the oven and place under the grill for 5 mins turning to ensure each side of the chops brown. Serve."
    }
  ],
