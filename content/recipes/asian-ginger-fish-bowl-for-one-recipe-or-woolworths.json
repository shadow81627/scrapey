  "recipeIngredient": [
    "150g Blue Grenadier",
    "150g broccolini",
    "0.5 cup water",
    "1 tsp crushed garlic (from jar this time)",
    "1 tsp WW dark balsamic vinegar",
    "1 tbs orange juice",
    "1 tsp soy sauce",
    "0.25 tsp sugar",
    "1 tsp toasted sesame seeds",
    "0.25 red onion, thinly sliced",
    "2 tbs white vinegar",
    "1 tsp crushed ginger (from jar)",
    "1 lemon, cut into wedges"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice red onion thinly and pickle in the vinegar with the sugar for 20 minutes. Put everything else except the fish, broccolini, sesame seeds and lemon wedge into a small saucepan and heat through until happily warmed and combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Microwave fish fillets in a small swish of water for one and a half minutes, then rest them."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Microwave broccolini for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Assemble with the broccolini being the base, the fish being on top. Pour the sauce around. Sprinkle toasted sesame seeds. Add lemon wedge."
    }
  ],
