  "recipeIngredient": [
    "1 cup coconut water",
    "1 large banana (sliced)",
    "1 large handful fresh spinach",
    "1/2 cup frozen mango",
    "1/2 cup frozen pineapple",
    "2 tablespoons shredded coconut",
    "1 tablespoon chia seeds"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Combine all ingredients in a high-powered blender and process until completely smooth. If it's too thick, add a little more coconut water. Alternatively, if the smoothie is too thin, add a few more pieces of frozen mango and pineapple."
    },
    {
      "@type": "HowToStep",
      "text": "Serve immediately."
    }
  ],
