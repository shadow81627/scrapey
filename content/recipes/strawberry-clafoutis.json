  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat oven to 350Â° F / 175o C. Grease a 2-quart baking dish."
    },
    {
      "@type": "HowToStep",
      "text": "Toss strawberries with cornstarch until evenly coated, then arrange berries, in bottom of dish and set aside."
    },
    {
      "@type": "HowToStep",
      "text": "In a food processor, mix eggs, milk, flour, granulated sugar, vanilla, and salt. Pour batter over strawberries."
    },
    {
      "@type": "HowToStep",
      "text": "Bake until puffed, golden brown, and set in the center, about 50 minutes. Serve warm."
    }
