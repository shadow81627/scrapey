  "recipeIngredient": [
    "1 1/2 cups strawberries (hulled)",
    "2 teaspoons cornstarch",
    "3 eggs",
    "1 cup milk",
    "2/3 cup all-purpose flour",
    "1/4 cup white sugar (organic for vegan-friendly)",
    "1 1/2 teaspoons vanilla extract",
    "1/4 teaspoon salt"
