  "recipeIngredient": [
    "600g potatoes, sliced into wedges",
    "1 tbs olive oil",
    "1 tbs truffle oil",
    "40g parmesan",
    "1 pinch black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 200°C/180°C fan-forced. Toss the potatoes in a bowl with the olive oil to coat well. Spread out on a baking tray and bake for 25 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the oven and drizzle over the truffle oil and sprinkle parmesan. Toss well to coat and place back in the oven for 10-12 minutes, until crisp and golden."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Season with cracked black pepper and serve."
    }
  ],
