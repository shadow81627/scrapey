  "recipeIngredient": [
    "1/2 cup water",
    "1/4 cup olive oil",
    "2 large lemons, juiced and then thinly sliced",
    "1 teaspoon kosher salt",
    "1 teaspoon dried oregano",
    "2 (12-ounce) salmon filets",
    "3 tablespoons fresh thyme leaves",
    "4 cloves garlic, minced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place the water, olive oil, lemon juice, salt, and oregano in a glass measuring cup and whisk to combine."
    },
    {
      "@type": "HowToStep",
      "text": "Place the lemon slices, salmon, thyme, and garlic in a large zip-top bag and pour the marinade over the salmon. Seal the bag and refrigerate at least 1 hour or overnight before cooking."
    }
  ],
