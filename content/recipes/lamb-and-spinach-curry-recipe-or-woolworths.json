  "recipeIngredient": [
    "750g lamb neck, boneless, trimmed and chopped into chunks",
    "1 medium onion, peeled and diced",
    "225g Greek yoghurt",
    "3 medium red chillies, sliced and seeded",
    "1 tbs garam masala",
    "1 tsp cumin",
    "4 cardamom pods, smashed",
    "2cm ginger, peeled and chopped",
    "100g spinach",
    "2 cloves garlic, peeled and sliced",
    "1 bunch coriander, fresh, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 140°C/120°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the lamb, yoghurt, spices, chillies, garlic, ginger, spinach and half the coriander in a heavy bottomed, oven proof casserole dish. Stir in 100ml of water."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place a layer of parchment over the top of the casserole, put the lid on and place in the oven for 3 hours."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the oven, stir well, top with the remaining coriander and serve with rice or naan bread."
    }
  ],
