  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180Â°C. Heat the oil in a large pan on medium high heat and cook the leeks and mushrooms for 3-4 minutes. Add the chicken thighs, garlic, mustard and tarragon and cook for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour over the chicken stock and stir in the creme fraiche and chopped tarragon. Cook for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "To assemble the pie, pour the chicken sauce into a large ovenproof dish and top with the puff pastry sheet. Brush the pastry with butter and bake for 30-35 minutes until puffed and golden brown."
    }
