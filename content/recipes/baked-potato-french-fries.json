  "recipeIngredient": [
    "1 large Russet",
    "2 teaspoons organic canola oil",
    "1/2 teaspoon salt",
    "1/4 teaspoon garlic powder",
    "a generous pinch chili powder",
    "2 Tablespoons cilantro finely chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Scrub the potato and cut into not more than and 1/4 inch thick wedges/fries."
    },
    {
      "@type": "HowToStep",
      "text": "Add to a big bowl, add oil and spices and toss well."
    },
    {
      "@type": "HowToStep",
      "text": "Spread out the wedges on a parchment lined baking sheet in single layer."
    },
    {
      "@type": "HowToStep",
      "text": "Bake in preheated 420 degrees F for 15 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Turn the tray and bake for another 5-10 minutes until golden or crisp to your preference."
    },
    {
      "@type": "HowToStep",
      "text": "Serve hot!"
    }
  ],
