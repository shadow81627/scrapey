  "recipeIngredient": [
    "500g wild rice, ready cooked",
    "2 large lemons, juice and zest",
    "3 tbs olive oil",
    "1 tsp honey",
    "50g pistachios, shelled and chopped",
    "1 bunch coriander, fresh, chopped",
    "100g pomegranate seeds",
    "4 spring onions, sliced",
    "100g feta, crumbled"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the rice according to pack instructions and tip into a large bowl. Stir in the lemon zest."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk the oil, lemon juice and honey together in a small bowl. Add the spring onions, chopped coriander, pistachios, pomegranate seeds and dressing to the rice and toss well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top with crumbled feta and serve."
    }
