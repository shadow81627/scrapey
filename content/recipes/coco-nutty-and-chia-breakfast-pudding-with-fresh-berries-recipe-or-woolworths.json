  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir the chia seeds into the coconut water in a glass jar or small bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Refrigerate for 15 minutes stirring occasionally."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add natural yogurt and stir well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top with granola, bananas and berries."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve chilled. Note: Berries can be replaced with sliced mango or other seasonal fruit."
    }
