  "recipeIngredient": [
    "1kg chinese roast pork (buy it at those chinese roast porks and roast ducks shops)",
    "340g dried fine rice vermicelli",
    "1 bunch small scallion (green onions)",
    "3 tbs olive oil",
    "3 tbs fish sauce",
    "3 tbs vinegar",
    "3 tbs water",
    "1.5 tbs sugar",
    "1 clove garlic",
    "1 chili (optional)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook fine rice vermicelli as per package instructions."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Wash scallion then finely chopped. Heat oil, fry garlic then add chopped scallion.Stir until cooked."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fish sauce: Combine fish sauce, water, vinegar and sugar. Stir until sugar dissolved. Optional for chili."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Reheat roast pork in the microwave. Bon appetit!"
    }
  ],
