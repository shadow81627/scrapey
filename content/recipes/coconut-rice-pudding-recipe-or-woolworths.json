  "recipeIngredient": [
    "150g pudding rice",
    "1.6 cup coconut milk, reduced fat",
    "1 cup almond milk, unsweetened",
    "2 tbs maple syrup",
    "1 tsp cinnamon",
    "1 tsp vanilla",
    "2 tbs coconut flakes, toasted"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add all the ingredients except the coconut flakes to a large, heavy bottomed saucepan and bring to the boil. Reduce the heat and simmer for 25 minutes, stirring occasionally, until the rice is cooked through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top with coconut flakes and serve."
    }
