  "name": "Silken Tofu Vegan Dressing",
  "prepTime": "PT10M",
  "recipeCategory": "Dressing",
  "recipeCuisine": "American",
  "recipeIngredient": [
    "1 lb. silken tofu",
    "2 tbsp. fresh chives (chopped)",
    "1/4 cup fresh parsley (chopped)",
    "3 garlic cloves (minced)",
    "1/4 tsp. onion powder",
    "1/4 tsp. salt",
    "1/4 tsp. pepper",
    "1/4 cup unsweetened (unflavored non-dairy milk)",
    "juice of 1 lemon (about 3 tbsp.)",
    "1 tbsp. vinegar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place tofu in blender or food processor and blend until smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Add remaining ingredients and blend until completely mixed."
    }
