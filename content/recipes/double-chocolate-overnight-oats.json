  "recipeIngredient": [
    "1 1/3 cup old-fashioned rolled oats ($0.24)",
    "2 Tbsp chia seeds (optional) ($0.44)",
    "2 Tbsp unsweetened cocoa powder ($0.07)",
    "2 Tbsp brown sugar ($0.08)",
    "1/4 cup mini chocolate chips ($0.32)",
    "1/4 tsp salt ($0.02)",
    "1 1/3 cup milk* ($0.27)",
    "1/2 tsp vanilla extract ($0.14)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Combine the oats, chia seeds, cocoa powder, brown sugar, chocolate chips, and salt in a bowl."
    },
    {
      "@type": "HowToStep",
      "text": "Add the milk and vanilla extract, then stir to combine. Allow the mixture to sit for about five minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Divide the oat mixture between four resealable containers. Close the containers and refrigerate overnight or up to five days."
    },
    {
      "@type": "HowToStep",
      "text": "Stir the oats just before enjoying. Eat cold or warm it up in the microwave."
    }
  ],
