  "recipeIngredient": [
    "150g butter, slightly softened",
    "0.5 cup brown sugar",
    "0.5 cup white sugar",
    "1 tsp vanilla essence",
    "0.25 tsp cinnamon",
    "1 egg",
    "1.5 cups plain flour",
    "0.25 cup self raising flour",
    "0.5 cup raspberry lollies, chopped",
    "0.75 cup milk chocolate melts",
    "0.25 cup raspberries, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180C, or 160C fan forced. Line 2 baking trays with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Use an electric mixer to mix the butter and sugars until creamy and well combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the vanilla and egg, and beat until well combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir in flours. Stir in raspberry lollies, raspberries and chocolate bits until well combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Roll into small balls and place on trays. Cook for about 15 minutes. Remove from oven and cool on the tray for a couple of minutes before transferring to a wire rack to cool completely."
    }
  ],
