  "recipeIngredient": [
    "0.5 small avocado",
    "2 red capsicums, deseeded, cut into thick strips",
    "3 Lebanese eggplants, thinly sliced lengthways",
    "300g kent pumpkin, thinly sliced",
    "2 red onions, cut into wedges",
    "2 zucchini, thinly sliced lengthways",
    "0.25 cup raw unsalted cashews, soaked overnight, drained",
    "2 tbs small basil leaves (to garnish)",
    "280g artichoke hearts in brine, drained",
    "2 tbs lemon juice",
    "0.25 cup olive oil",
    "1 clove small garlic, crushed",
    "2 tbs continental parsley leaves (to garnish)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat a barbecue grill on high heat. Cook capsicum, onion, pumpkin, eggplant, zucchini and artichokes, in batches, until browned and tender."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, to make the dressing, process avocado, oil, cashews, lemon juice, garlic and 1/4 cup water in a small food processor until smooth and combined. Season."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place chargrilled vegetables on a serving plate, drizzle with dressing, and serve scattered with basil and parsley leaves."
    }
  ],
