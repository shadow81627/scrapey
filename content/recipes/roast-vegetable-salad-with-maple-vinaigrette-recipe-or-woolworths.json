  "recipeIngredient": [
    "1 tbs maple syrup",
    "2 tbs red wine vinegar",
    "0.25 cup extra virgin olive oil",
    "1 tsp Dijon mustard",
    "120g baby rocket leaves",
    "320g mixa-mato tomatoes, halved",
    "500g leftover roasted vegetables (see Tip)",
    "150g feta, crumbled",
    "0.33 cup roasted almonds, chopped",
    "2 tbs small basil leaves (to serve)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place syrup, vinegar, oil and mustard in a screwtop jar. Season, secure lid and shake well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place rocket, tomato and roasted vegetables in a large bowl. Top with feta, almonds and basil. Serve drizzled with maple vinaigrette."
    }
