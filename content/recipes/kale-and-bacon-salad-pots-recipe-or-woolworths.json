  "recipeIngredient": [
    "100g bacon lardons",
    "40g salad dressing",
    "4 eggs, free range",
    "80g kale, washed, drained and chopped",
    "1 small red onion, peeled and diced",
    "12 baby tomatoes, halved"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a non-stick pan to a medium heat and fry the bacon lardons for 5 minutes until cooked."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Boil the eggs for 7 minutes in a pot of water and then crack open and chop."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine all ingredients in a mixing bowl and then divide into 4 sealable pots."
    }
