  "recipeIngredient": [
    "340g Barilla Penne Rigate",
    "190g Barilla Pesto Genovese",
    "24 cherry tomatoes",
    "2 tbs extra virgin olive oil",
    "1 tsp basil (to serve)",
    "pinch salt (to serve)",
    "1 tsp sugar (to serve)",
