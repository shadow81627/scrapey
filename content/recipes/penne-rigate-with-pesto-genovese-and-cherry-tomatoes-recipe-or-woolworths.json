  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Halve the tomatoes and cook in a pan with basil, a pinch of salt and olive oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, in a large pot of boiling salted water, cook the Penne according to the instructions on the pack. Once cooked al dente, drain and set aside some pasta water."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toss the pasta in a pan together with some of the reserved cooking water, to emulsify the sauce."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with fresh basil."
    }
  ],
