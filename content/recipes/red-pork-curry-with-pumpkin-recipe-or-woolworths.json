  "recipeIngredient": [
    "1.5 tbs sugar",
    "0.5 jar Valcom Red Curry Paste",
    "1 tbs Squid Brand Fish Sauce",
    "0.5 cup basil leaves",
    "1 sprigs coriander (for garnish)",
    "1 tbs oil",
    "400g pork scotch fillet",
    "400ml TCC Premium coconut milk",
    "1 cup pumpkin"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil over medium heat, add red curry paste and stir-fry for 1 minute or until oil appears."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add half of the coconut milk and cook for 2 minutes, stirring frequently."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add pumpkin and cook for further 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add pork, the remaining coconut milk, sugar and fish sauce and bring to boil, then keep boiling for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add basil and stir through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from heat and garnish with coriander."
    }
  ],
