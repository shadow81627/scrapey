  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Peel and chop 800g potatoes and boil until tender. Mash with 30g butter. Cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drain a can of red salmon or tuna, flake and add to potato with chopped parsley, 1 egg and grated lemon rind. Season with salt and pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Shape into rounds and coat in plain flour, beaten egg and dry breadcrumbs."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry until golden."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with lemon wedges."
    }
  ],
