  "recipeIngredient": [
    "200g butter, softened",
    "2 tsp Dijon mustard",
    "2 tsp chives, fresh, chopped",
    "1 bunch parsley, fresh, chopped",
    "2 cloves garlic, peeled and minced",
    "1 tsp Worcestershire sauce",
    "2 tsp parmesan",
    "1 lemon, 1 tsp of zest",
    "0.5 tsp sea salt",
    "1 pinch cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add all of the ingredients to a large bowl and mix well with a wooden spoon."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the softened butter on a piece of parchment paper and shape into a log. Wrap tightly with parchment and twist the ends to secure."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Chill in the fridge for 2 hours before using."
    }
  ],
