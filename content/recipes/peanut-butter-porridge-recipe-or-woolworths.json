  "recipeIngredient": [
    "2 cup Milk",
    "1 tsp vanilla extract",
    "4 ripe bananas",
    "1 tsp cinnamon",
    "2 tbs unsalted peanuts, dry roasted",
    "120g Macro organic quick oats",
    "2 tbs natural peanut butter"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mash two bananas in a medium bowl. Add oats, milk, cinnamon and vanilla and stir to combine. Microwave for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add 1 tbs of peanut butter, stir to combine, and microwave for a further 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice over remaining bananas, top with remaining peanut butter and sprinkle with peanuts to serve."
    }
  ],
