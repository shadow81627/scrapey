  "recipeIngredient": [
    "2 cup ice",
    "1 cup mint",
    "1 cup fresh mango",
    "1 orange",
    "1 tbs lime",
    "1.5 cup Schweppes Soda Water",
    "1 cup Nudie 100% Orange Juice"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Blend the orange juice with fresh mango and lime juice."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour mint leaves into a jug, muddle the mint with a muddler, or a wide handled wooden utensil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the liquids, stir to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Garnish each glass with orange rings, and 1/2 cup ice."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour virgin mojito over the ice in each cup."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve and enjoy!"
    }
  ],
