  "recipeIngredient": [
    "160g baby tomatoes, quartered",
    "1 small red onion, diced",
    "4 cloves garlic, peeled and chopped",
    "1 tbs basil, chopped",
    "1 medium sourdough bread, sliced at an angle for long slices",
    "2 tbs olive oil",
    "1 tbs balsamic vinegar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a mixing bowl, combine the tomatoes with the onion, garlic and basil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Equally spread the tomato mix over the slices of bread and bake in the oven for 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the oven and drizzle with olive oil and balsamic then garnish with basil."
    }
  ],
