  "keywords": "Bake",
  "name": "Potato-topped Beef Bake Recipe | Woolworths",
  "prepTime": "PT10M",
  "recipeCategory": "Lunch,Dinner",
  "recipeCuisine": "Australian",
  "recipeId": "a0K2y00000119MVEAY",
  "recipeIngredient": [
    "500g lean beef mince",
    "350 jar leggo's vegetable stir through sauce",
    "60g spinach leaves",
    "600g frozen scalloped potatoes",
    "1 cup shredded tasty cheese"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/m3kdul9pn7997ucjjaqn?$foodhub-method-images$&fmt=png-alpha",
      "text": "Grease a medium-large oven proof/casserole dish and pre-heat oven to 200°C."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/dhwqoqmqmluplhe4nqo6?$foodhub-method-images$&fmt=png-alpha",
      "text": "Break up and brown mince over a high heat in frying pan until mince cooked through. Stir in sauce and 1/4 cup of water. Bring to boil and add spinach. Stir until wilted. Season with salt and pepper and remove from heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/eyhxmv51wuyznckoy13z?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add mixture to oven-proof dish, top with scalloped potatoes (layering evenly) and sprinkle with cheese."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake for 30 minutes (200°C) or until potatoes are cooked and cheese is golden."
    }
  ],
