  "recipeIngredient": [
    "30g Lewis Road Creamery salted butter",
    "750ml milk",
    "10g white sugar",
    "7g Tandaco Yeast Dry",
    "500g plain flour",
    "1 tsp Saxa Table Salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Warm the milk (warm to the touch not hot)Stirin yeastto warm milk then leave it to disolve."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix the remaining ingredients together."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the wet mixture to the dry and combinewell to create a batter type consitency."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Leave mixture in a warm place for 6090 minutes or ubtil it has doubled in size."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "This is what helps create the open texture."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Scoop the mixture into a wellgreased medium to high heat pan with butter.Cook each side for2-3 minutes or until golden rown."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with Lewis Road Creamery salted butter."
    }
  ],
