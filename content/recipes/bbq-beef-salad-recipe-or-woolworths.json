  "recipeIngredient": [
    "2 tbs creamed horseradish",
    "0.5 cup sour cream",
    "2 fennel bulbs, thinly sliced",
    "1 tbs white wine vinegar",
    "400g seed and grain sourdough",
    "150g semi sundried tomatoes",
    "0.5 cup olives",
    "1 tbs chives, chopped",
    "200g salad leaves",
    "8 sirlion steaks (approx 1cm thick)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Brush sliced fennel and steaks with olive oil. Place onto a hot BBQ, cook for 1 minute on each side. Set aside, while making salad."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "To make the salad, toss together salad leaves, olives, tomatoes and barbecued fennel."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "For the dressing, mix together in a bowl: Horseradish, sour cream, vinegar and chives."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place steak on the plate and top with salad and dressing; then repeat to create a stacking effect."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with a slice of fresh wholegrain sourdough bread."
    }
  ],
