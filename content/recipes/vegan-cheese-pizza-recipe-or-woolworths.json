  "recipeIngredient": [
    "2 large vegan pizza base, thawed if frozen",
    "1 medium butternut squash, peeled and sliced into cubes",
    "100g green olives, pitted and halved",
    "100g vegan mozzarella, grated or chopped",
    "0.8 cup tomato paste",
    "100g baby tomatoes, halved",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "1 tbs olive oil",
    "1 tbs canola oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan to a medium heat and add the canola oil and butternut squash and fry for 8 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lay out the pizza base and evenly spread the tomato paste on top."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Evenly place the rest of the ingredients and the fried butternut squash on top of the paste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in the oven for 20 minutes or until the pizza begins to crisp at the edges."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the oven and serve immediately."
    }
  ],
