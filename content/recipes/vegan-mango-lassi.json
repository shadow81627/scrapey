  "recipeIngredient": [
    "2 cups ripe diced mango (about 2 medium mangoes)",
    "1 (5.3 ounce) container unsweetened and unflavored non-dairy yogurt",
    "1/2 cup full-fat coconut milk",
    "2 tablespoons maple syrup, (or to taste)",
    "1/4 teaspoon ground cardamom",
    "Pinch salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place all ingredients into a blender. Blend until smooth, stopping to scrape down sides of the pitcher as needed."
    },
    {
      "@type": "HowToStep",
      "text": "Taste-test and add more maple syrup if desired. Blend again."
    },
    {
      "@type": "HowToStep",
      "text": "Pour into glasses and serve."
    }
