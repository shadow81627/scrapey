  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/puab40je43tqyhsf9idi?wid=4032&hei=3024&fmt=png-alpha",
      "text": "Finely grate beetroot. To avoid stained hands, I used a food processor to do this."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/bu4pzcahvz139w9joivw?wid=4032&hei=3024&fmt=png-alpha",
      "text": "Place grated beetroot in a non-stick fry pan. Add a knob of butter and some sugar (to taste). The butter and sugar are optional, but it is the butter that'll give the beetroot a beautiful and creamy taste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/yhbwpv2lgzyxxstuvibi?wid=4032&hei=3024&fmt=png-alpha",
      "text": "Using medium heat, mix and cook until butter is melted and beetroot is warmed through. This should take about 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/kifmbqhdgdxmum4q1h3d?wid=4032&hei=3024&fmt=png-alpha",
      "text": "Serve beetroot as an accompaniment to barbecued meat, schnitzels or rissoles and rice, pasta or mashed potatoes."
    }
