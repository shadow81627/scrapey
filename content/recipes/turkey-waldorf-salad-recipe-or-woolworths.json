  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place cranberries, onion and orange juice in a bowl. Stand for 5 minutes to allow cranberries to soften. Strain over a small bowl to reserve juice. Add mayonnaise to juice and stir to combine. Season."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Arrange spinach, celery, avocado, cabbage and turkey on a platter. Drizzle with orange mayonnaise. Serve topped with cranberry mixture, walnuts and parsley."
    }
