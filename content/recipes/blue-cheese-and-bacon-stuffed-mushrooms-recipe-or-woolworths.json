  "recipeIngredient": [
    "2 large flat mushrooms",
    "1 small onion",
    "1 bacon rasher",
    "1 tomato",
    "60g blue cheese",
    "10ml oil or spray oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180 degrees."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Finely chop onion, bacon, and tomato. Mix in crumbled cheese."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Carefully remove mushroom stalks. Lightly spray or brush top of mushrooms with oil. Place on oven tray with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Firmly press filling onto mushrooms."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake about 20-25 minutes until mushrooms are tender and tops are golden."
    }
  ],
