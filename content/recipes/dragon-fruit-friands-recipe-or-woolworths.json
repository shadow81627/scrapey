  "recipeId": "a0K2y000000pVv9EAE",
  "recipeIngredient": [
    "1.25 cup almond meal",
    "0.25 cup plain flour",
    "0.25 cup sugar",
    "5 egg whites",
    "150g unsalted butter",
    "1 tsp vanilla extract",
    "400g Woolworths frozen dragon fruit chunks"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Defrost frozen dragon fruit chunks. Preheat oven to 180°C. Grease muffin tin."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Melt butter in microwave. Cool it then stir in vanilla extract."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sieve flour then add almond meal and sugar. Flatten any lumps. Mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add egg whites, butter and dragon fruit to the flour mixture. Mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon the mixture into the muffin tin."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pop it in the oven for 6 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Turn the heat down to 150°C then cook for another 16 minutes until cooked."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Turn oven off. Leave the tin in the oven for another 4 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from oven. Leave them cool down."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Optional - serve friands with whipped cream."
    }
  ],
