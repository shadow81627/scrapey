  "recipeCuisine": "English",
  "recipeId": "RGWW338192",
  "recipeIngredient": [
    "300g spinach, washed and drained",
    "2 tbs olive oil",
    "200g artichoke hearts, drained and chopped",
    "4 cloves garlic, peeled and mashed",
    "2 tbs butter",
    "1 tsp cracked black pepper",
    "8 slices bread",
    "60g parmesan, grated"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a large saucepan to a medium heat and add the olive oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the garlic and chopped artichoke hearts in the oil for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the butter to the pan and allow to melt for 1 minute."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once the butter has melted, add the spinach and cook by pressing the leaves down and tossing regularly for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toast the sourdough slices for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drain any excess liquid from the pan and place the spinach and artichoke mix on the bread and add pepper and parmesan before serving."
    }
  ],
