  "recipeIngredient": [
    "8 ounces fresh asparagus",
    "1 tablespoon good-quality olive oil",
    "1/2 teaspoon fresh lemon juice",
    "2 ounces Pecorino Romano cheese, diced and smashed into little bits",
    "a few cracks fresh black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Snap off the root ends of the asparagus and slice it cross-wise into 1/4-inch to 1/2-inch long pieces."
    },
    {
      "@type": "HowToStep",
      "text": "In a medium bowl whisk together the oil and lemon juice. Toss in the asparagus and cheese. Combine well."
    },
    {
      "@type": "HowToStep",
      "text": "Divide into small cups, ramekins or bowls between 4 and 8 ounces and top with black pepper."
    }
  ],
