  "recipeIngredient": [
    "30g butter, softened",
    "2 tbs caster sugar",
    "30g ground almonds",
    "1 egg, yolk",
    "1 tbs double cream",
    "20 baked pastry tartlets",
    "20 cherries",
    "2 tsp icing sugar, for dusting"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180 ̊C."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine the butter, sugar, almonds, egg yolk, and cream until well blended. Divide evenly among tartlets."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place 1 cherry on top of each tartlet. Bake until set and golden, 15 minutes. Cool completely. Dust with icing sugar to garnish. Serve at room temperature."
    }
  ],
