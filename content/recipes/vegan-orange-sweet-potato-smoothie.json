  "recipeIngredient": [
    "1 cup of unsweetened almond milk (I use Silk brand)",
    "2 small oranges (clementine oranges work well – peeled and seeds removed)",
    "1 baked medium sweet potato (approximately one cup – do not use the peel)",
    "1/8 teaspoon of fresh ginger – minced",
    "1 medjool date – pitted",
    "1/8 teaspoon of ground cinnamon",
    "3 ice cubes"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Blend all of the ingredients in a high powered blender until smooth and creamy."
    },
    {
      "@type": "HowToStep",
      "text": "Enjoy!"
    },
    {
      "@type": "HowToStep",
      "text": "NOTE: If you like your smoothie thinner add more almond milk. If you don’t like fresh ginger, leave it out. Make this your own."
    }
  ],
