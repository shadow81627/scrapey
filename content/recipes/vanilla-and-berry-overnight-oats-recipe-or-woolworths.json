  "recipeIngredient": [
    "200g oats",
    "2.4 cups oat milk, unsweetened",
    "300g mixed berries, frozen",
    "1 tbs maple syrup",
    "1 tbs sunflower seeds",
    "0.5 tsp vanilla extract"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Blitz the oats and sunflower seeds in a food processor and tip into a bowl with milk, 3/4 of the frozen berries, maple syrup and vanilla extract and mix well. Cover and place in the fridge overnight."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon the oats into bowls or jars and top with the rest of the berries."
    }
  ],
