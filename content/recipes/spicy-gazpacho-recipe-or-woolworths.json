  "recipeIngredient": [
    "300g tomatoes",
    "100g roasted red capsicums, drained",
    "1 clove garlic, peeled and minced",
    "2 tbs sherry vinegar",
    "0.6 cup passata",
    "1 medium red chilli, sliced",
    "1 small red onion, peeled and finely diced",
    "0.25 medium cucumber, finely sliced",
    "1 pinch cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the tomatoes, red capsicums, garlic, sherry vinegar, passata and red chilli in a blender and blitz until smooth."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add 500ml of cold water and blend to your desired consistency. Season with black pepper and salt if desired, cover and chill in the fridge for 1 hour."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve in bowls with cracked black pepper, finely sliced cucumber and diced onion on top."
    }
  ],
