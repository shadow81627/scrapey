  "recipeIngredient": [
    "500g brown rice, ready cooked",
    "2 large limes, juice and zest",
    "800g mixed beans, drained",
    "2 tsp maple syrup",
    "340g baby corn, chopped",
    "1 bunch coriander, fresh, chopped",
    "1 large red capsicum, diced",
    "4 spring onions, sliced",
    "100g feta, crumbled"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the rice according to pack instructions and tip into a large bowl. Stir in the lime zest."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the mixed beans, spring onions, baby corn, capsicum, chopped coriander, lime juice and maple syrup to the rice and toss well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top with crumbled feta and serve."
    }
  ],
