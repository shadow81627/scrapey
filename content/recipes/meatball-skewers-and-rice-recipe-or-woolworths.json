  "recipeIngredient": [
    "1 cup long grain rice",
    "0.5 cup frozen peas",
    "1 small red capsicum, finely diced",
    "1 small carrot, finely diced",
    "1 brown onion, chopped",
    "5ml extra virgin olive oil spray",
    "1 tbs tomato sauce",
    "0.5 tsp Italian herbs",
    "3 cup chicken stock",
    "2 tbs parmesan, finely grated",
    "200g beef mince (premium)",
    "0.25 cup panko breadcrumbs"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180Â°C. Line a tray with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine mince, parmesan, sauce and herbs in a bowl. Roll into balls. Coat with breadcrumbs. Thread onto skewers and place onto tray. Spray with oil and bake for 20 minutes or until golden and cooked through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, heat oil in a small saucepan over medium heat. Add onion, capsicum and carrot. Cook for 5 minutes or until softened. Stir in rice and stock. Cook for 10 minutes or until craters form. Add peas. Cover and simmer for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve meatballs with rice."
    }
  ],
