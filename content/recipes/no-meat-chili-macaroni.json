  "recipeIngredient": [
    "1 onion (finely chopped)",
    "1 Black Pepper",
    "1 lb ground soy (like Yves)",
    "1 tbsp parsley",
    "salt and pepper (to taste)",
    "1 garlic clove (crushed)",
    "1 can (1.36 l tomato juice)",
    "1 can diced tomato",
    "3 cups uncooked macaroni",
    "1 cup mozzarella (shredded)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Fry the onion and pepper in a pan. Add the ground soy and separate into small pieces."
    },
    {
      "@type": "HowToStep",
      "text": "Add the spices as well as the garlic."
    },
    {
      "@type": "HowToStep",
      "text": "Add tomato juice, just enough to cover the ground soy. Simmer until the juice has been absorbed."
    },
    {
      "@type": "HowToStep",
      "text": "In another saucepan, cook the pasta."
    },
    {
      "@type": "HowToStep",
      "text": "Add the rest of the tomato juice and the diced tomato to the ground soy mixture and continue to simmer for 15 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Once the pasta is cooked and drained, add the tomato and soy mixture."
    },
    {
      "@type": "HowToStep",
      "text": "Add cheese and mix well."
    },
    {
      "@type": "HowToStep",
      "text": "Serve."
    }
  ],
