  "recipeIngredient": [
    "1/4 cup all-purpose flour",
    "1 tbsp white sugar (organic for vegan-friendly)",
    "1/4 tsp baking powder",
    "1 pinch salt",
    "1/4 cup milk",
    "1 tbsp melted butter",
    "1/2 tsp vanilla extract",
    "1/4 cup cream cheese",
    "3 tbsp water",
    "1/4 tsp ground cinnamon"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "In a measuring cup, combine the flour, sugar, baking powder, and salt. Add milk, butter, and vanilla while whisking. Pour the dough into a coffee cup."
    },
    {
      "@type": "HowToStep",
      "text": "Microwave the mug cake on high for 1 to 2 minutes or until cake is well inflated."
    },
    {
      "@type": "HowToStep",
      "text": "In another small bowl, mix cream cheese and water. Microwave frosting till liquid, about 40 seconds."
    },
    {
      "@type": "HowToStep",
      "text": "Pour icing over the cake and sprinkle with cinnamon. Serve."
    }
  ],
