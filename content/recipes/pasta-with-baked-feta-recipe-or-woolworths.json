  "recipeIngredient": [
    "200g Dodoni Feta",
    "600g cherry tomatoes",
    "250g penne pasta",
    "1 tbs extra virgin olive oil",
    "1 pinch salt, to season",
    "1 pinch pepper",
    "1 pinch chilli flakes",
    "1 tsp garlic, finely chopped",
    "2 tsp fresh basil",
    "100g pitted kalamata olives"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat oven to 200 degrees/180 degrees fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drizzle olive oil over a large, shallow baking tray."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place Dodoni feta in the centre of the tray, and the cherry tomatoes surrounding the feta. Drizzle with oil once more. Sprinkle chilli flakes to taste and season with salt and pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake at 200 degrees for 35 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, cook pasta according to the directions on the pack"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove the baking tray from the oven, add garlic and mix with a fork."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add cooked pasta, olives and basil and continue to mix."
    }
  ],
