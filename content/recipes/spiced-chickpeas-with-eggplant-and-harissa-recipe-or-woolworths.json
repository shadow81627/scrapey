  "recipeIngredient": [
    "1 red capsicum, deseeded, sliced",
    "2 tbs coriander leaves (to garnish)",
    "1 eggplant, thinly sliced",
    "1 small red onion, chopped",
    "1 tsp ground coriander",
    "2 tsp ground cumin",
    "2 tsp smoky paprika",
    "400g can chickpeas no added salt, drained, rinsed",
    "0.25 cup hummus (to serve)",
    "sourdough bread, chargrilled (to serve)",
    "1 tsp dried harissa",
    "400g can crushed tomatoes",
    "2 tbs olive oil",
    "1 clove garlic, crushed"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat half the oil in a frying pan over medium heat. Add onion, capsicum and garlic and cook for 3 minutes until softened."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add spices and cook for 1 minute. Stir in tomatoes and chickpeas."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, brush eggplant with remaining oil. Cook on a chargrill for 1 minute each side until softened."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place eggplant on top of tomato mixture. Garnish with coriander and serve with hummus and chargrilled bread."
    }
  ],
