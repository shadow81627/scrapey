  "recipeIngredient": [
    "8 oz orzo",
    "1 cup green beans (trimmed)",
    "2 ears corn (kernels removed)",
    "2 cloves garlic (minced)",
    "1 cup grape or cherry tomatoes (halved)",
    "1 tbsp olive oil",
    "2 tbsp white wine vinegar",
    "1/4 cup torn basil leaves",
    "salt and pepper (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Cook orzo according to package. Two minutes before cooking time is done, add green beans to pot. Drain in a fine mesh colander and rinse with cold water."
    },
    {
      "@type": "HowToStep",
      "text": "Transfer orzo and green beans to a large bowl. Stir in remaining ingredients and season with salt and pepper to taste."
    }
  ],
