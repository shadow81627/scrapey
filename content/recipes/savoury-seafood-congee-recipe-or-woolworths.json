  "recipeId": "RGWW338290",
  "recipeIngredient": [
    "300g rice, rinsed",
    "1 tbs sesame oil",
    "80g mushrooms, sliced",
    "1 leeks, sliced",
    "80g peas",
    "12 medium prawns, de-shelled and sinew removed",
    "12 calamari rings",
    "24 mussels",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "1.2 cups vegetable stock",
    "2 tbs ginger, chopped",
    "1 tbs ras el hanout",
    "0.2 cup cooking white wine"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the rice as per packet instructions and then set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a saucepan to a medium heat and then add the sesame oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the mushrooms, leeks and peas to the pot and cook for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the seafood and cook for a further 5 minutes, turning regularly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients, including the cooked rice, to the pot, bring to a simmer and cook for a further 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once reduced by about half remove from the heat and serve."
    }
  ],
