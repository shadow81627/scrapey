  "recipeIngredient": [
    "5 cups sliced peaches",
    "2 tablespoons sugar",
    "1 tablespoon cornstarch",
    "1 teaspoon cinnamon",
    "1 1/4 cups flour",
    "3/4 cup sugar",
    "1 teaspoon baking powder",
    "1/2 teaspoon salt",
    "2/3 cup melted vegan butter",
    "3 tablespoons unsweetened almond milk"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat oven to 350 F"
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/06/vegan-peach-cobbler-2-of-20.jpg",
      "text": "In a large bowl, combine the sliced peaches, sugar, cornstarch and cinnamon together."
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/06/vegan-peach-cobbler-3-of-20.jpg",
      "text": "toss until all the peaches are well coated."
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/06/vegan-peach-cobbler-5-of-20.jpg",
      "text": "Transfer the peaches to a 2 quart baking dish; set aside"
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/06/vegan-peach-cobbler-4-of-20.jpg",
      "text": "In a separate bowl, combine the flour, sugar, baking powder, and salt. Whisk together until mixed."
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/06/vegan-peach-cobbler-6-of-20.jpg",
      "text": "Pour in the melted vegan butter and almond milk."
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/06/vegan-peach-cobbler-7-of-20.jpg",
      "text": "Mix until dry ingredients are just moistened and forms like a cake batter."
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/06/vegan-peach-cobbler-8-of-20.jpg",
      "text": "Spoon the cake batter over the peaches"
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/06/vegan-peach-cobbler-9-of-20.jpg",
      "text": "Bake in preheated oven for 50-55 minutes, or until the filling is bubbly and the cake is golden"
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/06/vegan-peach-cobbler-17-of-20.jpg",
      "text": "Serve warm with vegan whipped cream if desired"
    }
  ],
