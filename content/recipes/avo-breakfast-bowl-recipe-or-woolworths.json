  "recipeIngredient": [
    "1 frozen banana, chopped",
    "1 tbs pumpkin kernels",
    "1 lime, halved",
    "0.75 cup coconut water",
    "1 avocado, halved",
    "30g kale leave & spinach"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Scoop flesh from 1/2 the avocado into a blender. Add banana, leaf mix (reserve a few) and coconut water."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add juice of 1/2 the lime into the blender. Blend for 30 seconds or until mixture is smooth and creamy. Pour into bowls."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice remaining avocado half and arrange on top with reserved salad leaves and pumpkin kernels. Serve bowls with remaining lime half."
    }
  ],
