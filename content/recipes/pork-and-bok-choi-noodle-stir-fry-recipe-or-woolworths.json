  "recipeIngredient": [
    "2 tbs canola oil",
    "500g pork fillet, cut into strips",
    "250g rice noodles",
    "2 cloves garlic, peeled and minced",
    "5cm ginger, peeled and minced",
    "4 heads bok choi, shredded",
    "1 tbs soy sauce"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the noodles according to pack instructions. Heat a tablespoon of canola oil in a large wok on high heat. Sear the pork for 4 minutes, browning on all sides, and remove with a slotted spoon. Set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the rest of the oil and stir fry the garlic, ginger and bok choi for 3 minutes. Tip the pork back into the pan and cook for 3-4 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the noodles to the wok with soy sauce and cook for 1 minute."
    }
  ],
