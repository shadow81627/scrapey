  "recipeIngredient": [
    "1 1/2 cups almond flour",
    "1/2 cup coconut flour",
    "1 teaspoon baking powder",
    "4 egg whites",
    "1/2 cup brown sugar",
    "1/2 cup butter",
    "1 teaspoon almond extract",
    "1/2 cup of flaked almonds"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Mix almond flour, flour, and baking powder in a bowl."
    },
    {
      "@type": "HowToStep",
      "text": "In a bowl, whisk the egg whites into soft peaks. Add 1 tablespoon sugar and continue whisking until the whites are firm, smooth, and shiny."
    },
    {
      "@type": "HowToStep",
      "text": "Add the melted butter & the remaining sugar to the flour mixture."
    },
    {
      "@type": "HowToStep",
      "text": "Mix together by lifting the preparation with a spatula. Stir in almond extract."
    },
    {
      "@type": "HowToStep",
      "text": "Pour the dough into a round mold 22 cm in diameter. Sprinkle with flaked almonds & powder with 2 tablespoons sugar."
    },
    {
      "@type": "HowToStep",
      "text": "Preheat the oven to 375Â°F, slide the cake in the oven, and cook for 50 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Let stand 5 minutes before unmolding."
    }
  ],
