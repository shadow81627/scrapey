  "recipeIngredient": [
    "1 medium zucchini, spiralised",
    "200g bocconcini mozzarella, drained",
    "200g cherry tomatoes, halved",
    "2 medium shallots, sliced",
    "1 clove garlic, peeled and finely diced",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "2 tbs olive oil",
    "2 tbs balsamic vinegar",
    "4 sprigs basil (to garnish)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spiralise the zucchini and drain some of the excess liquid by pressing in some kitchen paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice the veg into bit size portions."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine all ingredients in a mixing bowl and garnish with a spring of basil."
    }
  ],
