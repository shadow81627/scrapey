  "recipeIngredient": [
    "2 avocados, sliced",
    "0.125 medium red cabbage, shredded",
    "0.5 red capsicum, deseeded, diced",
    "2 carrots, grated",
    "0.25 cup red wine vinegar",
    "425g corn cobettes",
    "0.5 bunch kale, leaves torn",
    "500g De Cecco Penne Rigate No 41",
    "0.5 cup olive oil",
    "0.5 bunch oregano, chopped",
    "crusty baguette, optional"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook pasta according to packet instructions. Drain."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, pierce packaging of corn cobettes and microwave for 5 minutes. Allow to cool slightly then remove kernels."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toss together pasta, kale, capsicum, corn kernels, carrot and cabbage in a large serving bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "To make dressing, combine olive oil, red wine vinegar and oregano and season to taste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toss dressing through salad and top with avocado. Serve immediately with sliced baguette if desired."
    }
  ],
