  "recipeIngredient": [
    "1kg pork belly",
    "3 tbs salt",
    "1 tbs white pepper",
    "2 tbs five-spice powder",
    "1 tbs white vinegar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Score the pork rind half an inch apart."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/ftczi8sby36lhqneclpq?wid=4032&hei=3024&fmt=png-alpha",
      "text": "Pierce the meat with the needle meat tenderizer"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Rub salt into the meat, then the pepper and the five-spice powder."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Rub at least 2 tablespoons of salt into the rind."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Season overnight in the fridge, uncovered, rind side up."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once seasoned, place the pork belly on the wire rack, rind side down and cook at 180°C for 50 minutes in the oven."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/sqn8fdracfkpqrgxzjle?wid=4032&hei=3024&fmt=png-alpha",
      "text": "Then flip the pork belly, rind side up, rub 1 tablespoon of white vinegar on the rind, and cook at 240°C for 20 minutes in the oven."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/arqoq40sitjhuwqf9k9o?wid=2625&hei=2019&fmt=png-alpha",
      "text": "Allow to rest for 10 minutes, then ready to serve."
    }
  ],
