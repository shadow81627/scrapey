  "recipeIngredient": [
    "3 tbs frozen coconut yoghurt",
    "1 tsp hemp seeds",
    "2 tsp shredded coconut",
    "1 tbs chia seeds",
    "1 tbs LSA",
    "2 large frozen bananas",
    "1 tbs cacao powder",
    "0.5 frozen zucchini",
    "500ml water"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add banana, zucchini, coconut yoghurt, cacao powder, chia seeds, LSA and water into a blender. Blitz until all ingredients are smooth and thick consistency."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour into two large glasses. Sprinkle hemp seeds & coconut on top and enjoy."
    }
  ],
