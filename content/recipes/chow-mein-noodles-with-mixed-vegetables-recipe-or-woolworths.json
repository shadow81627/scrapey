  "recipeIngredient": [
    "400g chow mein noodles, cooked as per packet instructions",
    "3 tbs sesame oil",
    "3 tbs oyster sauce",
    "3 tbs soy sauce",
    "4 medium white onion, sliced thinly",
    "2 medium capsicum, de-seeded and sliced",
    "200g mushrooms, sliced",
    "2 cloves garlic, peeled and mashed",
    "100g mangetout",
    "100g zucchini, sliced into strips",
    "100g carrots, sliced into strips",
    "2 tbs lemon juice"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the noodles as per packet instructions and then drain and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a large pan to a medium heat and then add the sesame oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the onions, garlic and mushrooms for 5 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the mangetout peppers, zucchini and carrots and fry for a further 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients to the pan, fry while tossing for 3 more minutes before serving."
    }
  ],
