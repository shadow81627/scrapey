  "name": "Blueberry and Banana Muffins",
  "prepTime": "PT20M",
  "recipeIngredient": [
    "2 cups whole wheat flour",
    "1 tsp baking powder",
    "1 tsp baking soda",
    "1/2 tsp salt",
    "1/4 tsp cinnamon",
    "4 bananas (peeled and pounded)",
    "3/4 cups brown sugar",
    "1/4 cup coconut oil (melted)",
    "1/2 cup milk",
    "2 tsp vanilla extract",
    "1 cup blueberries"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Mix the first 5 ingredients in a bowl and the other ingredients in another bowl."
    },
    {
      "@type": "HowToStep",
      "text": "Add the dry ingredients gently into the wet ingredients, and mix until combined."
    },
    {
      "@type": "HowToStep",
      "text": "Gently add the blueberries."
    },
    {
      "@type": "HowToStep",
      "text": "Fill the muffin cups up to 3/4 of the mold."
    },
    {
      "@type": "HowToStep",
      "text": "Bake at 350°F until a toothpick comes out clean—about 25 minutes."
    }
  ],
