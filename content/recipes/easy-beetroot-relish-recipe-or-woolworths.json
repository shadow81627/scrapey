  "recipeIngredient": [
    "3 medium beetroots (medium size)",
    "2 tbs horseradish cream",
    "2 tsp capers",
    "sprinkle of fresh dill"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Wash three medium sized beetroot. Keep the skin on and keep the beetroot whole. Steam until softened. Set aside to cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once cooled, peel the beetroot and grate. You may want to wear gloves so your hands are not stained red!"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the grated beetroot into a bowl, add horseradish cream and chopped capers and mix through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sprinkle some fresh dill on top."
    }
  ],
