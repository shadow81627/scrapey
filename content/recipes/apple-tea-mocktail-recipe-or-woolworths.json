  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place tea bags in a jug. Pour over 2 cups of boiling water. Set aside for 15 minutes, then discard bags. Add juice. Cover and chill overnight."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer to a large jug. Add apple, ice and mint. Top up with mineral water to serve."
    }
