  "recipeIngredient": [
    "600g pumpkin, peeled and chopped",
    "1 tbs olive oil",
    "3 tbs pesto",
    "4 wholemeal pitas",
    "4 tbs ricotta",
    "50g rocket",
    "2 tsp thyme, fresh",
    "2 tbs vegetarian hard cheese, grated"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced. Toss the pumpkin in a roasting tin with half the oil and fresh thyme. Roast for 20-30 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the pita pockets on a baking tray, spread each with pesto, top with ricotta, roasted pumpkin, grated cheese and the remaining thyme. Drizzle with oil and bake for 10 minutes until golden."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the oven, top with rocket and serve."
    }
  ],
