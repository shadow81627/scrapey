  "recipeIngredient": [
    "900g potatoes, peeled",
    "1 onion, peeled",
    "1 egg",
    "3 tbs plain flour",
    "1 tsp salt",
    "0.25 tsp baking soda",
    "6 tbs sunflower oil (for frying)",
    "2 tbs sour cream",
    "2 tbs chives, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the flour, egg, salt and baking soda and mix well. Heat some oil on medium heat in a large non-stick pan and spoon heaped tablespoons into the pan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook for 3 minutes on each side until crisp and repeat with the remaining batter and oil. Serve with sour cream and chopped chives on top."
    }
  ],
