  "recipeIngredient": [
    "2 sheets shortcrust pastry",
    "300ml cream",
    "1 pkt instant vanilla pudding",
    "50g chocolate",
    "1 handful fresh seasonal fruits"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 200Â°C. Trim shortcrust pastry to fit the size of your tart tin."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Blind bake pastry case for 20 mins or until pastry looks golden brown."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour cream in a bowl and sprinkle the instant vanilla pudding mix. Whip until cream thickens. Chill."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Melt chocolate in the microwave and brush tart shell with melted chocolate; this prevents the tart shell from getting soggy."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Carefully pipe the custard on the cooled tart shell and decorate with fresh seasonal fruits."
    }
  ],
