  "recipeIngredient": [
    "1 cup rolled oats",
    "1 cup sultanas",
    "0.5 cup wholemeal self-raising flour",
    "0.25 cup chopped walnuts",
    "0.5 cup raw sugar",
    "0.5 cup coconut",
    "155g butter",
    "1 tbs honey"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine rolled oats, sultanas, wholemeal flour, sugar. coconut and chopped walnuts."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Melt butter and honey on stove stir till mixture is melted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour melted butter and honey into dry ingredients until mixture clings together."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Press evenly over base of well greased 28cm x 18cm slice tin."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in oven at 190 degrees for 20 minutes or until golden."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut bars while hot. Remove from tin when cold."
    }
  ],
