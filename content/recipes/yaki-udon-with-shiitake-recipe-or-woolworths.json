  "recipeIngredient": [
    "250g udon noodles, dried",
    "2 tbs rice wine vinegar",
    "3 tbs mirin",
    "4 tbs soy sauce, reduced salt",
    "4 tbs brown sauce, reduced sugar",
    "5 spring onions, sliced, green tops reserved",
    "200g shiitake mushrooms, sliced",
    "1 tbs canola oil",
    "2 cloves garlic, peeled and minced",
    "2 large red capsicums, thinly sliced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook udon noodles according to pack instructions, drain and set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk the vinegar, mirin, soy and brown sauce together in a small bowl and add 1-2 tablespoons of water to loosen."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a wok on high heat, add the spring onions, mushrooms, garlic and capsicums and stir fry for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour the sauce into the wok, add the noodles and stir fry for 2 minutes. Serve with sliced green spring onion tops."
    }
  ],
