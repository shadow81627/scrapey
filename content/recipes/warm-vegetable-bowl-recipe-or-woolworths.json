  "recipeIngredient": [
    "2 tsp Maille Mustard Wholegrain Mild",
    "1 tbs Maille Mustard Dijon",
    "50g broccoli",
    "50g carrot",
    "50g cauliflower",
    "50g pumpkin",
    "1 tsp red wine vinegar",
    "1 chickpeas",
    "1 tsp pepper",
    "0.5 cup Macro Organic Quinoa, cooked",
    "1 handful kale, shredded",
    "1 tsp sesame seeds",
    "1 tsp Macro Hemp Seeds",
    "2 tbs extra virgin olive oil",
    "1 pinch sea salt ground"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat the oven to 180 degrees celsius."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a large mixing bowl, add vegetables. In a small mixing bowl add all ingredients under “dressing for veg” and mix well. Pour over vegetables and ensure all are coated."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place into a lined oven baking tray and bake for 20 minutes. After 10 minutes, add the chickpeas into the pan and mix well. With 5 minutes to go, add the kale."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve vegetable tray with quinoa and top with seeds."
    }
  ],
