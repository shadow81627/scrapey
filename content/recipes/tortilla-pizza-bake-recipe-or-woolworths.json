  "keywords": "Pizza,Quick & easy lunch,Tomato,Mushroom",
  "name": "Tortilla Pizza Bake Recipe | Woolworths",
  "prepTime": "PT10M",
  "recipeCategory": "Lunch",
  "recipeCuisine": "Italian",
  "recipeId": "a0K2y000000p8E3EAI",
  "recipeIngredient": [
    "1 tortilla wrap",
    "0.25 tsp fine milled black pepper",
    "1 tbs parmesan cheese, fresh or dried",
    "4 medium button mushrooms",
    "4 cherry tomatoes",
    "1 tbs grated mexican blend cheese",
    "1 tbs grated pizza blend cheese"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat your oven to 180c. Prepare your baking trays by lining with baking paper. Place 1 wrap onto the tray."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mill the black pepper over the wrap."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sprinkle the parmesan cheese over the wrap."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice the mushrooms finely and place over the wrap so that it is evenly distributed."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/bkibmyfrnlx4ysucjytt?wid=1504&hei=1157&fmt=png-alpha",
      "text": "Cut each cherry tomato into quarters and arrange over the wrap evenly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sprinkle the Mexican blend cheese over everything."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/uabcjdzup8vvpy0abbvs?wid=1504&hei=1157&fmt=png-alpha",
      "text": "Sprinkle the Pizza blend cheese over everything."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/xfyrz2zuw8dsalt9zgav?wid=1504&hei=1157&fmt=png-alpha",
      "text": "Bake in oven for 15 - 20 minutes depending on your oven. Most times, 15 Minutes is enough. Once cooked and golden brown, remove and cut into 4 and eat."
    }
  ],
