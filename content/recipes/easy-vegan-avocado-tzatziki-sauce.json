  "recipeIngredient": [
    "2 avocados (pits and skin removed)",
    "1/2 cucumber (peeled)",
    "2 garlic cloves",
    "1/2 lemon (juiced)",
    "sea salt and black pepper to taste",
    "1/4 cup dill (minced)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Grate the cucumber and squeeze the water out with a dry cloth."
    },
    {
      "@type": "HowToStep",
      "text": "In a food processor or blender, puree the avocado, garlic, lemon juice, and salt and pepper until smooth and creamy."
    },
    {
      "@type": "HowToStep",
      "text": "Scrape the avocado mixture into a medium bowl and stir in the cucumber and dill until well combined."
    },
    {
      "@type": "HowToStep",
      "text": "Enjoy!"
    }
  ],
