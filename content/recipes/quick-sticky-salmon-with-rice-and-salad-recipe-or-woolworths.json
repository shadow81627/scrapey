  "recipeIngredient": [
    "1 tbs vegetable oil",
    "460g Woolworths skin off Tasmanian salmon portions",
    "0.25 cup sweet chilli sauce",
    "450g microwave white longgrain rice",
    "315g Thai salad kit",
    "0.25 bunch coriander, leaves picked (to serve)",
    "1 lemon, cut into wedges (to serve)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Sticky_Salmon_Rice_Salad_Step_01?$foodhub-method-images$&fmt=png-alpha",
      "text": "Heat half of the oil in a large frying pan over mediumhigh heat. Cook salmon for 3 minutes or until almost cooked through. Turn over, then cook for 2 minutes, brushing with sweet chilli sauce, or until salmon is just cooked through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Sticky_Salmon_Rice_Salad_Step_02?$foodhub-method-images$&fmt=png-alpha",
      "text": "Meanwhile, heat rice according to packet instructions."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Sticky_Salmon_Rice_Salad_Step_03?$foodhub-method-images$&fmt=png-alpha",
      "text": "Prepare salad according to packet instructions, reserving crispy onions."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Sticky_Salmon_Rice_Salad_Step_04?$foodhub-method-images$&fmt=png-alpha",
      "text": "Divide rice and salad among bowls, then top with salmon, and sprinkle with coriander and reserved crispy onion. Serve with lemon."
    }
  ],
