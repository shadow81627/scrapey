  "recipeIngredient": [
    "250g Arnott's Marie Biscuits",
    "180g Cadbury Caramilk chocolate",
    "395g sweetened condensed milk",
    "100g desiccated coconut",
    "30g desiccated coconut (for rolling)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Blitz biscuits in a food blender, add to a bowl along with the coconut."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Break up chocolate and melt with condensed milk in microwave or on the stove top, mix until all combined and mixture is smooth."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour melted chocolate/condensed milk mixture into the bowl with the dry ingredients & mix until all combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Roll mixture into balls and toss in the extra coconut. Ensure ball is coated evenly with coconut."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place balls on a tray and put in the fridge to firm up."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once firm, store in an airtight container in the fridge."
    }
  ],
