  "recipeIngredient": [
    "200g oak smoked salmon",
    "1 large avocado, pitted",
    "3 tbs lemon juice, divided",
    "0.5 medium cucumber, peeled into ribbons",
    "60g radishes, thinly sliced",
    "0.5 tsp chilli flakes",
    "8 slices rye bread",
    "1 pinch black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Scoop the avocado into a bowl, add half the lemon juice and mash."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix the cucumber ribbons and sliced radishes in a bowl with the remaining lemon juice."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread the avocado on 4 slices of rye, top with a pinch of chilli flakes and black pepper, slices of salmon and the lemony vegetables. Press another slice of rye on top of each, slice and serve."
    }
  ],
