  "recipeIngredient": [
    "2 cup pure icing sugar",
    "100g unsalted butter, melted",
    "200g dark chocolate, melted, cooled",
    "0.5 cup cocoa",
    "0.33 cup hazelnuts, roasted, chopped",
    "0.66 cup chocolate hazelnut spread",
    "2 eggs",
    "1 cup hazelnut meal"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180Â°C and line 2 baking trays with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sift icing sugar and cocoa into a bowl. Stir in hazelnut meal. Add eggs, chocolate and butter and stir well to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Roll into 20 equalsized balls and place 3cm apart on trays. Flatten slightly with the back of a spoon."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake for 12 minutes or until just cooked. Cool slightly on a wire rack. Spread biscuits with chocolate hazelnut spread and scatter with hazelnuts. Serve warm."
    }
  ],
