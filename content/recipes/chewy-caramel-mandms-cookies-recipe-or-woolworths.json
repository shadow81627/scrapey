  "recipeIngredient": [
    "1 cup unsalted butter, softened",
    "1 cup brown sugar, packed",
    "0.5 cup white sugar",
    "1 tbs Queen Glucose Syrup",
    "1 eggs",
    "2 tsp Queen Natural Vanilla Extract",
    "2.25 cup plain flour",
    "0.75 tsp baking soda",
    "0.75 tsp salt",
    "1.5 cup M&M’s caramel"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180°C (350°F). Using electric mixer, beat together butter, brown and granulated sugar, and corn syrup until fluffy. Beat in egg and vanilla."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk together flour, baking soda and salt; stir into butter mixture until combined. Stir in M&M’s. Refrigerate for 15 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drop heaping tablespoonfuls of dough onto parchment paper–lined baking sheets, about 2 inches apart."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake for 8 to 10 minutes or until edges are golden but centre of cookie is still puffy. Let cool on baking sheet for 5 minutes. Transfer to rack; let cool completely."
    }
  ],
