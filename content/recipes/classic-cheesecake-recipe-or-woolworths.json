  "recipeIngredient": [
    "185g Arnott's Butternut Snap Cookie",
    "0.33 cup thickened cream",
    "50g butter",
    "395g Nestle sweetened condensed milk",
    "2 passionfruit",
    "3 tsp Mckenzie's Baking Aids Gelatine",
    "1 lemon (zest & juice)",
    "500g cream cheese (softened)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine biscuits and butter, press into a 22cm spring form pan, refrigerate until firm."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Beat cream cheese and Nestle sweetened condensed milk until smooth, beat in cream."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix gelatine with boiling water (1/4 cup - 60mL) to dissolve, beat into cream cheese mixture with lemon zest and 2 teaspoons of lemon juice."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour evenly over crumb crust, refrigerate until set (about 3 hours)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with a drizzle of passionfruit pulp."
    }
  ],
