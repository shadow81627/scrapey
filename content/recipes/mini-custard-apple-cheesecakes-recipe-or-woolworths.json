  "recipeIngredient": [
    "6 granita biscuits",
    "1 tsp vanilla",
    "1 banana, thinly sliced",
    "1 custard apple",
    "250g cream cheese",
    "2 eggs",
    "0.33 cup caster sugar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180Â°C. Line 6 large muffin pans with paper cases. Place a granita biscuit into each."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Beat the cream cheese, cup of caster sugar and 1tsp vanilla until creamy."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add 2 eggs, beating well after each addition. Halve 1 small ripe custard apple and scoop flesh into a bowl. Remove seeds, mash flesh and stir 1/2 cup into cream cheese mixture."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon mixture over biscuits and bake for 20 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cool for 10 minutes then refrigerate for 1 hour."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve topped with banana slices."
    }
  ],
