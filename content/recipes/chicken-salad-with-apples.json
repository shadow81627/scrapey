  "recipeIngredient": [
    "3 cups chopped cooked chicken ($7.97)",
    "1 apple (1.5 cups chopped) ($0.76)",
    "1/4 cup diced red onion ($0.20)",
    "1/4 cup dried cranberries ($0.22)",
    "1/3 cup mayonnaise ($0.53)",
    "1/3 cup plain yogurt ($0.35)",
    "1 Tbsp Dijon mustard ($0.18)",
    "1 Tbsp red wine vinegar ($0.10)",
    "1/4 tsp salt ($0.02)",
    "1/4 tsp freshly cracked black pepper ($0.02)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Chop the cooked chicken and the apple into small pieces. Finely dice the red onion."
    },
    {
      "@type": "HowToStep",
      "text": "Place the chicken, apple, onion, and dried cranberries in a large bowl."
    },
    {
      "@type": "HowToStep",
      "text": "Combine the mayonnaise, yogurt, Dijon, red wine vinegar, salt, and pepper in a bowl. Stir until smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Pour the dressing over the salad ingredients in the bowl and stir until everything is coated. Serve immediately or refrigerate until ready to eat."
    }
  ],
