  "recipeIngredient": [
    "1 tbs olive oil",
    "1 large onion, peeled and diced",
    "3 cloves garlic, peeled and minced",
    "2 celery sticks",
    "1 green capsicum, deseeded and diced",
    "200g pork mince",
    "1.5 tbs Cajun seasoning",
    "500g long grain rice, ready cooked",
    "100g prawns, cooked",
    "100g kale, destalked and chopped",
    "4 spring onions, sliced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a large pan on medium heat and cook the onion, capsicum and celery for 5 minutes, add the pork mince, garlic and Cajun seasoning and cook for 6-8 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Tip in the rice and cook, stirring, for 2 minutes. Add the kale and prawns and cook for 2 minutes until the kale has wilted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Scatter with spring onions and serve with lemon wedges on the side, if desired."
    }
  ],
