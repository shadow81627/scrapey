  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine chicken, garlic, oregano and oil in a bowl. Cover and marinate in the fridge for 30 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Thread chicken, chorizo and mushrooms onto skewers."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat a chargrill pan or BBQ on high. Cook skewers, turning occasionally for about 8 minutes or until chicken is cooked through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from heat and serve with lemon wedges."
    }
