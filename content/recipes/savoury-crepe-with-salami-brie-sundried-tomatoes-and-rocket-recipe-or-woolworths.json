  "recipeIngredient": [
    "0.8 cup milk",
    "2 medium eggs, free range",
    "1 pinch salt",
    "200g plain flour, sifted",
    "20g unsalted butter",
    "80g salami, cooked and sliced",
    "80g brie, chopped",
    "60g sundried tomatoes, chopped",
    "100g rocket, washed and drained"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a mixing bowl, combine the milk, 2 eggs, salt and combine fully."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Incrementally add the flour to the mix so that the mixture doesn't become too heavy and stays a little runny."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan to a medium heat with a little butter and pour 1/8 of the mixture onto the pan (you should get 8 pancakes from this batter)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Tilt the pan around so that the batter fills the pan fully and fry for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "After 2 minutes, flip the crêpe over and fry for 2 more minutes and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat, add your toppings into the crêpes. Roll or fold before serving."
    }
  ],
