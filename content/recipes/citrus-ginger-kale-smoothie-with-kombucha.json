  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place kale, banana, orange and ginger in blender and blend until smooth, stopping to scrape down sides of pitcher as needed."
    },
    {
      "@type": "HowToStep",
      "text": "Add kombucha and blend again until smooth."
    }
