  "recipeIngredient": [
    "200g couscous",
    "0.8 cup vegetable stock",
    "1 tbs canola oil",
    "200g broadbeans, de-shelled",
    "2 medium chili peppers, chopped",
    "1 medium red onion, chopped",
    "1 tsp mint, chopped",
    "1 tsp parsley, chopped",
    "2 tsp chili oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bring your vegetable stock to the boil. In a separate bowl mix the canola oil into the couscous thoroughly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour the vegeatable stock over the couscous and cover with cling film, set aside for approx 6 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the beans and peas as per pack instructions. Chop your vegetables and herbs. Using two forks, fluff the couscous. Add all ingredients and serve."
    }
  ],
