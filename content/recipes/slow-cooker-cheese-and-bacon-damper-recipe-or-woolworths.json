  "recipeIngredient": [
    "3 cup self raising flour",
    "2 cup lukewarm water",
    "1 tsp salt",
    "3 rashers short cut bacon, diced",
    "handful grated cheese",
    "some shallots, chopped",
    "1 tsp lemon thyme",
    "handful parsley, chopped",
    "2 tbs tomato paste (or pizza sauce)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Dice the bacon and chop the herbs of your choice. Add salt to self raising flour and mix in enough luke warm water to make a soft dough."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the bacon, shallots and other herbs of your choice."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Line the slow cooker with baking paper. Put the dough into the cooker. Spread the tomato paste or pizza sauce over the dough. Top with the bacon mix and scatter the cheese over the top."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook on high for 90 minutes."
    }
  ],
