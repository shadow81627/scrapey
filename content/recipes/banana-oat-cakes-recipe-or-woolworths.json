  "recipeCuisine": "Australian",
  "recipeId": "a0K2y0000011Mc1EAE",
  "recipeIngredient": [
    "1.5 cup rolled oats",
    "2 bananas",
    "0.5 cup flour",
    "1 egg",
    "2 tbs coconut oil, melted",
    "1 tsp vanilla extract (or similar)",
    "0.25 cup honey (can substitute)",
    "1 tsp bicarb soda"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 175 C"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Line a brownie tray with butter and/or greaseproof paper"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a mixing bowl, mash the bananas together with the back of a fork"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the egg, coconut oil, vanilla extract and honey to the bowl and mix thoroughly"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the plain flour, rolled oats and bicarb soda and incorporate to your mixture"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon the mixture into the brownie dish and smooth out"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place into the oven for around 20 minutes, or until golden brown and fully cooked in the middle (a skewer should come out clean)"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the oven and allow to cool"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once cooled, cut into squares, serve and ENJOY."
    }
  ],
