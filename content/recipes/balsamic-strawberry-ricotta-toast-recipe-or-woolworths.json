  "recipeIngredient": [
    "250g strawberries, sliced",
    "2 tsp balsamic vinegar",
    "4 slices sourdough bread",
    "200g ricotta",
    "1 tbs honey"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the ricotta in a food processor and blend for 2 minutes, until smooth and airy."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Tip the sliced strawberries into a bowl with the balsamic vinegar, stir to coat and leave to infuse for 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toast the sourdough to your liking. Spread each slice with a generous layer of whipped ricotta, spoon a layer of strawberries on top, drizzle with honey and serve."
    }
  ],
