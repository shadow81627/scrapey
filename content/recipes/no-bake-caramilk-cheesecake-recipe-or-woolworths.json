  "recipeCategory": "Desserts",
  "recipeCuisine": "Australian",
  "recipeId": "a0K0I00000SG4WFUA1",
  "recipeIngredient": [
    "1 pkt ginger nut biscuits",
    "0.5 cup desiccated coconut",
    "75g butter, melted",
    "1 block caramilk chocolate",
    "100g cream",
    "30g caster sugar",
    "1 tsp vanilla essence",
    "2 blocks philadelphia cream cheese",
    "1 block caramilk (to decorate)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Crush biscuits with a rolling pin or in blender. Add desiccated coconut and melted butter. Press into spring form tin"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place caramilk chocolate and cream in a glass/microwaveable bowl, and melt in microwave (approx 2-3mins)"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add in sugar, vanilla essence and Philadelphia cream cheese and beat with an electric mixer until all combined well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour over the base and place in fridge overnight."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Decorate if required. Slice and enjoy!"
    }
  ],
