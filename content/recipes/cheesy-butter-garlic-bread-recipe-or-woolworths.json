  "recipeIngredient": [
    "1 large French baguette",
    "100g butter, room temperature",
    "6 cloves garlic, peeled and finely diced",
    "2 tbs chives, chopped",
    "60g parmesan, grated"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice the baguette multiple times but not fully to the bottom so keeping the baguette intact."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine the butter with the garlic, chives and parmesan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread the butter evenly between the slices in the bread and then wrap the baguette in foil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in the oven for 15 minutes and then remove from the oven and serve with a choice of condiment."
    }
  ],
