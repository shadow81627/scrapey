  "recipeIngredient": [
    "1 pkt MasterFoods Roast Chicken Spice Blend",
    "1 tbs MasterFoods Oregano",
    "1.5kg chicken",
    "2 pkt vine small truss tomatoes",
    "1.5 cups pearl couscous",
    "80g green olives",
    "1 lemon, zested and juiced",
    "1 pkt white finishing sauce"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven 180c."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Prepare the chicken by coating all over with a generous of sprinkling of MasterFoodsTM Roast Chicken Seasoning blend."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Roast chicken in oven as per instructions (20 to 25min per 500g)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile cook pearl couscous as pack instructions and add lemon juice, zest, olives and oregano."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "For the final 10min of the chicken being cooked season the tomatoes and roast for 8min to 10min."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the finishing sauce."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Rest the chicken and cut into 6 pieces, divide couscous on dinner plates and add chicken and tomatoes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour over the finishing sauce and serve."
    }
  ],
