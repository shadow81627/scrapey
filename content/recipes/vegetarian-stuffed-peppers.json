  "recipeIngredient": [
    "1 cup rice",
    "1 cup vegetable broth",
    "3/4 cup salsa",
    "1 tbsp olive oil",
    "3 green onions (chopped)",
    "4 bell peppers (top and seeds removed)",
    "1 tbsp chili powder",
    "1/2 tbsp ground cumin",
    "salt and pepper (to taste)",
    "1/2 cup red beans",
    "3/4 cup cheese (grated)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "In a saucepan over medium heat, add olive oil, and cook green onion and pepper for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Add the rice, broth, salsa, spices, and red beans."
    },
    {
      "@type": "HowToStep",
      "text": "Simmer for 20 minutes until the rice is al dente."
    },
    {
      "@type": "HowToStep",
      "text": "Cut off the tops of the peppers and remove the seeds inside."
    },
    {
      "@type": "HowToStep",
      "text": "Fill the peppers with the rice stuffing, compacting slightly. Place the peppers in a baking dish and garnish with cheese."
    },
    {
      "@type": "HowToStep",
      "text": "Pour an inch of water into the bottom of the dish. Cook 30 minutes or until peppers are very tender."
    }
  ],
