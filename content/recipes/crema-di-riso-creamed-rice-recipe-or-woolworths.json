  "name": "Crema Di Riso (Creamed Rice) Recipe | Woolworths",
  "prepTime": "PT5M",
  "recipeCategory": "Desserts",
  "recipeCuisine": "Italian",
  "recipeId": "a0K2y0000012aWHEAY",
  "recipeIngredient": [
    "150g arborio rice",
    "1L whole milk",
    "1 tsp vanilla essence",
    "150g sugar",
    "1 tsp lemon essence",
    "1 tsp cinnamon (for dusting)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a medium saucepan on medium heat bring to boil the milk, sugar and vanilla. Stirring continuously."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Then add rice and cook for 30 minutes on low heat, stirring occasionally to avoid sticking."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour into ramekins and let cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Refrigerate for 1 hour or more before serving. Optional: Dust with cinnamon. Enjoy!"
    }
  ],
