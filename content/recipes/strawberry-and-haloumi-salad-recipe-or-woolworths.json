  "recipeIngredient": [
    "250g strawberries, hulled, halved",
    "5ml olive oil spray",
    "0.33 cup extra virgin olive oil",
    "2 tbs balsamic vinegar",
    "1 pinch freshly ground black pepper",
    "2 tsp dijon mustard",
    "1 pinch sea-salt flakes",
    "120g baby Spinach & rocket leaf salad",
    "1 tbs fresh chives, snipped",
    "0.25 cup pistachios",
    "250g haloumi, drained, sliced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk oil, vinegar and mustard in a jug. Season with salt and ground black pepper. Set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat a chargrill over high heat. Spray halloumi with oil. Chargrill for 1-2 minutes each side or until golden. Transfer to a plate."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place spinach and rocket in a large bowl. Add the strawberries, pistachios, halloumi and chives. Toss until well combined. Add the dressing to salad just before serving. Toss and serve."
    }
  ],
