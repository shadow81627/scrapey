  "recipeIngredient": [
    "1 teaspoon honey",
    "5 whole cloves",
    "1 cinnamon stick",
    "1 whole star anise",
    "1 1/2 ounces bourbon",
    "1/2 cup boiling water",
    "Slice of lemon"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Add honey and spices to the bottom of a heat-proof snifter or mug. Add the boiling water and then bourbon. Squeeze the lemon, drop it in, and stir until the honey is melted."
    }
