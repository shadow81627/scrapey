  "recipeIngredient": [
    "400g chickpeas, drained",
    "3 tbs soy yoghurt, unsweetened",
    "2 tbs vegan mayonnaise",
    "1 tbs mango chutney",
    "1.5 tsp curry powder",
    "30g sultanas",
    "1 tsp lime juice, fresh",
    "8 slices sourdough bread"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the chickpeas in a medium bowl and mash roughly with a potato masher or fork."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix in the yoghurt, mayonnaise, mango chutney, curry powder, sultanas and lime juice and stir to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread the filling between slices of sourdough and serve."
    }
  ],
