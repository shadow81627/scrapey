  "name": "Easy Pumpkin Scones Recipe | Woolworths",
  "prepTime": "PT10M",
  "recipeCategory": "Breakfast",
  "recipeCuisine": "Australian",
  "recipeId": "a0K2y000000ouRvEAI",
  "recipeIngredient": [
    "60g softened butter",
    "3 tbs caster sugar",
    "0.5 cup mashed pumpkin, cooled",
    "1 large egg",
    "2.5 cups self raising flour, sifted"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cream butter and sugar."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add cooled pumpkin and mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add egg and milk a little at a time to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add sifted flour and mix to a soft dough."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place dough on a lightly floured board and knead lightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Using a rolling pin roll out to a 2cm thickness. Using a cutter cut into rounds."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place rounds on prepared baking tray lined with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Glaze with milk and bake in a hot oven (200 degrees) for 15 to 20 minutes."
    }
  ],
