  "recipeIngredient": [
    "2 1/2 cups all-purpose flour",
    "1/2 tsp baking powder",
    "1/2 tsp salt",
    "3/4 cup unsalted butter",
    "3/4 cup brown sugar",
    "1/4 cup white sugar (organic for vegan-friendly)",
    "1 egg",
    "1 tbsp lemon zest (finely grated)",
    "1/2 tsp vanilla extract"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "In a bowl, combine flour, baking powder, and salt."
    },
    {
      "@type": "HowToStep",
      "text": "In a larger bowl, beat butter, brown sugar, and sugar with an electric mixer until mixture is light and puffy."
    },
    {
      "@type": "HowToStep",
      "text": "Add egg, zest, and vanilla while beating. Turn mixer to low speed and stir in the dry ingredients."
    },
    {
      "@type": "HowToStep",
      "text": "Divide the dough in half and shape each portion into a disc. Wrap each dough disc with plastic wrap and refrigerate for 1 hour."
    },
    {
      "@type": "HowToStep",
      "text": "Make little balls with the dough and place them on a baking sheet. Press them a little with your hand."
    },
    {
      "@type": "HowToStep",
      "text": "Bake in a preheated oven at 350Â°F for about 12 minutes or until bottom and edge of cookies are lightly browned."
    },
    {
      "@type": "HowToStep",
      "text": "Place on a rack and let cool for 1 minute."
    }
  ],
