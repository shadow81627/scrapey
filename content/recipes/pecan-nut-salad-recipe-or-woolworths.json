  "recipeIngredient": [
    "40g mixed salad greens",
    "1 vine ripened tomato",
    "0.25 small red capsicum",
    "1 small carrot",
    "0.5 celery stick",
    "4 snow peas",
    "0.5 cup pecan nuts",
    "1 drizzle french dressing",
    "1 pinch cracked black pepper (for sprinkling)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/j2xrsqrsul3ci1yu1pbb?$foodhub-method-images$&fmt=png-alpha",
      "text": "Place the salad greens in to bowl and then top with all other ingredients.Drizzle with French dressing, toss and serve with your favourite meat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Gather ingredients and salad bowl. Wash the tomato capsicum carrot celery and snow peas."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Now cut tomato in half and then slice each half into thin slices. Slice the capsicum into small pieces. Diagonal slice the snow peas and celery. Grate the carrot."
    }
