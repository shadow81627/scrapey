  "recipeIngredient": [
    "1 tsp sesame oil",
    "2 cup bean shoots, trimmed",
    "1 carrot, cut into julienne",
    "1 red chilli, finely chopped",
    "0.5 cup coriander leaves",
    "0.5 cup mint leaves, torn",
    "2 spring onions, diagonally sliced",
    "0.5 Chinese cabbage, finely shredded",
    "0.5 cup rice wine vinegar",
    "1 tbs sesame seeds, toasted",
    "300g frozen prawn hargow",
    "1 tbs caster sugar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toss cabbage, shoots, carrot, spring onion and herbs together in a bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine dressing ingredients in a bowl, stirring, until sugar dissolves."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Boil hargow for 8 minutes or until tender. Pour dressing over salad and toss to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sprinkle hargow with sesame seeds and serve with salad."
    }
  ],
