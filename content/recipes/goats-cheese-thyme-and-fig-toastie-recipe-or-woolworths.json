  "recipeIngredient": [
    "8 slices sourdough bread, thinly sliced",
    "8 tbs goats cheese, soft",
    "4 figs, fresh, thinly sliced",
    "1 tbs honey",
    "1 tsp thyme, freshly picked leaves",
    "1 tbs butter, softened"
