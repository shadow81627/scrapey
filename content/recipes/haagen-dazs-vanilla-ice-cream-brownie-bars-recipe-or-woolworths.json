  "recipeIngredient": [
    "115g Lindt Dark No Sugar Added",
    "0.75 cup butter",
    "2 cup white sugar",
    "3 eggs",
    "1 tsp Queen Organic Vanilla Essence",
    "1 cup flour",
    "1 cup pecans, chopped (or walnuts (optional)",
    "1 cup whipped cream (optional)",
    "914ml Häagen-Dazs vanilla ice-cream",
    "1 cup unsweetened cocoa powder",
    "1 cup sugar",
    "1 cup Pura Pure Cream",
    "0.5 cup butter",
    "3 tsp Queen Natural Vanilla Concentrated Extract"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Brownies Directions:"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180 degrees Celsius."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a double pan, melt chocolate and butter until chocolate is melted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir in sugar, eggs and vanilla."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from heat and add flour and nuts (if using)"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place batter in a greased 13x9\" pan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake 30-35 minutes until a toothpick comes out clean. Cool completely."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Let ice cream soften on the counter until it is spreadable. Spread over brownies, return to the freezer for two hours."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top with hot fudge (directions below) and refreeze for at least one hour up to one day, covered."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pull out of freezer 15 minutes before you intend to serve them. Garnish with whipped cream."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Hot Fudge Directions:"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine cocoa powder and sugar in a medium saucepan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk in cream and place over medium heat, whisking until the mixture warms."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add butter, vanilla, and heat until right before boiling."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Turn off the heat and allow mixture to cool for a few minutes. Can be stored in a glass jar. Fudge needs to be cooled for 2-3 hours before you can pipe it."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fill a plastic bag with fudge. Cut a small hole in one of the bag corners. Pipe out stripes of fudge, diagonally across the brownie."
    }
  ],
