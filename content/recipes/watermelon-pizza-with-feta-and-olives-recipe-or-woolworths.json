  "recipeIngredient": [
    "1 small watermelon, sliced into 1 inch slices",
    "100g feta, crumbled",
    "0.4 cup balsamic vinegar",
    "100g black olives, drained and halved",
    "2 tbs basil, chopped",
    "1 tbs cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up the balsamic vinegar in a pot on a medium heat and cook until it reduces by half and remove from the heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Take each slice of watermelon and evenly spread the olives, feta and basil. Now spread the balsamic glaze and cracked black pepper over before serving."
    }
