  "name": "Date & Macadamia Nut slice Recipe | Woolworths",
  "prepTime": "PT20M",
  "recipeCategory": "Desserts",
  "recipeCuisine": "Australian",
  "recipeId": "a0K0I00000SHQa7UAH",
  "recipeIngredient": [
    "125g butter, cubed",
    "1 cup brown sugar",
    "1 cup dates, sliced",
    "1 egg, beaten",
    "1 cup macadamia nuts",
    "1 cup self raising flour",
    "1 tsp vanilla extract",
    "pinch salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre heat oven to 170 deg. Grease a slice tin, line with baking paper. Spray well with cooking spray."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the butter, dates, sugar, vanilla and salt on a saucepan on medium heat, stir until melted. Take off heat, cool slightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add egg and flour and macadamias. Stir well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread onto prepared tin, smooth the top, bake for 20 minutes until golden. Cool in the tin. Then cut into squares."
    }
  ],
