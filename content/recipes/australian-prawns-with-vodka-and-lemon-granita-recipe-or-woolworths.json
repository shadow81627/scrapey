  "recipeIngredient": [
    "24 large cooked prawns",
    "2 lemon, juiced and rind finely grated (save 3/4 cup lemon juice)",
    "0.75 cup caster sugar",
    "2 tbs vodka"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place 2 1/2 cups water, sugar in a small saucepan over a medium heat & stir until the sugar has dissolved."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Set aside to cool completely."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir in the lemon rind, lemon juice & vodka."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place in a shallow metal container."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Freeze for 4-5 hours or until completely set."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Rake with a fork."
    }
  ],
