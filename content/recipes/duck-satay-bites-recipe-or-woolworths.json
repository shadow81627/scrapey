  "recipeId": "RGWW338324",
  "recipeIngredient": [
    "2 tbs peanut oil",
    "600g duck breasts, chopped",
    "1 medium white onion, peeled and finely diced",
    "3 cloves garlic, peeled and mashed",
    "1 tbs chilli flakes",
    "1 tsp turmeric powder",
    "2 tbs curry powder",
    "3 tbs peanut butter",
    "0.8 cup coconut milk",
    "1 tsp sesame seeds",
    "1 tbs scallions, chopped",
    "1 tsp peanuts"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a large non-stick pan to a medium heat and then add the peanut oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the duck for 10 minutes while turning."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the onion and garlic to the pan and continue to fry for a further 4 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients to the pan and combine fully."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bring the dish to the boil while stirring and then bring to a simmer for 15 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once the sauce has started to reduce and thicken, remove from the heat and garnish with sesame seeds, scallions and peanuts."
    }
  ],
