  "recipeIngredient": [
    "220g arborio rice",
    "3.4 cups almond milk, unsweetened",
    "0.25 tsp cardamom, ground",
    "5 tbs honey, divided",
    "1 tsp cinnamon",
    "200g raspberries",
    "1 tbs chia seeds"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rice, almond milk and spices to a large, heavy bottomed saucepan and bring to the boil. Reduce the heat and simmer for 25 minutes, stirring occasionally, until the rice is cooked through. Stir through half of the honey."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the raspberries and remaining honey in a saucepan on medium heat until melted, stir in the chia seeds and set aside to cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve the rice pudding with the raspberry jam on top."
    }
  ],
