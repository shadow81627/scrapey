  "recipeIngredient": [
    "2 oz Bourbon",
    "3/4 oz Lime Juice",
    "1/2 oz. Cherry Juice (liquid from Maraschino Cherries",
    "1 tsp. Simply Syrup",
    "Maraschino Cherry and Lime Wedge, (for garnish)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Combine the ingredients in a shaker along with a few ice cubes and shake vigorously."
    },
    {
      "@type": "HowToStep",
      "text": "Fill two glasses with ice and pour the cocktail through a strainer into the glasses."
    },
    {
      "@type": "HowToStep",
      "text": "Garnish with lime and cherry and serve immediately."
    }
