  "recipeIngredient": [
    "400g peaches in syrup",
    "150g ricotta",
    "1 tsp allspice",
    "4 tbs strawberry jam",
    "80g gingernut biscuits, lightly bashed",
    "0.4 cup whipped cream"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a blender, add the peaches with only 2 tablespoons of the syrup."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the ricotta, allspice and jam to the blender and blend until smooth."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour the mixture into 4 serving glasses and top with half of the crumbled gingernut biscuits."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top with whipped cream garnish with the rest of the gingernut biscuits before serving."
    }
  ],
