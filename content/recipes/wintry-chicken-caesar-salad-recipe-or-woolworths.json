  "recipeIngredient": [
    "3 cloves garlic, crushed",
    "0.5 free-range BBQ chicken",
    "1 tsp worcestershire sauce",
    "1 tbs lemon juice",
    "4 rasher bacon, rind removed, chopped",
    "0.5 cup parmesan, shaved",
    "4 cup kale leaves, shredded",
    "0.5 cup mayonnaise",
    "8 slice Turkish bread, toasted",
    "4 eggs, soft-boiled",
    "2 anchovies, mashed"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk mayonnaise, lemon juice, sauce, garlic and anchovies together in a bowl. Season with salt and pepper. Chill while making the salad."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place a frying pan over medium heat. Cook bacon until crisp, then transfer to a plate."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide kale evenly between serving bowls. Top each with chicken, bacon and an egg. Sprinkle with parmesan and drizzle with dressing. Serve with torn pieces of toast."
    }
  ],
