  "recipeIngredient": [
    "400ml canned coconut milk",
    "0.25g maple syrup",
    "125g strawberries",
    "62.5g raspberries",
    "62.5g blueberries",
    "0.5 cup dollop cream (to serve)",
    "50g Woolworths mini meringues, crushed (to serve)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk coconut milk and maple syrup in a large jug until combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide two-thirds of the berries and milk mixture, in alternate layers, among 8 x 1/3-cup capacity iceblock moulds. Insert a popsicle stick into each mould, then freeze for 6 hours or overnight, if time permits."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove pops from moulds. Serve topped with cream and remaining fruit, and scattered with meringue."
    }
  ],
