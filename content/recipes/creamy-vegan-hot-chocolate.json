  "recipeIngredient": [
    "3 cups plain, unsweetened almond milk",
    "1 (15 oz) can unsweetened coconut milk, I used lite",
    "3 tablespoons unsweetened cocoa powder",
    "1/4 cup cane sugar",
    "1 tablespoon coconut sugar",
    "1/3 cup vegan chocolate chips",
    "1/2 teaspoon vanilla extract",
    "1 pinch sea salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "To a large pot, add the almond milk, coconut milk, cocoa powder, cane sugar and coconut sugar. Place over medium heat and bring to a simmer, stirring frequently."
    },
    {
      "@type": "HowToStep",
      "text": "Once hot chocolate is warm, add the chocolate chips, vanilla extract and sea salt. Cook until chocolate chips are fully melted and hot chocoalte is desired temperature."
    },
    {
      "@type": "HowToStep",
      "text": "Enjoy immediately. Top with your favorite vegan whipped topping, marshmallows and shaved chocolate."
    }
  ],
