  "recipeIngredient": [
    "220g arborio rice",
    "3.4 cup almond milk, unsweetened",
    "0.25 tsp almond extract",
    "4 tbs honey, divided",
    "1 tsp vanilla extract",
    "100g lychees, tinned, sliced",
    "4 tbs flaked almonds, toasted"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rice, almond milk and extracts to a large, heavy bottomed saucepan and bring to the boil. Reduce the heat and simmer for 25 minutes, stirring occasionally, until the rice is cooked through. Stir in half of the honey."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide between 4 bowls, top with lychees, honey and toasted almonds and serve."
    }
  ],
