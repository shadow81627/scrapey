  "recipeIngredient": [
    "2 tbs canola oil",
    "4 medium spring onions, sliced",
    "5cm ginger, peeled and minced",
    "3 cloves garlic, peeled and minced",
    "3 tbs Thai green curry paste",
    "1.6 cup coconut milk, reduced fat",
    "2 tbs fish sauce",
    "12 scallops, shelled, fresh",
    "250g tenderstem broccoli, sliced",
    "2 tbs lime juice",
    "0.5 bunch coriander, fresh, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat half of the oil in a pan on medium high heat and cook the spring onions, ginger and garlic for 3 minutes. Add the curry paste, fish sauce and coconut milk and bring to a simmer. Cook for 10 minutes to thicken."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir in the tenderstem broccoli and cook for 8 minutes, until tender."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a large non-stick pan and cook the scallops for 1-2 minutes each side, depending on their size."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve the curry sauce in bowls topped with scallops, fresh coriander and a squeeze of lime juice."
    }
  ],
