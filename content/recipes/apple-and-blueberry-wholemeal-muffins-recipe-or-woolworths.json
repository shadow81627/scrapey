  "recipeIngredient": [
    "1.6 cups Woolworths wholemeal self-raising flour",
    "1 cup white sugar",
    "0.25 tsp cooking salt",
    "1 cup vegetable oil",
    "2 free range eggs",
    "0.5 cup milk",
    "0.5 cup apple sauce",
    "0.75 cup blueberries"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 210°C/190°C fan-forced. Line a 12-hole 1/3-cup capacity muffin pan with paper cases."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place flour, sugar and salt in a large bowl and stir to combine. Whisk oil, eggs, milk and apple sauce in a large jug until combined. Add egg mixture to flour mixture and stir until just combined (do not over-mix). Fold in blueberries."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide mixture among holes and bake for 25 minutes or until golden and a skewer inserted into centres comes out clean. Cool for 10 minutes. Serve muffins warm or at room temperature."
    }
  ],
