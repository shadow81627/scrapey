  "recipeIngredient": [
    "2 potatoes cut length wise (1/4 inch width)",
    "2 teaspoons olive oil",
    "1/2 teaspoon salt",
    "4 cloves of garlic mashed",
    "1/2 teaspoon cajun spice"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Cut the potatoes and boil them for 5 mins or until tender."
    },
    {
      "@type": "HowToStep",
      "text": "Strain and keep for 10 minutes on a paper towel"
    },
    {
      "@type": "HowToStep",
      "text": "In a small bowl, mash teh garlic."
    },
    {
      "@type": "HowToStep",
      "text": "Add olive oil and mix well."
    },
    {
      "@type": "HowToStep",
      "text": "Add in the potatoes, cajun spice and salt and toss well to coat."
    },
    {
      "@type": "HowToStep",
      "text": "Place potatoes on a baking sheet, individually."
    },
    {
      "@type": "HowToStep",
      "text": "Bake for 10 minutes at 350 degrees F and then broil for 2-3 minutes. You can broil longer for crispier and browner potatoes."
    }
  ],
