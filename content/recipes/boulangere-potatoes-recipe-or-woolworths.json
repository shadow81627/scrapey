  "recipeIngredient": [
    "700g potatoes, peeled and finely sliced",
    "1 onion, peeled and sliced",
    "1 tsp fresh sage",
    "0.8 cup vegetable stock, chopped",
    "30g parmesan, grated",
    "30g butter",
    "2 tbs unsalted butter"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180°C/160°C fan-forced. Layer the sliced potatoes, sliced onions and chopped sage alternatively in a baking dish. Pour over the stock and top with parmesan cheese."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Distribute the butter on top of the dish. Bake for 60 minutes or until the potatoes are soft and cooked through. Serve with butter."
    }
