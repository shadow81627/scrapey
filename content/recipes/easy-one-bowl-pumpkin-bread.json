  "recipeIngredient": [
    "1 cup coconut sugar",
    "1 cup avocado oil",
    "2 cups pumpkin puree",
    "3 eggs",
    "1/2 tsp baking powder",
    "1 tsp baking soda",
    "Pinch nutmeg",
    "Pinch ground cinnamon",
    "3 cups all-purpose flour"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat oven to 350 degrees F."
    },
    {
      "@type": "HowToStep",
      "text": "In a large mixing bowl, mix together all the ingredients until a smooth batter is formed. (Yes, it's that simple!)"
    },
    {
      "@type": "HowToStep",
      "text": "Pour the mixture evenly into two greased 9x5 loaf pans."
    },
    {
      "@type": "HowToStep",
      "text": "Bake for 50 to 60 minutes, until a toothpick inserted comes out clean."
    }
  ],
