  "name": "Cheese & Spring Onion Melt Recipe | Woolworths",
  "prepTime": "PT2M",
  "recipeCategory": "Snacks",
  "recipeCuisine": "Australian",
  "recipeId": "a0K0I00000SHq1cUAD",
  "recipeIngredient": [
    "2 slices multigrain bread",
    "0.25 cup Woolworths Italian cheese blend",
    "1 stem spring onions",
    "0.25 tsp black pepper, ground"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180 degrees celsius. Chop spring onions. Prepare cheese melt & place 2 slices of bread on oven tray covered in foil or baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/ca90e6on6y1kemamypqh?wid=4608&hei=3456&fmt=png-alpha",
      "text": "Sprinkle chopped spring onions onto bread then add shredded cheese. Sprinkle more spring onions on top. Sprinkle black pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place in oven & cook for 8 mins or until the cheese starts to bubble & turns light golden brown."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/dtf2isnv2lbnpxfbiwpz?wid=4608&hei=3456&fmt=png-alpha",
      "text": "Take out of oven & serve immediately."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/fvfbhjdoc8h5uykhqt0u?wid=4608&hei=3456&fmt=png-alpha",
      "text": "Enjoy!"
    }
  ],
