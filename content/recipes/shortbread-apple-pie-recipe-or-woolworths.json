  "keywords": "Apple pie & tart,Christmas",
  "name": "Shortbread Apple Pie Recipe | Woolworths",
  "prepTime": "PT25M",
  "recipeCategory": "Desserts",
  "recipeCuisine": "Australian",
  "recipeId": "a0K0I00000Sh9AzUAJ",
  "recipeIngredient": [
    "125g butter (or margarine)",
    "0.5 cup caster sugar",
    "1 egg",
    "2 cup self raising flour",
    "1 tin pie apples"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 200°C (fan forced). Cream butter and sugar."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add egg."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/horfgjerht3jxoghycom?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add flour then mix with fork. Mixture should not be too wet, if you find that it is add a little more flour."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Set pastry aside, open apples and pour into bowl. If too much liquid strain apples, add sugar if you like the apples sweeter."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/ecympptfwrozyclzcno9?$foodhub-method-images$&fmt=png-alpha",
      "text": "Grease pie plate. Divide pastry in half and take a heaped tsp of pastry. Press between palms of hand. Then place in pie dish. Repeat this method until base has been completed. Making sure to overlap pieces."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/kt8mamcqhm0t0jsmvw9q?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add apples to base."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/qhnok4qcswujev7ofwln?$foodhub-method-images$&fmt=png-alpha",
      "text": "Finish pastry topping using the same method as base."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook in oven 200°C for 30-35 mins or until golden brown."
    }
  ],
