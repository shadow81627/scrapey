  "recipeIngredient": [
    "2L salt-reduced vegetable stock",
    "1L water",
    "1 piece ginger, 4cm, cut into matchsticks",
    "2 cloves garlic, sliced",
    "4 whole star anise",
    "0.33 cup salt-reduced soy sauce",
    "100g green beans, trimmed, halved diagonally",
    "0.33 quantity prawn dumplings",
    "150g shredded carrot",
    "400g broccoli, cut into small florets",
    "1 bunch baby bok choy, halved",
    "3 spring onions, sliced",
    "1 long fresh red chilli, sliced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place vegetable stock, water, ginger, garlic, star anise and soy sauce in a large heavy-based saucepan. Bring to the boil over high heat. Reduce heat. Add beans. Cook for 1 minute, then add carrot, broccoli and bok choy. Cook for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add dumplings to soup. Cook for 2 minutes or until they float to the surface and vegetables are just tender."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve sprinkled with spring onion and chilli."
    }
  ],
