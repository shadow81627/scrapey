  "recipeIngredient": [
    "1 bell pepper (thinly sliced)",
    "1 poblano pepper (thinly sliced (or use a second bell pepper)",
    "1 red onion (thinly sliced)",
    "1/2 tsp cumin",
    "1/4 tsp paprika",
    "1/8 tsp black pepper",
    "1 cup refried black beans (about half of a can)",
    "4 oz kimchi (about 3/4 cup) (check label for fish sauce)",
    "2 flour tortillas",
    "2 tsp vegetable oil",
    "scallions and sesame seeds (optional, for serving)",
    "vegan chipotle mayonnaise (optional, for serving)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Warm the oil in a skillet over medium heat. When the oil is hot, add the sliced peppers and onion and stir. Continue to cook, stirring only occasionally, until the vegetables are slightly browned and softened a bit, about 5-6 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Add the cumin, paprika, black pepper, and salt to taste, and stir well to combine. Reduce the heat to medium-low and continue cooking until the onion is soft, about 2-4 minutes more."
    }
  ],
