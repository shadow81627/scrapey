  "recipeIngredient": [
    "300g yellow split peas",
    "2 tbs olive oil",
    "1 large onion, peeled and chopped",
    "2 cloves garlic, peeled and minced",
    "200g chopped tomatoes",
    "5cm ginger, peeled and minced",
    "2 tsp cumin",
    "2 tsp turmeric",
    "1 tsp curry powder",
    "1 large green chilli, sliced",
    "2.8 cup vegetable stock, made with 1 stock cube",
    "1 tbs honey",
    "1 bunch silverbeet, stems removed, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add all of the ingredients except the silverbeet to the slow cooker and cook on high for 4 hours."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir in the chopped silverbeet, lemon zest and cook until wilted, 5-6 minutes. Serve with rice or naan bread, if desired."
    }
