  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180°c Place vegetables except for tomatoes and herbs on an oven proof dish."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place chicken legs on top of vegetables. Top with diced tomatoes, herbs and olive oil. Add salt and pepper if desired."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake at 180° for approximately 1 hour or until chicken and vegetables are fully cooked and have slight caramelisation."
    }
  ],
