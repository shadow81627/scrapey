  "recipeIngredient": [
    "10 chicken legs",
    "4 potatoes, cut in wedges",
    "0.25 pumpkin, peeled if preferred and cut into large pieces",
    "3 carrots, peeled and cut into large pieces",
    "1 zucchini, cut into large pieces",
    "1 tomato, diced",
    "1 onion, peeled and sliced",
    "2 clove garlic, peeled and chopped",
    "1 sprig rosemary",
