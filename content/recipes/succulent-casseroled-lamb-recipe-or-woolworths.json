  "recipeIngredient": [
    "5-6 forequarter lamb choP",
    "1 red onion, cut into wedges",
    "1 red caPicum, diced",
    "2 medium carrots, diced",
    "0.5 cup marmalade jam",
    "0.25 cup bbq sauce",
    "0.25 cup sweet chilli sauce",
    "0.25 cup soya sauce",
    "2 tsp fresh ginger, grated",
    "2 cloves fresh garlic, finely chopped",
    "0.33 cup woolies gravy mix",
    "1 cup water",
    "1 large oven bag",
    "2 tbs plain flour (rice flour)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/skgtrdaumyv0p2izoybc?wid=3264&hei=2448&fmt=png-alpha",
      "text": "Gather all ingredients and preheat oven to 160 deg fan. Read instructions on oven bag."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/osxbddyfspf0vormjjgo?wid=3264&hei=2448&fmt=png-alpha",
      "text": "Prepare the veg and place on a plate."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/kaedfd8sfnc2dk4hch1p?wid=3264&hei=2448&fmt=png-alpha",
      "text": "In a separate plastic bag place the meat and flour and shake well to coat. Add total contents to the oven bag that is laying in a baking dish."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/fpzeqlsyycqhygplfcah?wid=3264&hei=2448&fmt=png-alpha",
      "text": "Add the veggies to cover the meat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/zgwn5ef6ebo7pb4lvyzp?wid=3264&hei=2448&fmt=png-alpha",
      "text": "Combine all remaining ingredients in a jug. Mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/vzm9ee9wycd3lsttommw?wid=3264&hei=2448&fmt=png-alpha",
      "text": "Pour the sauce over the meat and veg in the oven bag. Tie off the end of bag with enclosed tie."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place in oven and cook for 2 hours. If cooking shanks allow 1/2 to 3/4 hour longer."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/mfauhesaegqpphe5ycyl?wid=3264&hei=2448&fmt=png-alpha",
      "text": "When cooked remove from oven and slit open oven bag being careful not to burn yourself with the steam. Serve and enjoy."
    }
  ],
