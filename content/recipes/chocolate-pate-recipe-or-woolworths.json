  "recipeIngredient": [
    "80g butter, chopped",
    "0.33 cup Dutch cocoa powder*, sifted",
    "80g pistachios, chopped",
    "300g orange flavoured dark chocolate",
    "250g Scotch finger biscuits, roughly chopped",
    "395g sweetened condensed milk"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Grease and line a 20x10x8cm deep loaf pan with baking paper. Stir butter, milk and chocolate in a saucepan over medium-low heat for 4 minutes or until melted and smooth. Remove from heat. Add cocoa, biscuits and 3/4 of the pistachios and stir well to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer to pan and spread evenly. Gently press remaining pistachios over surface. Cover with plastic wrap and then refrigerate for 1 hour or until firm."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from pan and cut into slices."
    }
  ],
