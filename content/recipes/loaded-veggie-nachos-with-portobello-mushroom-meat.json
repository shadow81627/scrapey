  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Heat olive oil in a large skillet over medium-high heat."
    },
    {
      "@type": "HowToStep",
      "text": "Add onion and cook until softened, about 3 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Add garlic, cumin, and paprika; cook 1 minute."
    },
    {
      "@type": "HowToStep",
      "text": "Add mushrooms and chipotle and cook until liquid from mushrooms evaporates, about 8 minutes more."
    },
    {
      "@type": "HowToStep",
      "text": "Remove from heat. Stir in salt and pepper."
    }
