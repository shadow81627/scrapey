  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat half of the oil in a pan on medium high heat and cook the garlic, lemon zest and anchovies for 2 minutes, stir in the spring onions, kale and cavolo nero and cook 3-4 minutes until wilted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the rest of the oil in a large non-stick pan and cook the scallops for 1-2 minutes each side, depending on their size. Stir in butter and lemon juice and cook for 1 minute, until bubbling."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve the scallops on the wilted greens with lemony butter drizzled on top."
    }
  ],
