  "recipeIngredient": [
    "2 tbs olive oil, divided",
    "4 spring onions, sliced",
    "150g kale, destalked and shredded",
    "150g kale, shredded",
    "3 cloves garlic, peeled and minced",
    "50g anchovies, drained, chopped",
    "2 medium lemons, juice and zest",
    "12 scallops, shelled, fresh",
    "1 tbs butter"
