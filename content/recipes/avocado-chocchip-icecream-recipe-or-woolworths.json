  "recipeIngredient": [
    "2 Hass avocados",
    "395g canned sweetened condensed milk",
    "300ml Woolworths thickened cream, whipped",
    "0.5 cup mini dark chocolate chips",
    "0.25 cup pistachio kernels, roughly chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Scoop out avocado flesh and place in a blender. Add milk, then blitz until smooth. Transfer to a large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fold cream and chocolate chips into avocado mixture. Transfer to a 1L capacity loaf pan, then scatter over pistachios. Cover and freeze for 4 hours or until firm. Serve."
    }
  ],
