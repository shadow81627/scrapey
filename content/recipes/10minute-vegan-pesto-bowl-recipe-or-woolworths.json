  "recipeIngredient": [
    "2 cup fresh basil leaves",
    "3 tbs lemon juice",
    "1 lemon, cut into wedges (to serve)",
    "60g baby leaf spinach",
    "250g zucchini spaghetti",
    "250g sweet potato spaghetti",
    "0.25 cup olive oil",
    "0.5 tsp dried thyme",
    "120g blanched almonds",
    "80g pine nuts"
