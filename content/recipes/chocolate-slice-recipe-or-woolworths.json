  "recipeIngredient": [
    "1 cup plain flour",
    "1 cup self raising",
    "1 cup sugar",
    "2 cup desiccated coconut",
    "350g butter",
    "0.25 cup cocoa",
    "1 cup icing sugar",
    "1 tbs butter",
    "1 tsp milk",
    "desiccated coconut",
    "1 tbs cocoa"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven 180Â°C. Sift together the flours and cocoa in a bowl"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the sugar and coconut"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Melt the butter and add to the mixture. Mix it all together."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lightly grease and line a baking tray With baking paper and put mixture on it. Smooth down to about 1 cm depth using a spatula."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in oven for 15 mins."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Allow to cool and make icing by sifting cocoa and icing sugar together and Then adding milk and butter. Mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Ice the slice and sprinkle with extra coconut. Cut into squares."
    }
  ],
