  "recipeIngredient": [
    "200g spinach, washed and drained",
    "1 cloves garlic, minced",
    "1 tbs canola oil",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "4 tsp cream"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a non-stick saucepan to a medium heat and add the canola oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once the oil is heated, add the garlic and fry for 1 minutes while stirring."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the cream to the pan and continue cooking until reduced by half."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the spinach and cook about 30 seconds or until wilted then serve."
    }
  ],
