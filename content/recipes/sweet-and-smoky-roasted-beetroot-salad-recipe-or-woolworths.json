  "recipeIngredient": [
    "4 medium beetroot, cooked",
    "2 tbs canola oil",
    "1 medium white onion, peeled and diced",
    "2 cloves garlic, peeled and diced",
    "2 tbs olive oil",
    "2 tbs balsamic vinegar",
    "1 tbs Dijon mustard",
    "200g mixed salad leaves, washed and drained"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Dice the beetroot into small cubes and toss in a baking dish with the canola oil, onion and garlic."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in the oven for 30 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Now combine your dressing ingredients and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove the beetroot from the oven and allow to cool, then toss with the salad leave and drizzle with dressing before serving."
    }
  ],
