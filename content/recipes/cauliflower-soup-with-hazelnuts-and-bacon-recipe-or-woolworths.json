  "recipeIngredient": [
    "4 cup chicken stock",
    "1kg cauliflower, trimmed, chopped",
    "1 onion, thinly sliced",
    "200g washed potato, peeled, chopped",
    "0.75 cup pure cream",
    "1 tbs olive oil",
    "1 clove garlic, crushed",
    "0.5 cup hazelnuts, toasted",
    "100g streaky bacon, thinly sliced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil in a large saucepan. Add bacon. Cook, stirring occasionally, for about 5 minutes, or until crispy. Drain on absorbent kitchen paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add onion and garlic to same pan. Cook, stirring occasionally, until soft. Add cauliflower, potatoes and stock. Bring to boil. Simmer, uncovered, for about 25 minutes, or until vegetables are tender. Remove from heat. Cool slightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Blend mixture in a blender, in batches until smooth. Stir in cream."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "To serve, top with bacon and hazelnuts."
    }
  ],
