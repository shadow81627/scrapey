  "recipeIngredient": [
    "1 bunch kale, (chopped)",
    "juice of 1 lemon",
    "1/2 teaspoon salt",
    "1/4 cup tahini",
    "1/4 cup water",
    "2 clove garlic",
    "2 teaspoons maple syrup",
    "1/2 teaspoon cumin",
    "1 can chickpeas, (drained and rinsed)",
    "juice of 1/2 lemon",
    "2 tablespoons nutritional yeast"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place kale in a large bowl. Add lemon juice and salt and gently massage for 1-2 minutes. Set aside while preparing the remaining salad ingredients."
    },
    {
      "@type": "HowToStep",
      "text": "Combine all the ingredients for the tahini dressing in a food processor and blend until smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Place chickpeas in a small bowl and add lemon juice and nutritional yeast. Stir together."
    },
    {
      "@type": "HowToStep",
      "text": "Add chickpeas and tahini dressing to kale, toss to combine, and refrigerate until ready to serve."
    }
  ],
