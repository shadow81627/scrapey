  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 200Â°C. Heat butter in a frying pan over medium-low heat. Add pancetta and chopped mushroom. Cook for 5 minutes. Stir in wine until absorbed. Transfer pancetta mixture to a bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add parmesan, breadcrumbs and parsley to bowl. Season. Stir to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place mushroom cups onto a tray and spray with oil. Spoon filling into centres. Bake for 10 minutes. Serve with parsley."
    }
