  "recipeIngredient": [
    "6 tbs mayonnaise",
    "0.5 tsp tabasco",
    "0.5 lemon, juiced",
    "600g prawns, cooked",
    "1 celery stick, finely sliced",
    "0.5 red onion, peeled and diced",
    "4 hot dog buns",
    "1 head baby gem lettuce, chopped",
    "0.5 tsp old bay seasoning"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk the mayonnaise, tabasco and lemon juice together in a large bowl, tip in the prawns, celery and red onion and toss to coat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the hot dog rolls in a warm oven for 3-4 minutes. Slice down the middle, add lettuce, top with the prawn mix and sprinkle with old bay seasoning."
    }
