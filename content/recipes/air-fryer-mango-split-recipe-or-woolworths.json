  "recipeIngredient": [
    "2 mangoes",
    "2 tbs macadamia nuts, roughly chopped",
    "1 tbs coconut flakes",
    "0.33 cup dairy-free coconut yoghurt",
    "0.25 bunch mint, leaves picked"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat air fryer for 3 minutes at 180°C. Cut cheeks from mango and place on a chopping board, cut-side up."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top mango with macadamia nuts and coconut flakes. Place mango in air-fryer basket and cook for 4-5 minutes or until mango has softened and nuts and coconut are golden."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove mango from air fryer. Serve dolloped with yoghurt and sprinkled with mint."
    }
  ],
