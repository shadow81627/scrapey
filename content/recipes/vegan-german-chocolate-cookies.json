  "recipeIngredient": [
    "1/4 cup maple syrup",
    "1/4 cup almond butter (use natural smooth drippy almond butter)",
    "1/2 tsp vanilla extract",
    "2 tbsp non-dairy milk",
    "1/3 cup oats",
    "2 tbsp shredded coconut",
    "1/2 cup flour",
    "3 tbsp cocoa powder",
    "1/4 tsp baking soda",
    "1/4 tsp salt",
    "1/3 cup coconut",
    "1/4 cup chopped pecans",
    "3-4 tbsp maple syrup or sugar",
    "2 tbsp nondairy milk",
    "2 tsp cornstarch"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Mix all wet ingredients for the cookie (maple syrup through non dairy milk) in a bowl, and set aside."
    },
    {
      "@type": "HowToStep",
      "text": "Add all dry ingredients, and mix well, set aside."
    },
    {
      "@type": "HowToStep",
      "text": "Combine wet ingredients into the dry. If you want a sweeter cookie, add a tbsp of sugar or maple. Mix well."
    }
