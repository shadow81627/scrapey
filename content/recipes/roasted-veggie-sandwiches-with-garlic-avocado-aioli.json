  "recipeIngredient": [
    "2 medium garlic bulbs",
    "1 tablespoon olive oil",
    "2 ripe avocados",
    "3 tablespoons lemon juice",
    "3/4 teaspoons salt, (or to taste)",
    "1/2 cup fresh basil leaves",
    "3-4 tablespoons unsweetened non-dairy milk, (as needed)",
    "8 ounces cremini mushrooms, (cleaned and sliced)",
    "1 medium broccoli crown, (broken into florets)",
    "1 medium red bell pepper, (sliced into strips)",
    "1 small onion, (roughly chopped)",
    "2 tablespoons olive oil",
    "1/2 teaspoon salt (or to taste)",
    "1/2 teaspoon black pepper, (or to taste)",
    "4 (6 inch) sandwich rolls, (sliced open and optionally toasted)",
    "Fresh basil",
    "Red pepper flakes, (optional)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat the oven to 425oF."
    },
    {
      "@type": "HowToStep",
      "text": "Use a sharp knife to cut the top off of each garlic bulb."
    },
    {
      "@type": "HowToStep",
      "text": "Place each garlic bulb on a sheet of foil, then drizzle it with about 1/2 tablespoon of olive oil."
    },
    {
      "@type": "HowToStep",
      "text": "Loosely close the foil to enclose the garlic."
    },
    {
      "@type": "HowToStep",
      "text": "Bake the garlic until the cloves are soft and lightly browned, about 30 minutes."
    }
  ],
