  "recipeIngredient": [
    "500g chicken mince",
    "0.5 lime, juiced",
    "1 tbs fish sauce",
    "1 tsp chilli flakes (optional, to taste)",
    "1 tsp shaved palm sugar",
    "2-3 tbs Panko breadcrumbs",
    "3 tbs vietnamese coriander, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix together all ingredients in a bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Roll into palm sized balls and refrigerate for 20 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook on the BBQ, medium heat for approximately 5 minutes each side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with a good squeeze of lime or sweet chilli sauce and a nice crisp salad."
    }
