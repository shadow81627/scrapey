  "recipeIngredient": [
    "3 cups almond milk",
    "1 cup water",
    "1 cup white rice",
    "Zest of 1 orange",
    "1/4 cup maple syrup",
    "1 tsp vanilla extract",
    "1 tsp ground cinnamon",
    "1/2 tsp salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2021/03/vegan-rice-pudding-4.jpg",
      "text": "Place all the ingredients in a pot."
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2021/03/vegan-rice-pudding-2.jpg",
      "text": "Grate the orange. Whisk ingredients together well."
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2021/03/vegan-rice-pudding-3.jpg",
      "text": "Put the pot over the stove to boil, stirring occasionally to make sure the rice doesn't stick to the bottom."
    },
    {
      "@type": "HowToStep",
      "text": "When it starts to thicken, stir continuously until rice is cooked."
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2021/03/vegan-rice-pudding-5.jpg",
      "text": "Serve warm with more cinnamon sprinkled on top."
    }
