  "recipeIngredient": [
    "2 cloves garlic ($0.16)",
    "1 Tbsp grated fresh ginger ($0.30)",
    "1/2 cup neutral oil* ($0.32)",
    "1/4 cup rice vinegar ($0.53)",
    "2 Tbsp soy sauce ($0.12)",
    "3 Tbsp honey ($0.36)",
    "1 Tbsp tahini ($0.19)",
    "1/2 tsp toasted sesame oil ($0.05)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Mince the garlic and grate the ginger (I use a small-holed cheese grater)"
    },
    {
      "@type": "HowToStep",
      "text": "Add the minced garlic, grated ginger, oil, rice vinegar, soy sauce, honey, tahini, and toasted sesame oil to a blender. Blend until the mixture is smooth and creamy. Serve over salad or as a dipping sauce."
    }
