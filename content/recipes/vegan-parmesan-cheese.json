  "recipeIngredient": [
    "1/2 cup raw cashews",
    "1/4 cup nutritional yeast flakes",
    "1/2 teaspoon garlic powder",
    "1/2 teaspoon onion powder",
    "1/2 teaspoon white pepper (optional)",
    "1/2 teaspoon salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place all ingredients into the bowl of a food processor fitted with an s-blade."
    },
    {
      "@type": "HowToStep",
      "text": "Blend for a few seconds, until the mixture resembles Parmesan cheese."
    },
    {
      "@type": "HowToStep",
      "text": "Taste-test and adjust any seasonings to your liking."
    },
    {
      "@type": "HowToStep",
      "text": "Serve immediately or store in sealed container in the refrigerator or freezer."
    }
