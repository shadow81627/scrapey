  "recipeIngredient": [
    "3/4 cup raw cashews",
    "1/2 cup hemp seeds",
    "1/4 cup nutritional yeast",
    "1/2 tsp salt",
    "1/4 tsp garlic powder"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Combine all the ingredients in a food processor and blend until the cashews and hemp seeds are fully incorporated and broken down to resemble parmesan cheese, about 1 minute."
    },
    {
      "@type": "HowToStep",
      "text": "Use immediately or transfer to an airtight container. Store in the refrigerator for up to 3 weeks."
    }
  ],
