  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a bowl, mix yoghurt, 2 tsp tandoori paste and 1 tsp salt. Marinate chicken in this mixture for at least 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a separate bowl, mix the chopped vegetables with 1 tsp salt and 1 tspn tandoori paste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/hvmmterwsqtja8xezzgf?$foodhub-method-images$&fmt=png-alpha",
      "text": "Skewer the chicken, onion and capsicum."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lay the skewers on a baking tray and cook in oven for 20 minutes at 200 degrees. Turn the skewers midway through the cook."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve hot and enjoy."
    }
