  "recipeIngredient": [
    "400g pearl couscous, rinsed",
    "1.6 cup vegetable stock",
    "1 small red onion, peeled and diced",
    "4 cloves garlic, peeled and diced",
    "1 medium cucumber, diced",
    "80g black olives, pitted and halved",
    "1 tbs canola oil",
    "1 tbs oregano, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the couscous and 400ml of the vegetable stock in a pot and bring to the boil, allow to simmer for 25 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan to a medium heat the canola oil and fry onion and garlic for 4 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove the onion and garlic from the heat and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once the pearl couscous is cooked, remove from the heat and allow to cool slightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine the couscous with the onions, olives, cucumber and garlic and then toss with oregano before serving."
    }
  ],
