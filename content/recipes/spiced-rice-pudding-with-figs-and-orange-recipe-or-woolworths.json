  "recipeIngredient": [
    "220g arborio rice",
    "3.4 cups milk",
    "1 tsp cinnamon",
    "0.5 tbs mixed spice, divided",
    "1 tsp vanilla extract",
    "1 large orange, zest",
    "8 figs, quartered",
    "2 tbs light brown sugar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rice, milk, spices and vanilla extract to a large, heavy bottomed saucepan and bring to the boil. Reduce the heat and simmer for 25 minutes, stirring occasionally, until the rice is cooked through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the figs with the sugar and orange zest in a large pan on medium heat. Cook for 45 minutes until the figs are tender. Serve the rice pudding with figs on top."
    }
  ],
