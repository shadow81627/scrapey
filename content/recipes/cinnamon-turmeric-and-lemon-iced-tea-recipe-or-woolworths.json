  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour the tea leaves into a French press with cinnamon, turmeric and honey. Pour over hot water and leave to steep for 5 minutes, stirring once, then press."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fill the press with ice cubes and leave to cool to room temperature. Divide between 4 glasses of ice and serve."
    }
