  "name": "Orange Medley Smoothie Bowl",
  "prepTime": "PT5M",
  "recipeCategory": "Breakfast",
  "recipeCuisine": "Kosher / Vegan",
  "recipeIngredient": [
    "1 1/4 cup unsweetened vanilla almond milk",
    "1 1/2 cups Orange Medley Dole® Fruit & Veggie Blend",
    "1 oz vanilla protein powder (about 2 scoops, depending on the size)",
    "Toppings:",
    "2 tsp chia seeds",
    "1/4 cup pecan halves",
    "2 kiwis, sliced",
    "4 strawberries, sliced",
    "1/4 cup raspberries"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Combine almond milk, Orange Medley Dole® Fruit & Veggie Blend and protein powder in the NutriBullet cup. Blend until smooth"
    },
    {
      "@type": "HowToStep",
      "text": "Split between 2 bowls. Top each bowl with chia seeds, pecan halves, kiwi and strawberry slices and raspberries"
    }
  ],
  "recipeYield": "2",
