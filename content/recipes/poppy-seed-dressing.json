  "recipeIngredient": [
    "1/4 cup apple cider vinegar",
    "1/4 cup olive oil",
    "1/4 cup maple syrup",
    "1/4 cup finely chopped shallot (about 1 shallot)",
    "1 tablespoon Dijon mustard",
    "1/2 teaspoon salt, (plus more to taste)",
    "1 tablespoon poppy seeds"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place all ingredients except for the poppy seeds into a blender or the bowl of a mini food processor. Blend until smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Taste-test the mixture and adjust any seasonings to your taste. Add the poppy seeds."
    },
    {
      "@type": "HowToStep",
      "text": "Pulse the blending device once more to mix the ingredients."
    },
    {
      "@type": "HowToStep",
      "text": "Serve immediately or store in a sealed container in the refrigerator."
    }
  ],
