  "recipeIngredient": [
    "1 tbs extra virgin olive oil",
    "1 clove garlic, crushed",
    "250g Woolworths zucchini spaghetti",
    "1 pinch dried chilli flakes",
    "0.5 tsp cracked black pepper",
    "35g Woolworths parmesan, finely grated",
    "40g leafy salad mix",
    "2 tsp balsamic vinegar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat 3 tsp oil in a medium frying pan over medium-high heat. Add garlic and cook for 30 seconds, stirring, or until fragrant."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add zucchini, chilli flakes and pepper, then cook for 1 minute, stirring, or until zucchini is almost tender. Add half of the cheese and toss to coat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, combine salad mix, vinegar and remaining oil in a small bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top zucchini with remaining cheese. Serve with salad."
    }
  ],
