  "recipeIngredient": [
    "1 tsp miso paste",
    "1 tbs ginger, peeled and grated",
    "1 tsp soy sauce (reduced salt)",
    "2 cloves garlic, peeled and minced",
    "1 tsp chilli garlic sauce",
    "2 tsp maple syrup",
    "1 tsp cornflour",
    "1 tbs canola oil",
    "1 large eggplant, sliced into batons",
    "200g shiitake mushrooms, sliced",
    "1 medium onion, peeled and chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk the ingredients for the sauce together in a small bowl and set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a large wok on a high heat and fry the mushrooms and eggplant for 6-7 minutes until starting to brown, add the onion and cook for 5 more minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour over the sauce and stir to coat the vegetables. Cook until the sauce thickens, 1-2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve immediately with rice or noodles."
    }
  ],
