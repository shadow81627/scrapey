  "recipeIngredient": [
    "1 tbs canola oil",
    "1 medium white onion",
    "4 cloves garlic, peeled and mashed",
    "1.6 cup passata",
    "3 medium capsicum, mixed colours, de-seeded and sliced",
    "1 tbs basil, chopped",
    "1 tbs oregano, chopped",
    "1 pinch salt",
    "1 pinch cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a sauce pan to a medium heat and then add the canola oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the onions and garlic for 4 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the capsicum to the pan and fry for a further 4 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients to the pan and cook while stirring for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer everything to an oven safe casserole dish and bake in the oven for 30 minutes before serving."
    }
  ],
