  "recipeIngredient": [
    "1 1/2 cups whole wheat pastry flour",
    "1 tsp Kosher salt",
    "1/2 tsp baking powder",
    "1/2 tsp baking soda",
    "2 tsp ground cinnamon",
    "1/2 cup unsalted butter (softened)",
    "1/4 cup white sugar (organic for vegan-friendly)",
    "1/4 cup light brown sugar (packed)",
    "1/4 cup honey",
    "2 large eggs",
    "1 tsp vanilla extract",
    "1 cup cooked quinoa (cooled)",
    "1 cup old-fashioned oats",
    "1 cup apple (diced (about 1 medium apple)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat oven to 375°F. Line two baking sheets with parchment paper."
    },
    {
      "@type": "HowToStep",
      "text": "In a medium mixing bowl, whisk together flour, salt, baking powder, baking soda, and cinnamon. Set aside."
    },
    {
      "@type": "HowToStep",
      "text": "In a large mixing bowl, cream butter, sugars, and honey with a hand mixer until light and fluffy (about 3 minutes). Beat in eggs and extract; continue beating for 2 minutes. Gradually beat in flour mixture. Fold in quinoa, oats, and apple."
    },
    {
      "@type": "HowToStep",
      "text": "Spoon two tablespoon-sized balls of dough onto baking sheets, spaced one inch apart. Bake until golden brown, 12–15 minutes. Cool on wire racks."
    }
  ],
