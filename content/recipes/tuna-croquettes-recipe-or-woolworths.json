  "recipeIngredient": [
    "425g tuna in oil (flaked)",
    "0.66 cup plain flour",
    "0.5 cup frozen peas (thawed)",
    "4 spring onions (thinly sliced)",
    "1 lime",
    "2 tbs sweet chilli sauce",
    "0.66 cup mayonnaise",
    "0.25 cup olive oil",
    "1 free range egg (lightly beaten)",
    "475g roasted garlic potato mash"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place mash, tuna, peas, three-quarters of the onion, egg and half the flour in a large bowl. Stir to combine and season to taste. Shape mixture into 12 sausage shapes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place remaining flour on a plate and roll croquettes in flour to lightly coat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil in a large frying pan over medium heat. Fry croquettes in 2 batches, turning, for 8 minutes or until golden brown. Drain on paper towel-lined plate."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Swirl sauce through mayonnaise in a bowl. Serve croquettes with remaining onion, lime and mayo."
    }
  ],
