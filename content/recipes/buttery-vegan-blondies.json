  "recipeIngredient": [
    "3/4 cup vegan butter, (melted and cooled)",
    "1 1/4 cup organic brown sugar",
    "1/4 cup unflavored and unsweetened non-dairy milk",
    "2 teaspoons vanilla extract",
    "1 3/4 cup all-purpose flour",
    "1 tablespoon cornstarch",
    "1 teaspoon baking powder",
    "1/2 teaspoon salt",
    "1/2 teaspoon ground cinnamon",
    "1 cup vegan white chocolate chips",
    "1/2 cup chopped walnuts"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat the oven to 350Â°F."
    },
    {
      "@type": "HowToStep",
      "text": "Line the bottom of a 9-inch square baking pan with parchment paper."
    },
    {
      "@type": "HowToStep",
      "text": "Whisk the butter and brown sugar together in a large mixing bowl, then whisk in the milk and vanilla."
    },
    {
      "@type": "HowToStep",
      "text": "Add the flour to the bowl, then sprinkle the cornstarch, baking powder, salt, and cinnamon on top of the flour. Stir everything together just until combined."
    },
    {
      "@type": "HowToStep",
      "text": "Fold in the white chocolate and walnuts."
    },
    {
      "@type": "HowToStep",
      "text": "Transfer the batter to the prepared baking pan, smoothing out the top with a spoon."
    },
    {
      "@type": "HowToStep",
      "text": "Bake the blondies until set on top, 20 to 25 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Transfer the baking dish to a cooling rack and allow the blondies to cool before slicing and serving."
    }
  ],
