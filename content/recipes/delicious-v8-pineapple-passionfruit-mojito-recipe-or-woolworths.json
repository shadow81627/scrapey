  "recipeIngredient": [
    "0.5 lime",
    "6 mint leaves",
    "1 passionfruit",
    "0.5 cup V8 Pineapple Passion Juice",
    "40ml coconut rum",
    "0.5 cup soda water",
    "1 cup ice",
    "0.25 pineapple, cut into wedges (to garnish)",
    "1 leaf mint (to garnish)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place lime and mint in a glass."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Using a muddler, crush up lime and mint."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add passionfruit, rum and V8 juice."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add ice and stir."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top with soda and garnish with pineapple wedge and mint."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Enjoy!"
    }
