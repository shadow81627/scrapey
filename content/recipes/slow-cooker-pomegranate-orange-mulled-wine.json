  "recipeIngredient": [
    "1 medium apple",
    "1 medium orange",
    "2 (750-ml) bottles red wine",
    "1 cup bourbon",
    "1 cup pomegranate juice",
    "1 cup fresh or frozen cranberries",
    "3/4 cup packed brown sugar",
    "5 cinnamon sticks, plus more for serving",
    "2 star anise pods, plus more for serving"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Slice 1 apple and 1 orange and place in a 3 to 4-quart slow cooker. Add 2 bottles red wine, 1 cup bourbon, 1 cup pomegranate juice, 1 cup cranberries, 3/4 cup packed brown sugar, 5 cinnamon sticks, and 2 star anise pods, and stir to combine."
    },
    {
      "@type": "HowToStep",
      "text": "Heat on the LOW setting until the wine is hot, about 1 hour. Lower the temperature to the Warm setting if available. Serve in mugs with extra cinnamon sticks and star anise pods."
    }
  ],
