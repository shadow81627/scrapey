  "recipeIngredient": [
    "4 earl grey tea bag",
    "4 lavender tea bags",
    "100g caster sugar",
    "2 medium lemons, zested and chopped",
    "1L water",
    "4 sprigs lavender, garnish"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a large saucepan with the water."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the teabags and sugar and allow to simmer until the sugar melts."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and add the chopped lemon."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer to a large jug and store in the fridge overnight."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Next day, sieve the iced tea from the jug into your serving glass with ice and garnish with a sprig of lavender."
    }
  ],
