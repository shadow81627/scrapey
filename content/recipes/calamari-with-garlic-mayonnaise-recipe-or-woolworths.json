  "recipeIngredient": [
    "400g calamari",
    "0.4 cup lemon juice",
    "200g plain flour, sifted",
    "0.4 cup milk",
    "6 tbs mayonnaise",
    "3 cloves garlic, peeled and mashed well",
    "1 pinch cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice the calamari into rings and soak in the lemon juice to one side for 20 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the milk in one bowl and sift the flour into another."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine the mayonnaise with the mashed garlic and black pepper and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan with oil for shallow frying to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove the calamari from the lemon juice, cover fully in flour, then dip into the milk, and cover in flour again and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Shallow fry the battered calamari for 4 minutes and then place on a plate with kitchen towel to drain some excess oil before serving with the garlic mayonnaise."
    }
  ],
