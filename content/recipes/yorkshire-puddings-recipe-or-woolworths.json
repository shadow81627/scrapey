  "recipeIngredient": [
    "140g plain flour",
    "4 eggs",
    "200ml milk",
    "salt (to taste)",
    "pepper (to taste)",
    "2 tbs vegetable oil (or lard)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 210 fan forced. Add flour and eggs to a bowl. Whisk until no lumps."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Gradually and milk and whisk until there are no lumps and a smooth consistency. Season with salt & pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add a drizzle of oil or lard to each muffin tin/try. Heat in oven until smoking hot."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour batter mixture into each hole and bake for 20 to 25 mins."
    }
  ],
