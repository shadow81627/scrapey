  "name": "Agave Green Tea Mojitos",
  "nutrition": {
    "@type": "NutritionInformation",
    "calories": "134 kcal",
    "servingSize": "1 serving"
  },
  "prepTime": "PT15M",
  "recipeCategory": "Drinks",
  "recipeCuisine": "Mexican",
  "recipeIngredient": [
    "10 - 20 fresh mint leaves",
    "1 cup soda water or sparkling water",
    "1/2 cup green tea, (cooled)",
    "2 tablespoons agave",
    "2 oz white rum",
    "1/2 lime, (juiced)",
    "Ice"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Add 5-10 mint leaves to two glasses and then use a muddler or the handle of a wooden spoon to lightly mash the leaves a bit to release their juices."
    },
    {
      "@type": "HowToStep",
      "text": "Divide the remaining ingredients equally among the glasses and top with ice."
    }
  ],
