  "recipeIngredient": [
    "1.5 cup plain flour",
    "1 tsp vanilla essence",
    "1 tsp bicarbonate of soda",
    "0.33 cup premium cocoa powder",
    "0.5 cup light olive oil",
    "1 cup caster sugar",
    "1 tbs lemon juice",
    "100g dried cranberries",
    "1 cup cool water"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 190Â°C. Grease and line a 20cm round cake tin."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sift dry ingredients and 1/2 tsp salt in a bowl and add cranberries."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a jug combine cool water, oil, lemon juice and essence. Add to the dry ingredients and gently stir by hand until lump free. Pour into cake tin and bake for 30 minutes or until a skewer inserted in the centre comes out clean."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cool completely in the tin. Cut into wedges and serve."
    }
  ],
