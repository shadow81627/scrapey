  "recipeIngredient": [
    "2 tbs cocoa powder",
    "200 cup condensed milk",
    "40g macadamia nuts, crushed",
    "1 pinch salt",
    "50g milk chocolate",
    "100g chocolate sprinkles"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add all the ingredients apart from the sprinkles to a large pot and bring to the boil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once it starts to bubble, reduce the heat and allow to simmer for 15 minutes while constantly stirring."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and transfer to a bowl to set in the fridge for 2 hours."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the chcolate sprinkles in a bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the mix from the fridge, roll into about 10 balls and cover all fully with sprinkles."
    }
  ],
