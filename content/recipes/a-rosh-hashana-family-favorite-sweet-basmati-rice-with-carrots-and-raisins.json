  "recipeIngredient": [
    "1 medium onion, sliced",
    "4 cups freshly shredded carrots",
    "2 tbsp vegetable oil",
    "2 1/3 cups water",
    "1 1/2 tsp salt",
    "3/4 cup raisins",
    "1 1/2 cups white basmati rice",
    "Topping:",
    "5 tbsp raw slivered almonds",
    "1/2 tsp vegetable oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "In a 3 qt sauté pan, heat oil and cook onions at medium heat until translucent, about 5 minutes"
    },
    {
      "@type": "HowToStep",
      "text": "Add shredded carrots and continue cooking at medium-low heat for about 7 minutes"
    },
    {
      "@type": "HowToStep",
      "text": "Add water, salt and raisins and bring to a boil"
    },
    {
      "@type": "HowToStep",
      "text": "Add rice and stir well. Bring to a boil, cover and simmer for 25 minutes"
    },
    {
      "@type": "HowToStep",
      "text": "In a separate skillet, slivered almonds in 1/2 tsp of vegetable oil until golden brown, 3-5 minutes. Set aside"
    },
    {
      "@type": "HowToStep",
      "text": "Uncover the rice and quickly and gently fluff it up with a fork. Put the lid back on and let it sit, off the heat, for 10 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Top with almonds before serving"
    },
    {
      "@type": "HowToStep",
      "text": "ENJOY! from May I have that Recipe"
    }
  ],
