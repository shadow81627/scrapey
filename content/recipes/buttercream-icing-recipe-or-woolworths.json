  "recipeIngredient": [
    "250g butter",
    "3 cups icing mixture",
    "1 tsp vanilla essence",
    "1-2 tbs milk"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Chop butter into little squares and beat with electric beater until flight and fluffy."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sift icing mixture and slowly add to butter."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add vanilla and milk and beat until light and fluffy."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "This iced 12 cupcakes with lots more mixture leftover so could serve up to 20 cupcakes."
    }
