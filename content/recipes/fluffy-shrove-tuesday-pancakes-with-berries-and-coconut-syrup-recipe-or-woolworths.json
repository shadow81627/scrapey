  "recipeIngredient": [
    "2 cups White Wings Self-Raising Flour",
    "0.5 cup CSR Brown Sugar",
    "2 tbs CSR Coconut Syrup (for drizzling)",
    "4 eggs",
    "2 cups milk",
    "1 cup oatmeal",
    "0.25 cup yoghurt (to serve)",
    "1 cup berries (to serve)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix all the ingredients in a bowl or food processor until a smooth, 'spoonable' batter is formed."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Grease a non-stick frying pan and pre-heat pan over a medium heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drop in 60ml quarter cup measurement of the mixture, cook for 2 or 3 minutes on either side until golden brown."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with thick yoghurt, a handful of your favourite berries and Coconut Syrup."
    }
  ],
