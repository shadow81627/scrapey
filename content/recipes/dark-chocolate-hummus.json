  "recipeIngredient": [
    "1 can no sodium chickpeas (rinsed and drained)",
    "2/3 cup cocoa powder",
    "1/3 cup maple syrup",
    "1/4 cup tahini",
    "2 tablespoons almond milk (or more to reach desired consistency)",
    "1 teaspoon vanilla extract"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/10/dark-chocolate-hummus-2-of-12.jpg",
      "text": "Combine all ingredients in a food processor and blend until smooth"
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/10/dark-chocolate-hummus-3-of-12.jpg",
      "text": "Add additional almond milk until desired consistency is reached"
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/10/dark-chocolate-hummus-8-of-12.jpg",
      "text": "Serve with pretzels, strawberries, and any other desired dipping snacks"
    }
