  "recipeCuisine": "Chinese",
  "recipeId": "RGWW337978",
  "recipeIngredient": [
    "800g pork tenderloin",
    "0.8 cup beef stock",
    "4 tbs hoisin sauce",
    "1 tbs oyster sauce",
    "4 tbs soy sauce",
    "4 tbs maple syrup",
    "2 tsp five spice powder",
    "8 slices bacon"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice the pork into 8 slices and place into a large saucepan with the rest of the ingredients apart from the bacon."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Allow this to simmer on a medium heat for 10 minutes turning halfway."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and fire up your BBQ."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Now remove each pork slice from the marinade once its cool to touch and wrap in a piece of bacon."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "BBQ the bacon wrapped pork for 15 minutes while turning regularly and marinating with the sauce regularly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the BBQ once cooked and serve with a choice of dips and condiments."
    }
  ],
