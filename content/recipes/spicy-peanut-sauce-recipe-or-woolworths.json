  "recipeIngredient": [
    "100g peanut butter",
    "4 tsp honey",
    "1 tsp paprika",
    "4 tsp tamari",
    "1 tsp fresh ginger, chopped",
    "2 tbs lime juice",
    "1 small red chilli, deseeded",
    "2 tbs water"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add all ingredients to a food processor apart from the water and blend until smooth."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "If the sauce is too thick, use the water to loosen it up."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "If the sauce is too thin, you can use more peanut butter to thicken it."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "The sauce is ready to serve or can be stored in the fridge."
    }
  ],
