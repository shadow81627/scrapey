  "recipeIngredient": [
    "0.25 white cabbage",
    "2 carrots",
    "2 small red caPicums",
    "2 bird's eye chilli",
    "3 tbs water",
    "2 tbs oyster sauce",
    "2 cloves garlic",
    "1-2 tbs oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Dice the garlic into small pieces, the capsicums, carrots and the cabbage into chunks."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the frying pan then add oil and saute the garlic. Once brown, remove the garlic from the heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Saute the carrots, cabbage then add 3 tablespoons of water and 2 tablespoons of oyster sauce."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/auzdeh8r1tva5tdqs1dd?wid=3785&hei=2838&fmt=png-alpha",
      "text": "Once the carrots and cabbage are slightly tender, add the capsicums, the garlic and the bird's eyes chilli (chopped into small pieces) and cook until tender. Season lightly with white pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/zvvii90gm9ffbtfssscb?wid=3024&hei=2326&fmt=png-alpha",
      "text": "Ready to serve. With rice on the side."
    }
  ],
