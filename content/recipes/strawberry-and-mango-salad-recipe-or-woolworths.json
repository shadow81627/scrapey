  "recipeIngredient": [
    "1 tbs white balsamic vinegar",
    "1 tbs honey",
    "0.25 cup extra virgin olive oil",
    "0.5 tsp sea salt flakes",
    "1 mango, roughly chopped",
    "250g strawberries, hulled, halved",
    "120g baby spinach & rocket",
    "2 Lebanese cucumbers, peeled into ribbons",
    "0.25 cup slivered almonds, toasted",
    "2 tbs small basil leaves (to serve)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk vinegar, honey, oil and salt flakes in a large bowl until combined. Add mango, strawberries, salad mix and cucumber, then gently toss to combine. Serve salad sprinkled with almonds and basil."
    }
