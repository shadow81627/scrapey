  "recipeIngredient": [
    "400g raw peeled king prawns",
    "1 tsp five spice powder",
    "2 tbs mirin",
    "200g bean sprouts",
    "125g snowpeas, shredded",
    "100g water chestnuts, thinly sliced",
    "0.5 iceberg lettuce, shredded",
    "12 radishes, thinly sliced",
    "1 tbs sesame seeds, lightly toasted",
    "2 tbs rice vinegar",
    "0.5 cup sunflower oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Set a steamer over a pan of simmering water and steam the king prawns for 2-3 minutes until cooked and pink. Set aside and leave to cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Make the dressing by mixing together all the ingredients in a small bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toss together the bean sprouts, snowpeas, water chestnuts, lettuce and radishes and scatter over the prawns and sesame seeds. Drizzle over the dressing and serve immediately."
    }
  ],
