  "recipeIngredient": [
    "1/4 cup salted butter ($0.60)",
    "1/2 cup brown sugar, packed ($0.22)",
    "1/3 cup heavy cream ($0.50)",
    "1/2 tsp vanilla extract ($0.36)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "In a heavy-bottomed medium-sized pot, melt the butter over medium-low heat. Once it is frothy, add the brown sugar."
    },
    {
      "@type": "HowToStep",
      "text": "Whisk the brown sugar and butter until the sugar transforms into a caramel sauce."
    },
    {
      "@type": "HowToStep",
      "text": "Take the caramel sauce off the heat and add the heavy cream. Continue to whisk to incorporate the cream into the sauce."
    },
    {
      "@type": "HowToStep",
      "text": "Whisk in the vanilla."
    }
  ],
