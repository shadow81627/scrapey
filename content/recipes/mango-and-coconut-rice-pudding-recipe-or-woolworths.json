  "recipeIngredient": [
    "3.2 cups coconut milk, reduced fat",
    "100g arborio rice",
    "1 tsp vanilla extract",
    "2 tbs desiccated coconut, sweetened",
    "2 tbs coconut flakes, toasted",
    "1 large mango, peeled, stoned and sliced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the milk to a heavy bottomed saucepan and bring to the boil. Stir in the rice, reduce the heat and simmer for 20-25 minutes, stirring occasionally, until the rice is tender."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir in the desiccated coconut and serve with mango slices and coconut flakes on top."
    }
