  "recipeIngredient": [
    "1 cup cooked rice (preferably jasmine rice)",
    "1 tsp soy sauce*",
    "1 tsp sriracha*",
    "Splash of sesame oil*",
    "1/3 cup chopped pineapple",
    "1 green onion, sliced",
    "1 large egg",
    "Salt and pepper to taste"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Reheat the cooked rice in the microwave, then season with soy sauce, sriracha, and a splash of sesame oil."
    },
    {
      "@type": "HowToStep",
      "text": "Roughly chop the pineapple pieces and thinly slice the green onion. Stir them into the seasoned rice."
    },
    {
      "@type": "HowToStep",
      "text": "Fry the egg, seasoning it with salt and pepper, and leaving the yolk slightly runny. Top the seasoned rice bowl with the egg and enjoy."
    }
