  "recipeIngredient": [
    "2 medium zucchini, spiralised",
    "2 tbs olive oil",
    "1 medium white onion, peeled and sliced",
    "2 cloves garlic, peeled and diced",
    "4 tbs pesto",
    "40g parmesan, grated",
    "1 tsp cracked black pepper",
    "100g cherry tomatoes, halved"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spiralise your zucchini and then drain the excess liquid in some kitchen paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a non-stick pan to a medium heat with the oil and fry the onion and garlic for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the cherry tomatoes and fry for a further 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the spiralised zucchini and fry while tossing for 4 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and combine fully with the pesto before serving with parmesan grated on top."
    }
  ],
