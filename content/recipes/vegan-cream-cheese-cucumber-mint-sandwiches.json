  "name": "Vegan Cream Cheese Cucumber Mint Sandwiches",
  "prepTime": "PT5M",
  "recipeCategory": "Entree",
  "recipeCuisine": "American",
  "recipeIngredient": [
    "2 slices of sandwich bread (toasted)",
    "2 tbsp. cashew cream cheese",
    "6 cucumber slices",
    "dash salt",
    "dash pepper",
    "1 scallion (chopped)",
    "2 tbsp. fresh mint leaves"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Spread 1 tablespoon of cream cheese on each bread slice."
    },
    {
      "@type": "HowToStep",
      "text": "Sprinkle with scallions."
    },
    {
      "@type": "HowToStep",
      "text": "Arrange cucumber slices in a single layer over one bread slice. Sprinkle with salt and pepper."
    },
    {
      "@type": "HowToStep",
      "text": "Arrange mint over top of cucumber slices."
    },
    {
      "@type": "HowToStep",
      "text": "Place your other bread slice over mint/cucumber bread slice, cream cheese side down."
    }
  ],
