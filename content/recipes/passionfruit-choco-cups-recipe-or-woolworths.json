  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place both sugars into a saucepan with water. Stir over medium heat until sugar dissolves. Simmer for 23 minutes until syrupy. Add passionfruit pulp. Chill in fridge for 20 minutes or until needed."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place chocolate cups onto serving plates. Fill each with ice cream. Pour over chilled syrup. Serve."
    }
