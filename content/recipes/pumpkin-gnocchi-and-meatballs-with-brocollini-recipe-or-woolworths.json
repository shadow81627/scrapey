  "recipeIngredient": [
    "2 pork and fennel sausages",
    "6 stalk broccolini",
    "6 pieces pumpkin gnocchi",
    "1 dollop sour cream",
    "1 splash oil, for frying"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "skin sausages and create 4 meatballs from each sausage."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Chop stalks of brocollini into small pieces."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Oil frypan and add meatballs turning to brown all over."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "After 10 minutes add gnocchi and brocollini stalks."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook until meatballs cooked through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook florets on brocollini in microwave for 1 minute."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Plate and dollop sour cream on base of florets."
    }
  ],
