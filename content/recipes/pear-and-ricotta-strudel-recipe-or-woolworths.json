  "recipeIngredient": [
    "1 tbs icing sugar (to dust)",
    "80g butter, melted",
    "0.25 tsp ground cinnamon",
    "8 sheet sheets filo pastry, thawed",
    "1 cup fresh ricotta",
    "3 tbs caster sugar",
    "0.5 cup sultanas",
    "0.25 cup ground hazelnut meal",
    "820g pear slices in fruit juice, drained"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 200Â°C."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Line a baking tray with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Chop pears and combine with ricotta, sultanas, hazelnut meal, 2 1/2 tablespoon of the sugar and cinnamon."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lay 2 sheets of pastry on a clean work surface. Brush well with melted butter. Repeat layers until all of the pastry has been used."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon filling along one long side of the pastry, leaving a 5cm border at each end."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fold in ends and roll up firmly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place seam-side down on the tray."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Brush top with butter and sprinkle with remaining sugar."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake for 30-35 minutes or until golden brown."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Dust with icing sugar and cut into slices to serve."
    }
