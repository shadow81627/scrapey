  "recipeIngredient": [
    "2 12oz. cans chunk light tuna in water ($3.98)",
    "1 cup diced celery (about 2 ribs) ($0.35)",
    "1/4 cup chopped walnuts ($0.30)",
    "2 green onions, sliced ($0.12)",
    "1/2 cup mayonnaise* ($0.40)",
    "1 Tbsp lemon juice ($0.04)",
    "1/4 tsp freshly cracked black pepper ($0.02)",
    "1/4 tsp salt ($0.02)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Drain the canned tuna well. Finely dice the celery, chop the walnuts, and slice the green onions."
    },
    {
      "@type": "HowToStep",
      "text": "Combine the tuna, celery, walnuts, green onions, mayonnaise, lemon juice, pepper, and salt in a bowl. Stir to combine."
    },
    {
      "@type": "HowToStep",
      "text": "Serve as a sandwich, salad over a bed of greens, or with crackers and vegetables for dipping. Refrigerate up to four days."
    }
  ],
