  "recipeIngredient": [
    "1 cup raw cashews (see note)",
    "1 cup water",
    "1/4 cup lemon juice",
    "3 cloves garlic",
    "1 tablespoon sriracha (add 1 teaspoon for little to no spice, add more for more spice)",
    "1 teaspoon sea salt",
    "1 teaspoon smoked paprika",
    "1/4 cup cilantro"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Put all ingredients, except the cilantro, into a blender and blend on high until nice a smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Add cilantro and pulse until chopped but not pureed in."
    }
