  "name": "Coconut Milk Smoothie Popsicles",
  "recipeIngredient": [
    "1 can full fat coconut milk",
    "1 tablespoon chia seeds",
    "1/2 banana",
    "1 cup fresh fruit (use whatever you have on hand)",
    "Handful of spinach or kale (optional)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "image": "https://hellonest.co/wp-content/uploads/2022/02/Coconut-Milk-Popsicles.jpg",
      "text": "Combine ingredients in blender and mix until combined."
    },
    {
      "@type": "HowToStep",
      "image": "https://hellonest.co/wp-content/uploads/2022/02/Coconut-Milk-Popsicles-2.jpg",
      "text": "Pour into popsicle molds and freeze."
    }
  ],
