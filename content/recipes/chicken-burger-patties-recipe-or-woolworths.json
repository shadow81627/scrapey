  "recipeIngredient": [
    "1 small brown onion, grated",
    "0.5 cup parsley leaves, roughly chopped",
    "2 tsp worcestershire sauce",
    "1 tbs BBQ sauce",
    "1 tbs tomato sauce",
    "1 pinch pepper (for seasoning)",
    "1 egg",
    "500g chicken mince",
    "1 cup fresh white breadcrumbs",
    "1 pinch pink salt ground (to taste)",
    "1 tbs olive oil",
    "4 hamburger buns, split",
    "4 butter lettuce leaves",
    "0.33 cup whole egg mayonnaise"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine chicken mince, brown onion, breadcrumbs, tomato sauce, BBQ sauce, Worcestershire sauce, egg, parsley leaves, salt and pepper in a bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix and form into 4 patties."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil in a large saucepan over medium-high heat. Cook patties for 4 minutes on each side or until cooked through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve patties in buns with lettuce and mayonnaise."
    }
  ],
