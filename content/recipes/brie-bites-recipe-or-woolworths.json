  "recipeIngredient": [
    "0.5 sheet puff pastry",
    "2 tbs Woolworths cranberry sauce",
    "50g Thomas dux triple cream brie",
    "1 handful fresh rosemary"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Chop the cheese into 12 small squares."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/oh757y7cbdszexqmwwdr?$foodhub-method-images$&fmt=png-alpha",
      "text": "Place brie cheese on the pastry. Scoop about 1/2 tsp cranberry sauce onto the cheese. Finally add fresh rosemary leaf over."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spray a mini muffin pan with oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut the puff pastry into 2cm x 2cm squares."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Line the pastry onto the muffin pan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in a fan forced oven at 180 degrees for 20 mins or until the pastry is cooked and crispy."
    }
  ],
