  "recipeIngredient": [
    "1 large lemon",
    "1/2 medium yellow onion",
    "2 cloves garlic",
    "1 cup mayonnaise",
    "1/2 cup ketchup",
    "1/2 cup chili sauce, such as Heinz",
    "1/2 cup neutral oil, such as safflower or canola",
    "1 tablespoon Worcestershire sauce",
    "1 tablespoon paprika",
    "1 teaspoon kosher salt",
    "1 teaspoon freshly ground black pepper",
    "1/2 teaspoon dry mustard"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Prepare the following, adding each to a blender or food processor with the blade attachment as you complete it: Juice 1 large lemon until you have 1/4 cup, coarsely chop 1 medium yellow onion until you have 1/2 cup, and coarsely chop 2 garlic cloves."
    },
    {
      "@type": "HowToStep",
      "text": "Add 1 cup mayonnaise, 1/2 cup ketchup, 1/2 cup chili sauce, 1/2 cup safflower oil, 1 tablespoon Worcestershire sauce, 1 tablespoon paprika, 1 teaspoon kosher salt, 1 teaspoon black pepper, and 1/2 teaspoon dry mustard. Blend until smooth, about 1 minute."
    }
  ],
