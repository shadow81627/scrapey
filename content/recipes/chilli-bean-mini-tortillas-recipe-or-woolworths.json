  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil in a large non-stick pan and cook mince for 5 minutes, or until well browned."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add Beanz CreationzTM Medium Salsa Chilli Beanz and simmer for 3 minutes. Season to taste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place a spoonful of chilli bean filling in the base of each warm mini tortilla."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sprinkle with a little chopped tomato and top with shredded lettuce."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Finish with grated cheese and enjoy immediately."
    }
  ],
