  "recipeCategory": "Dinner",
  "recipeCuisine": "Australian",
  "recipeId": "a0K2y0000011xsREAQ",
  "recipeIngredient": [
    "500g classic tofu regular",
    "230g bamboo shoot",
    "1 bitter melon",
    "1 bunch chinese mustard greens (gai choi)",
    "1 small chilli",
    "1 tbs soy sauce",
    "1 tsp black pepper",
    "2 tbs olive oil",
    "4 clove garlic, crushed",
    "0.25 cup water",
    "1 cube vegetarian stock",
    "1 tsp salt (for rinsing bitter melon)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut bitter melon in half lengthwise. Remove the seeds. Cut them into bite size pieces. Rinse them in water with salt to remove the bitterness flavor."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Rinse mustard greens, chop them into small pieces. Cut tofu and bamboo shoots into bite size pieces."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil in a saucepan over the medium heat. Fry crushed garlic til golden."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add tofu, bamboo shoots, bitter melon, Chinese mustard greens, chilli, soy sauce, stock cube, pepper and water into the saucepan. Stew for 20 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve it with boiled rice."
    }
  ],
