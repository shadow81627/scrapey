  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 150Â°C. Line a large shallow baking dish with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk coconut oil, maple syrup and honey together in a large bowl. Add oats, almond, seed mix, chia seeds and cinnamon and mix until combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread mixture in a single layer in prepared baking dish. Bake for 30 minutes, stirring every 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from oven and stir through crushed coconut chips. Cool completely."
    }
  ],
