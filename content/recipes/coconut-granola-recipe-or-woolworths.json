  "recipeIngredient": [
    "1 tsp ground cinnamon",
    "0.25 cup honey",
    "0.25 cup maple syrup",
    "40g coconut chips, roasted, crushed",
    "0.25 cup white chia seeds",
    "3 cup rolled oats",
    "165g pepitas & sunflower kernels seed mix",
    "0.5 cup natural sliced almonds",
    "2 tbs coconut oil"
