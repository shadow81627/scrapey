  "recipeIngredient": [
    "2 large handfuls fresh spinach",
    "4 large strawberries, (sliced thin)",
    "1/2 small avocado, (pitted and sliced)",
    "1/4 cup pecans,",
    "1/4 cup red onions, (sliced thin)",
    "1/2 cup extra-virgin olive oil",
    "1/4 cup balsamic vinegar",
    "1-2 cloves garlic, (minced)",
    "1 tbsp dijon mustard",
    "1 tsp maple syrup",
    "salt and pepper, (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "In a small blender, combine the ingredients for the Balsamic Vinaigrette and set aside."
    },
    {
      "@type": "HowToStep",
      "text": "Roast the pecans at 400 degrees for 5-7 minutes, until lightly brown. Remove from oven and let cool."
    },
    {
      "@type": "HowToStep",
      "text": "Divide spinach between two plates and top with the remaining salad ingredients. Serve immediately with Balsamic Vinaigrette"
    }
  ],
