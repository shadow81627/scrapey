  "recipeIngredient": [
    "400g minced beef",
    "80g gluten free bread crumbs",
    "2 tbs fajita spices",
    "60g jalapeno, drained",
    "1 medium white onion, peeled and diced",
    "1 medium egg, free range",
    "2 tbs canola oil",
    "4 medium gluten free buns, sliced in half",
    "4 tbs sriracha sauce, peeled and sliced",
    "4 slices chilli cheese"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a bowl, mix together the beef, bread crumbs, spices, jalapeno, onion and egg."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide the mixture into 4 patties and pack tightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan to a medium heat and then add the oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the burgers for 6 minutes, flip and fry for a further 6 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lightly toast the buns and serve with the burgers, sriracha and chilli cheese."
    }
  ],
