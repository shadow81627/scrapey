  "recipeIngredient": [
    "2 tbs Meredith Dairy marinated goat cheese",
    "0.25 cup pecans, halved",
    "0.25 cup dried cranberries",
    "2 cup baby spinach",
    "1 Granny Smith apple, sliced",
    "2 slice bacon, cooked, chopped",
    "2 tbs olive oil",
    "1 tbs apple cider vinegar",
    "1 tbs maple syrup",
    "1.5 tsp dijon mustard",
    "1 pinch salt (to taste)",
    "1 pinch pepper (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place baby spinach in a large bowl. Add pecans, cranberries, bacon and Meredith Dairy Marinated Goat Cheese."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a jar, add olive oil, apple cider vinegar, maple syrup, dijon mustard, and salt & pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Secure the lid and shake the jar until well combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add apple slices to salad bowl and top with half the dressing mixture."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine and enjoy!"
    }
  ],
