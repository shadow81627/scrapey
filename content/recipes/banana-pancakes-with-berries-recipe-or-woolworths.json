  "recipeIngredient": [
    "1.5 cup flour",
    "1 cup milk",
    "2 ripe bananas",
    "pinch cinnamon",
    "pinch salt",
    "1.5 tsp baking soda",
    "2 eggs",
    "some butter or oil for pan"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add flour, baking soda, salt and cinnamon in mixing bowl. Stir mixture"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add in eggs and milk to mixture and mix well"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mash banana and mix in mixture. Rest the mixture for a couple of minutes"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up pan and add some oil/butter. Scoop mixture into pan and turn over once it turns a bit brown / bubbles form. Repeat the other side. Take it out the pan and repeat steps."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with berries and maple syrup"
    }
  ],
