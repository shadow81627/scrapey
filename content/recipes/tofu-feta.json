  "recipeIngredient": [
    "1/3 cup lemon juice",
    "1/4 cup water",
    "2 tablespoons white miso paste",
    "1 garlic clove, (minced)",
    "1/2 teaspoon salt",
    "Fresh herbs of choice (optional), (I used parsley and oregano)",
    "Black pepper to taste (optional)",
    "1 (14 ounce) package firm or extra firm tofu, (drained and pressed for at least 30 minutes)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Whisk the lemon juice, water, miso, garlic, salt, herbs (if using), and black pepper (if using) together in a medium bowl."
    },
    {
      "@type": "HowToStep",
      "text": "Add the tofu and toss to coat."
    },
    {
      "@type": "HowToStep",
      "text": "Allow the tofu to marinate for 30 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Serve immediately or refrigerate the tofu and marinade together in a sealed container for 2-3 days."
    }
  ],
