  "recipeIngredient": [
    "3 tbs black olives, pitted",
    "2 tbs basil, chopped",
    "4 ripe tomatoes, cut into eight",
    "2 red capsicums, cut into squares",
    "1 eggplant, diced",
    "2 onions, finely diced",
    "3 zucchini, thickly sliced",
    "1 large potato, peeled and diced",
    "extra virgin olive oil",
    "0.5 cup parmesan, grated (to serve)",
    "0.33 cup olive oil",
    "2 garlic cloves, sliced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a saucepan over a medium heat. Add onion and potato and cook for 5 minutes until the onion is soft."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add eggplant, capsicum, zucchini and garlic. Cook for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add tomato, cover and gently simmer for 20 minutes or until the vegetables are tender. Stir through the olives."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Scatter over basil. Serve with a drizzle of olive oil and grated parmesan."
    }
  ],
