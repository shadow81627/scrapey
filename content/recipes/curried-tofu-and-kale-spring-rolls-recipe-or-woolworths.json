  "recipeIngredient": [
    "8 sheets spring roll pastry, thawed if frozen",
    "200g tofu, drained and diced",
    "100g kale, washed and drained",
    "3 tbs curry paste",
    "2 tbs olive oil",
    "2 cloves garlic, peeled and diced",
    "80g peanuts, chopped",
    "2 tbs sesame seeds",
    "3 tbs sesame oil (for basting)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180Â°C."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a non-stick pan to a medium heat with 1 tablespoon of sesame oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the diced tofu pieces for 6 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients and fry for a further 3 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and place a large spoonful into the centre of a sheet of pastry, fold over the edges and roll tightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Brush a baking tray with some sesame oil, place the 8 spring rolls on the baking tray and brush the top with oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in the oven for 20 minutes or until starting to crisp, turning halfway and brushing the other side with oil when you turn."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the oven and serve with a choice of dips/condiments."
    }
  ],
