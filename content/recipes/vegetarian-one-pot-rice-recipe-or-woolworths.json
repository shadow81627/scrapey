  "recipeIngredient": [
    "500ml Campbell’s Real Stock Vegetable",
    "2 tbs olive oil",
    "500g carrots, halved, thinly sliced",
    "2 tsp mild curry powder",
    "1 cup basmati rice",
    "420g chickpeas, drained, rinsed",
    "1 green onion, sliced (to serve)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil in a large frying pan on high. Cook carrot for 3-4 mins or until starting to soften. Add curry powder and stir for 1 min."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add rice, stirring for 1 minute. Add Campbell’s Real Stock and bring it to the boil. Cover and simmer on low for 15 mins. Add chickpeas and stir to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve garnished with green onion."
    }
  ],
