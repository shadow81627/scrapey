  "recipeIngredient": [
    "4 weetbix, crushed",
    "1 cup flour",
    "0.5 cup brown sugar",
    "0.5 cup rolled oats",
    "2 tbs cocoa",
    "70g melted coconut oil",
    "0.25 cup milk",
    "1 cup icing sugar",
    "1 tbs milk",
    "100g crushed nuts",
    "1 tbs instant coffee"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix oats, flour, brown sugar and cocoa in a bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add melted coconut oil and milk. Mix all together."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Press into baking tin (20cm x 20cm, baking paper at the bottom)"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake 20 min in 180Â°C"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cool it in tin."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Make icing by mix icing sugar with milk. Add dry instant coffee and mix it for 2 min."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread on the top of baked base and sprinkle with crushed nuts."
    }
  ],
