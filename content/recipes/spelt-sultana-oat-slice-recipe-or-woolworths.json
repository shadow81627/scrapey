  "recipeIngredient": [
    "0.5 tsp mixed spice",
    "0.33 cup raw sugar",
    "0.5 tsp ground cinnamon",
    "0.5 tsp bicarbonate of soda",
    "2 tbs cacao powder",
    "0.5 cup sultanas*",
    "0.5 cup Macro Organic shredded coconut",
    "0.75 cup wholemeal spelt flour",
    "1 cup rolled oats",
    "1 egg, lightly whisked",
    "0.25 cup pepitas",
    "0.5 cup coconut oil",
    "2 tbs honey"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180Â°C. Grease a 26x16cm shallow tin and line with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Melt coconut oil and honey together in a small saucepan. Set aside to cool for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, combine oats, flour, coconut, sultanas, sugar, pepitas, cacao and spices in a bowl. Stir bicarbonate of soda into the oil mixture. Add to the dry ingredients with the egg and stir well to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Press mixture evenly into tin. Bake for 25 minutes or until set. Cool in tin for 10 minutes, then remove and cut into pieces. Serve."
    }
  ],
