  "recipeIngredient": [
    "4 apples (, see note)",
    "1/2 cup oats (, gluten-free if needed)",
    "2 tablespoons chopped pecans",
    "2 tablespoons coconut sugar",
    "1 teaspoon molasses",
    "1 teaspoon cinnamon",
    "1/2 teaspoon vanilla extract",
    "1/4 teaspoon sea salt",
    "Vanilla ice cream to top (optional)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat oven to 350 F/ 175 C."
    },
    {
      "@type": "HowToStep",
      "text": "Core the apples and place on a parchment lined baking sheet."
    },
    {
      "@type": "HowToStep",
      "text": "Mix the oats, pecans, sugar, molasses, cinnamon, vanilla and salt in a bowl until combined and crumbly."
    },
    {
      "@type": "HowToStep",
      "text": "Fill the apples with the mixture."
    },
    {
      "@type": "HowToStep",
      "text": "Bake for 30 minutes until browning and the soft to the touch."
    },
    {
      "@type": "HowToStep",
      "text": "Allow to cool a bit then serve with vanilla ice cream if preferred!"
    }
  ],
