  "recipeIngredient": [
    "0.33 cup thai red curry paste",
    "400g salmon fillets",
    "225g bamboo shoots, sliced and drained",
    "200g rice noodles",
    "1 medium carrot, sliced",
    "1 tbs coriander (for garnish)",
    "100g snow peas, trimmed and sliced",
    "270ml reduced fat coconut milk",
    "1 lime, cut into wedges (to serve)",
    "4 cup chicken stock"
