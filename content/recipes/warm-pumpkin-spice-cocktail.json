  "recipeIngredient": [
    "2 cups pumpkin spice coconut milk beverage (we used So Delicious)",
    "1 cup unsweetened coconut milk beverage (we used So Delicious)",
    "4 tbsp organic canned pumpkin",
    "1/2 tsp pumpkin pie spice",
    "1 tsp cinnamon",
    "1 1/2 oz dark rum (or to taste)",
    "1 1/2 oz vanilla flavored vodka (or to taste)",
    "Vegan whipped topping for garnish (we used So Delicious CocoWhip)",
    "Cinnamon sticks for garnish"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Combine pumpkin spice drink, coconut milk beverage, pumpkin and spices in a medium size sauce pan. Bring to a simmer, stirring often. Add rum and vodka and continue simmering for 1-2 minutes"
    },
    {
      "@type": "HowToStep",
      "text": "Serve hot with a dollop of vegan whipped topping and a cinnamon stick"
    }
  ],
