  "recipeIngredient": [
    "2 tbs basil pesto",
    "1 free range egg",
    "1 thick slice sourdough bread, toasted",
    "2 tbs smooth ricotta",
    "0.25 tsp chilli flakes (optional)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat a frying pan over medium heat. Add pesto and cook, stirring, for 1 minute or until pesto starts to bubble."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Crack egg over pesto and cook for 2 minutes or until egg white has set. Remove pan from heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread toast with ricotta. Top with pesto egg and sprinkle with chilli flakes, if using. Serve."
    }
  ],
