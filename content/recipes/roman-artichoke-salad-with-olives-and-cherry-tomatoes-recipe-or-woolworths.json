  "recipeIngredient": [
    "100g artichokes, store bought jar and drained",
    "50g green olives, sliced",
    "200g cherry tomatoes, quartered",
    "200g rocket",
    "2 tbs olive oil",
    "2 tbs balsamic vinegar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pan fry the drained artichokes with the cherry tomatoes in a pan on a medium heat with the olive oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the olives and fry for another 2 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drizzle over the balsamic and then remove from the heat and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Plate up the rocket and then add the warm ingredients on top to serve."
    }
  ],
