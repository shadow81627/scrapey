  "recipeIngredient": [
    "2 5oz. cans tuna* ($1.36)",
    "1 carrot ($0.11)",
    "2 green onions ($0.23)",
    "2 tsp brown sugar ($0.03)",
    "2 tsp olive oil ($0.08)",
    "1.5 tsp apple cider vinegar ($0.03)",
    "1/4 tsp cayenne pepper ($0.02)",
    "1/2 tsp garlic powder ($0.05)",
    "1/2 tsp smoked paprika ($0.05)",
    "1/4 tsp salt ($0.02)",
    "freshly cracked pepper ($0.03)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Drain the tuna well, then place it in a bowl."
    },
    {
      "@type": "HowToStep",
      "text": "Shred the carrot using a cheese grater. Slice the green onion."
    },
    {
      "@type": "HowToStep",
      "text": "Add the carrot, green onion, brown sugar, olive oil, apple cider vinegar, cayenne, garlic powder, smoked paprika, salt, and some freshly cracked pepper to the bowl with the drained tuna."
    },
    {
      "@type": "HowToStep",
      "text": "Stir everything to combine, then let the mixture sit for about five minutes to allow the flavors to blend. Serve immediately, or refrigerate until ready to eat."
    }
  ],
