  "recipeIngredient": [
    "100g All-purpose flour",
    "5ml canola cooking spray",
    "0.25 cup Nando's Perinaise Sauce Mild",
    "120g crushed cornflakes",
    "2g chicken breast",
    "1 bottle Nando’s PERi-PERi sauce"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 220°C. Crush cornflakes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut chicken breasts into 1/4 inch strips."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Coat chicken in flour and shake off excess. Then dunk chicken in PERi-PERi sauce and coat with crushed cornflakes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spray a baking sheet with the oil and spread the chicken pieces out. Spray with oil and bake for 12-15 minutes, turning halfway for optimal crispiness."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with PERi-PERi for dipping."
    }
  ],
