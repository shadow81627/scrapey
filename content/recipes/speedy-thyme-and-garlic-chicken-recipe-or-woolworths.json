  "recipeIngredient": [
    "2 tsp extra virgin olive oil",
    "50g salted butter",
    "600g Woolworths RSPCA-approved chicken tenderloins",
    "2 tsp minced garlic",
    "0.25 punnet thyme",
    "475g classic potato mash, warmed (to serve)",
    "200g green beans, trimmed, steamed (to serve)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil and half of the butter in a large frying pan over medium-high heat. Cook chicken for 8 minutes, turning, or until golden and cooked through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Reduce heat to medium, then add remaining butter to pan and cook for 1 minute or until melted and heated through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add garlic and thyme, then cook for 1 minute, tossing to coat chicken."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve chicken on mash with beans and drizzled with thyme and garlic butter."
    }
  ],
