  "recipeIngredient": [
    "200g oats",
    "40g butter",
    "4 tsp honey",
    "4 tbs macadamia nuts, chopped",
    "1 tbs dates, chopped",
    "2 tbs raisins, chopped",
    "2 tbs dessicated coconut"
