  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 170Â°C. Toast the oats in the oven for 13 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "While the oats are roasting, melt the butter and the honey together in a pan on medium heat for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove the oats from the oven and mix into the butter mix. Then place the new mixture back into the oven on a further 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove form the oven and combine all ingredients together in a bowl to cool before serving."
    }
