  "recipeIngredient": [
    "2 cups rolled oats (not the instant kind)",
    "2 tablespoons chia seeds",
    "2 tablespoons unsweetened shredded coconut",
    "2 tablespoons maple syrup",
    "1 14-ounce can lite coconut milk",
    "1 cup unsweetened almond milk",
    "1 teaspoon vanilla extract",
    "Fresh ripe strawberries (quartered)",
    "Slivered almonds",
    "Pepitas (pumpkin seeds)",
    "Additional almond milk or coconut milk"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Add all oatmeal ingredients to a large container with an airtight lid and stir well. Seal and place in the refrigerator overnight (or at least 6 hours)."
    },
    {
      "@type": "HowToStep",
      "text": "Divide the oats between four bowls and pile each with strawberries, pepitas, and almonds. Add additional almond milk or coconut milk if desired."
    }
  ],
