  "recipeIngredient": [
    "600g French fries",
    "400g plant based mince",
    "150g vegan cheddar, grated",
    "0.4 cup passata",
    "2 tbs rosemary, chopped",
    "1 medium red onion, peeled and diced",
    "3 cloves garlic, peeled and mashed",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "1 tbs canola oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the store bought French fries as per packet instructions."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "While the fries are cooking, heat up a pan to a medium heat and add the canola oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the onions and garlic for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the mince and rosemary and cook for a further 6 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the passata to the pan and allow to simmer or reduce the sauce for about 10 minutes stirring occasionally."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "On a serving dish, place the fries, cover with the mince mixture and top with vegan grated cheese before serving."
    }
  ],
