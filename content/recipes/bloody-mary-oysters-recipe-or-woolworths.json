  "recipeIngredient": [
    "24 oysters, shucked, drained and rinsed",
    "400g tomatoes, chopped",
    "0.4 cup vodka",
    "4 tbs tobasco",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "2 tbs worcestershire sauce",
    "2 medium lemons, sliced into wedges"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Shuck, drain and rinse the oysters and place on your serving dish with the lemon wedges on the side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients to a food processor and blend until smooth."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour the contents into the oyster shells and serve chilled."
    }
  ],
