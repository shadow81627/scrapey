  "recipeIngredient": [
    "2 bacon rashers",
    "1 can corn, drained",
    "0.33 cup of cream",
    "0.5 tsp curry powder",
    "500g pkt frozen vegetables"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook bacon between sheets of absorbent paper in microwave for 2 mins. Check after 1 as microwave times vary."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place corn in a shallow microwave dish."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut bacon into pieces & add bacon, cream, curry powder & vegetables to corn dish."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook, covered on high for 7 mins. Stir & serve."
    }
  ],
