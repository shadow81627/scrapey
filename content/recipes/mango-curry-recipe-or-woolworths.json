  "recipeIngredient": [
    "1 medium mango, peeled, de-seeded and cut into bite size chunks.",
    "100g broccolini",
    "1 medium sweet potato, peeled and cut into bite size chunks",
    "1 tbs sesame oil",
    "1.6 cup coconut milk",
    "0.4 cup vegetable stock",
    "1 medium red chilli, sliced",
    "1 medium green chilli, sliced",
    "1 tsp tumeric powder",
    "1 tsp cumin",
    "1 tsp paprika",
    "1 tbs curry paste",
    "280g rice"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a large pot with the sesame oil to a medium heat and fry the sweet potato for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the mango, chillies and broccolini and fry for a further 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients, lower the gas to a simmer and allow the curry to cook for 20 minutes stirring occasionally."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and suggested to serve with rice or naan."
    }
  ],
