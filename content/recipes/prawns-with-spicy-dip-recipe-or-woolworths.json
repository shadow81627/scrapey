  "recipeIngredient": [
    "200g cream cheese",
    "100g natural yoghurt",
    "1 clove garlic, crushed",
    "2-3 drops lemon juice",
    "0.25 tsp dried chilli flakes",
    "1 handful chives, snipped",
    "2 Little Gem lettuces, leaves separated",
    "400g king prawns, cooked and peeled",
    "1 pinch salt",
    "1 pinch pepper"
