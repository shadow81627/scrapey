  "recipeIngredient": [
    "2 large flatbread, thawed if frozen",
    "300g chicken breast, cooked and torn",
    "160g feta, crumbled",
    "160g seedless grape, quartered",
    "100g baby tomatoes, halved",
    "60g pine nuts",
    "0.4-plus 4 tsp cup tomato paste",
    "80g rocket",
    "2 tbs olive oil",
    "1 tsp cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lay out the flatbreads and evenly spread the tomato paste on top."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Evenly place the rest of the ingredients apart from the rocket."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in the oven for 20 minutes or until the flatbread begins to crisp at the edges."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the oven and dress with rocket before serving."
    }
  ],
