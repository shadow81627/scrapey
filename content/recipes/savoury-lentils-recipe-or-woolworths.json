  "recipeIngredient": [
    "3 tin lentils, drained and rinsed",
    "2 onion, diced",
    "3 clove garlic, minced",
    "2 carrots, diced",
    "2 stalk celery, diced",
    "1 zucchini, diced",
    "5 mushrooms, diced",
    "1 tin corn kernels",
    "1 cup green peas",
    "1 broccoli stem, diced",
    "1 cup broccoli florets",
    "1 tbs Massel beef stock",
    "4 tbs Worcestershire sauce",
    "6 tbs tomato paste",
    "1 bay leaf",
    "2 tbs olive oil",
    "0.5 cup dried red lentils",
    "3 cup water"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/nvoheyl4u8b60z89nj0v?$foodhub-method-images$&fmt=png-alpha",
      "text": "Fry onion and garlic in olive oil on medium heat in a large saucepan till onion is transparent."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/yti96uvxbibr2msgiplw?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add all the diced vegetables to the saucepan and season well with salt and pepper. Cook well for 10 minutes, stirring regularly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/cipzbtf6tgbu9fhvu7sj?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add tinned and dried lentils, 3 cups of water, beef stock powder, Worcestershire sauce, tomato paste and bay leaf. Stir well, taste and season with salt and pepper if needed."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bring to the boil then turn heat down to low and simmer for 20-30 min until lentils are soft and sauce has thickened. If it becomes too thick add more water as required."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/bmh7gvtqd05qz6tx3ryh?$foodhub-method-images$&fmt=png-alpha",
      "text": "Serve hot as is or with bread, rice or your favourite potato such as mashed, roasted, boiled or with potato bake as pictured."
    }
  ],
