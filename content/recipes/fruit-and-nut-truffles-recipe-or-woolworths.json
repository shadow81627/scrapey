  "recipeIngredient": [
    "1 tbs cocoa",
    "1 cup coconut",
    "0.75 cup sultanas",
    "2 tbs glace cherries, chopped",
    "0.25 cup walnuts, chopped",
    "125g plain sweet biscuits, crushed",
    "0.66 cup condensed milk",
    "1 tbs sweet sherry",
    "250g chocolate melts",
    "30g coconut oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine cocoa, coconut, sultanas, glace cherries, walnuts, plain biscuits, condensed milk & sherry. Mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Roll into small balls. Melt chocolate & copha or coconut oil over hot water on top of a double saucepan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Dip truffles into chocolate mixture, place on a foil lined tray and allow to set. Can sprinkle with crushed candy canes if you wish before they set."
    }
  ],
