  "recipeIngredient": [
    "1 cup plain flour",
    "125g butter, softened",
    "0.66 cup brown sugar",
    "1 tsp vanilla essence",
    "2 tbs milk",
    "0.5 cup milk chocolate chips"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 200°C/180°C fanforced. Line a small baking tray with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread flour evenly on tray and bake for 7 minutes. Allow to cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Using an electric mixer, beat butter, sugar and vanilla on high for 5 minutes or until pale and creamy. Reduce speed to medium. Gradually add flour and milk, beating for 1 minute or until just combined. Fold through chocolate chips."
    }
  ],
