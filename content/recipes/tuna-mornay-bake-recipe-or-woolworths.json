  "recipeIngredient": [
    "250g pasta spirals",
    "20g margarine",
    "2 tbs plain flour",
    "1.5 cups milk",
    "2 cups grated cheese",
    "125g corn kernels, drained",
    "425g tuna in springwater, drained",
    "0.5 cup breadcrumbs"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat the oven to 180 degrees C"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook pasta according to packet directions and drain"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Melt butter in a medium saucepan over medium heat"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add flour and stir until smooth"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Gradually add milk, a little at a time, so no lumps form (I tend to smoosh the lumps against the side of the saucepan as I'm adding). Stir constantly until mixture comes to the boil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir through 1 cup of the grated cheese and then add corn, pasta and tuna."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer to casserole dish, top with remaining cheese and sprinkle over breadcrumbs."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake for approximately 20 mins or until cheese is lightly browned."
    }
  ],
