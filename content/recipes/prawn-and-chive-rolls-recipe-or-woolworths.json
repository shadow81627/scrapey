  "recipeIngredient": [
    "1 cup celery, chopped",
    "6 cups iceberg lettuce, shredded",
    "500g cocktail prawns",
    "1 lemon, rind finely grated",
    "1 pinch pepper (for seasoning)",
    "1 cup mayonaise",
    "12 petite pain rolls",
    "0.25 cup chives",
    "1 pinch sea salt ground (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine celery, mayonnaise, chives, finely grated rind of 1 lemon and lemon juice in a large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove tails from cocktail prawns and add to mayonnaise mixture. Season with salt and pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Split 12 petite plain rolls and fill with shredded iceberg lettuce and the prawn mixture. Serve."
    }
  ],
