  "recipeIngredient": [
    "1 large bulb roasted garlic* (or 2 smaller bulbs)",
    "1 (14 ounce or 400 gram) can chickpeas, (drained and rinsed)",
    "1/4 cup tahini",
    "2 tablespoons olive oil",
    "2 tablespoons lemon juice",
    "1 teaspoon ground cumin",
    "3/4 teaspoon salt, (or to taste)",
    "Water, (as needed (1 to 2 tablespoons)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place all ingredients except for water into the bowl of a food processor fitted with an s-blade."
    },
    {
      "@type": "HowToStep",
      "text": "Blend while drizzling in as much water as you need to reach the desired consistency. Continue blending until smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Taste-test and adjust any seasonings to your liking."
    },
    {
      "@type": "HowToStep",
      "text": "Serve."
    }
