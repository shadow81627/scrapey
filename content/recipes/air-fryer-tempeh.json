  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Cube the tempeh and toss with the avocado oil. Season with salt and pepper."
    },
    {
      "@type": "HowToStep",
      "text": "Pre-heat your air fryer to 375°F /190°C"
    },
    {
      "@type": "HowToStep",
      "text": "Place the tempeh cubes into the air fryer, and air fry for 10 minutes, shaking occasionally, until golden brown on all sides."
    },
    {
      "@type": "HowToStep",
      "text": "Add sauce or marinade to the fried tempeh if desired and let it sit for 5 minutes to soak it up."
    }
  ],
