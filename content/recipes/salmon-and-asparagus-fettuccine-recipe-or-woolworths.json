  "recipeIngredient": [
    "400g fettuccine",
    "1 tbs olive oil",
    "250g asparagus, trimmed and chopped",
    "2 cloves garlic, peeled and minced",
    "200g hot smoked salmon",
    "0.4 cup creme fraiche",
    "1 medium lemon, zest",
    "2 tbs capers",
    "30g parmesan, grated",
    "1 pinch cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the fettuccine according to pack instructions and drain, reserving a couple of tablespoons of starchy water."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in large pan on medium heat and add the garlic and asparagus and cook for 3 minutes. Stir in the lemon zest, juice and capers and cook for 2 minutes. Break the salmon up into flakes and stir in."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the creme fraiche and 2-3 tablespoons of starchy water. Cook for 2 minutes. Add the pasta and toss well. Season with cracked black pepper, top with parmesan and serve."
    }
  ],
