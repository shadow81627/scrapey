  "recipeIngredient": [
    "500g celeriac, peeled",
    "4 celery sticks, shredded",
    "2 tbs caster sugar",
    "3 tbs white wine vinegar",
    "2 tbs mayonnaise",
    "1 tsp wholegrain mustard",
    "1 tbs parsley, fresh, finely chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice the celeriac on a mandolin then chop into thin matchsticks. Place in a bowl with the celery, tip in the sugar and vinegar, cover and set aside for 20 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drain the celery and celeriac well and add to a clean bowl. Stir in the mayonnaise, mustard and parsley and serve."
    }
  ],
