  "recipeIngredient": [
    "200g choc coated biscuits (of choice)",
    "250g cream cheese",
    "400g melted chocolate"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Blitz chocolate coated biscuits of choice until they resemble fine crumbs"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix in cream cheese"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Roll into small balls and freeze"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Melt chocolate in the microwave in 30 sec bursts. Take cheesecake bites out of freezer once firm and dip into chocolate"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Refrigerate and set then enjoy"
    }
  ],
