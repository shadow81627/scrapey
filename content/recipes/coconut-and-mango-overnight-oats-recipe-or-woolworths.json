  "recipeIngredient": [
    "300g oats",
    "2.4 cups coconut milk, from a carton",
    "1 tbs chia seeds",
    "1 tbs maple syrup",
    "0.5 tsp vanilla extract",
    "45g coconut flakes, toasted",
    "1 medium mango, peeled and chopped into cubes"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the oats, coconut milk, chia seeds, maple syrup and vanilla extract in a bowl. Cover and place in the fridge overnight."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon into bowls or jars and top with toasted coconut flakes and mango and more milk, if desired."
    }
