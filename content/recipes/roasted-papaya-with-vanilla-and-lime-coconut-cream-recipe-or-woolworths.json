  "recipeIngredient": [
    "2 papaya, sliced in half",
    "2 limes, juice and 1 tsp zest",
    "2 tbs coconut sugar",
    "1.5 cup coconut cream, save 4 tsp"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 200°C/180°C fan-forced. Scoop out the papaya seeds and place the halves on a baking tray, cut side up."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Squeeze over the juice of 1.5 limes and sprinkle each half with coconut sugar. Roast for 15-20 minutes until caramelised and tender."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk the remaining lime juice and teaspoon of zest with the coconut cream, drizzle on the papaya and serve."
    }
  ],
