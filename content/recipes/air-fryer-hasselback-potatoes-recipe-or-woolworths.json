  "recipeIngredient": [
    "4 potatoes, washed and peeled",
    "1 pinch pepper (to taste)",
    "1 pinch salt (to taste)",
    "240g butter, melted",
    "1 tsp parsley, finely chopped",
    "1 tsp thyme, finely chopped",
    "1 tsp rosemary, finely chopped",
    "3 cloves garlic, minced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice the potatoes thinly leaving a half an inch base at the bottom. Season with salt and pepper. Place them inside the air fryer basket."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a small bowl, combine the melted butter, parsley, thyme, rosemary and garlic. Brush the garlic herb butter along the tops of the potatoes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook for 15-20 minutes at 185°C/165°C fan-forced until tender."
    }
  ],
