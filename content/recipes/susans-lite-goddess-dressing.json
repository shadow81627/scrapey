  "recipeIngredient": [
    "6 ounces firm or extra-firm silken tofu (1/2 pkg. of Mori-Nu, lite preferred)",
    "1/4 cup water",
    "1 1/2 tablespoons cider vinegar",
    "2 tablespoons lemon juice",
    "1 tablespoon tahini",
    "1 tablespoon chopped fresh parsley (packed)",
    "2 teaspoons low-sodium soy sauce",
    "1/2 teaspoon salt",
    "1/4 teaspoon sesame oil (optional, but good)",
    "2 green onions (tops only, chopped)",
    "1 large clove garlic",
    "1/4 teaspoon black pepper (optional)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place all ingredients in blender and process until smooth. Keep in covered jar in refrigerator for 1-2 weeks."
