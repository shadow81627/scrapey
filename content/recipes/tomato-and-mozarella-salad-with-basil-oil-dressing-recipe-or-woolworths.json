  "recipeIngredient": [
    "1 clove garlic, crushed",
    "0.5 cup fresh basil leaves",
    "8 large vine-ripened tomatoes, cut into wedges",
    "1 small red onion, thinly sliced",
    "0.5 cup extra virgin olive oil",
    "1 tbs balsamic vinegar",
    "250g buffalo mozzarella, sliced",
    "1 tbs lemon juice"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Process oil, half the basil leaves, garlic and lemon juice together in a small food processor until basil is finely chopped."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Layer tomato, mozzarella and onion in a serving bowl and season with salt and pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drizzle with dressing. Sprinkle with balsamic and remaining basil and serve immediately."
    }
  ],
