  "recipeIngredient": [
    "2 pkts woolworths cooked basmati rice 2 x 250 gram packs",
    "6 fresh dates, cut in half",
    "1 pkt fresh blackberries",
    "1 cup green grapes",
    "0.5 cup feta cheese, in fine dice",
    "6 fresh whole dates, torn up",
    "1 pomegranate, cut up",
    "6 tbs olives, sliced",
    "4 tbs dried tomatoes, shredded",
    "8 tbs greek style yoghurt",
    "0.25 cup fresh parsley, chopped",
    "1 cup chicken, diced, cooked (or prawns)",
    "1 tsp sumac",
    "0.5 purple onion, diced fine",
    "0.25 cup cooked chickpeas",
    "150g halloumi, sliced and cooked",
    "4 feta bell peppers, cut in half"
  ],
  "recipeInstructions": [
