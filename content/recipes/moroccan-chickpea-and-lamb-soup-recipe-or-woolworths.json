  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil over medium-high heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Brown carrots, onions and garlic - approximately 5mins."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add tomatoes, stock, seasoning, lamb and chickpeas, heat until boiling - approximately 15 mins."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Reduce heat and simmer for approximately 10 mins."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Tip: Depending on how much room I have in the freezer, I often reduce the amount of water, so I can fit more into my freezer. When I take out a container for reheating - I will add a cup of water to it."
    }
  ],
