  "recipeIngredient": [
    "50g butter",
    "4 onions, thinly sliced",
    "2 ripe pears, sliced",
    "125g gorgonzola blue cheese, crumbled",
    "1 tbs honey",
    "1 tbs balsamic vinegar",
    "1 pinch pepper (for seasoning)",
    "1 tbs olive oil",
    "1 cup walnuts",
    "4 vol au vonts",
    "1 pinch sea salt ground (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat butter and olive oil in a heavy based pan over a medium heat until the butter melts. Add thinly sliced onions. Cook, stirring occasionally, for 20-25 minutes until onion turns golden."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir in balsamic vinegar and cook for 5 minutes. Season. Transfer to a bowl. Preheat oven to 180Â°C."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place walnuts onto a lined tray. Drizzle over honey. Toss to coat. Bake for 10 minutes until golden. Cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place Vol Au Vents on a tray and heat following packet instructions."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon onion into bases. Add crumbled blue cheese, walnuts and sliced ripe pears. Top with onion. Cook for 8-10 minutes until cheese melts. Serve."
    }
  ],
