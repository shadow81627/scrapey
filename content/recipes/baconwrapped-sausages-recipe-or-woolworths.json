  "recipeIngredient": [
    "2 tbs maple syrup",
    "2 tbs extra virgin olive oil",
    "1 tbs seeded mustard",
    "500g Woolworths Classic Cumberland pork sausages",
    "12 slices streaky bacon, thin slices",
    "3 truss tomatoes, halved",
    "3 vineripened tomatoes, halved",
    "200g pkt mixed salad leaves, to serve"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat a barbecue grill or chargrill pan over mediumhigh heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk syrup, oil and mustard in a bowl. Wrap sausages in bacon."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Grill sausages and tomatoes, turning and brushing with syrup mixture occasionally, for 10 minutes or until cooked through and browned. Transfer to a plate. Cover with foil. Rest 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve sausages with tomatoes and salad."
    }
  ],
