  "recipeIngredient": [
    "4-6 potatoes",
    "1 tbs sweet paprika",
    "1 tsp onion powder",
    "0.25-0.5 tsp salt (to taste)",
    "2 tbs plain flour",
    "2 tbs olive oil",
    "1 tbs butter (Nuttelex for dairy free)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut your potatoes into wedge shapes. Normally one potato will get you 4-6 wedges."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place in a pot of water and par boil potatoes for 5-7 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drain potatoes in a colander and lightly chuff up potatoes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "While potatoes are still hot mix in your butter, oil, flour, onion powder, paprika and salt."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Air fry at 200Â° for 15 minutes tossing half way through. These can also be done in the oven just allow a longer cook time. 25-30 minutes or until golden brown."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with your favourite sauce. Sweet chilli and sour cream or good old tomato!"
    }
  ],
