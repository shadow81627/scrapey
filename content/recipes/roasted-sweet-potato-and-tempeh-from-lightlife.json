  "recipeIngredient": [
    "3 large sweet potatoes",
    "1 package of Lightlife Original Organic Soy Tempeh",
    "1/4 cup olive oil",
    "1 tablespoon chopped fresh rosemary",
    "Sea salt and black pepper",
    "1 cup marmalade",
    "1 tablespoon butter",
    "2 tablespoons brown sugar",
    "1/4 teaspoon ground cinnamon",
    "1 teaspoon dried oregano",
    "2 tablespoons water"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat the oven to 400 degrees F."
    },
    {
      "@type": "HowToStep",
      "text": "Peel the sweet potatoes and chop into small cubes; transfer to a large bowl."
    },
    {
      "@type": "HowToStep",
      "text": "Slice the tempeh into cubes and add to the bowl with the olive oil and rosemary."
    },
    {
      "@type": "HowToStep",
      "text": "Toss well to coat everything and place on a large baking tray."
    },
    {
      "@type": "HowToStep",
      "text": "Season with salt and black pepper."
    },
    {
      "@type": "HowToStep",
      "text": "Roast in the oven for 35-40 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "While the sweet potatoes are cooking make the sauce."
    },
    {
      "@type": "HowToStep",
      "text": "In a small pan add the marmalade, butter, brown sugar, cinnamon, oregano and water."
    },
    {
      "@type": "HowToStep",
      "text": "Bring to a simmer and cook for about 5 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Transfer the sweet potatoes to a platter and drizzle the sauce over the top."
    },
    {
      "@type": "HowToStep",
      "text": "Serve at once."
    }
  ],
