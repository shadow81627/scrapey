  "recipeIngredient": [
    "7 g gelatin (unflavored)",
    "1/3 cup blueberry juice (or grape juice)",
    "2 cups 0% Greek yogurt (vanilla flavored)",
    "1 cup blueberries",
    "3 tbsp maple syrup",
    "garnish (mint leaves & whole blueberries)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Pour the contents of the gelatin sachet and the blueberry juice into a bowl. Let the gelatin bloom for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "In a blender container, add the yogurt, blueberries, juice, and the maple syrup. Emulsify for 1 minute, or until mixture is homogeneous."
    },
    {
      "@type": "HowToStep",
      "text": "Heat the gelatin in the microwave for 20 to 30 seconds. With the blender running, slowly pour gelatin mixture into the blueberry and yogurt base."
    },
    {
      "@type": "HowToStep",
      "text": "Divide the mixture into four small ramekins and chill for 2 to 3 hours. Garnish with the mint leaves and reserved blueberries."
    }
  ],
