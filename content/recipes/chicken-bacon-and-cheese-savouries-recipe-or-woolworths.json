  "recipeIngredient": [
    "1 cup white rice, cooked",
    "50g cooked chicken meat, finely chopped",
    "2 pieces short bacon, cooked and finely chopped",
    "0.5 cup Woolworths Italian cheese blend",
    "1 egg yolk",
    "40g panko breadcrumbs"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 220C degrees."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Put all ingredients except panko breadcrumbs in a bowl and stir to combine well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Form mixture into balls a little smaller than golf balls. Roll in panko breadcrumbs and place on baking tray. Cook approximately 20 minutes."
    }
  ],
