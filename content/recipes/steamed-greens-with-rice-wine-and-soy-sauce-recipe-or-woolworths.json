  "recipeIngredient": [
    "1 tbs cornflour",
    "1 tsp sesame oil",
    "400g broccoli, cut into florets",
    "1.5 tsp finely grated ginger",
    "200g sugar snap peas, topped, tailed",
    "0.5 tsp dried chilli flakes",
    "2 tbs soy sauce",
    "1 tbs peanut oil",
    "0.25 cup Chinese cooking wine",
    "60g slivered almonds, toasted (to serve)",
    "1 clove garlic, crushed"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil in a wok over medium heat. Add garlic, ginger and chilli and cook for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add wine, soy sauce, 1/4 cup water and sesame oil. Bring to a gentle boil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Blend cornflour with 1 tbs water until smooth. Add to wok and simmer until sauce thickens slightly. Steam broccoli for 8 minutes or until tender. Add peas and cook for further 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add steamed vegetables to warm sauce. Serve topped with toasted almonds."
    }
  ],
