  "recipeIngredient": [
    "1/2 cup (50 grams) oats (, old fashioned or rolled oats (use certified gluten-free oats if necessary)",
    "1 to 2 tablespoons chocolate chips",
    "1 tablespoon chia seeds",
    "1 tablepoon cocoa powder",
    "1 tablespoon maple syrup (, or 1 medjool date)",
    "1 small pinch fine salt",
    "3/4 cup (180 mls) plant milk"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Add all of the ingredients to a small jar or lidded container that will hold at least 8fl oz (250ml)."
    },
    {
      "@type": "HowToStep",
      "text": "Stir, or put the lid on and shake well."
    },
    {
      "@type": "HowToStep",
      "text": "Place in the fridge for at least 4 hours or up to 5 days."
    }
  ],
