  "recipeIngredient": [
    "1 tsp mustard seeds",
    "1 tbs parsley, chopped finely",
    "1 tsp garlic powder",
    "0.5 tsp salt",
    "1 cup sour cream",
    "1 cup mayonnaise"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix all ingredients in a bowl until well combined or you can use a food processor on low speed for 1-2 minutes until well combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer to a container and cover."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Refrigerate for at least 1-2 hours before serving and enjoy with chips or crackers."
    }
  ],
