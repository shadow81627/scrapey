  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Rinse the beans: Pour the beans into a colander and rinse them under cool running water. Check and discard any stones or other debris. Drain."
    },
    {
      "@type": "HowToStep",
      "text": "Transfer to the saucepan and cover with water: Tip the beans into a pot and cover with about 2 inches of water."
    },
    {
      "@type": "HowToStep",
      "text": "Boil the beans: Bring the water to a boil over high heat. Cook the beans for 1 minute, then remove the pot from the heat."
    },
    {
      "@type": "HowToStep",
      "text": "Soak for 1 hour: Cover with a lid and let the beans to soak in the warm water for one hour."
    },
    {
      "@type": "HowToStep",
      "text": "Drain the beans: Drain the beans into a colander. Rinse and begin to cook according to your recipe."
    }
  ],
