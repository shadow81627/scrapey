  "recipeIngredient": [
    "1.5 cups rice",
    "3 cups water",
    "1/2 cup chopped/sliced carrots",
    "1/2 cup chopped green onions",
    "1/2 cup chopped bell pepper",
    "1/2 cup chopped green beans",
    "2 tsp oil",
    "4 cloves of garlic sliced",
    "1 tsp soy sauce",
    "1 tsp vinegar",
    "1/2 teaspoon sugar",
    "some chopped coriander"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Prepare rice in a rice cooker."
    },
    {
      "@type": "HowToStep",
      "text": "In a large pan, add oil and let it get hot."
    },
    {
      "@type": "HowToStep",
      "text": "Add the garlic slices and saute for a few minutes till the garlic stars turning brown. Let the garlic burn a little."
    },
    {
      "@type": "HowToStep",
      "text": "Add the chopped vegetables and stir fry for a few minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Add egg and quickly scramble it. (optional)"
    },
    {
      "@type": "HowToStep",
      "text": "Add soy sauce, vinegar, sugar and the rice and mix well."
    },
    {
      "@type": "HowToStep",
      "text": "Add salt if needed."
    },
    {
      "@type": "HowToStep",
      "text": "Stir fry on high for a minute."
    },
    {
      "@type": "HowToStep",
      "text": "Sprinkle some chopped cilantro and serve hot!"
    }
  ],
