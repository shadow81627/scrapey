  "recipeIngredient": [
    "90g oats",
    "30g sunflower seeds",
    "30g walnuts",
    "75g dates, pitted and chopped",
    "1 tsp vanilla",
    "150g carrots, peeled, grated",
    "40g raisins",
    "1.5 tsp cinnamon",
    "0.25 medium orange, juice and zest"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place all the ingredients in a food processor. Blend until a sticky dough forms, 2-3 minutes, and ensure there are no large date or nut pieces remaining."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Roll into 15 balls using your hands and place on a large plate. Chill in the fridge or serve at room temperature."
    }
  ],
