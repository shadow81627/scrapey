  "name": "Chocolate Ferrero Cake Recipe | Woolworths",
  "prepTime": "PT20M",
  "recipeCategory": "Desserts",
  "recipeCuisine": "Australian",
  "recipeId": "a0K2y000000ocfUEAQ",
  "recipeIngredient": [
    "4 eggs",
    "1 tsp vanilla essence",
    "0.25 tsp salt",
    "2 cups sugar",
    "4 tbs cocoa",
    "2 cups cake flour",
    "4 tsp baking powder",
    "1 cup oil",
    "1 cup boiling water",
    "300g cadbury milk chocolate",
    "250g fresh whipped cream (125g for filling remainder for ganache)",
    "8 ferrero chocolates",
    "6 tbs caramel"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Separate eggs and beat egg white stiff."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sift all dry ingredients and mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add egg yolks and vanilla essence to dry ingredients."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add oil and water to dry ingredients and mix."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fold in egg whites."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide batter in 2 equal parts."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place in the oven for 20 - 25 min on 180 degrees Celsius."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once cooled spread caramel in the centre."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pipe 125g whipped cream in the centre."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sandwich the cake together and cover with chocolate ganache. Melt Cadbury chocolate with 25g of unwhipped cream in the microwave for approximately one minutes checking at 30 seconds and stir until smooth,place in the refrigerator for 10 minutes"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Take out of fridge and whip cream together with chocolate and cover the sides of the cake"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Decorate as desired."
    }
  ],
