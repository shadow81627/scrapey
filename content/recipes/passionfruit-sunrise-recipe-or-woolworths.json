  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fill 2 chilled glasses with ice. Add 30ml vodka, 1 tbs coconut liqueur."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Carefully pour 1 tbs grenadine down the side of each glass."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add pulp from 1 passionfruit and top with orange juice. Garnish with raspberries. Serve Note: This recipe contains alcohol."
    }
