  "recipeIngredient": [
    "200g long fresh red chillies",
    "2 tbs garlic powder",
    "1 cup white vinegar",
    "1 cup white sugar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a food processor put in the chillies that have been deseeded and washed."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add garlic powder and white vinegar."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Process on low speed until all ingredients are well combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a medium saucepan add chilli mixture and cook on low heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Keep stirring for 5 minutes until all sugar is dissolved."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bring to boil then simmer for 30 minutes or until sauce thickens."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer to bowl and cool completely."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Store in closed container in the fridge until needed. Enjoy!"
    }
  ],
