  "recipeId": "a0K2y00000178vcEAA",
  "recipeIngredient": [
    "1 sheet Woolworths puff pastry",
    "1 punnet blueberries",
    "3 tsp sugar",
    "0.5 tsp vanilla essence",
    "1 tsp cornstarch",
    "20g butter",
    "1 egg",
    "1 tbs coconut flake"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/vzzsmqyeqap01nmwdat5?$foodhub-method-images$&fmt=png-alpha",
      "text": "Allow the puff pastry sheet to thaw before dividing it into 4 squares"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/jqdoyccou284omukflzs?$foodhub-method-images$&fmt=png-alpha",
      "text": "Wash the blueberries, allow to dry. Once dry, mix with sugar, cornstarch, and vanilla essence."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/qsrtukn0cvxys13r0cjp?$foodhub-method-images$&fmt=png-alpha",
      "text": "Melt the butter in the microwave, then brush it on the pastry sheet."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/skfife8ar2pwoehql643?$foodhub-method-images$&fmt=png-alpha",
      "text": "Fold up the four corners of the pastry sheet (1-2 cm) on each sides. Place a handful of blueberries in the centre of the sheets."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/b0htmeupxvhc5lctlq1b?$foodhub-method-images$&fmt=png-alpha",
      "text": "Brush the edges with egg wash, then sprinkle the coconut flakes on top of the blueberries."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/jlc7iobxgv27rwmy22d5?$foodhub-method-images$&fmt=png-alpha",
      "text": "Preheat the oven and bake for 20 mins or until brown at 210Â°C. Ready to serve."
    }
  ],
