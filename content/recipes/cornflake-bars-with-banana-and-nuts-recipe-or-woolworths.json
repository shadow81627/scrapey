  "recipeIngredient": [
    "30g unsalted butter",
    "50g brown sugar",
    "3 tbs honey",
    "2 medium banana, peeled and mashed",
    "300g cornflakes",
    "60g macadamia nuts, crushed"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a saucepan to a medium heat and melt the butter with the sugar and honey for 5 minutes until all melted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and allow to cool slightly before pouring into a mixing bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the banana, cornflakes and nuts and combine fully suing a spoon."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pack the mixture into a loaf tin and allow to set in the fridge for at least 60 minutes before slicing into bars."
    }
  ],
