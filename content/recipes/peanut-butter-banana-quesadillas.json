  "recipeIngredient": [
    "1 8-inch flour tortilla ($0.30)",
    "2 Tbsp peanut butter ($0.14)",
    "1/2 medium banana, sliced ($0.11)",
    "1 Tbsp chocolate chips ($0.10)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Spread the peanut butter over the surface of the tortilla."
    },
    {
      "@type": "HowToStep",
      "text": "Thinly slice the banana, then lay the slices over one half of the tortilla. Sprinkle the chocolate chips over the same half of the tortilla. Fold the tortilla to close over the banana and chocolate."
    },
    {
      "@type": "HowToStep",
      "text": "Cook the quesadilla in a skillet over medium-low heat until it's golden brown and crispy on both sides and the chocolate and banana slices are melty inside."
    }
  ],
