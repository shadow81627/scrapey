  "recipeIngredient": [
    "375g flat mushrooms",
    "3 tbs olive oil",
    "1 clove garlic",
    "1 pinch rock salt",
    "4 beef burger",
    "4 hamburger buns",
    "1 tomato, thinly sliced",
    "1 Lebanese cucumber, thinly sliced",
    "100ml aioli",
    "4 leaves lettuce (optional)",
    "4 slices tasty cheese (optional)",
    "4 rashers bacon (optional)",
    "400g oven fries"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat a barbecue flat plate on medium."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "BBQ patties and bacon (refer to pack instructions for cooking)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine olive oil and crushed garlic, then set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook mushrooms stem down (for approximately 46 mins), drizzle with olive oil & garlic, salt to taste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Buns can be served fresh or toasted. Cook fries as per packaging instructions."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread sauce on base of the bun and layer with bacon, cheese, tomato, burger pattie, mushroom, cucumber and lettuce."
    }
  ],
