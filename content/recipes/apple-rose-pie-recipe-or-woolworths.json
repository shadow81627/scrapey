  "recipeIngredient": [
    "2 piece puffy pastry",
    "1 apple",
    "1 tsp lemon juice",
    "2 tbs orange jam (or fruit jam)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice the apple into 5mm thick strips and put into a big bowl with hot water and the lemon juice. then put into microwave for 1~2min. the apple is ready when it becomes soft and doesn't break when you bend it."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/prcxqbgac5hglc2sptdh?wid=3000&hei=4000&fmt=png-alpha",
      "text": "Cut the pastry equally lengthwise into 4 rectangular pieces, put some orange jam on the pastry. Then lay the apple slices out on top. then roll it and put into the muffin bake tin."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/prcxqbgac5hglc2sptdh?wid=3000&hei=4000&fmt=png-alpha",
      "text": "Preheat oven 180 degrees C."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake for 15~20 mins."
    }
  ],
