  "recipeIngredient": [
    "0.75 cup desiccated coconut",
    "2 cup self-raising flour",
    "2 tbs milk",
    "60g butter, melted",
    "0.5 cup macadamias, chopped",
    "20g roasted coconut chips",
    "2 eggs",
    "0.5 cup caster sugar",
    "0.33 cup macadamia oil"
