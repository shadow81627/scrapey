  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180Â°C. Line 12 holes of a muffin pan with paper cases. Combine flour, coconut, sugar and macadamia in a bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lightly whisk eggs, oil, butter and milk in a bowl. Fold into flour mixture."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide among muffin holes. Top with coconut. Bake for 20 minutes or until a skewer inserted into centre comes out clean. Serve warm or at room temperature."
    }
