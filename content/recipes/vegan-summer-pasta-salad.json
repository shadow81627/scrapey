  "recipeIngredient": [
    "2 1/2 cups vegetable broth",
    "1 cup orzo pasta (uncooked)",
    "4 tbsp olive oil",
    "2 cups tomatoes (diced)",
    "1 cup cucumber (diced)",
    "1 bell pepper",
    "6 green shallots (chopped)",
    "3 tbsp parsley",
    "3 tbsp white balsamic vinegar",
    "2 cloves garlic (crushed)",
    "1/2 tsp salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "In a saucepan, bring broth to a boil."
    },
    {
      "@type": "HowToStep",
      "text": "Add the orzo and boil for 10 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Drain. Place in a bowl with 1 tablespoon olive oil."
    },
    {
      "@type": "HowToStep",
      "text": "Add all the vegetables (tomatoes, cucumbers, peppers, shallots) to the orzo."
    },
    {
      "@type": "HowToStep",
      "text": "In a small bowl, mix together the balsamic vinegar, remaining oil, garlic, salt, pepper, and parsley."
    },
    {
      "@type": "HowToStep",
      "text": "Add the dressing to the orzo and mix well."
    }
  ],
