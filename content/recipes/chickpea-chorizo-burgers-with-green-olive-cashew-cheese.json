  "recipeIngredient": [
    "1 cup raw cashews, (soaked in water 4-8 hours, drained and rinsed)",
    "1/4 cup unflavored non-dairy milk",
    "1 tablespoon red wine vinegar",
    "1 garlic clove",
    "1/2 teaspoon salt",
    "1 cup pimento-stuffed green olives",
    "1 (14 ounce) can chickpeas, (drained and rinsed)",
    "1/2 cup chopped walnuts",
    "1 cup panko breadcrumbs",
    "1 small onion, (coarsely chopped)",
    "3 garlic cloves, (minced)",
    "1 tablespoon soy sauce",
    "2 teaspoons smoked paprika",
    "1 teaspoon sweet paprika",
    "1 teaspoon ancho chile powder",
    "1 teaspoon ground cumin",
    "1/2 teaspoon dried thyme",
    "1/4 teaspoon ground cinnamon",
    "1/4 teaspoon ground cloves",
    "1/4 teaspoon black pepper",
    "1/4 teaspoon cayenne pepper, (or to taste)",
    "Salt to taste",
    "High heat cooking oil, (such as canola)",
    "6 burger buns",
    "Lettuce, tomato, and/or onions"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place the cashews, milk, vinegar, garlic, and salt into the bowl of a food processor and blend until smooth, stopping to scrape down the sides of the bowl as needed."
    },
    {
      "@type": "HowToStep",
      "text": "Add the olives to the food processor and blend for a couple seconds more, just until they are finely chopped and well incorporated into the mixture."
    }
  ],
