  "recipeIngredient": [
    "0.5 cup salt-reduced soy sauce",
    "1.5 tbs brown sugar",
    "1 tbs cornflour",
    "375g portobello mushrooms, sliced",
    "4cm ginger, peeled, finely grated",
    "4 cloves garlic, crushed",
    "450g frozen broccoli florets",
    "0.5 bunch spring onion, thinly sliced",
    "2 tbs vegetable oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Mongolian_mushrooms_with_broccoli_step_1?$foodhub-method-images$&fmt=png-alpha",
      "text": "Combine soy, sugar, cornflour and 1/3 cup water in a small jug."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Mongolian_mushrooms_with_broccoli_step_2?$foodhub-method-images$&fmt=png-alpha",
      "text": "Heat oil in a wok or large frying pan over high heat. Stir-fry mushroom, ginger and garlic for 5 minutes or until mushroom is tender. Season with pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Mongolian_mushrooms_with_broccoli_step_3?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add soy mixture to pan. Stir-fry for 2 minutes or until sauce thickens."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Mongolian_mushrooms_with_broccoli_step_4?$foodhub-method-images$&fmt=png-alpha",
      "text": "Heat rice and cook broccoli according to packet instructions. Divide rice, mushroom mixture and broccoli among bowls. Scatter over onion to serve."
    }
  ],
