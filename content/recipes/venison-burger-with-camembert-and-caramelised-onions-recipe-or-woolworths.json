  "keywords": "Burger",
  "name": "Venison Burger with Camembert & Caramelised Onions Recipe | Woolworths",
  "prepTime": "PT20M",
  "recipeCategory": "Lunch,Dinner",
  "recipeCuisine": "French",
  "recipeId": "RGWW337931",
  "recipeIngredient": [
    "500g ground venison",
    "4 cloves garlic, peeled and diced",
    "1 tsp onion powder",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "2 tbs balsamic vinegar",
    "2 tbs worcestershire sauce",
    "2 tbs butter",
    "100g camembert, sliced into 4",
    "4 brioche buns, sliced in half",
    "4 tbs mayonnaise",
    "2 tbs wholegrain mustard",
    "100g caramelised onions, drained"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat a chargrill to a medium heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a bowl, combine all of your burger ingredients and ensure fully mixed together."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide the mixture into 4 and shape into burger patties."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the burgers on the chargrill and allow to cook for 15 minutes while turning halfway."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "While the burgers are cooking, toast the brioche buns for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread an equal amount of mayonnaise and wholegrain mustard on each bun, a slice of camembert and 25g of onions."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove the burgers from the chargrill and place in the filled brioche buns before serving."
    }
  ],
