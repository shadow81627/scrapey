  "recipeIngredient": [
    "1/4 cup chopped red onion",
    "1/4 cup diced fresh tomato",
    "1 jalapeño pepper, (finely chopped, seeds and stem removed (optional)",
    "1/4 cup fresh cilantro",
    "1/2 teaspoon salt, (plus more to taste)",
    "3 medium avocados, (peeled and pitted)",
    "2 tablespoons lime juice",
    "3/4 cup diced mango",
    "1 chipotle pepper (from a can)",
    "1 tablespoon adobo sauce (from your can of chipotles), plus more to taste)",
    "1/2 cup fresh corn kernels",
    "1/2 cup diced poblano pepper",
    "1 teaspoon olive oil",
    "1 teaspoon ground cumin"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place the onion, tomato, jalapeño, cilantro, and 1/2 teaspoon salt into your molcajete base. If you're making chipotle guacamole, include the chipotle pepper as well. Use the grinder to mash everything to a pulp."
    },
    {
      "@type": "HowToStep",
      "text": "Add the avocado and mash until creamy. Add the lime juice and continue mashing."
    },
    {
      "@type": "HowToStep",
      "text": "Taste-test and add more salt or lime juice if needed."
    }
  ],
