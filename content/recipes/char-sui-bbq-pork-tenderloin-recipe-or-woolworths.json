  "recipeIngredient": [
    "800g pork tenderloin",
    "400g noodle, cooked as per packet instructions",
    "0.4 cup beef stock",
    "4 tbs hoisin sauce",
    "1 tbs oyster sauce",
    "4 tbs soy sauce",
    "4 tbs maple syrup",
    "2 tsp five spice powder",
    "2 tbs sesame oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add all ingredients to a saucepan apart from the tenderloin and allow to simmer for 5 minutes while mixing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and allow to cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the pork tenderloin in a bowl or a sealable bag and empty the sauce into the container to marinate in the fridge overnight."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fire up the BBQ and take the pork from the container and onto the BBQ."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "You can cook the pork for about 15 minutes, turning regularly and also basting with the marinate constantly while cooking. Cook noodles as per packet instructions."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once cooked serve pork on a bed of noodles."
    }
  ],
