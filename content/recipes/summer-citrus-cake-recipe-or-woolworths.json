  "recipeIngredient": [
    "125g butter",
    "2 eggs",
    "2 cups wholemeal plain flour",
    "1 orange, zested",
    "2 lemon, zested",
    "1 tsp vanilla essence",
    "0.5 cup caster sugar",
    "0.5 cup greek yoghurt",
    "1 orange, juiced",
    "2 lemons, juiced",
    "2 limes, juiced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180Â°c & line a round cake tin."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a large bowl combine the butter, orange, lemon & lime zests and sugar. Beat on medium until the mixture is light & fluffy."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the eggs one at a time & best to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add vanilla essence, Greek yoghurt and juice from the orange, lemons and limes. Using a wooden spoon stir to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a separate bowl sift the flour."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Gradually add the flour to the creamed butter mixture until all combined. Do not over beat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/c9tnefi5fj7iwax5qdrz?$foodhub-method-images$&fmt=png-alpha",
      "text": "Pour batter into prepared tin & spread evenly. Cook for roughly 50 minutes or when a toothpick comes out clean."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/a8vnejkywbp6udlzwdjq?$foodhub-method-images$&fmt=png-alpha",
      "text": "Allow to cool before serving."
    }
  ],
