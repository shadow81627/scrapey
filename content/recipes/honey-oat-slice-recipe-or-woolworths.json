  "recipeIngredient": [
    "1 cup rolled oats",
    "1 cup sultanas",
    "0.5 cup wholemeal self raising flour",
    "0.5 cup coconut",
    "0.5 cup raw sugar",
    "160g margarine, melted",
    "1 tbs honey"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven at 170 deg. In a oven baking tray about 27cm x 18cm. Line the bottom with baking paper and grease the sides with margarine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a large bowl mix the first 5 ingredients."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In the microwave melt the margarine for 30 seconds. Add honey to melted margarine and stir well. Add honey and margarine mix to dry ingredients, mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In your prepared oven tin flatten mixture down. Cook in the oven for 15 minutes or until golden. Cut your slice in the tin while still warm. Once slice is cooled remove from tin."
    }
  ],
