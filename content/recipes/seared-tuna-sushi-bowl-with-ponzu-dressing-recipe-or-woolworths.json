  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Brush the tuna steaks with half of the oil and sear in a griddle pan on the highest heat for 20-30 seconds on each side. Rest for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the brown rice according to pack instructions and cook the edamame for 2-3 minutes in a pan of boiling water, until tender."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Arrange the rice and vegetables in bowls, keeping each ingredient in a separate section. Top with sliced tuna, fresh coriander and ponzu sauce."
    }
