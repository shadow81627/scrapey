  "recipeIngredient": [
    "200g tuna steaks",
    "2 tbs olive oil, divided",
    "400g brown rice, ready cooked",
    "250g edamame, frozen",
    "5 spring onions, sliced",
    "1 large avocado, peeled and diced",
