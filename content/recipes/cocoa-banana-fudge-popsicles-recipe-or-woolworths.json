  "recipeIngredient": [
    "3 medium bananas, ripe",
    "0.5 cup milk",
    "2 tbs cocoa powder, unsweetened",
    "2 tbs honey",
    "1 tsp vanilla"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place all the ingredients in a blender and blend until thick, smooth and creamy."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Taste and add more honey if necessary. Pour into popsicle moulds or small cups with wooden sticks."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Freeze overnight, remove from moulds and serve."
    }
