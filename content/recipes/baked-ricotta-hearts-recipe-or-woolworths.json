  "recipeIngredient": [
    "1 pinch dried chilli flakes",
    "1 tsp lemon, rind finely grated",
    "0.75 cup parmesan, grated",
    "1 pinch pepper (for seasoning)",
    "4 slices thick bread, toasted",
    "250g full fat ricotta",
    "2 tsp olive oil",
    "1 egg",
    "1 pinch sea salt ground (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 200°C. Combine ricotta, grated parmesan, egg, finely grated lemon rind, thyme leaves and pinch dried chilli flakes in a bowl. Season."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon into 4x10cm heartshaped pans. Brush tops with olive oil. Bake for 20 minutes or until golden and set."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with toast and tomato medley and basil."
    }
  ],
