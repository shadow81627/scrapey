  "keywords": "Oysters,Christmas,Seafood,Spinach,Summer",
  "name": "Rockefeller Oysters Recipe | Woolworths",
  "prepTime": "PT15M",
  "recipeCategory": "Lunch,Dinner",
  "recipeCuisine": "American",
  "recipeId": "RGWW338010",
  "recipeIngredient": [
    "24 oysters, shucked, drained and rinsed.",
    "4 tbs butter",
    "2 shallots, diced",
    "1 stalk celery, diced",
    "100g spinach, washed and drained",
    "200g panko bread crumbs",
    "1 tbs tarragon",
    "2 tbs tobasco",
    "80g parmesan, grated"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up the grill on the oven and prepare a baking tray with shaped kitchen foil to hold the oysters steady."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the oysters on the foil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a pan on a medium heat, fry the butter with shallots and celery for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the spinach to the pan and cook for a further minute until the spinach has wilted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and combine with the breadcrumbs."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Equally cover the oysters with the crumb mix and broil in the grill for 3 minutes until the crumbs start to brown."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve drizzled with tabasco, tarragon and grated parmesan."
    }
  ],
