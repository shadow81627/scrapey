  "recipeIngredient": [
    "200g black glutinous rice",
    "60g brown sugar",
    "250g coconut milk",
    "3 medium banana, peeled and sliced",
    "2 cups water"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rice and water to a large pot and bring to the boil, then simmer for 40 minutes stirring occasionally."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once the water has started to reduce, add the coconut milk and sugar and allow to simmer for a further 20 minutes while stirring."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and allow to cool slightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once cooled, layer with sliced banana in your serving glasses or bowls and allow to set for 10 minutes before serving."
    }
  ],
