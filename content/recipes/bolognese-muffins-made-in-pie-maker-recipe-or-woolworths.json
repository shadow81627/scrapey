  "recipeIngredient": [
    "2 eggs",
    "0.33 cup self raising flour",
    "1 tbs milk",
    "0.25 cup grated tasty cheese",
    "0.75 cup bolognese sauce (leftover)",
    "1 pinch salt",
    "1 pinch pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Beat egg, milk together in separate bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix flour salt and pepper together."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix flour mix together with egg milk mix."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fold through grated cheese and bolognese sauce."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide mix into 4 (for deep pie maker) or 6 for traditional pie maker. Cook 5 minutes (lid closed)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Turn over and cook (lid down) another couple of minutes."
    }
  ],
