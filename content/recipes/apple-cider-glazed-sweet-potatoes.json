  "recipeIngredient": [
    "1 1/2 lbs. sweet potatoes (peeled and cut into 3/4-inch pieces)",
    "1 tbsp. olive oil",
    "salt + pepper to taste",
    "1/2 c. spiced apple cider"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat oven to 400 degrees."
    },
    {
      "@type": "HowToStep",
      "text": "Toss sweet potatoes with oil on a rimmed baking sheet. Season with salt and pepper. Bake 25 minutes, stirring halfway through cooking time."
    },
    {
      "@type": "HowToStep",
      "text": "While potatoes are roasting, heat apple cider in a small saucepan over high heat. Bring to a boil and cook until reduced to about 2 tablespoons, about 12 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Drizzle sweet potatoes with apple cider reduction; toss to coat. Bake 5 minutes more."
    }
  ],
