  "name": "Cinnamon, Cashew & Almond Granola Recipe | Woolworths",
  "prepTime": "PT10M",
  "recipeCategory": "Breakfast",
  "recipeCuisine": "American",
  "recipeId": "RGWW338418",
  "recipeIngredient": [
    "160g oats",
    "70g cashew nuts, chopped",
    "35g almonds",
    "35g sesame seeds",
    "1 tsp vanilla",
    "1 tbs cinnamon",
    "3 large egg whites",
    "1 pinch salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 110°C/90°C fan-forced and line two large baking trays with parchment."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix all the dry ingredients together in a large bowl and add the vanilla."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Beat the egg whites in a large bowl with a whisk or handheld mixer to form soft peaks."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fold the egg whites gently into the oat mix until well coated."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Tip the mixture on to the baking trays and spread out to form thin layers. Bake for 60 minutes, stirring every 20 minutes."
    }
  ],
