  "recipeIngredient": [
    "1.5 cup self raising flour, sifted",
    "0.8 cup sugar",
    "1 tbs lemon rind, finely grated",
    "0.33 cup lemon juice, freshly squeezed",
    "0.5 cup milk",
    "125g butter, softened",
    "2 eggs, lightly beaten"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180C."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine Self raising flour and sugar in a bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Make a well in the center."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add lemon rind, eggs, lemon juice, milk, and butter."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine ingredients to form a smooth batter."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour batter into a well greased 20cm cake tin or a 10x20cm loaf tin."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in a moderate oven for about 4045 minutes."
    }
  ],
