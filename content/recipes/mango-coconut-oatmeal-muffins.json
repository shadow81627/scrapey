  "recipeIngredient": [
    "1/2 cup whole wheat flour",
    "1/2 cup all-purpose flour",
    "3/4 cup brown sugar",
    "1 tsp baking powder",
    "1/2 tsp baking soda",
    "1 cup old fashioned oats",
    "1/3 cup dried shredded coconut",
    "1 6 oz container plain non-fat Greek yogurt",
    "1 cup mashed ripe banana",
    "1/4 cup canola oil",
    "1 egg",
    "1 tsp vanilla extract",
    "1 cup diced mango (about 1 large mango)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat oven to 400°F. Spray a muffin pan with cooking spray or fill with paper liners."
    },
    {
      "@type": "HowToStep",
      "text": "In a large bowl, whisk together flours, brown sugar, baking powder, and baking soda."
    },
    {
      "@type": "HowToStep",
      "text": "In a medium bowl, stir together oats, coconut, yogurt, banana, oil, egg, and vanilla extract."
    },
    {
      "@type": "HowToStep",
      "text": "Pour wet mixture into the flour mixture. Stir until moistened—do not over-mix! Fold in mango."
    },
    {
      "@type": "HowToStep",
      "text": "Divide mixture evenly into muffin cups. Bake for 17–20 minutes or until toothpick inserted into the center of a muffin comes out clean."
    }
  ],
