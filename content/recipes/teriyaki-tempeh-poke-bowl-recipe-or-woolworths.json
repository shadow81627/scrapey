  "recipeIngredient": [
    "3 tbs soy sauce, reduced salt",
    "2 tsp maple syrup",
    "2 cloves garlic, peeled and minced",
    "1 tsp ginger, fresh, grated",
    "2 tbs canola oil, divided",
    "400g tempeh, chopped into cubes.",
    "500g quinoa, ready cooked",
    "1 large cucumber, sliced into ribbons",
    "150g pineapple, fresh, chopped",
    "3 tbs carrots, sliced into ribbons",
    "150g spinach",
    "3 tbs coriander, fresh, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk soy sauce, maple syrup, half the oil and the garlic and ginger together in a medium bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the remaining oil in a large nonstick pan on medium high heat and add the tempeh cubes. Cook for 5 minutes, tossing occasionally, until browned on all sides."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the quinoa according to pack instructions."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide the quinoa between four bowls, arrange the cucumber and carrot ribbons, pineapple and spinach in separate sections. Top with the tempeh and fresh coriander and serve."
    }
  ],
