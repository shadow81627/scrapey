  "recipeIngredient": [
    "0.5 cup dates, chopped",
    "0.25 cup honey",
    "1 orange, zested, juiced",
    "1 cup Greek style yoghurt",
    "820g tin two fruits in juice",
    "0.75 cup couscous",
    "0.25 cup pistachios kernels, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drain fruit, reserving 1/4 cup of juice, and set fruit aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer juice to a small saucepan. Add orange rind, 2 tbs orange juice and 1/2 cup water to saucepan. Bring to the boil. Add dates. Simmer for 2 minutes. Remove from heat. Stir in couscous. Cover and set aside for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Layer couscous, reserved fruit and yoghurt into glasses. Top with pistachios and drizzle over honey. Serve."
    }
  ],
