  "recipeIngredient": [
    "800g cooked medium tiger prawns (tails intact)",
    "2 tsp wasabi paste",
    "0.5 avocado (finely chopped)",
    "1 carrot (shredded)",
    "1 Lebanese cucumber (cut into matchsticks)",
    "3 spring onions (thinly sliced)",
    "1 sheet nori, finely sliced",
    "270g dried soba noodles",
    "120g pickled ginger (to serve)",
    "0.33 cup reduced-salt soy sauce",
    "2 tbs mirin seasoning",
    "2 tsp sesame seeds (toasted)",
    "2 tbs peanut oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook noodles in a large saucepan of boiling salted water for 4 minutes or until tender. Drain, refresh under cold water and drain well. Transfer to a large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk soy, oil, wasabi and mirin in a small jug until combined. Set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add prawns, cucumber, carrot and most of onion to noodles. Toss to mix."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top with avocado, sesame seeds, nori and remaining onion. Serve with dressing and pickled ginger."
    }
  ],
