  "recipeIngredient": [
    "1/2 cup packed pitted dates (soaked in warm water for at least an hour)",
    "1 tsp coconut oil (I use refined coconut oil)",
    "1/4 tsp vanilla extract",
    "1 pinch sea salt",
    "1/4 cup toasted pecans (I buy them raw and toast them freshly for recipes)",
    "1/2 cup + 2 tbsp all-purpose flour",
    "1/2 cup whole wheat pastry flour",
    "1/4 tsp salt",
    "3 tbsp maple syrup",
    "1 and 1/2 tsp vanilla extract",
    "3 tbsp coconut oil (room temperature (I use refined coconut oil)",
    "1 and 1/2 tbsp plain, unsweetened non-dairy milk",
    "1/3 cup cane sugar",
    "1 tsp cornstarch",
    "1 tbsp plain, unsweetened non-dairy milk",
    "1/3 cup coconut sugar (or use light brown sugar)",
    "1/2 tbsp brandy",
    "1/2 tbsp plain, unsweetened non-dairy milk"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Add the dates to a blender along with the coconut oil, vanilla extract and salt, and blend until very smooth. Add a splash of nondairy milk if needed to blend."
    },
    {
      "@type": "HowToStep",
      "text": "Add half of the toasted pecans to the blender and combine until the pecans are crumbly."
    },
    {
      "@type": "HowToStep",
      "text": "Add the rest of the toasted pecans and pulse just a few times to break them into smaller pieces. Set the filling aside."
    }
  ],
