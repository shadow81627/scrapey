  "recipeIngredient": [
    "16 whole pitted dates",
    "2 tsp vanilla extract",
    "0.5 cup cashews",
    "1 cup rolled oats",
    "1.33 cup dark chocolate bits",
    "2 tbs toasted coconut",
    "0.5 cup natural almonds",
    "4 tbs Macro organic coconut oil",
    "0.66 cup smooth peanut butter"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place all the ingredients, except chocolate bits and peanut butter, in a food processor and process until finely chopped and clumping together. Transfer to a bowl. Roll tablespoons of mixture into balls and freeze for 30 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, place chocolate bits and peanut butter in a microwave-safe bowl. Microwave on low in 30-second bursts for 1-2 minutes or until melted and smooth. Dip balls into chocolate mixture to half-cover. Store in the freezer for 2-3 months."
    }
  ],
