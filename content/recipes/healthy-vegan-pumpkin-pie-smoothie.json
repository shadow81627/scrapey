  "recipeIngredient": [
    "3/4 cup pumpkin puree (, not pumpkin pie filling)",
    "3/4 cup full fat canned coconut milk (, or other dairy free milk (see note)",
    "1/2 cup cashews (, see note)",
    "1/2 cup maple syrup",
    "1 tablespoon pumpkin pie spice",
    "1 teaspoon vanilla extract",
    "1/8 teaspoon sea salt",
    "2 cups ice",
    "Homemade vegan whipped cream",
    "Ice cream"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Blend all ingredients, except the ice, until creamy."
    },
    {
      "@type": "HowToStep",
      "text": "Add the ice and blend until smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Serve!"
    }
