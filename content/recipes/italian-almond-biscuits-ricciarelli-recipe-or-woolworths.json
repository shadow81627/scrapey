  "recipeIngredient": [
    "3 cups almond meal",
    "2 egg whites, at room temp",
    "0.5 cup caster sugar",
    "0.25 tsp salt",
    "2 zested lemons",
    "0.5 cup icing sugar, for dusting"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 170 degrees. Combine almond meal, caster sugar, lemon zest and salt in a bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add egg whites and mix until combined (this will take some time - it will look as though it is not coming together, but don't worry, it will)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mold into squarish shapes and squish slightly in the centre before dusting in icing sugar"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake for 10-15 minutes"
    }
  ],
