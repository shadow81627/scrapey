  "recipeIngredient": [
    "300g macaroni pasta, cooked as per packed instructions",
    "2 medium eggs, free range",
    "0.8 cup cream",
    "1 tsp paprika",
    "1 tbs garlic powder",
    "1 tsp onion powder",
    "1 tsp cracked black pepper",
    "200g feta, crumbled"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the pasta as per packet instructions and then drain and set to the side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a bowl, crack and beat the eggs lightly and then combine with the cream and seasoning."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In an oven safe casserole dish, combine the pasta with the egg mixture fully and mix in the feta."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake the dish in the oven for 20 minutes and allow to cool for 5 minutes before serving."
    }
  ],
