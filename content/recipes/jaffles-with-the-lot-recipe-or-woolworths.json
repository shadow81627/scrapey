  "recipeIngredient": [
    "12 slices wholemeal bread",
    "0.33 cup tomato paste",
    "0.75 cup pizza cheese, grated",
    "6 slices ham, off the bone",
    "6 slices mild salami",
    "1 tsp dried oregano",
    "1 red capsicum, 2/3 cup roasted, chopped",
    "200g Solanato tomatoes, halved",
    "0.33 cup kalamata olives, pitted",
    "120g baby salad leaves",
    "5ml extra virgin olive oil spray",
    "1 tbs balsamic vinegar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/jaffles-with-the-lot-step-01?$foodhub-method-images$&fmt=png-alpha",
      "text": "Preheat a jaffle maker or sandwich press. Spread bread with tomato paste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/jaffles-with-the-lot-step-02?$foodhub-method-images$&fmt=png-alpha",
      "text": "Top half the bread slices evenly with cheese, ham, salami, capsicum and oregano. Finish with a second slice of bread, tomato paste-side down."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/jaffles-with-the-lot-step-03?$foodhub-method-images$&fmt=png-alpha",
      "text": "Spray the jaffle maker or sandwich press with oil. Cook sandwiches for 5 minutes or until golden."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/jaffles-with-the-lot-step-04?$foodhub-method-images$&fmt=png-alpha",
      "text": "Combine salad leaves, tomato, olives and vinegar in a bowl. Season. Serve jaffles with salad."
    }
  ],
