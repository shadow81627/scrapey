  "recipeId": "RGWW338164",
  "recipeIngredient": [
    "400g minced beef",
    "1 tbs chilli flakes",
    "1 tsp paprika",
    "1 tbs Dijon mustard",
    "1 medium egg, free range",
    "10 medium crackers, smashed",
    "2 tbs canola oil",
    "4 medium brioche buns, sliced in half",
    "80g iceberg lettuce, torn",
    "1 medium Roma tomato, sliced",
    "1 small red onion, peeled and sliced",
    "80g cheddar cheese, grated"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a bowl, mix together the minced beef, chilli flakes, paprika, mustard, egg and crackers."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide the mixture into 4 patties and pack tightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan to a medium heat and then add the oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the burgers for 6 minutes, flip and fry for a further 6 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lightly toast the buns and serve with the burgers, salads and cheese."
    }
  ],
