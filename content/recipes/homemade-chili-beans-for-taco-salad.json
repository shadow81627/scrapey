  "recipeIngredient": [
    "1 pound dried pinto beans (rinsed, picked over, and soaked)",
    "3 cups water",
    "1 medium onion (chopped)",
    "2 cloves garlic (chopped)",
    "2 teaspoons oregano",
    "1/2 teaspoon ground cumin",
    "2 whole dried red chilies (optional)",
    "2 tablespoons tomato paste (see tip below)",
    "2-3 teaspoons chili powder (may use all or part chipotle chili powder)",
    "salt (to taste)",
    "1 green bell pepper (minced)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Cover the beans with water and presoak them overnight; OR do a quick-soak by bringing them to a boil for one minute, removing them from the heat, and allowing them to stand, covered, for one hour."
    }
