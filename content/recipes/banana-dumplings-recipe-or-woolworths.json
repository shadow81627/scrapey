  "recipeIngredient": [
    "2 medium banana, peeled and mashed",
    "150g plain flour",
    "1 pinch bicarbonate soda",
    "1 pinch salt",
    "4 tbs icing sugar",
    "200g chocolate, melted",
    "1 handful berries, strawberries or other for garnish"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine the mashed banana with the flour, salt and baking soda."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once combined, separate the mixture into small balls."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan suitable to shallow fry and fry the dumplings for 4 minutes while turning."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and place on a plate with some kitchen paper to remove some excess oils."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toss the dumplings in bowl with the icing sugar serve with melted chocolate and berries."
    }
  ],
