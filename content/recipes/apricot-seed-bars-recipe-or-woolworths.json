  "recipeIngredient": [
    "1 cup dried apricot, finely chopped",
    "0.25 cup coconut, shredded",
    "0.5 cup hulled tahini",
    "0.25 cup sesame seeds",
    "0.25 cup sunflower seeds",
    "0.33 cup light olive oil",
    "3 cup rolled organic oats",
    "0.25 cup pepitas",
    "0.5 cup organic honey"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 160Â°C. Line a 18x28cm pan with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place rolled oats, sesame seeds, sunflower seeds, pepitas, shredded coconut and dried apricot in a large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place honey, hulled tahini and light olive oil in a small saucepan and stir over medium heat until melted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour honey mixture over oats and stir thoroughly. Pour mixture into prepared pan, smooth top and press down firmly. Bake for 30 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Leave to cool. Chill for 30 minutes, before cutting into bars. Store in an airtight container in the fridge."
    }
  ],
