  "recipeIngredient": [
    "300g pasta, linguine or spagetti",
    "2 cloves garlic, peeled and crushed",
    "1 pinch salt",
    "1 tsp cracked black pepper",
    "1 cup cream",
    "8 bacon, cut into strips",
    "200g minced beef, roll into meatballs",
    "200g mixed mushrooms, sliced",
    "40g parmesan, shaved",
    "3 medium shallots, sliced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add canola oil to a heated pan. Add the meatballs and bacon and fry for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add garlic, shallots and mushrooms to the pan and fry for a further 6 minutes. Turn to low heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook your pasta as per packet instructions, drain and set to one side. Add the pasta to the pan and toss to combine. Garnish with parmesan."
    }
