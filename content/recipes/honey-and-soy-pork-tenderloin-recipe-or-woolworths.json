  "recipeIngredient": [
    "0.4 cup beef stock",
    "6 tbs soy sauce",
    "6 tbs honey",
    "800g pork tenderloin"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add all ingredients to a saucepan apart from the tenderloin and allow to simmer for 5 minutes while mixing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and allow to cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the pork tenderloin in a bowl and cover with the sauce to set while you fire up the BBQ."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the pork on the BBQ for up 20 minutes while turning regularly and marinating regularly before serving."
    }
