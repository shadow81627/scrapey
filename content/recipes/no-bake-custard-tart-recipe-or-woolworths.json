  "recipeIngredient": [
    "300ml Woolies thickened Cream",
    "150ml Woolies milk",
    "1 pkt cottees vanilla instant pudding",
    "1 Woolies flan case (from the Bakery)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Gather the ingredients together."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Into a large bowl add cream, milk and vanilla pudding."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "With a beater, beat until the mixture thickens (this will only take a couple of minutes)"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon mixture into the flan case and smooth the top."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {
        "tipText": "Prep time does not include the chill time. "
      },
      "text": "Chill in the fridge for at least an hour."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Decorate as desired. I just piped on fresh whipped cream, added fresh cherries and a dusting of icing sugar. Enjoy!"
    }
  ],
