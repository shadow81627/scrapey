  "recipeCuisine": "American",
  "recipeId": "RGWW338238",
  "recipeIngredient": [
    "400g peaches in syrup",
    "100g self raising flour",
    "100g brown sugar",
    "0.4 cup milk",
    "30g unsalted butter",
    "0.6 cup cream",
    "2 tbs icing sugar",
    "1 tsp allspice"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a mixing bowl, combine the milk, butter and sugar for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once combine, incrementally add the flour."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Now add the peaches and also the syrup and mix together well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour the mixture into a 6 inch baking tin and bake in the oven for 60 minutes and then allow to cool slightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whip the cream with the icing sugar and all spice and serve on top of the cobbler."
    }
  ],
