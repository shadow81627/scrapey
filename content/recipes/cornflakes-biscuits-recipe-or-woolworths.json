  "recipeIngredient": [
    "125g butter",
    "0.33 cup sugar",
    "1 egg",
    "1 cup sultanas",
    "3 tbs crushed walnuts",
    "1 cup self-raising flour",
    "3 cup crushed cornflakes"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 160 fan forced and lightly grease baking tray."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cream butter and sugar until light and fluffy. Add egg, beat well. Fold in flour alternately with fruit (crushed walnuts) mix well"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drop heaped teaspoon mixture into cornflakes and roll lightly. Place on baking tray and lightly flatten with back of a fork. Allow room for spreading, bake for 15 - 20 mins"
    }
  ],
