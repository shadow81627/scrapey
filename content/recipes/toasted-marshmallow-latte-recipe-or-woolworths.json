  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat grill to high. Line a baking tray with foil. Place marshmallow halves, cut-sides up, on tray and grill for 3 minutes or until just golden and toasted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide espresso between 2 cups, top with milk and marshmallow, then drizzle with chocolate. Sprinkle over biscuit and serve."
    }
