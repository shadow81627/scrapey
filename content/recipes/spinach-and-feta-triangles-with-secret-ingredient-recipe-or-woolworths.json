  "recipeIngredient": [
    "1 clove garlic",
    "1 tsp fresh lemon juice",
    "a little pepper",
    "6 sheet fill pastry",
    "1 pkt frozen spinach",
    "1 block feta cheese",
    "1 tbs butter",
    "1 small shallot onion"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 210 degrees"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/gu1elwxbsmymavegbegi?wid=3793&hei=2844&fmt=png-alpha",
      "text": "Get all your ingredients."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add all ingredients into a bowl and give a good mix around. You can add as much feta or as little as you like."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Next it's time to add your ingredients to the filo pastry. You can make triangles or spring rolls it's really up to you."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once rolled or folded for triangles brush with some melted butter."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place in oven and bake for approximately 10 minutes or the colour that you like them cooked."
    },
