  "recipeIngredient": [
    "40g pine nuts, toasted",
    "6 tbs olive oil",
    "75g parsley, fresh, leaves picked",
    "2 cloves garlic, peeled and minced",
    "1 large lemon, juice and zest",
    "40g parmesan, grated",
    "400g tenderstem broccoli, chopped",
    "500g gnocchi, drained and chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the parsley, pine nuts, garlic, lemon zest and juice and olive oil to a food processor and blitz to a coarse sauce consistency. Stir in the parmesan and set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bring a large pan of water to the boil and cook the gnocchi and broccoli for 2-3 minutes. Drain well and place back in the pan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir through the pesto and serve with extra parmesan if desired."
    }
  ],
