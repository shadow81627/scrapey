  "recipeIngredient": [
    "500g frozen French fries",
    "5ml extra virgin olive oil cooking spray",
    "250g hotdogs",
    "4 long jumbo crusty bread rolls, split, toasted",
    "340g Woolworths Buffalo style salad kit"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Speedy_Hot_Dogs_Buffalo_Slaw_Step_01?$foodhub-method-images$&fmt=png-alpha",
      "text": "Preheat oven to 200°C/220°C fan-forced. Line a large baking tray with baking paper. Arrange fries, in a single layer, on tray. Bake for 12-15 minutes, turning halfway, or until golden and cooked through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Speedy_Hot_Dogs_Buffalo_Slaw_Step_02?$foodhub-method-images$&fmt=png-alpha",
      "text": "Meanwhile, spray a chargrill pan with oil. Place it over medium heat. Cook hotdogs for 3 minutes on each side or until charred and heated through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Speedy_Hot_Dogs_Buffalo_Slaw_Step_03?$foodhub-method-images$&fmt=png-alpha",
      "text": "Fill rolls with two-thirds of the salad, then top with hotdogs."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Speedy_Hot_Dogs_Buffalo_Slaw_Step_04?$foodhub-method-images$&fmt=png-alpha",
      "text": "Drizzle with dressing. Serve with fries and remaining salad."
    }
  ],
