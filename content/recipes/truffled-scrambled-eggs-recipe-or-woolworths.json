  "recipeIngredient": [
    "8 eggs",
    "3 tbs cream",
    "0.25 tsp truffle oil",
    "20g butter",
    "4 slices sourdough bread",
    "1 pinch black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk the eggs, cream, truffle oil and black pepper together in a large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the butter in a large, non-stick frying pan on medium heat. Pour in the egg mix and swirl to coat the pan. Cook for 30 seconds without stirring, then 2 minutes stirring often until set."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toast the sourdough and serve topped with scrambled eggs."
    }
  ],
