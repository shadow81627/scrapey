  "keywords": "Prawn dishes,Prawn,Rice,Mushroom",
  "name": "Spicy Seafood Congee Recipe | Woolworths",
  "prepTime": "PT15M",
  "recipeCategory": "Entrees",
  "recipeCuisine": "Asian",
  "recipeId": "RGWW338289",
  "recipeIngredient": [
    "300g rice, rinsed",
    "12 prawns, de-shelled and sinew removed",
    "24 mussels",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "1.2 cups vegetable stock",
    "80g peas",
    "80g mushrooms, sliced",
    "2 medium chillies, de-seeded and sliced",
    "1 tbs sesame oil",
    "0.2 cup cooking white wine"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the rice as per packet instructions and then set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a saucepan to a medium heat and then add the sesame oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the mushrooms, chillies and peas to the pot and cook for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the seafood and cook for a further 2 minutes, turning regularly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients, including the cooked rice, to the pot, bring to a simmer and cook for a further 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once reduced by about half remove from the heat and serve."
    }
  ],
