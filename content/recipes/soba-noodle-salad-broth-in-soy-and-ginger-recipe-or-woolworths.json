  "recipeIngredient": [
    "320g soba noodles, cooked",
    "2 tbs soy sauce",
    "2 tbs ginger, sliced",
    "3 tbs miso paste",
    "1.2 cup water",
    "1 head bok choi, shredded",
    "100g kale",
    "2 tbs sesame oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a wok with the oil to a medium heat and fry the cooked noodles in the sesame oil for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the ginger and bok choi and continue to fry for a further 1 minute."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients and allow to simmer while stirring for 5 minutes before serving."
    }
  ],
