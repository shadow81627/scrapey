  "recipeIngredient": [
    "8 slices sourdough bread",
    "160g vegan cheese, grated",
    "100g heirloom tomato, quartered",
    "2 tbs olive oil",
    "2 tbs balsamic vinegar",
    "40g basil, chopped",
    "1 tsp cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Equally spread the grated vegan cheese on top of the sourdough slices."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix the chopped tomatoes with the balsamic, oil and pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the tomato mixture on top of the cheese."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the bruschetta on a baking tray and bake in the oven for 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the oven and garnish with chopped basil before serving."
    }
  ],
