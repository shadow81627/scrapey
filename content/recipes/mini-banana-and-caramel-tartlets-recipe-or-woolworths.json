  "recipeIngredient": [
    "4 small short crust pastry, thawed",
    "2 medium banana, lightly mashed",
    "60g flaked almonds",
    "200g caramel",
    "0.6 cup whipped cream",
    "1 tsp cocoa powder"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place pastry sheets on some baking paper on a baking tray and roll the pastry edges to create a dip in the centre."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the banana and almonds inside and cover with the caramel."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in the oven for 20 minutes and then allow to cool slightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cover the pastry with some whipped cream and garnish with cocoa powder."
    }
  ],
