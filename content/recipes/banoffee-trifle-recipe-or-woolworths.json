  "recipeIngredient": [
    "2 medium banana, sliced",
    "200g cream cheese, room temperature",
    "0.4 cup chocolate sauce",
    "12 sponge fingers, broken",
    "0.8 cup whipped cream",
    "0.8 cup custard",
    "4 tbs caramel",
    "80g dark chocolate, grated"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Prepare 4 serving glasses by placing the broken sponge fingers at the bottom."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drizzle the chocolate sauce over the sponge."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Layer the sliced bananas on top of the sponge."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the caramel on top of the bananas."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whip the cream cheese together with the custard and once combine, empty on top of the bananas."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whip the cream and place on top of the cream cheese and custard."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Garnish with grated chocolate before serving."
    }
  ],
