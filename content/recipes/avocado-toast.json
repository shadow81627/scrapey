  "recipeIngredient": [
    "2 avocados",
    "2 tsp lemon juice",
    "salt and pepper (to taste)",
    "4 slices toasted bread",
    "1/2 cup feta cheese",
    "1 cup cherry tomatoes (halved)",
    "1/4 cup basil",
    "balsamic vinegar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Cut the avocado in half and put the flesh in a small bowl. Add lemon juice, salt, and pepper. With a fork, crush the ingredients together."
    },
    {
      "@type": "HowToStep",
      "text": "Spread the mixture over the toast."
    },
    {
      "@type": "HowToStep",
      "text": "Top with feta cheese, tomatoes, and basil."
    },
    {
      "@type": "HowToStep",
      "text": "Season to your liking with salt and pepper."
    },
    {
      "@type": "HowToStep",
      "text": "Pour a small trickle of balsamic vinaigrette on top."
    }
  ],
