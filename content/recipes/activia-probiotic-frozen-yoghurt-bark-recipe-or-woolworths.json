  "recipeIngredient": [
    "500g Activia Probiotic No Added Sugar Vanilla Yoghurt Pots",
    "2 tbs honey",
    "2 tbs choc chips (optional)",
    "2 tbs hazelnuts, chopped",
    "2 tbs cranberries",
    "2 tbs strawberries, dried (optional)",
    "2 tbs oats, toasted (for topping)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour the yoghurt into a large mixing bowl"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add in all other ingredients except oats and mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread the mixture out onto a small baking tray lined with baking paper and top with oats. Place in freezer for at least 2 hours or overnight."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "When ready to serve, cut into pieces and serve alongside fresh fruit of your choice."
    }
  ],
