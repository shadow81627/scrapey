  "recipeIngredient": [
    "0.33 cup plain flour",
    "60g butter",
    "1L Campbell's Real Chicken Liquid Stock",
    "500g spiral pasta",
    "2 cup shredded ham",
    "1.5 cup tasty cheese shredded",
    "1 cup frozen peas"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 200 degrees C / 180 degrees C fan-forced. Lightly grease a large ovenproof dish."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook pasta in a large pan of boiling, salted water until al dente. Drain well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat butter in a large pan over medium-high heat. Add flour and cook for 1 min. Gradually stir in stock. Bring to the boil. Simmer for 5 mins. Stir in pasta, ham, peas and 1/2 cup of cheese. Spoon into dish. Sprinkle with remaining cheese. Bake for 15-20 mins until golden."
    }
  ],
