  "recipeIngredient": [
    "4 eggs, scrambled",
    "400g pad Thai noodles, cooked as per packet instructions",
    "2 tbs sesame oil",
    "2 cloves garlic",
    "400g chicken breasts, diced",
    "200g mushrooms, sliced",
    "100g bean sprouts",
    "2 scallions, sliced (for garnish)",
    "3 tbs oyster sauce",
    "3 tbs fish sauce",
    "3 tbs tamarind",
    "1 medium lime, segmented"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Scramble the eggs and set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the noodles as per packet instructions and then drain and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a large pan to a medium heat and then add the sesame oil and chicken and fry for 7 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the garlic and mushrooms for 5 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients to the pan, fry while tossing for 3 more minutes before serving with lime segments to garnish."
    }
  ],
