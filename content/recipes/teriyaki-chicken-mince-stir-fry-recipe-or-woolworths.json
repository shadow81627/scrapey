  "recipeIngredient": [
    "500g chicken breasts, diced",
    "400g soba noodles, store bought cooked",
    "1 tbs canola oil",
    "200g snow peas",
    "20g bean sprouts",
    "2 cloves garlic, peeled and chopped",
    "100g broccoli, sliced",
    "2 stems scallion, sliced",
    "1 tsp ginger, diced",
    "1 medium chili peppers, sliced",
    "3 tbs coriander",
    "1 tbs honey",
    "2 tbs soy sauce",
    "4 tbs water",
    "3 tbs brown sugar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "To make the sauce, pan fry on a medium heat the honey, soy sauce, water and sugar together for 3 minutes until the sugar melts and the sauce thickens."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and set one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pan fry the diced chicken breasts on a medium heat non-stick pan with the canola oil for 5 minutes while turning."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the prepared vegetables, cooked noodles and garlic to the pan and fry up for a further 3 minutes while turning."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Now add the sauce to the pan and allow to simmer while constantly stirring for 2 minutes before serving. Garnish with chillies and coriander."
    }
  ],
