  "recipeIngredient": [
    "4 medium passion fruit, flesh and pips only needed",
    "50g brown sugar",
    "4 eggs, whites only needed",
    "1 tsp vanilla extract",
    "0.6 cup custard",
    "60g white chocolate, grated",
    "30g unsalted butter",
    "30g icing sugar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 200°C/170°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Prepare 4 medium sized ramekins by buttering the bottom and edges, then cover with icing sugar and topple out any extra."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a mixing bowl beat the egg whites until peaks form."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Now fold in the rest of the ingredients apart from the white chocolate and pour into the ramekins."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in the oven for 10 minutes or until nicely risen and browned. Serve with grated white chocolate on top."
    }
  ],
