  "recipeIngredient": [
    "500g large cooked tiger prawns, deveined",
    "iceberg lettuce leaves, torn",
    "1 tsp lemon zest",
    "2 tsp lemon juice",
    "1 lemon, cut into wedges",
    "5ml extra virgin olive oil spray",
    "6 brioche slider buns, halved",
    "1-2 tsp olive oil",
    "small tarragon sprigs (to serve)",
    "0.33 cup parsley (dill & tarragon seafood sauce)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place prawns into a bowl. Add seafood sauce, zest and lemon juice and stir to combine. Cover and keep chilled until ready to serve."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven grill to medium-high. Place buns, cut-sides up, onto a baking tray and spray lightly with oil. Grill for 30 seconds to 1 minute or until lightly toasted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pile lettuce onto bun bases and top with seafood sauce and prawns. Season with pepper and oil. Top with tarragon and serve with lemon."
    }
  ],
