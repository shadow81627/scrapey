  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a large wok on high heat and cook the brussels sprouts, vegetable mix and spring onions for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir in the garlic and chilli paste and ginger and cook for 2 minutes. Pour in the maple syrup and stir in the miso paste. Cook for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Tip in the noodles and cook for 23 minutes, stirring, until hot. Serve with spring onion tops scattered on top."
    }
  ],
