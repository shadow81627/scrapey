  "recipeIngredient": [
    "500g brussels sprouts, trimmed and shredded",
    "2 tbs canola oil",
    "300g mix vegetable stir fry",
    "4 spring onions, sliced, green tops reserved",
    "2 chilli garlic sauce",
    "5cm ginger, peeled and grated",
    "1 tbs miso paste",
    "1 tbs maple syrup",
    "300g noodles, fresh, wokready"
