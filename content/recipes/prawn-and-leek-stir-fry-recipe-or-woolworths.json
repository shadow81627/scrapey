  "recipeIngredient": [
    "2cm ginger, grated",
    "600g green prawns, peeled, deveined, tails intact",
    "2 leeks",
    "1 tbs peanut oil",
    "1 red capsicum, deseeded, finely sliced",
    "1 carrot, cut into matchsticks",
    "1 bunch broccolini, sliced",
    "0.5 tbs sesame seeds",
    "0.25 cup salt-reduced soy sauce",
    "1 tbs rice wine vinegar",
    "2 tsp sesame oil",
    "1 tsp brown sugar",
    "1 clove garlic"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/prawn-and-leek-stir-fry-step-01?$foodhub-method-images$&fmt=png-alpha",
      "text": "Combine soy sauce, vinegar, sesame oil, sugar, ginger and garlic in a medium bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/prawn-and-leek-stir-fry-step-02?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add prawns and stir to coat. Set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/prawn-and-leek-stir-fry-step-03?$foodhub-method-images$&fmt=png-alpha",
      "text": "Wash leek well and roughly chop. Heat a wok or large frying pan over medium-high heat. Add peanut oil and swirl to coat. Add vegetables and toss for 4 minutes or until they start to soften."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/prawn-and-leek-stir-fry-step-04?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add prawns and sauce and cook for 2-3 minutes or until prawns change colour. Serve sprinkled with sesame seeds."
    }
  ],
