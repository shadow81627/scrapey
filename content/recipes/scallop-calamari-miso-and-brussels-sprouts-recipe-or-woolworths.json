  "recipeIngredient": [
    "5 scallops",
    "1 sachet miso soup (makes 1 cup)",
    "3 calamari rings",
    "4 frozen brussels sprouts, thawed and halved",
    "1 tbs butter"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat 1/2 tbsp butter in pan on medium/high heat. Sear scallops to golden about 30  45 seconds each side and set aside. You can also do the calamari rings at the same time. And rest."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the other 1/2 tbsp butter to the pan and and bring to medium high heat. Sear the halved brussels sprouts until golden."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Get your kettle going and make up your miso from the little packet. You won't need very much."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Plate up. No skills required. Happy days!"
    }
  ],
