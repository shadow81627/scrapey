  "recipeIngredient": [
    "2-4 drops pink food colouring",
    "375g white chocolate",
    "4 cup cooked cake crumbs",
    "sprinkles (for decoration)",
    "0.33 cup frosting",
    "bamboo skewers"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix cake crumbs with frosting. Roll tablespoonfuls of mix into balls and freeze until firm."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Melt white chocolate until smooth."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Dip bamboo skewer tips into chocolate and push into cake balls."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add pink food colour to chocolate. Swirl cake pops in melted chocolate to coat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add butterfly sprinkles. Leave to set."
    }
  ],
