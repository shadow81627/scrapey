  "recipeIngredient": [
    "500g chuck steak, diced",
    "2 tbs plain flour",
    "1 tsp dry mustard powder",
    "2 tbs tomato paste",
    "1 tbs worcestershire sauce",
    "1 tbs vinegar",
    "1 tsp salt",
    "1 tsp sugar",
    "300ml water",
    "1 carrot, diced",
    "2 stalk celery, chopped",
    "1 brown onion, sliced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven on 180 degrees. Mix together flour and mustard. Roll meat in mixture then place in casserole dish. Add diced carrot and celery and spread onion over the top."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/jubcoi2dew6doxdkzyji?wid=3264&hei=2448&fmt=png-alpha",
      "text": "Mix all remaining ingredients together and pour over meat and veggies in casserole dish."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cover with lid. Place on middle rack in oven for 1.1/2 hrs. Serve with mash potato or rice."
    }
  ],
