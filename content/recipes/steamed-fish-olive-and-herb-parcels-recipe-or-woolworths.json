  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180Â°C. Cut 6 pieces of baking paper, 25x35cm. Place a fish fillet in the middle of each piece of baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Arrange artichoke, olives, semi-dried tomatoes, rind, thyme and garlic over the fish. Drizzle with olive oil and lemon juice. Season to taste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bring the sides of the baking paper up to meet in the middle to form a pouch, twist the top gently and secure with kitchen string."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place parcels on a baking tray and bake for 10-12 minutes or until fish is cooked. Scatter with spinach leaves, to serve."
    }
