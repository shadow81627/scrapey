  "recipeIngredient": [
    "3 cloves garlic",
    "1 tbs cornflour",
    "0.25 cup dark soy sauce",
    "0.33 cup sweet sherry",
    "800g chicken, sliced",
    "2 tbs peanut oil",
    "1 tbs brown sugar",
    "1 tsp sesame oil",
    "8 green onions, thinly sliced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine garlic, cornflour, half the soy sauce and half the sherry in a large bowl. Add chicken, mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat peanut oil in wok. Stir fry chicken mixture in batches until browned."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Return chicken to wok with sugar, sesame oil and remaining soy sauce and sherry. Stir fry until sauce thickens slightly. Remove from heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve stir fry sprinkled with green onions."
    }
  ],
