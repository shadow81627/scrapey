  "recipeIngredient": [
    "5 tbs ketchup, reduced sugar",
    "4 tbs soy sauce, reduced salt",
    "3 tbs sweet chilli sauce, reduced sugar",
    "5cm ginger, peeled and grated",
    "3 cloves garlic, peeled and minced",
    "700g chicken thighs, skinless, boneless, chopped",
    "2 red capsicums, chopped",
    "1 pineapple, peeled, cored and chopped",
    "2 red onions, peeled and chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Soak 8 skewers in cold water. Mix the marinade ingredients together in a large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the chicken thighs to the bowl and mix well to coat. Cover and set aside for 30 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the BBQ. Thread the chicken thighs, capsicums, pineapple and red onions onto the skewers and brush with any remaining marinade left in the bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook on the BBQ for 12-15 minutes until the chicken is cooked through, turning often."
    }
  ],
