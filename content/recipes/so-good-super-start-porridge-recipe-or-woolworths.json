  "recipeIngredient": [
    "2 tbs desiccated coconut (optional)",
    "1 cup apple juice",
    "0.5 cup rolled oats",
    "300ml almond milk unsweetened",
    "2 tsp chia seeds (optional)",
    "0.5 cup dried apricot",
    "6 weet-bix"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the unsweetened almond milk, apple juice, oats and chia seeds into a medium-sized pot and bring to a gentle boil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Turn down and simmer for 3 minutes, then crumble the Weet-Bix into the porridge."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon the mixture into some glass jars or bowls and sprinkle with desiccated coconut and chopped apricots on top."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve hot or cold."
    }
  ],
