  "recipeIngredient": [
    "150g basmati and wild rice",
    "3 tbs olive oil",
    "200g broad beans, frozen",
    "2 large spring onions, chopped",
    "4 tbs lemon juice",
    "1 lemon, 1 tbs zest grated",
    "100g feta, crumbled",
    "3 tbs basil, fresh, chopped",
    "5 tbs parsley, fresh, chopped",
    "1 tbs mint, fresh, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Rinse the rice and boil according to instructions on pack. Drain and place in a large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Boil the broad beans in a large pan of salted boiling water for 2-3 minutes, drain and add to the rice."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix the lemon juice and olive oil and then pour over the rice and beans mix."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix in the chopped spring onions, herbs and lemon zest, tossing to combine. Crumble the feta on top and serve."
    }
  ],
