  "recipeIngredient": [
    "350g linguine",
    "390g artichokes, drained and chopped",
    "2 tbs olive oil",
    "2 cloves garlic, peeled and minced",
    "70g cream cheese",
    "1 large lemon, zest and juice",
    "125g spinach",
    "60g parmesan, grated"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the linguine according to pack instructions and drain, reserving 100ml of starchy water."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a large pan on medium heat and cook the artichokes and garlic for 3 minutes. Stir in the spinach and cook for 1 minute, until wilted. Add the cream cheese, lemon juice and zest and cooking water and cook for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toss in the linguine and coat well. Sprinkle over grated parmesan and serve."
    }
  ],
