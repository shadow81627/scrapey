  "recipeIngredient": [
    "6 potatoes",
    "0.66 cup mayonnaise",
    "4 eggs",
    "3 shallots",
    "3 bacon rashers"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Peel potatoes and cut into small cubes. Boil until just soft. Drain and once cooled transfer into large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Dice bacon rashers and fry until crispy. Drain on absorbent paper"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Boil the eggs for 7-10 mins. Then once cooled, peel and cut into small pieces."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Thinly slice shallots."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add all ingredients into a large bowl and combine gently. Then add mayonnaise and mix through. You can adjust the amount of mayonnaise to suit your taste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Season with salt and pepper if desired."
    }
  ],
