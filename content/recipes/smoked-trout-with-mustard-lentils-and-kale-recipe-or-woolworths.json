  "recipeIngredient": [
    "1 tbs olive oil",
    "2 medium leeks, sliced",
    "500g puy lentils, ready cooked",
    "500g hot smoked trout, 4 fillets",
    "150g kale, destalked, chopped",
    "4 tbs Dijon mustard",
    "1 large lemon, 1/2 juiced, 1/2 in wedges",
    "2 tbs creme fraiche, reduced fat"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a large pan on medium heat and cook the leeks for 5 minutes. Add the kale and cook for 2 minutes. Add the lentils and warm through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and stir through the creme fraiche, lemon juice and mustard."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with smoked trout on top and a lemon wedge on the side."
    }
  ],
