  "recipeIngredient": [
    "2 medium egg, whites only needed",
    "0.5 tsp cornstarch",
    "0.5 tsp cream of tartar",
    "80g caster sugar",
    "1 pinch salt",
    "0.2 cup custard",
    "40g almonds, crushed",
    "4 tsp amaretto",
    "4 strawberries (to garnish)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 90Â°C."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Beat the egg whites until stiff peaks appear."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "While mixing, add the cream of tartar, salt and cornstarch and keep mixing to keep the peaks."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Incrementally add the sugar always beating to keep the stiff peaks."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer to a piping bag and dollop about make 6 circular nests with a gap in the centre of each."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in the oven for 60 minutes until lightly browned and hard."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "While they are baking you can combine the custard and amaretto in a bowl. (You can serve the custard cold or hot)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove nests from the oven and allow to cool fully before serving with the custard and drizzle the almonds on top."
    }
  ],
