  "recipeId": "a0K0I00000ShbtQUAR",
  "recipeIngredient": [
    "3 cup rice bubbles",
    "250g copha",
    "0.5 cup chopped walnuts",
    "0.5 cup slivered almonds",
    "1 cup milk powder",
    "2 cup coconut",
    "1 cup mixed fruit",
    "100g glace cherries, quartered",
    "1 tsp vanilla",
    "0.75 cup icing sugar, sifted",
    "500g white choc bits"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix all the dry ingredients together well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Melt the copha slowly over a low heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour copha over dry ingredients, add the vanilla & mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Line a slice tray with foil (for traditional blocks) ORuse a tart baking tray like I do for less mess."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Press mixture into tray & chill for approx 1 hour or until set (depends on how thick you make your slice)"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Chop into squares if you've used a slice tray."
    }
  ],
