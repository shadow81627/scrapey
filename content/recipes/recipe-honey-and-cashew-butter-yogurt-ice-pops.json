  "recipeIngredient": [
    "2 cups plain Greek yogurt",
    "2/3 cup milk",
    "1/3 cup creamy cashew butter",
    "1/3 cup honey",
    "1 teaspoon vanilla extract",
    "1/4 teaspoon fine salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place all the ingredients in a large bowl and whisk until completely smooth. Divide the mixture among ice pop molds, insert the sticks, and freeze until completely solid, at least 8 hours."
    },
    {
      "@type": "HowToStep",
      "text": "To serve, run the molds briefly under running hot water until the pops loosen from the molds."
    }
  ],
