  "recipeIngredient": [
    "2 tbsp olive oil",
    "1 onion (diced)",
    "1 clove garlic (minced)",
    "1 tbsp chili powder",
    "2 tsp ground cumin",
    "2 tsp dried oregano",
    "1/8 –1/4 tsp cayenne pepper (optional; 1/4 teaspoon is very spicy!)",
    "1 small zucchini (diced)",
    "1 small yellow squash (diced)",
    "1 small red pepper (diced)",
    "2 10 oz cans diced tomatoes with green chili peppers",
    "2 tbsp tomato paste",
    "1 ear corn (kernels removed)",
    "1 1/2 cups cooked black beans (or 1 can, rinsed)",
    "1 1/2 cups cooked kidney beans (or 1 can, rinsed)",
    "salt and pepper (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Heat the oil in a Dutch oven over medium heat."
    },
    {
      "@type": "HowToStep",
      "text": "Add the onion and cook until it’s softened, 5–7 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Add the garlic, chili powder, cumin, oregano, and cayenne and cook until fragrant, about 1 minute."
    },
    {
      "@type": "HowToStep",
      "text": "Stir in the zucchini, yellow squash, and red pepper; cook until they’re softened, 7–10 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Add the tomatoes, tomato paste, corn, and beans."
    },
    {
      "@type": "HowToStep",
      "text": "Bring the chili to a boil, then reduce the heat to low and simmer for 20 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Season with salt and pepper to taste."
    }
  ],
