  "recipeIngredient": [
    "600 ml thickened cream",
    "2 tsp vanilla bean paste",
    "1 tbs caster sugar",
    "500 g Woolworths pavlova base",
    "125 g raspberries",
    "125 g blackberries",
    "125 g strawberries",
    "2 passionfruit",
    "2 sprigs mint"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToStep": {},
      "text": "Using an electric mixer, beat cream, vanilla and sugar until soft peaks form. Top pavlova with vanilla cream. Scatter over berries. Drizzle with passionfruit and top with mint. Serve immediately."
