  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Prepare the jelly according to pack instructions, set aside to cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour cooled jelly in the bottom of a large glass dish. Refrigerate until set."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place one sponge cake half on the set jelly and then top with peaches and pour on top the vanilla flavoured custard."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top with mixed fruit, as desired and leave covered in fridge overnight to allow sponge cake to soak before serving."
    }
  ],
