  "recipeIngredient": [
    "2 tsp peanut or vegetable oil",
    "3/4 cup thinly sliced green onions (about 1 bunch)",
    "6 ears fresh corn (kernels removed)",
    "1/2 cup light coconut milk",
    "salt (to taste)",
    "1 –2 tsp sriracha (depending on how much spiciness you like)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Heat oil in a large skillet over medium-high heat."
    },
    {
      "@type": "HowToStep",
      "text": "Add green onions and cook 2 minutes or until softened, stirring frequently."
    },
    {
      "@type": "HowToStep",
      "text": "Add corn and coconut milk; continue to cook, stirring occasionally, until most liquid has evaporated, about 5 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Stir in sriracha and salt to taste."
    }
  ],
