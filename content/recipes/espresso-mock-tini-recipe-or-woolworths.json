  "recipeIngredient": [
    "350ml chilled espresso milk drink",
    "0.75 cup thickened cream",
    "300ml freshly made espresso coffee",
    "2 tbs dark chocolate, finely grated",
    "0.33 cup caramel syrup (for coffee)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour espresso milk into an ice-cube tray and freeze for 4 hours or until set. Meanwhile, chill coffee."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place espresso milk ice cubes into 4 glasses. Add 1 tbs caramel syrup to each glass. Pour over coffee and top with whipped cream. Garnish with chocolate. Serve immediately."
    }
