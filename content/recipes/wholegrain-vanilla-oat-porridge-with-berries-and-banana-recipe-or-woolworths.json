  "recipeIngredient": [
    "1 cup Woolworths traditional rolled oats",
    "2 cups reduced-fat milk",
    "1 tsp vanilla extract",
    "1 cup strawberries, chopped",
    "1 banana, sliced",
    "2 tsp maple syrup"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine oats, milk and vanilla extract in a saucepan. Bring to the boil over medium heat, and then reduce heat to low and simmer, stirring for 4-5 minutes or until oats are creamy. Stand for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In two jars, layer spoonfuls of porridge, strawberries and banana. Repeat layers until jars are filled. Drizzle with maple syrup and serve."
    }
  ],
