  "recipeIngredient": [
    "400g cooked turkey",
    "4 medium eggs, free range",
    "50g cranberries, dried",
    "2 roma tomatoes, diced",
    "4 baby potatoes, cooked and diced",
    "2 avocado, diced",
    "200g mixed salad leaves",
    "4 tbs olive oil",
    "2 tbs balsamic vinegar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pot of boiling water and boil the eggs for 7 minutes. Remove from the heat and refresh with cold water. Combine all of your dressing ingredients and mix well and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once the eggs are cool to the touch, peel and quarter. Slice or cut the cooked turkey into desired size. Combine all ingredients or serve deconstructed and serve with the dressing on the side."
    }
  ],
