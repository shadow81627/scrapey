  "recipeIngredient": [
    "butter (for greasing the ramekin)",
    "1/4 cup ricotta cheese",
    "1/2 cup roughly chopped spinach",
    "1/2 tsp dried thyme",
    "1/2 tsp lemon zest (optional)",
    "1 Tbsp olive oil",
    "2 eggs",
    "salt and pepper",
    "2 slices bread for serving"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat your oven to 375°F / 195°C"
    },
    {
      "@type": "HowToStep",
      "text": "Butter a ramekin or small baking dish."
    },
    {
      "@type": "HowToStep",
      "text": "In a small bowl, mix together ricotta, spinach, thyme, lemon zest, olive oil, and a bit of salt and pepper."
    },
    {
      "@type": "HowToStep",
      "text": "Place the ricotta mixture into the prepared ramekin, and create two small wells for the eggs."
    },
    {
      "@type": "HowToStep",
      "text": "Crack 2 eggs into the ramekins, drizzle with a touch of olive oil, and place in the oven."
    },
    {
      "@type": "HowToStep",
      "text": "Bake for 12-15 minutes, until whites are set but yolks are still a bit runny."
    },
    {
      "@type": "HowToStep",
      "text": "Remove from the oven and serve hot with buttered toast."
    }
  ],
