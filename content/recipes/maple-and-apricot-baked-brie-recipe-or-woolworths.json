  "recipeIngredient": [
    "50g Angas park apricots, coarsely chopped",
    "200g brie cheese",
    "2 tsp maple syrup",
    "1 tbs pistachio kernels, chopped",
    "1 pinch pepper (for seasoning)",
    "crackers (for serving)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 200°C/180°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Line a baking tray with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place brie onto prepared tray."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour over maple syrup, top with apricots and pistachios; season to taste with pepper"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake for 10 minutes or until apricots are slightly softened and sticky. Serve drizzled"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "With maple syrup from the tray, and with crackers"
    }
  ],
