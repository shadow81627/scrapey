  "recipeIngredient": [
    "10 chocolate biscuits",
    "0.5 tsp vanilla extract",
    "2 tbs cornflour",
    "210g dark chocolate, chopped (curl 10g to serve)",
    "1.25 cup milk",
    "0.25 cup sugar",
    "30g butter",
    "0.5 tsp peppermint essence",
    "2 tbs mint leaves (to serve)",
    "300ml pure cream",
    "1 tbs cocoa"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place sugar, cornflour and cocoa in a medium saucepan over medium heat. Add cream, milk, vanilla and whisk constantly for 5 minutes or until mixture is smooth and thick."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Reduce heat to low. Add chocolate, butter and essence and whisk until melted. Pour mixture into 6 serving glasses or cups. Chill for 30 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Crush biscuits to fine crumbs, then spoon 1 tbs over each pudding. Garnish with mint and chocolate curls. Serve."
    }
  ],
