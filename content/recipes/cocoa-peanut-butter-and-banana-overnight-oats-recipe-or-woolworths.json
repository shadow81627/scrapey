  "recipeIngredient": [
    "200g oats",
    "1.6 cups semiskimmed milk, unsweetened",
    "2 small banana, peeled and mashed",
    "1 tbs honey",
    "2 tbs peanut butter",
    "4 tbs cocoa powder",
    "0.5 tsp vanilla extract",
    "25g peanuts, salted, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the banana, cocoa powder, peanut butter and honey in a bowl and mix well. Tip in the oats, milk and vanilla extract and mix to combine. Cover and place in the fridge overnight."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon the oats into bowls or jars and top with chopped peanuts and extra banana and peanut butter, if desired."
    }
  ],
