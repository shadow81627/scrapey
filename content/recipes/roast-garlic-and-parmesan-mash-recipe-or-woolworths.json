  "recipeIngredient": [
    "6 cloves garlic, peeled",
    "4 tsp milk",
    "40g parmesan",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "40g butter",
    "1 tbs rosemary",
    "4 medium potatoes"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pot of boiling salted water."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the potatoes in the water and boil for 10 minutes and then remove from the water."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the potatoes in a baking dish with the garlic and roast in the oven for 30 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove the baking dish from the oven, empty contents into a mixing bowl and mash together with the rest of the ingredients before serving."
    }
  ],
