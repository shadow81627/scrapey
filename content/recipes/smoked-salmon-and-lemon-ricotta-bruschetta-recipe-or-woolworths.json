  "recipeIngredient": [
    "200g ricotta",
    "1 large lemon, juiced and zested",
    "0.5 large cucumber",
    "200g natural oak smoked salmon",
    "4 slices sourdough bread",
    "1 tbs dill, fresh, chopped",
    "1 pinch cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk the ricotta, half the lemon juice and zest together in a small bowl. Stir in cracked black pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toast the sourdough to your liking. With a vegetable peeler, slice the cucumber into ribbons, place in a bowl and cover with the remaining lemon juice."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread lemon ricotta on each slice of sourdough, top with smoked salmon, cucumber ribbons, lemon zest and fresh dill and serve."
    }
  ],
