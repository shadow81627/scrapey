  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Combine all the ingredients to a high-speed blender and blend until smooth"
    },
    {
      "@type": "HowToStep",
      "text": "Strain the raspberry seeds using a fine mesh colander over a large bowl (discard them after they're strained). Refrigerate and serve over ice"
    }
