  "recipeIngredient": [
    "1 cup baby spinach",
    "1 tbs olive oil",
    "500g pasta",
    "250ml Woolworths mascarpone cheese",
    "1 chicken breast",
    "3 mushrooms",
    "2 spring onions",
    "1 cup chicken stock",
    "1 pinch salt",
    "1 pinch pepper",
    "1 cup cherry tomatoes, quartered"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut the chicken breast into small pieces. Cut all the vegetables."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Put the olive oil in the pan and heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the chicken to the hot pan and cook until well browned. Once the chicken is brown add the mushrooms and spring onions."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the pasta in a separate pot (be sure to not overcook it)"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the cup of chicken stock and low heat to a simmer."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/atkhcb2pi2bfr6lozgur?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add the mascarpone cheese to the chicken mixture and stir well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/zjtedssz37lerqhgbptb?$foodhub-method-images$&fmt=png-alpha",
      "text": "Drain the past and add to the chicken mixture."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the cherry tomatoes, baby spinach, salt and pepper.It's now ready to serve."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/pgqlpcm1xkflygd4g4z7?$foodhub-method-images$&fmt=png-alpha",
      "text": "Optional step: served with green vegetables like beans."
    }
  ],
