  "recipeId": "FID-2210-10",
  "recipeIngredient": [
    "1kg cocktail potatoes, halved",
    "2 tbs lime pickle",
    "2 tsp extra virgin olive oil",
    "1 Lebanese cucumber, halved lengthways, sliced",
    "0.5 small red onion, diced",
    "60g baby spinach leaves",
    "100g Greek-style yoghurt",
    "0.25 cup toasted coconut flakes",
    "1 tsp dried chilli flakes"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cover potatoes in water in a large pot. Bring to a boil over medium heat. Reduce heat to low and simmer potatoes for 10 minutes or until tender."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add lime pickle and oil to a large bowl. Add drained potatoes and stir gently to coat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix yoghurt and 1 tbs water in a small bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add cucumber, onion and spinach. Toss gently to combine. Serve salad dolloped with yoghurt and sprinkled with coconut and chilli."
    }
  ],
