  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Saute the onion and garlic with oil (or broth) over medium low heat until the onions are caramelized. It takes about 10 minutes. Add them to a blender."
    },
    {
      "@type": "HowToStep",
      "text": "Place the peppers into the blender with the onions and garlic mixture."
    },
    {
      "@type": "HowToStep",
      "text": "Add the rest of the ingredients, except the basil/parsley, into the blender."
    },
    {
      "@type": "HowToStep",
      "text": "Blend on high until smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Add the basil/parsley and pulse to chop into the sauce."
    },
    {
      "@type": "HowToStep",
      "text": "Cook your pasta of choice according to package directions, and serve with the sauce!"
    }
  ],
