  "recipeIngredient": [
    "400g salmon (4 skinless, boneless fillets)",
    "1 tbs olive oil",
    "2 tsp jerk seasoning",
    "150g red cabbage, shredded",
    "200g pineapple, fresh, chopped",
    "2 carrots, peeled and sliced into matchsticks",
    "3 spring onions, finely chopped",
    "0.5 bunch coriander, fresh, chopped",
    "2 limes, juiced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Brush the salmon fillets with oil and coat all over with jerk seasoning. Set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the red cabbage, pineapple, carrots, spring onions, coriander and lime juice to a large bowl and toss well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Brush a little oil in a griddle pan and place on medium high heat. Cook the salmon fillets for 3-4 minutes on each side until cooked through. Serve with pineapple slaw."
    }
  ],
