  "recipeIngredient": [
    "24 oysters, shucked, drained and rinsed",
    "200g bacon lardons",
    "2 tbs butter",
    "4 tsp tobasco",
    "4 tsp worcestershire sauce",
    "2 lemon, quartered"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Shuck, drain and rinse the oysters and place them on a serving dish."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the bacon lardons with the butter for 4 minutes and then place then onto the oysters."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drizzle the tabasco and Worcestershire sauce on top and serve with wedges of lemon."
    }
  ],
