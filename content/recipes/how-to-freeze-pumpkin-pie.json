  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "image": "https://cdn.apartmenttherapy.info/image/upload/f_auto,q_auto:eco,c_fit,w_730,h_487/k%2Farchive%2F7fdc49e91fa37fc491747e7efb09f4659acd5b5e",
      "text": "Cool the pie completely. Cool the pie completely on a wire rack at room temperature. This will take several hours."
    },
    {
      "@type": "HowToStep",
      "image": "https://cdn.apartmenttherapy.info/image/upload/f_auto,q_auto:eco,c_fit,w_730,h_487/k%2Farchive%2F37ccb22113b634dbc899577c3d82b04d4350ac38",
      "text": "Tightly wrap the pie. Wrap the pie tightly in a few layers of plastic wrap. An additional wrap of aluminum foil is optional, but recommended."
    },
    {
      "@type": "HowToStep",
      "image": "https://cdn.apartmenttherapy.info/image/upload/f_auto,q_auto:eco,c_fit,w_730,h_487/k%2Farchive%2F5251db1a6efc77877e7aa9ac052e56620cb77c59",
      "text": "Label and freeze the pie. Label the pie with the freeze date. Freeze on a level shelf until solid."
    },
    {
      "@type": "HowToStep",
      "image": "https://cdn.apartmenttherapy.info/image/upload/f_auto,q_auto:eco,c_fit,w_730,h_487/k%2Farchive%2F1f159ec2484d03fdc713be3e58901010053f399a",
      "text": "Thaw overnight before serving. Remove the aluminum foil if using and thaw in the fridge overnight, making sure the pie is on a level shelf in the refrigerator as it thaws."
    }
  ],
