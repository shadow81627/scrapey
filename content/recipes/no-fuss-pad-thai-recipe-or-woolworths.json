  "name": "No Fuss Pad Thai Recipe | Woolworths",
  "prepTime": "PT15M",
  "recipeCategory": "Lunch,Dinner",
  "recipeCuisine": "Thai",
  "recipeId": "a0K2y0000012iIhEAI",
  "recipeIngredient": [
    "350g chicken thigh fillets",
    "250g thick rice stick noodles",
    "6 eggs, beaten",
    "1 bunch shallots",
    "6 tbs lemon, juiced",
    "1 tsp garlic, crushed",
    "2 tbs fish sauce",
    "1 tsp sugar",
    "2.5 tbs olive oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the rice stick noodles as per package instructions."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Beat the eggs well. Thinly slice the chicken and chop the shallots into small pieces."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix well sugar and fish sauce."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a wok until hot. Add crushed garlic until brown then add chicken and fish sauce mixture. Stir constantly until chicken just slightly cooked then add shallots."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the noodles, eggs and lemon juice to the mixture. Toss well until heated through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with crushed peanuts (optional)."
    }
  ],
