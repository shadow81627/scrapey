  "recipeIngredient": [
    "440g wok-ready rice noodles",
    "150g broccoli florets",
    "500g pork stir-fry strips",
    "100g san choy bow stir-fry sauce",
    "2 tbs Thai chilli jam stir-fry paste",
    "150g shredded carrot",
    "120g baby spinach leaves",
    "0.33 cup fried shallots",
    "0.33 cup unsalted roasted cashews",
    "1 tbs vegetable oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Pork_Noodle_Stir_Fry_Step_01?$foodhub-method-images$&fmt=png-alpha",
      "text": "Place noodles and broccoli in a large heatproof bowl. Cover with boiling water. Stand for 2 minutes or until noodles are tender and broccoli is bright green. Drain."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Pork_Noodle_Stir_Fry_Step_02?$foodhub-method-images$&fmt=png-alpha",
      "text": "Meanwhile, heat oil in a large non-stick frying pan or wok over medium-high heat. Add pork and stir-fry for 2 minutes or until browned and almost cooked through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Pork_Noodle_Stir_Fry_Step_03?wid=1300&hei=1300&fmt=png-alpha",
      "text": "Add sauce and paste to pork, then stir-fry for 2 minutes or until well combined and sauce mixture boils."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Pork_Noodle_Stir_Fry_Step_04?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add noodles, broccoli, carrot and spinach to pan and stir-fry for 2 minutes or until spinach is just wilted. Spoon into serving bowls. Serve sprinkled with shallots and cashews."
    }
  ],
