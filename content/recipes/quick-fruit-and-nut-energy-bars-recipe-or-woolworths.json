  "recipeIngredient": [
    "400g dates, soft, pitted, chopped",
    "150g cashew nuts, chopped",
    "150g peanuts, chopped",
    "60g cocoa powder",
    "1 tbs peanut butter, smooth",
    "1 tsp vanilla extract"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Tip all of the ingredients into a food processor and blend well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add a tablespoon of water and blitz again, adding more water if necessary to make a sticky dough."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Line a baking tin with parchment paper and spoon the mix in, pressing down with the back of a spoon to form an even layer. Place in the freezer for 30 minutes to set, then slice into 15 bars."
    }
  ],
