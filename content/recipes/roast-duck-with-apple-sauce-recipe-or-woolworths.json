  "recipeIngredient": [
    "4 medium duck breasts",
    "2 tbs sesame oil",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "50g butter",
    "5 medium apples, cored and diced",
    "1 tbs ginger, chopped",
    "1 star anise",
    "40g brown sugar",
    "0.8 cup apple juice"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 160°C/140°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Marinate the duck with the oil, salt and pepper and roast in a roasting tray for 90 minutes turning occasionally."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "While the duck is cooking heat up a non-stick pan to a medium heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the butter, apple and ginger for 2 minutes, then add the rest of the ingredients."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook for 10 minutes and then remove from the heat, remove the star anise and blend using a hand blender and cook for a further 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove the duck from the oven and tear the meat using 2 forks and cover with the apple sauce before serving with a choice of sides."
    }
  ],
