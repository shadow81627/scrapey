  "recipeIngredient": [
    "1 cup carrots chopped",
    "2/3 cup leek chopped",
    "2/3 cup parsnips chopped",
    "1 teaspoon oil",
    "1/2 teaspoon salt (or to taste)",
    "1/2 teaspoon cajun spice blend",
    "freshly ground black pepper or red pepper flakes to taste",
    "1 cup water"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "In a pan, heat oil at medium heat and add leeks to it."
    },
    {
      "@type": "HowToStep",
      "text": "Cook for 2-3 minutes until golden."
    },
    {
      "@type": "HowToStep",
      "text": "Add the carrots and parsnip and cook covered for 2-3 minutes until parnips start to get tender."
    },
    {
      "@type": "HowToStep",
      "text": "Blend these with 1/2 cup or more water into a puree and pour puree back into the pan."
    },
    {
      "@type": "HowToStep",
      "text": "Add salt, cajun spice and freshly ground black pepper to taste, mix well and bring to a boil."
    },
    {
      "@type": "HowToStep",
      "text": "Taste and adjust, salt spices and water content."
    },
    {
      "@type": "HowToStep",
      "text": "Serve hot topped with parsley or cilantro."
    }
  ],
