  "recipeIngredient": [
    "2 medium eggs, free range",
    "1 medium orange, juice and zest",
    "1 tsp vanilla essence",
    "250g brown sugar",
    "600g ground almonds",
    "50g chopped almonds",
    "100g dried cranberries",
    "2 tsp mixed spice",
    "0.2 cup golden syrup"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat your oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a mixing bowl, combine the eggs with the orange juice, zest and vanilla essence."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the sugar to the mixing bowl and combine fully."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Incrementally add the ground almonds while mixing and then add the chopped almonds, cranberries and mixed spice."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide the dough into 6 and shape into a log form and place evenly spaced on a baking tray."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in the oven for 13 minutes, then remove and allow to cool slightly before coating with maple syrup. All to cool before serving."
    }
  ],
