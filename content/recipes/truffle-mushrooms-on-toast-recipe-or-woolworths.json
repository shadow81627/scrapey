  "recipeIngredient": [
    "400g chestnut mushrooms, sliced",
    "1 tbs olive oil",
    "200g spinach",
    "1 tbs butter",
    "2 cloves garlic, peeled and minced",
    "0.5 tsp truffle oil",
    "4 slices sourdough bread",
    "1 pinch black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the butter and olive oil in a large pan on medium heat. Add the mushrooms and cook for 5 minutes, stir in the spinach and garlic and cook for 2 minutes, until the spinach has wilted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove the pan from the heat and stir in the truffle oil and black pepper. Toast the bread to your liking and top with mushrooms."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Season with more cracked black pepper and extra truffle oil and serve."
    }
  ],
