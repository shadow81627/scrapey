  "recipeIngredient": [
    "1.5 cup self-raising flour",
    "2 apples, grated",
    "3 tsp ground cinnamon",
    "0.5 cup rolled oats",
    "2 tbs black chia seeds",
    "0.33 cup buttermilk",
    "0.75 cup rice bran oil",
    "2 eggs, lightly beaten",
    "0.5 cup brown sugar",
    "1 tbs brown sugar, extra",
    "375g sultanas"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180Â°C. Grease a 12-hole medium muffin pan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine flour, oats, sugar, chia seeds and sultanas in a bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine eggs, apple, buttermilk and oil in a separate bowl. Fold into flour mixture. Divide among muffin holes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine extra sugar and cinnamon and sprinkle over muffins. Bake for 20 minutes or until a skewer inserted into the centre comes out clean. Serve warm or at room temperature."
    }
  ],
