  "recipeIngredient": [
    "2 tbs dark brown sugar",
    "2 limes, rind finely grated",
    "2 tsp ground allspice",
    "0.5 tsp cayenne pepper",
    "1 tsp ground cinnamon",
    "0.5 tsp ground nutmeg",
    "3 tsp dried thyme",
    "1 pinch black pepper, coarsley ground",
    "2 tbs sea salt flakes",
    "16 lamb loin chops"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Finely grate rind of limes and combine with sea salt flakes, brown sugar, thyme, allspice, black pepper, nutmeg, ground cinnamon and cayenne pepper in a mortar and pestle. Grind until a fine salt forms."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sprinkle mixture evenly onto both sides of the chops. Cook on a preheated BBQ plate for 3 minutes each side for medium-rare or until cooked to your liking."
    }
  ],
