  "recipeIngredient": [
    "500g beef brisket, cooked",
    "2 tbs olive oil",
    "4 medium floury baps, sliced in half",
    "0.5 head iceberg lettuce, torn",
    "2 medium roma tomato, sliced",
    "1 medium red onion, sliced",
    "2 tbs bbq sauce"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Tear away about 500g of beef brisket by using a fork for long shreds."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "On a pan set to a medium heat, fry your onions and torn brisket pieces for 5 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Prepare your baps by lightly toasting. Now serve the torn brisket and onions with the tomato and lettuce on a floury bap with BBQ sauce."
    }
