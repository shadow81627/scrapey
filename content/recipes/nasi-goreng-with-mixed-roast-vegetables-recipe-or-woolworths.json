  "recipeIngredient": [
    "1 medium white onion, peeled and chopped",
    "1 medium yellow capsicum, peeled and chopped",
    "1 medium red capsicum, peeled and chopped",
    "4 cloves garlic, peeled and diced",
    "1 pinch cracked black pepper",
    "2 tbs sesame oil",
    "800g rice",
    "2 tbs oyster sauce",
    "2 medium carrot, peeled and diced",
    "2 tbs rice wine vinegar",
    "2 tbs soy sauce",
    "4 stalks scallions, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toss the onion, capsicum, garlic and pepper in the sesame oil, place on a baking tray and roast for 20 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the rice as per packet instructions and drain."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan to a medium heat, remove the vegetables from the oven and transfer to the pan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rice to the pan and the rest of the ingredients apart from the scallions and fry everything together for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and garnish with scallions to serve."
    }
  ],
