  "recipeIngredient": [
    "4 slices rye bread, toasted (to serve)",
    "1 avocado",
    "2 Lebanese cucumbers",
    "4 radishes, thinly sliced",
    "2 spring onions, sliced",
    "200g smoked salmon",
    "0.33 cup garlic aioli mayonnaise",
    "4 cups salad leaves (to serve)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Halve cucumbers lengthways, scoop out seeds using a teaspoon and discard. Slice on an angle."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Halve avocado, remove stone and slice. Scoop out flesh using a large spoon."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pile salad leaves onto serving plates then top with cucumber, avocado and radish. Add slices of smoked salmon."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drizzle with aioli and garnish with spring onions. Serve with rye toast."
    }
  ],
