  "recipeIngredient": [
    "0.3333 cup white sesame seeds",
    "2 tbs dried onion",
    "2 tbs dried garlic granules",
    "2 tbs poppy seeds",
    "3 tsp sea salt flakes"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place seeds, onion and garlic in a bowl. Stir to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat a small non-stick frying pan over medium heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the sesame mixture to pan. Cook, stirring, for 1 minute or until golden and fragrant. Remove from heat and return to the bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add poppy seeds and salt. Stir to combine. Serve sprinkled over salad, barbecued meat or toast spread with ricotta."
    }
  ],
