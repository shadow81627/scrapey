  "recipeIngredient": [
    "260g blade beef, sliced",
    "3 capsicum, roughly chopped",
    "1 shallot, sliced into 3cm length",
    "1 tbs lee kum kee black bean & garlic sauce",
    "1 tbs soy sauce",
    "1 tsp corn flour",
    "0.5 tsp white sugar",
    "1 tbs olive oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/tmhoajsfiq2a49nsfupl?wid=3072&hei=4096&fmt=png-alpha",
      "text": "Gather and prepare all the ingredients."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Marinate beef with soy sauce, corn flour and sugar."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat pan and oil with medium heat. Add beef and quickly stir for 3 minutes. While still medium rare, set it aside on plate."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add capsicum in pan and cook for 3-4 minutes. Keep stirring."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add back the beef and combine with capsicum."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add black bean & garlic sauce and stir through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Turn off heat. Stir in shallot and serve."
    }
  ],
