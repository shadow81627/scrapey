  "recipeIngredient": [
    "75g bean sprouts",
    "50g rice noodles",
    "1.1L chicken stock",
    "1 tbs light soy sauce",
    "75g parma ham or lean english smoked bacon",
    "2 tbs fresh coriander, finely chopped",
    "2 tbs spring onions, finely chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Soak your noodles in a bowl of warm water till soft. Drain and then cut into 5 cm pieces."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top and tail bean sprouts. This will make your soup look much cleaner (I do when I have time to or they just go in as is). Can use fresh or canned both work just as well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bring stock to simmer in a large pot."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add drained noodles, soy sauce simmer for two minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Then add ham or bacon, coriander spring onions then simmer for 1 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Then finally add bean sprouts and simmer for a minute."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add pepper to taste. Check before you add salt as soy sauce may make it salty enough already."
    }
  ],
