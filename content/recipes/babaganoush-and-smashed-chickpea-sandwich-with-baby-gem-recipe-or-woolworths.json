  "recipeIngredient": [
    "8 slices sourdough bread",
    "40g baba ganoush, store bought",
    "40g chickpeas, store bought and drained",
    "30g baby gem lettuce",
    "1 tbs olive oil",
    "1 pinch salt",
    "1 pinch pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a bowl, place the drained chicks with the oil, salt and pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mash the ingredients together until all the chickpeas are mashed and combine fully. Leave to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Layer the baba ganoush with the smashed peas onto the sourdough bread."
    }
  ],
