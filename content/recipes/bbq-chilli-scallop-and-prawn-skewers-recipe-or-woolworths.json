  "recipeIngredient": [
    "12 scallops, shelled, fresh",
    "250g king prawns, peeled and deveined",
    "4 tbs olive oil, divided",
    "1 clove garlic, peeled and minced",
    "2 medium limes, 1 juiced and zested, 1 chopped into wedges",
    "0.5 medium red chilli, diced",
    "4 tbs coriander, fresh, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Soak 16 skewers in cold water. Heat the barbecue to medium high. Thread each skewer with scallops and prawns and brush with 1 tablespoon of oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk the remaining oil, garlic, lime juice and zest, chilli and coriander together."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the skewers on the barbecue for 5 minutes, turning occasionally. Serve drizzled with dressing and with lime wedges on the side."
    }
  ],
