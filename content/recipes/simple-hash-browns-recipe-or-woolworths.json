  "recipeId": "a0K2y0000012g0rEAA",
  "recipeIngredient": [
    "500g potatoes, grated",
    "2 tbs plain flour",
    "1 egg",
    "5 tbs sunflower",
    "1 tsp salt (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Peel potatoes and grate into a stainless steel bowl. Set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add plain flour, egg and salt to grated potatoes. Stir all ingredients until well combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a frypan on medium heat add sunflower or olive oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Using a tablespoon of mixture shallow fry for about 3 minutes or until golden brown. Turning only once."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Repeat process until all mixture is finished."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer to serving plate, sprinkle hash browns with salt and are best eaten warm. Enjoy!"
    }
  ],
