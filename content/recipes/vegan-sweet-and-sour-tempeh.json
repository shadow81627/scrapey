  "recipeIngredient": [
    "2 tbsp soy sauce (or tamari)",
    "1 tbsp cornstarch",
    "1/3 cup rice vinegar",
    "1/4 cup ketchup",
    "2 tbsp brown sugar",
    "1/2 cup water",
    "1 tbsp coconut oil",
    "2 8 oz packages tempeh, quartered and cut into thin strips",
    "1 large green pepper (seeded and diced)",
    "2 cups fresh pineapple chunks",
    "5 cloves garlic (sliced)",
    "cooked white rice (for serving)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Whisk together soy sauce, cornstarch, rice vinegar, ketchup, brown sugar, and water. Set aside."
    },
    {
      "@type": "HowToStep",
      "text": "Heat coconut oil in a large skillet over medium-high heat. Add tempeh, green pepper, pineapple, and garlic. Cook until just beginning to brown, 5–7 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Whisk sauce again and add to skillet. Cook until sauce is thickened, 1–2 minutes more, then remove from heat. Serve over rice."
    }
  ],
