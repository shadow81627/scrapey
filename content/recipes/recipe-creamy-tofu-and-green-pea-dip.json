  "recipeCuisine": [],
  "recipeIngredient": [
    "12 ounces silken tofu, drained",
    "1 1/2 cups green peas, defrosted if frozen",
    "2 teaspoons extra virgin olive oil",
    "Juice of 1 lime",
    "1/4 cup loosely packed cilantro leaves",
    "1 scallion, roughly chopped",
    "1 jalapeño, seeded and roughly chopped (optional)",
    "1 teaspoon salt",
    "1/4 teaspoon cumin seeds, toasted and ground"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place all ingredients in the bowl of a food processor. Process until smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Taste and adjust seasonings, if desired."
    },
    {
      "@type": "HowToStep",
      "text": "If possible, cover and refrigerate the dip for at least an hour to let the flavors mingle."
    },
    {
      "@type": "HowToStep",
      "text": "Serve with chips or crudités."
    }
  ],
