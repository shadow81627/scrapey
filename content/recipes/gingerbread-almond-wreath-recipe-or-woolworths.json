  "recipeCuisine": "Australian",
  "recipeId": "8209",
  "recipeIngredient": [
    "1 cup desiccated coconut",
    "1 cup currants",
    "2 tsp mixed spice",
    "0.5 cup sultanas",
    "250g butter, diced and softened",
    "2 sprigs rosemary (for serving)",
    "1 tbs ground ginger",
    "0.5 tsp bicarbonate of soda",
    "0.25 cup flaked almonds",
    "4 extra large eggs",
    "1.25 cup brown sugar",
    "2 cup almond meal",
    "100g raspberries (for serving)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 170°C (150°C fan-forced)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lightly grease and line a 22cm ring tin."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Beat the butter and sugar until pale and creamy."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add eggs one at a time, beating well between each addition."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir in the almond meal, coconut, bicarbonate of soda and spices. Mix well, then stir in the currants and sultanas."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour into prepared pan and scatter the top with flaked almonds."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake for 55-60 minutes until an inserted skewer comes out clean."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Allow to cool in the pan for 10 minutes before removing to a cooling rack to cool completely."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve on a platter surrounded by fresh rosemary sprigs and fill the cavity with raspberries."
    }
  ],
