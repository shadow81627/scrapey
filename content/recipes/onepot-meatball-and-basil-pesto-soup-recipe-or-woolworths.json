  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil in a large saucepan over high heat. Cook meatballs for 3 minutes, turning, or until browned."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add stock and veggie mix to pan, then bring to the boil. Reduce heat to medium and simmer, covered, for 10 minutes, stirring occasionally, or until meatballs are cooked through and vegetables are tender."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide soup among bowls. Top with pesto."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve scattered with parmesan and basil leaves."
    }
  ],
