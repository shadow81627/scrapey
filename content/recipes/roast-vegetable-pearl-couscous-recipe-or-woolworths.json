  "recipeIngredient": [
    "400g pearl couscous",
    "1.6 cup vegetable stock",
    "1 small red onion, peeled and diced",
    "1 large capsicum, diced",
    "80g butternut squash, diced",
    "80g zucchini, diced",
    "80g broccoli, chopped",
    "50g carrot, peeled and chopped",
    "2 tbs olive oil",
    "1 pinch cracked black pepper",
    "1 pinch salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toss the vegetables with the olive oil and place on a baking tray."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Roast in the oven for 20 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the couscous and 400ml of the vegetable stock in a pot and bring to the boil, allow to simmer for 25 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once the pearl couscous is cooked, remove from the heat and allow to cool slightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine the couscous with the roast vegetables, toss well before serving."
    }
  ],
