  "recipeId": "RGWW338921",
  "recipeIngredient": [
    "500g basmati rice, ready cooked",
    "1 tbs olive oil",
    "1 medium onion, peeled and diced",
    "2 tbs Indian curry paste",
    "1 tsp turmeric",
    "50g sultanas",
    "250g carrots, peeled and grated",
    "4 tbs flaked almonds, toasted",
    "0.5 bunch coriander, fresh, chopped",
    "1 medium lemon, juice and zest"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a large pan and cook the onion for 5 minutes. Stir in the curry paste and turmeric then tip in the rice. Stir to coat and cook for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Tip the rice and onion mix into a large bowl and stir in the carrots, sultanas, chopped coriander, lemon juice, lemon zest and flaked almonds. Serve warm or cold."
    }
  ],
