  "recipeIngredient": [
    "2/3 cup sushi rice",
    "3/4 cup + 2 tbsp. water",
    "1/2 tbsp. salt",
    "1/2 tbsp. rice vinegar",
    "2 tbsp. vegetable oil",
    "1 garlic clove (minced)",
    "2 tbsp. Asian chili paste",
    "1 scallion",
    "1/2 medium eggplant (sliced into thin strips)",
    "2 nori sheets",
    "sesame seeds",
    "soy sauce or tamari",
    "wasabi",
    "pickled ginger"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place rice in a strainer and rinse under cold, running water for 1-2 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Place in a small saucepan with other rice ingredients, stir and heat to a simmer. Cover and allow to continue simmering for about 20 minutes, or until all liquid is absorbed."
    },
    {
      "@type": "HowToStep",
      "text": "Remove from heat and allow to sit for another 10 minutes, covered."
    }
