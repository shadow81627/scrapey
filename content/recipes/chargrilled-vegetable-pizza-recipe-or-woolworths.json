  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 200°C (180°C fanforced)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Chargrill eggplant, zucchini, mushroom and capsicum on a non-stick grill or BBQ until tender. Remove skin from blackened red capsicum and cut into strips."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread the pita breads with tomato sauce and top with vegetables. Dollop evenly with cottage cheese and place in the oven for 1015 minutes or until base is crisp and the cheese has browned."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve topped with basil leaves."
    }
  ],
