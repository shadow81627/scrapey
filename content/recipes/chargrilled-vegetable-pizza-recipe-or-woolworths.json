  "recipeIngredient": [
    "200g Bulla Cottage Cheese, Original",
    "0.5 cup tomato pizza sauce",
    "1 red capsicum, quartered",
    "1 large eggplant, sliced",
    "12 button mushrooms, sliced",
    "4 wholemeal pita breads",
    "4 medium zucchini, sliced",
    "2 tbs basil leaves"
