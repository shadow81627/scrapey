  "recipeId": "a0K2y0000011NvREAU",
  "recipeIngredient": [
    "750g cream cheese",
    "395g sweetened condensed milk",
    "1 cup thickened cream",
    "5 tsp gelatine powder",
    "0.5 cup water",
    "360g Fry's turkish delight (2x share bags)",
    "400g choc ripple biscuits",
    "160g butter"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Chop the Turkish Delight bars into small pieces (around 1cm x 1cm). Crush the choc ripple biscuits."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Melt butter and combine with crushed biscuits until well coated. Pack mixture firmly into a 26cm springform pan. Place in refrigerator and allow to set for 2 hours."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Boil water, sprinkle gelatine powder and mix until well combined. Add all fillings into a electric mixer, except 120g of the chopped Turkish Delight pieces. Mix until well combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour mixture on top of previously prepared base, spread mixture evenly.Sprinkle the remaining 120g Turkish Delight pieces on top."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place into fridge and allow to set for at least 6 hours. For best results, allow to set overnight."
    }
  ],
