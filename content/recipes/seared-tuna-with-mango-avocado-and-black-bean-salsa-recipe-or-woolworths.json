  "recipeIngredient": [
    "400g tuna steaks",
    "2 tbs olive oil, divided",
    "400g black beans, canned, drained",
    "5 spring onions, sliced",
    "1 avocado, peeled and diced",
    "1 mango, peeled and diced",
    "2 tomatoes, diced",
    "1 bunch coriander, fresh chopped",
    "2 limes, juiced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Brush the tuna steaks with half of the oil and sear in a griddle pan on the highest heat for 20-30 seconds on each side. Rest for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toss the black beans, spring onions, avocado, mango, tomatoes, coriander and lime together with the remaining oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve the tuna steaks piled on top of the salsa with fresh coriander on top."
    }
  ],
