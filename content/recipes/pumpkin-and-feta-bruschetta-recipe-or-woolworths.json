  "keywords": "Bruschetta & crostini,Microwave,Roast,Pumpkin,Avocado",
  "name": "Pumpkin & Feta Bruschetta Recipe | Woolworths",
  "prepTime": "PT15M",
  "recipeCategory": "Lunch",
  "recipeCuisine": "European",
  "recipeId": "a0K0I00000SHaJaUAL",
  "recipeIngredient": [
    "200g kent pumpkin",
    "1 sourdough bread loaf",
    "1 small avocado",
    "20g pine nuts",
    "50g feta cheese",
    "2 tbs caramelised onions",
    "1 tbs olive oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180 degrees (fan forced)"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Chop the pumpkin into cubes (roughly 3x3cm) and place on a baking tray, drizzle with olive oil and bake in the oven for 45 mins or until roasted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice sourdough and toast 4 slices."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide the caramelised onion evenly between 4 slices of toast and spread evenly over each slice."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide roasted pumpkin pieces evenly between 4 slices of toast and place on top of toast."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice avocado and place slices evenly between the pieces of toast."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Crumble feta cheese evenly between pieces of toast."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place pine nuts in a microwave safe dish without a lid and cook on high for 1 minute 30 seconds (or until cooked to liking)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide pine nuts between pieces of toast."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Enjoy."
    }
  ],
