  "recipeIngredient": [
    "1 tbs sesame oil",
    "1 medium white onion",
    "4 cloves garlic, peeled and mashed",
    "5 cardamon seeds",
    "3 medium capsicum, mixed colours, de-seeded and sliced",
    "1 stick cinnamon",
    "3 tbs madras powder",
    "1.2 cup coconut milk",
    "1 tbs lime juice",
    "1 medium carrot, peeled and diced",
    "1 small zucchini, diced",
    "400g tomatoes"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a large saucepan to a medium heat and then add the sesame oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the onions, garlic and seeds to the pan and fry for 4 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the capsicum and spices to the pan and fry for a further 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients to the pot and bring to the boil, then bring to a simmer and cook for 20 minutes stirring occasionally."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and serve with suggested condiments of naan bread or rice."
    }
  ],
