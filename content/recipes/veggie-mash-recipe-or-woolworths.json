  "recipeIngredient": [
    "1 carrot",
    "0.5 large potato",
    "1 small swede",
    "salt and pepper (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/lkoq46t8xbex5bkxmd3i?wid=3900&hei=3000&fmt=png-alpha",
      "text": "Chop the carrot, swede and potato into rough chunks or any other root veg you are using. Place into a pan of slightly salted water over heat for 2 mins until softened"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/brp6ajrtciuc0sprppiz?wid=3000&hei=4000&fmt=png-alpha",
      "text": "Once soften, drain and transfer to a blender."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/urkqs6prdxovxnrnbqem?wid=3000&hei=4000&fmt=png-alpha",
      "text": "Whisk them together until they form a puree."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer back to the pan over the heat for a couple of minutes to warm through and reduce moisture. Season."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/kcqwuquld6mlsiobllbx?wid=3000&hei=2308&fmt=png-alpha",
      "text": "Serve with your choice of other foods e.g meat, Yorkshire puddings, nut loaf etc.Has you can see we served ours with Woolworth's sausages."
    }
  ],
