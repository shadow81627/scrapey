  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Cook pasta al dente in a saucepan of boiling, salted water. Drain."
    },
    {
      "@type": "HowToStep",
      "text": "Heat the oil over medium heat in a large skillet. Cook garlic for 1 to 2 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Add tomatoes and green peas. Bring to a boil. Salt and pepper to taste."
    },
    {
      "@type": "HowToStep",
      "text": "Add pasta. Stir and cook 1 minute."
    },
    {
      "@type": "HowToStep",
      "text": "When serving, sprinkle with basil."
    }
