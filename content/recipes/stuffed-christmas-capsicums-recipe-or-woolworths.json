  "recipeIngredient": [
    "1 medium red capsicum",
    "3 medium capsicums tri colour",
    "1 brown onion, finely diced",
    "1 tbs flat-leaf parsley, finely chopped",
    "0.25 tsp ground cinnamon",
    "0.25 tsp ground cumin",
    "0.25 tsp ground turmeric",
    "1 pinch freshly cracked black pepper",
    "1 tbs olive oil",
    "3 cup brown rice, cooked",
    "2 clove garlic, finely chopped",
    "0.5 cup pine nuts, toasted",
    "0.5 cup dried cranberries",
    "1 pinch sea salt ground (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180Â°C"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place a large high-sided frying pan over medium heat. Add olive oil, garlic, onion and ground spices and cook for 2-3 minutes or until onion has softened and spices are fragrant."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add pine nuts, cranberries, parsley and rice to the spice mixture and stir to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut tops off capsicums and remove any core and seeds. Fill capsicums with rice mixture, top with their lid and bake on a lined baking tray for 20 minutes or until capsicum is cooked through but not collapsing. Serve."
    }
  ],
