  "recipeIngredient": [
    "2 cup white rice long grain (rinsed)",
    "400g chicken thigh fillets (trimmed, chopped)",
    "1L Campbell's Real Chicken Liquid Stock",
    "1 tbs tomato paste",
    "1 brown onion (finely chopped)",
    "Tempo Shredded Cheddar Cheese",
    "2 tbs extra virgin olive oil",
    "2 clove garlic cloves (crushed)",
    "0.5 cup frozen peas"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to moderate (180C)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil in a large, flameproof casserole dish (16-cup capacity) over a medium heat. Add onion and garlic. Cook, stirring occasionally, until soft. Add chicken. Cook, stirring occasionally, for about 6 minutes, or until browned."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir in rice and paste. Add Campbellâ€™s chicken stock and peas. Stir well to combine. Bring to boil. Cover with lid. Transfer to oven."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook for about 25 minutes, or until rice is just tender and liquid has almost been absorbed."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve immediately with cheese."
    }
  ],
