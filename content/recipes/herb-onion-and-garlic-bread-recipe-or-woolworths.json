  "recipeIngredient": [
    "1 medium bread stick",
    "125g butter, softened (or margarine)",
    "1 onion, finely chopped",
    "2 tsp parsley flakes",
    "1 tsp tuscan seasoning",
    "1 tsp french mustard",
    "1 tsp lemon juice",
    "1 dollop minced garlic"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice bread stick diagonally into thin rounds. Be careful not to cut right through to the bottom of the rounds as you don't want the butter to leak out if possible"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine all ingredients and spread them between each slice of bread. Wrap the bread stick securely in foil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
