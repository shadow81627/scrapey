  "recipeIngredient": [
    "4 medium bananas, ripe",
    "150g dark chocolate",
    "60g almonds, finely chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Peel the bananas and slice each in half across the middle to make 8 halves. Place a wooden lolly stick in each."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the bananas on a tray and freeze for 3 hours."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Finely chop the dark chocolate and place in a glass bowl. Microwave on high in 2 30 second bursts, stirring to melt completely."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the chopped almonds on a plate, dip the frozen bananas in the melted chocolate and roll in the almonds to coat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the popsicles on a lined tray and return to the freezer to firm up until ready to serve."
    }
  ],
