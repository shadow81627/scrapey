  "recipeIngredient": [
    "400g spinach, washed and drained",
    "240g burrata, drained and sliced",
    "4 medium heirloom tomato, sliced",
    "60g pine nuts",
    "80g croutons",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "2 tbs olive oil",
    "1 tsp garlic powder",
    "2 tbs lemon juice",
    "1 tbs thyme, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a small bowl, combine the olive oil with the salt, pepper, garlic, lemon and thyme and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the spinach leaves on your serving plates and then equally place the sliced burrata and tomatoes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sprinkle the croutons and pine nuts over the salad plates."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drizzle with the sauce before serving."
    }
  ],
