  "recipeIngredient": [
    "500g chocolate ripple biscuits",
    "600ml thickened cream",
    "1 tsp sugar",
    "1 tsp vanilla essence",
    "1 flake",
    "340g Jaffas",
    "1 bag peppermint leaves"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Beat cream, sugar and vanilla until thick."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread cream on one side of biscuit and then repeat with remaining biscuits turning slightly with each one to get the circle shape."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once you've made your circle spread remaining cream over biscuits."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sprinkle with chocolate flake and set in fridge (overnight is best)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "I put toothpicks on top and cover with cling wrap."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once ready to serve decorate with Jaffa's and peppermint leaves or you could leave plain if preferred."
    }
  ],
