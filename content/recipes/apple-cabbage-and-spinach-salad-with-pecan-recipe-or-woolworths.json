  "recipeIngredient": [
    "1 clove garlic, crushed",
    "0.5 red cabbage, finely shredded",
    "0.5 small red onion, thinly sliced",
    "2 tbs parsley (to serve)",
    "2 red apples",
    "0.33 cup extra virgin olive oil",
    "1 tsp honey",
    "0.25 cup white wine vinegar",
    "0.5 cup pecans, toasted",
    "2 tbs dijon mustard",
    "60g baby spinach leaves"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place cabbage, spinach and onion into a serving bowl and toss gently with your hands to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine oil, vinegar, mustard, garlic and honey into a screw-top jar and shake well to combine. Season to taste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Halve apples and remove core. Slice very thinly. Add to salad with pecans, pour over some of the dressing and top with parsley leaves. Serve salad with remaining dressing."
    }
  ],
