  "recipeIngredient": [
    "0.66 cup plain flour",
    "0.33 cup self-raising flour",
    "125g butter, softened",
    "1 tsp vanilla extract",
    "1 cup dark chocolate chunks",
    "1 egg",
    "0.5 cup brown sugar",
    "0.33 cup caster sugar",
    "0.5 cup crunchy peanut butter"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180Â°C. Line a 18x28cm shallow pan with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Using an electric mixer, cream butter and peanut butter until well combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add sugars and beat until light and creamy. Add egg and vanilla and beat to combine. Stir in sifted flours and chocolate chunks."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread mixture evenly into pan. Bake for 25 minutes or until cooked when tested with a skewer."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cool in pan then cut into wedges."
    }
  ],
