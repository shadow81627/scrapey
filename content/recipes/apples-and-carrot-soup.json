  "recipeIngredient": [
    "6 carrots",
    "2 apples",
    "2 potatoes",
    "1 onion",
    "1 teaspoon curry",
    "Salt and pepper to taste",
    "1 cup cream (milk for a lighter version)",
    "5 cups vegetable broth",
    "Basil (I use purple, but you can use whatever you have on hand)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Wash and peel carrots, potatoes, and apples. Cut into small pieces."
    },
    {
      "@type": "HowToStep",
      "text": "Cut the onion and let it sweat with oil in a casserole dish."
    },
    {
      "@type": "HowToStep",
      "text": "Add carrots, potatoes, apples, curry, and vegetable broth."
    },
    {
      "@type": "HowToStep",
      "text": "Bring to a boil, then simmer for 40 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Sprinkle with basil. Serve hot."
    }
  ],
