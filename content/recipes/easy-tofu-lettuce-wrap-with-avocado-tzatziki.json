  "recipeIngredient": [
    "4 ounce tofu (sliced into 3-inch long strips)",
    "1 cup teriyaki sauce",
    "1 large Swiss chard leaf",
    "1/3 cucumber (thinly sliced)",
    "1/4 cup carrots (shredded)",
    "1 Roma tomato (thinly sliced)",
    "2 tablespoon tzatziki sauce"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Make the tzatziki sauce"
    },
    {
      "@type": "HowToStep",
      "text": "Press the tofu to remove the excess water for 30 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Slice the tofu into strips and put in a shallow dish. Cover with the teriyaki sauce and marinate for 15 minutes"
    },
    {
      "@type": "HowToStep",
      "text": "Preheat oven to 375 degrees F (176C)"
    },
    {
      "@type": "HowToStep",
      "text": "Put the tofu strips on a baking sheet and bake for 5 minutes on each side, or until they're nicely browned."
    },
    {
      "@type": "HowToStep",
      "text": "Lay the Swiss chard leaf flat and fill with the vegetables, tofu, and tzatziki sauce and wrap them up with the leaf."
    }
  ],
