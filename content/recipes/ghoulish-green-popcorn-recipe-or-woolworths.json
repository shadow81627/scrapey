  "recipeIngredient": [
    "1 tbs icing sugar mixture",
    "1 tbs matcha tea powder",
    "2 tbs olive oil",
    "0.33 cup popping corn kernels"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sift matcha and icing sugar together in a large snap lock bag. Set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil in a large saucepan over medium heat. Add corn kernels, shaking pan to coat. Cover with a lid and cook until popping begins. Cook, shaking pan occasionally, for 5 minutes or until popping finishes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from heat and tip popped corn into bag with matcha mixture, discarding any unpopped kernels. Seal and shake vigorously to coat with powder. Serve."
    }
  ],
