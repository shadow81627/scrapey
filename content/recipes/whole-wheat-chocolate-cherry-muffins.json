  "recipeIngredient": [
    "1 cup whole wheat pastry flour",
    "1 tsp baking powder",
    "1/2 tsp baking soda",
    "1/4 cup cocoa powder",
    "3/4 cup brown sugar",
    "1 6 oz container plain non-fat Greek yogurt",
    "1/4 cup canola oil",
    "1 egg",
    "1/2 cup dried cherries",
    "1/4 cup mini chocolate chips"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat oven to 400°F. Spray muffin pan with cooking spray or fill with paper liners."
    },
    {
      "@type": "HowToStep",
      "text": "Combine flour, baking powder, baking soda, and cocoa powder in a large bowl, whisking until lumps are gone."
    },
    {
      "@type": "HowToStep",
      "text": "In a medium bowl, combine sugar, yogurt, canola oil, and egg."
    },
    {
      "@type": "HowToStep",
      "text": "Add wet mixture to flour mixture and stir until moistened. Fold in cherries and chocolate chips."
    },
    {
      "@type": "HowToStep",
      "text": "Divide mixture evenly into muffin cups. Bake for 15–17 minutes or until toothpick inserted into the center of a muffin comes out clean."
    }
  ],
