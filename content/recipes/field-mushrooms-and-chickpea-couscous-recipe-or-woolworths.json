  "recipeIngredient": [
    "400g can chickpeas, drained",
    "1 carrot, peeled and small diced",
    "0.5 cup baby peas",
    "0.5 cup corn kernels",
    "100g diced onion",
    "400g field mushrooms, thickly sliced",
    "1 tsp olive oil",
    "1 cup vegetable stock",
    "2 cloves garlic, peeled and minced",
    "0.25 tsp salt",
    "0.25 tsp pepper",
    "10g tarragon, finely chopped",
    "1 stalk celery, small diced",
    "1 tsp minced chilli",
    "1.5 cups cous cous"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Saut√© the onion, garlic celery and carrots in the oil with the tarragon and chilli."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the mushrooms and cook till cooked through and tender."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the chickpeas, peas and corn, cook a further 3 minutes stirring frequently."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/gqmslgnnj7kak5qszj9s?wid=3024&hei=4032&fmt=png-alpha",
      "text": "Add the stock, bring to the boil, add the couscous all at once, stir well and then seal with a lid and leave 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove lid and fluff up by stirring well, adjust seasoning and serve immediately."
    }
  ],
