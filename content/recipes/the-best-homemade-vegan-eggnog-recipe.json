  "recipeIngredient": [
    "3 cups lite coconut milk (, kind in a can)",
    "1/2 cup raw cashews (, see note)",
    "1/4 - 1/2 cup maple syrup (, adjust for preferred sweetness)",
    "1 tablespoon vanilla extract (, reduce for less vanilla taste)",
    "1 1/2 teaspoons nutmeg",
    "1 1/2 teaspoons cinnamon",
    "1/8 teaspoon sea salt",
    "1/8 teaspoon cloves (, optional for added flavor)",
    "Dairy Free Whipped Cream",
    "Brandy or rum"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Put all ingredients into a blender and blend until creamy and smooth. Put in the fridge to get cold or serve over ice."
    },
    {
      "@type": "HowToStep",
      "text": "Add brandy or rum for an alcohol treat. Top with whipped cream if desired."
    },
    {
      "@type": "HowToStep",
      "text": "Serve!"
    }
  ],
