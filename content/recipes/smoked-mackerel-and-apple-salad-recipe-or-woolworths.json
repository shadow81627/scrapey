  "recipeIngredient": [
    "350g smoked mackerel",
    "2 medium red apples, peeled, cored and finely sliced",
    "100g radishes, thinly sliced",
    "150g watercress",
    "0.5 medium cucumber, finely sliced",
    "40g walnuts, chopped",
    "4 tbs olive oil",
    "2 tsp wholegrain mustard",
    "1 tbs apple cider vinegar",
    "2 tsp honey"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Flake the smoked mackerel into medium sized pieces and add to a bowl with the watercress, apples, radishes, cucumber and walnuts."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk the mustard, honey and vinegar together in a small bowl and whisk in the oil to make a dressing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour the dressing into the salad bowl, toss and serve."
    }
  ],
