  "recipeIngredient": [
    "260g chargrilled capsicum in oil",
    "1 cup beetroot leaves, rinsed",
    "1 tsp red chilli flakes",
    "0.5 cup finely grated parmesan",
    "0.5 cup sundried tomatoes in oil",
    "3 tsp extra virgin olive oil",
    "0.25 cup oregano leaves",
    "1 clove garlic"
