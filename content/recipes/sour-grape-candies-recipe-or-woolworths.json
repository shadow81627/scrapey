  "recipeIngredient": [
    "200g seedless grape, washed and halved",
    "4 tbs lemon juice",
    "4 tbs lime juice",
    "50g caster sugar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a sealable bag, place the halved grapes with the lemon and lime juice and leave to soak in the fridge for 1 hour."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove the grapes from the bag and drain away the juice."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the grapes in a bowl and toss in the caster sugar to cover evenly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the bowl in the freezer for 30 minutes before serving."
    }
  ],
