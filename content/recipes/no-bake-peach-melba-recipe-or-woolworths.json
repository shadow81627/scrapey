  "recipeIngredient": [
    "400g peaches in syrup",
    "200g flan sponge",
    "4 scoops vanilla ice-cream",
    "4 tbs strawberry jam",
    "60g macadamia nuts, lightly bashed",
    "0.4 cup whipped cream",
    "1 tsp vanilla extract"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Using a small bowl, cut out 4 circular pieces of flan sponge and place them into the base of 4 serving bowls."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Evenly spread each sponge circle with strawberry jam."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Evenly divide the peaches from the tin onto the sponge and drizzle 1 tablespoon of the syrup on top of each."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place 1 scoop of ice cream in the centre of each bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whip the cream with the vanilla extract and cover the desserts evenly before sprinkling with crushed macadamia nuts before serving."
    }
  ],
