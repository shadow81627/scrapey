    "500g beef brisket, cooked",
    "2 tbs olive oil",
    "1 head garlic, peeled and sliced",
    "400g ramen noodles",
    "200g cabbage, thinly sliced",
    "2 stalks scallions, sliced",
    "1 tbs soy sauce",
    "1 tbs bbq sauce",
    "1 tbs sriracha sauce",
