  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Tear away about 500g of brisket using a fork and set to one side. Cook the noodles as per packet instructions and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a wok or similar pan, fry the torn brisket with the oil for 2 minutes while tossing. Add the rest of the chopped vegetables to the pan and fry while tossing for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the noodles and all the sauces to the pan and keep frying for minutes ensuring to combine all flavours."
    }
  ],
