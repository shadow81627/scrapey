  "recipeIngredient": [
    "1 pkt Unicorn Classic Camembert Cheese 125g",
    "4 pears, halved with seeds removed",
    "0.33 cup walnuts",
    "3 sprigs bay leaves",
    "35g unsalted butter",
    "0.5 tsp vanilla extract",
    "4 slices fresh bread (to dip)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Halve pears, scoop out seeds."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Scatter bay leaves into baking tray."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place camembert into tray, surround with pears. Place walnuts on top of pears."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add butter, maple syrup, vanilla extract and 2 bay leaves to a small saucepan and bring to the boil. Drizzle 3/4 of the mixture over the pears."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place tray in oven and bake for 20 minutes until pears are softened and cheese is gooey."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Enjoy with some fresh bread!"
    }
  ],
