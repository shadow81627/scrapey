  "recipeIngredient": [
    "1kg chuck steak, cubed into 2cm",
    "2 brown onions, diced",
    "4 cloves garlic, crushed",
    "2 tbs olive oil",
    "2 tsp ras el hanout",
    "0.5 tsp Harissa pasta",
    "0.25 tsp black pepper, freshly ground",
    "0.75 cup diced tinned tomatoes",
    "1 tbs lemon juice, or 1/2 preserved lemons rinsed and chopped",
    "2 tsp honey",
    "1 tbs coriander, chopped",
    "2 tbs flat leaf parsley, chopped",
    "2 tbs pine nuts, toasted"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Trim and cut beef into 2cm cubes. Place olive oil in frying pan and in batches brown beef."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place beef into a slow cooker and add remaining ingredients (except holding 1 tbs parsley and pine nuts for garnish). Stir until mixed."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slow cook beef for 8 hours or until beef is tender. Serve with couscous and steamed green beans. Scatter pine nuts and parsley before serving."
    }
  ],
