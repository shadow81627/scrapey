  "recipeIngredient": [
    "40g butter, softened",
    "2 tbs thousand island dressing",
    "500g coleslaw salad",
    "150g swiss cheese, thinly sliced",
    "8 slice rye bread",
    "250g corned beef, shaved",
    "8 dill cucumbers (to serve)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat sandwich press. Place 4 slices of bread onto chopping board."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top with beef, coleslaw, dressing and cheese. Finish with remaining bread slices and press on firmly. Spread both sides of each sandwich with butter."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook sandwiches in the heated sandwich press for about 8-10 minutes or until golden and heated through. Serve with dill cucumbers."
    }
  ],
