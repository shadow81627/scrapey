  "recipeIngredient": [
    "1 cup oat flour",
    "1 cup almond flour",
    "2 teaspoons baking powder",
    "1/2 teaspoon sea salt",
    "1 cup maple syrup",
    "1/2 cup cooked chickpeas (, rinsed and drained)",
    "1/2 cup almond butter",
    "2 teaspoons vanilla extract",
    "3/4 cup chocolate chips"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat oven to 375 F/ 190 C."
    },
    {
      "@type": "HowToStep",
      "text": "Put flours, baking powder and salt in a large bowl and whisk to break up clumps."
    },
    {
      "@type": "HowToStep",
      "text": "Put maple syrup, chickpeas, almond butter and vanilla into a food processor. You can use a blender too if you prefer. Blend until smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Add the blended mix into the bowl with the flour and mix until combined."
    },
    {
      "@type": "HowToStep",
      "text": "Fold in the chocolate chips."
    },
    {
      "@type": "HowToStep",
      "text": "Pour into a prepared square baking dish."
    },
    {
      "@type": "HowToStep",
      "text": "Bake for 25-28 minutes or until a toothpick comes out clean."
    },
    {
      "@type": "HowToStep",
      "text": "Allow to cool completely before cutting, this allows it to set more and helps with the texture."
    },
    {
      "@type": "HowToStep",
      "text": "Devour!"
    }
  ],
