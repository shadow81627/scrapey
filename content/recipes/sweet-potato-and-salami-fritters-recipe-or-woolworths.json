  "recipeIngredient": [
    "1 cup salami, diced",
    "1 cup self-raising flour",
    "0.25 bunch mint leaves, torn",
    "2 cup sweet potatoes, coarsely grated",
    "1 medium zucchini, coarsely grated",
    "2 tbs extra virgin olive oil",
    "1 pinch freshly-ground black pepper",
    "garlic aioli (to serve)",
    "0.75 cup buttermilk",
    "2 eggs",
    "2 sprig rosemary, finely chopped",
    "2 clove garlic, minced",
    "pesto (to serve)",
    "1 pinch sea salt ground (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine the flour, eggs and buttermilk in a bowl and whisk until smooth. Mix the sweet potato, zucchini, salami, garlic and herbs in a large bowl, then season generously with salt and pepper. Add the batter, then stir to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the extra virgin olive oil in a pan over a moderate flame, then fry in tablespoon amounts for 2-3 minutes each side, then drain on kitchen paper. Serve with aioli and pesto."
    }
  ],
