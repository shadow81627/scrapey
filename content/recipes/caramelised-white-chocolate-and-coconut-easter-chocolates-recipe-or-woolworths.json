  "recipeIngredient": [
    "0.5 block caramelised white chocolate",
    "2 tbs shredded coconut",
    "1 Woolworths small chocolate Easter mould"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Chop chocolate, then place 3/4 in a microwave-safe bowl. Microwave on high for 1 minute, stirring halfway. Add remaining chocolate and stir until melted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place chocolate mould on a baking tray. Spoon chocolate evenly throughout the cavities. Sprinkle with coconut."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Refrigerate for 1 hour or until set. Unmould and store in fridge until ready to serve."
    }
  ],
