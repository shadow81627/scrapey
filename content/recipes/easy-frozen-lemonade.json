  "recipeIngredient": [
    "1 1/2 cups water",
    "1/3 cup fresh lemon juice",
    "1-2 teaspoons lemon zest",
    "1/3 cup can sugar",
    "4 cups ice cubes"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Add all ingredients to a blender and blend until there are no large chunks of ice left, about 30 seconds or so."
