  "recipeIngredient": [
    "2 tbs olive oil",
    "4 cloves garlic, peeled, whole",
    "3 red onions, peeled and sliced",
    "1 tsp thyme",
    "10 anchovies, drained, whole",
    "125g spinach",
    "60g olives, pitted and sliced",
    "200g puff pastry sheet, ready rolled",
    "2 tbs milk"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C. Heat the oil in a large pan on medium high heat and cook the onions for 10 minutes. Stir in the garlic and thyme and cook for 1 minute."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the spinach to the pan and cook until wilted, 2 minutes or so. Place the puff pastry sheet on a lined baking sheet and score a border around the edge."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon the mixture inside the border and scatter with olives and dot with anchovies. Brush the border with milk and bake for 15 minutes, until golden."
    }
  ],
