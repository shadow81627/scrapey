  "recipeIngredient": [
    "2 cups cooked mung beans",
    "1/2 cup tahini (prefer runny tahini)",
    "1 medium lime (juiced - add more to taste)",
    "1 clove garlic (smashed)",
    "1 tsp salt (more to taste)",
    "1/2 cup water (add one tablespoon at a time)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place the cooked mung beans in the bowl of a food processor, and pulse to break down into a paste."
    },
    {
      "@type": "HowToStep",
      "text": "Add the tahini, smashed garlic, lime juice, and salt, and run the food processor until smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Add water, one tablespoon at a time, until the hummus has reached the desired consistency."
    },
    {
      "@type": "HowToStep",
      "text": "Serve!"
    }
  ],
