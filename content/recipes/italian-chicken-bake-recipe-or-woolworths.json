  "recipeIngredient": [
    "500g chicken mince",
    "1 cup soup pasta, uncooked",
    "1 onion, diced",
    "4 cloves garlic, minced",
    "1 cup chicken stock",
    "1 can crushed italian tomatoes",
    "120g baby spinach",
    "pinch salt",
    "pinch pepper",
    "5ml olive oil",
    "1 cup mozzarella",
    "0.5 cup parmesan cheese"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180 degrees. Heat large fry pan with olive oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add garlic and onion to saut√© for 2min before adding chicken mince and breaking up/stirring until cooked."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add tin of tomatoes, stock, pasta, spinach and salt and pepper. Bring to the boil and stir for a couple of minutes until spinach has wilted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer to an ovenproof dish. Top with both cheeses."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cover and bake in the oven for 20 min. Then uncover and bake for a further 15-20 min until cheese is golden and crispy."
    }
  ],
