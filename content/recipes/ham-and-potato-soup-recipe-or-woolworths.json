  "name": "Ham & Potato Soup Recipe | Woolworths",
  "prepTime": "PT20M",
  "recipeCategory": "Entrees",
  "recipeCuisine": "Australian",
  "recipeId": "a0K0I00000S11srUAB",
  "recipeIngredient": [
    "1 ham bone with leftover meat attached, plus extra leftover ham",
    "4 carrots",
    "4 stalks celery",
    "4 potatoes",
    "1 onion",
    "6-8 cup chicken stock",
    "1 can kidney beans, drained",
    "1 cup corn kernels",
    "1 tbs garlic powder or minced garlic",
    "4 pkt chicken noodle soup",
    "salt & pepper (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Dice carrots, celery, potatoes and onion. Add to the slow cooker pot."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add ham bone and leftover ham to the slow cooker."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add corn and kidney beans, drained."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add chicken stock and season with garlic powder, dry chicken noodle soup (no water added) and salt & pepper to taste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook in the slow cooker on low for 8 hours."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve and enjoy!"
    }
  ],
