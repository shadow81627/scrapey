  "recipeIngredient": [
    "100g milk chocolate, broken",
    "4 tsp honey",
    "20g unsalted butter",
    "100g cornflakes",
    "40g macadamia nuts, crushed"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Melt the milk chocolate in a pot on a medium heat with the honey and butter until the chocolate has melted fully."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once melted, remove from the heat and mix the cornflakes and nuts into the chocolate so that the flakes are covered fully."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer the joys into paper cupcake holders and allow to set in the fridge for up to 1 hour before serving."
    }
  ],
