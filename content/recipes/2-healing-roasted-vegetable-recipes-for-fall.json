  "recipeIngredient": [
    "2 large carrots (chopped)",
    "2 large beets (diced)",
    "1 large squash (butternut, acorn, etc, chopped)",
    "1 large onion (chopped)",
    "1 tbsp coconut oil",
    "1 tsp turmeric",
    "1 tsp salt",
    "1 tsp black pepper",
    "1/2 tsp cayenne pepper",
    "3 to 4 large sweet potatoes (chopped)",
    "1 tablespoon coconut oil",
    "1 teaspoon black pepper",
    "1 teaspoon sea salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat your oven to 400oF."
    },
    {
      "@type": "HowToStep",
      "text": "Add all the ingredients to a large bowl. Mix well."
    },
    {
      "@type": "HowToStep",
      "text": "Spread the mixture evenly onto a baking sheet."
    },
    {
      "@type": "HowToStep",
      "text": "Bake for about 30 minutes, until the vegetables are tender."
    }
  ],
