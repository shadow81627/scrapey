  "recipeIngredient": [
    "160g corn kernels",
    "50g butter",
    "2 drops green food colouring",
    "2 drops red food colouring",
    "2 drops yellow food colouring",
    "2 drops blue food colouring",
    "2 drops purple food colouring"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the popcorn as per packet instructions."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Split butter into 5 microwaveable cups, adding 3 drops of one food colouring into each separate cup."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Microwave on high for 1 minute to melt the butter."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Blend each cup of butter with a hand mixer to combine thoroughly with the colour."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Separate the popcorn into 5 batches and toss each batch with each colour of butter."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Put the 5 batches back together in one bowl and toss again to mix."
    }
  ],
