  "recipeIngredient": [
    "24 baby potatoes, washed and halved",
    "2 tbs olive oil",
    "6 cloves garlic, peeled and sliced",
    "1 tbs parsley, chopped",
    "1 pinch salt",
    "1 pinch cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a baking dish, combine all of the ingredients fully."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Roast in the oven for 45 minutes or until the potatoes are cooked fully."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the oven carefully and transfer to a serving dish then garnish with parsley."
    }
  ],
