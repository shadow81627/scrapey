  "recipeIngredient": [
    "1 tbs sesame oil",
    "1 medium white onion",
    "4 cloves garlic, peeled and mashed",
    "4 tbs fajita seasoning",
    "0.4 cup salsa",
    "0.4 cup sour cream",
    "2 medium avocado, peeled, de-seeded and sliced",
    "4 medium tortillas",
    "4 medium capsicum, mixed colours, de-seeded and sliced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan to a medium heat and then add the sesame oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the onions and garlic for 4 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the capsicum to the pan with the seasoning and fry for a further 4 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the tortillas in the microwave for 30 seconds."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve the fried and seasoned vegetables on the tortillas with some sour cream, salsa and avocado."
    }
  ],
