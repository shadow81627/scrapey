  "keywords": "Damper,Pescatarian,Halal,Seafood free,Soy free,Vegetarian,Tree nut free,Egg free,Sesame free,Peanut free",
  "name": "Aussie Damper Recipe | Woolworths",
  "prepTime": "PT10M",
  "recipeCategory": "Breakfast",
  "recipeCuisine": "Australian",
  "recipeId": "a0K2y0000012hATEAY",
  "recipeIngredient": [
    "0.75 cup water",
    "0.33 cup butter, cubed",
    "3 cups self raising flour",
    "1 Pinch salt",
    "1 tbs milk"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven 200 degrees C"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine flour and salt in a large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Rub cubed butter with fingertips into flour, until mixture looks like breadcrumbs."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add water and milk to flour mixture. Use a bread knife to mix. Add a little more water if mixture is a little dry."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Turn dough onto a floured board and gently knead for 2 minutes until smooth."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Shape into a round circle about 15cm in diameter and place on a baking tray lined with paper. Dust with flour."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/amzgtnvwyic4ujtlzckq?$foodhub-method-images$&fmt=png-alpha",
      "text": "Bake for 30 minutes or until cooked. Place on a rack to slightly cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/wc8pxni0lq7fghjczwtt?$foodhub-method-images$&fmt=png-alpha",
      "text": "Serve warm with butter and golden syrup. Enjoy."
    }
  ],
