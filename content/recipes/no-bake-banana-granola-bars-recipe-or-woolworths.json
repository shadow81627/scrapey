  "recipeIngredient": [
    "2 tbs extra virgin olive oil",
    "2 tbs honey",
    "4 ripe bananas, peeled, roughly chopped",
    "2 tsp cinnamon, ground",
    "2 cup rolled oats",
    "1 cup pepitas",
    "1 cup shredded coconut",
    "1 cup sultanas",
    "0.5 cup black chia seeds"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/02Steps-June-NoBakeGranolaBars?wid=1080&hei=1080&fmt=png-alpha",
      "text": "Grease and line base and sides of a 20 x 30cm lamington pan, extending paper 2cm above edges."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/03Steps-June-NoBakeGranolaBars?wid=1080&hei=1080&fmt=png-alpha",
      "text": "Place all the ingredients in a food processor. Pulse for 15 seconds or until banana is smooth and mixture is just combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/04Steps-June-NoBakeGranolaBars?wid=1080&hei=1080&fmt=png-alpha",
      "text": "Spoon mixture into pan and press out evenly. Freeze for 2 hours or until firm. Remove from freezer and cut into bars. Keep refrigerated until ready to serve."
    }
  ],
