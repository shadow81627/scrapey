  "recipeIngredient": [
    "0.66 cup thickened cream",
    "200g leftover milk or dark chocolate Easter eggs (must be plain)",
    "2 tsp cocoa powder, sifted",
    "1L ice-cream, to serve (to serve)",
    "0.5 tsp sea salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Roughly chop leftover Easter eggs and place in a bowl over a saucepan of simmering water. Stir until chocolate has melted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add cocoa, cream and salt and stir to combine. Remove from the heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve drizzled over your favourite ice cream."
    }
  ],
