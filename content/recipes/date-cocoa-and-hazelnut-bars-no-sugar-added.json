  "recipeIngredient": [
    "8oz dates, pitted an chopped",
    "2 tbsp raisins",
    "Warm water (just enough to soak the dates and raisins)",
    "1/2 cup hazelnuts",
    "3 tbsp unsweetened cocoa powder",
    "1 tsp natural vanilla extract"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Soak dates and raisins in warm water for 10 minutes. Drain all the water and transfer to a food processor"
    },
    {
      "@type": "HowToStep",
      "text": "Add hazelnuts, cocoa powder and vanilla and pulse until all the ingredients are well incorporated"
    },
    {
      "@type": "HowToStep",
      "text": "Transfer to a cutting boar lined with wax or parchment paper. Form a log ( 7 x 4 x 1/2 inch approximately). Cut into 6 bars and refrigerate until ready to eat"
    }
  ],
