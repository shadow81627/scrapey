  "recipeIngredient": [
    "500g cans tuna, drained, 2 x 250 cans",
    "4 tbs mayonnaise",
    "2 tbs celery, sliced",
    "0.25 tsp smoked paprika",
    "0.25 tsp cayenne pepper",
    "1 tbs red onion, finely chopped",
    "0.5 lemon, juice only",
    "0.25 cucumber, thinly sliced",
    "4 slices pumperknickel bread",
    "3 sprigs watercress",
    "0.5 lemon, cut into wedges (to serve)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Flake the tuna in a bowl, then mix together with the mayonnaise, celery, paprika, cayenne pepper, onion and lemon juice."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Arrange the slices of cucumber on the pumpernickel, then top with the tuna mixture. Top with a few sprigs of watercress."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with lemon wedges."
    }
  ],
