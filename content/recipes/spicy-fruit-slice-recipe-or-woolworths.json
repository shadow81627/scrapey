  "recipeIngredient": [
    "1 cup SR flour, sifted",
    "1 tsp mixed spice",
    "0.5 cup coconut",
    "0.75 cup sugar",
    "0.75 cup sultanas",
    "2 eggs, lightly beaten",
    "90g unsalted butter - melted",
    "1 tbs sweet sherry",
    "icing mixture (for dusting)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Gather all ingredients together. Preheat oven to 160Â°C Fan. Grease and line (leave overhang for ease of lifting out of tin) a 20 cm square baking pan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Into a large mixing bowl, sift flour and mixed spice, add sugar, coconut and sultanas, mix to combine. Stir in combined eggs, sherry and butter, mix well again."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon into prepared baking tin, smooth the top, bake for 25 minutes or until a skewer comes out clean from the centre (each oven is different)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/piznyudyitzvohwdg7l5?$foodhub-method-images$&fmt=png-alpha",
      "text": "Cool completely in baking tin, dust over icing mixture and cut into squares."
    }
  ],
