  "recipeIngredient": [
    "5 tbs oats",
    "4 tsp chia seeds",
    "1.2 cup coconut milk, save 1 tbs for later (canned, reduced fat)",
    "2 cups almond milk (unsweetened)",
    "240g pineapple (frozen chunks)",
    "240g mango (frozen chunks)",
    "2 medium bananas (frozen)",
    "2 medium limes, juiced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
