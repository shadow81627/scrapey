  "recipeIngredient": [
    "800g gnocchi, fresh",
    "4 salmon fillets",
    "300g spinach",
    "1 tbs olive oil",
    "1 tbs dill",
    "2 medium lemons, juice and zest",
    "60g parmesan, grated",
    "1 pinch black pepper (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fill a large pot with water and bring to the boil. Add the gnocchi and cook according to pack instructions. Drain and set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a separate pot, poach the salmon fillets for 5 minutes, drain and flake."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a large pan on medium heat and cook the spinach with dill and half the lemon juice for 2-3 minute until wilted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir in the gnocchi, salmon, lemon zest and remaining juice and gently mix. Top with grated parmesan and black pepper and serve."
    }
  ],
