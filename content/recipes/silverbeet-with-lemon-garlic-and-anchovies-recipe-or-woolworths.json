  "recipeIngredient": [
    "1 bunch silverbeet, stems removed, chopped",
    "2 tsp olive oil",
    "0.5 medium lemon, zest and juice",
    "2 cloves garlic, peeled and minced",
    "1 tbs anchovies, drained and finely chopped",
    "1 pinch cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a large pan on medium heat and cook the garlic and anchovies for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stir in the chopped silverbeet, lemon zest and juice and cook until wilted, 23 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with cracked black pepper."
    }
  ],
