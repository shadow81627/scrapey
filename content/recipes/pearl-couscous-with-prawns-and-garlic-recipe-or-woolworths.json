  "recipeIngredient": [
    "400g pearl couscous, rinsed",
    "1.6 cup vegetable stock",
    "2 tbs canola oil",
    "200g prawns, cooked and de-shelled",
    "1 white onion, peeled and diced",
    "200g peas",
    "4 cloves garlic, peeled and diced",
    "1 tbs chilli flakes",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "3 tbs lemon juice"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the couscous and 400ml of the stock in a pot and bring to the boil, allow to simmer for 25 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan to a medium heat the canola oil and fry the prawns for 4 minutes while turning."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the onion, peas and garlic to the pan and fry for a further 4 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once the pearl couscous is cooked, remove from the heat and allow to cool slightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine the couscous with the rest of the ingredients before serving."
    }
  ],
