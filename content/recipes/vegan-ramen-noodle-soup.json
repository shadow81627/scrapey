  "recipeIngredient": [
    "1 block firm tofu (cubed)",
    "2 green onions (chopped)",
    "1 1/2 cups mushrooms (sliced)",
    "4 cloves garlic (minced)",
    "1 tbsp white miso soup",
    "3 tbsp soy sauce",
    "2 tbsp maple syrup",
    "1 1/2 tsp grilled sesame oil",
    "6 cups vegetable broth",
    "1/4 cup tomato paste",
    "8 oz ramen noodles",
    "coriander (sesame seeds, and sprouts for garnish)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Sauté the green onions, mushrooms, and garlic with a little olive oil in a large saucepan for 2–3 minutes. Then add the tofu, miso, syrup, sesame oil, vegetable broth, and tomato paste."
    },
    {
      "@type": "HowToStep",
      "text": "Bring to a boil, then reduce to low heat and let simmer 10 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Add ramen noodles and let simmer for 2–3 minutes until noodles are cooked."
    },
    {
      "@type": "HowToStep",
      "text": "Serve ramen with fresh coriander, sprouts, and sesame seeds."
    }
  ],
