  "recipeIngredient": [
    "2 tbsp olive oil",
    "1 onion (diced)",
    "2 cloves garlic (minced)",
    "2 tbsp tomato paste",
    "2 tsp garam masala",
    "1 tsp curry powder",
    "1 tsp turmeric",
    "1/2 tsp coriander",
    "1/2 tsp salt",
    "1 can coconut milk",
    "1/2 cup vegetable broth",
    "1 carrot (sliced)",
    "1 crown broccoli (cut into florets)",
    "1 large potato (washed and cut into 1/2 ” cubes)",
    "1 cup frozen peas",
    "1/4 cup sliced almonds"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/01/vegetable-korma-3-of-19.jpg",
      "text": "In a large saute pan, heat oil over medium heat."
    },
    {
      "@type": "HowToStep",
      "text": "Then add onion and garlic and cook, stirring frequently, for 3–4 minutes or until onion turns translucent."
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/01/vegetable-korma-4-of-19.jpg",
      "text": "Add the tomato paste, garam masala, curry powder, turmeric, coriander, and salt, and stir until completely combined."
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/01/vegetable-korma-6-of-19.jpg",
      "text": "Stir in the coconut milk and vegetable broth, and bring to a simmer."
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/01/vegetable-korma-9-of-19.jpg",
      "text": "Add the carrots, broccoli, potato, and peas."
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/01/vegetable-korma-11-of-19.jpg",
      "text": "Cook 20 minutes or until veggies are tender."
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2022/01/vegetable-korma-15-of-19.jpg",
      "text": "Top with sliced almonds, and serve warm over rice or with naan."
    }
  ],
