  "recipeIngredient": [
    "0.5 tsp baking powder",
    "0.5 cup storebought labne",
    "1 cup self raising flour",
    "24 medium cooked prawns",
    "0.5 cup frozen peas, thawed",
    "2 tbs fresh mint (to serve)",
    "3 tbs extra virgin olive oil",
    "2 lemon, 1 rind finely grated, 1 cut into wedges",
    "1 pinch black pepper",
    "1.25 cup buttermilk",
    "2 eggs, lightly beaten",
    "1 pinch sea salt ground (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the flour, baking powder, eggs & buttermilk into a bowl, season with salt & pepper, whisk until smooth."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Peel & devein prawns, halve lengthways."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add peas, lemon & prawns, mix to combine. Heat 1 tbsp oil in a large nonstick frying pan over a medium heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook 1/4 cup of the mixture, in batches, for 23 minutes each side or until golden & cooked through, adding more oil in between batches."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with labne, mint & lemon wedges."
    }
  ],
