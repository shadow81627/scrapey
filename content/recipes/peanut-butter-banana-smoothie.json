  "recipeIngredient": [
    "1 frozen banana, sliced ($0.22)",
    "2 Tbsp peanut butter ($0.14)",
    "1 tsp brown sugar ($0.02)",
    "1/4 tsp vanilla extract ($0.13)",
    "1 cup milk ($0.20)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Add the sliced banana, peanut butter, brown sugar, vanilla extract, and milk to a blender."
    },
    {
      "@type": "HowToStep",
      "text": "Blend the ingredients until smooth. If the smoothie is too thick, add more milk. If the smoothie is too thin, add more frozen banana. Taste and adjust the sweetness to your liking."
    }
