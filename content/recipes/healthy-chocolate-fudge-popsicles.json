  "recipeIngredient": [
    "3/4 cup milk chocolate chips",
    "1/2 cup of milk",
    "3 tsp unsweetened cocoa",
    "1 tbsp maple syrup",
    "1/2 tsp vanilla extract",
    "1 avocado",
    "1 banana"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "In a saucepan, combine chocolate chips, milk, cocoa, maple syrup, and vanilla. Heat over low heat for about 5 minutes, stirring until chocolate is melted and smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Place the avocado and banana in the bowl of a blender. Mix until smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Add the chocolate mixture and mix again until it reaches a pudding consistency, about 5 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Transfer to popsicle mold and freeze for 6 hours before eating."
    }
  ],
