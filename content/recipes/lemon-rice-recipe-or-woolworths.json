  "recipeIngredient": [
    "3 tbs vegetable oil",
    "3 tbs lemon juice, to taste",
    "1 tsp yellow mustard seeds",
    "6 green cardamom pods, split",
    "10 curry leaves",
    "2 red chillies, split lengthways",
    "0.5 tbs turmeric",
    "1 fresh root ginger, finely chopped",
    "1 clove garlic, crushed",
    "300g white basmati rice",
    "60g cashews, lightly toasted",
    "2 tbs coriander leaves"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a large frying pan, add the spices, ginger, and garlic, and fry over a medium heat for 2 minutes, or until aromatic, stirring all the time."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the lemon juice and cook for 1 minute, then add the rice. Stir until the rice is heated through and coated in the spices."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer to a serving dish, scatter with the cashews and coriander, and serve at once."
    }
  ],
