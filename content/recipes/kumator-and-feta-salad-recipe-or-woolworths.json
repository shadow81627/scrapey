  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice Kumatoes速 into 1 cm thick slices and arrange over a serving board or platter. Season with salt & pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine oil, vinegar, oregano, chives and green onions together. Spoon over the Kumatoes速."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cover and set aside for 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Just before serving, crumble over the feta, scatter over the pistachio nuts and rocket."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drizzle with a little extra virgin olive oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Enjoy!"
    }
