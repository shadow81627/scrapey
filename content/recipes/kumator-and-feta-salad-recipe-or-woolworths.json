  "recipeIngredient": [
    "150g marinated feta",
    "4 green onions, finely chopped",
    "500g kumato tomatoes",
    "0.25 cup extra virgin olive oil",
    "1 tsp dried Greek oregano",
    "1 tbs red wine vinegar",
    "0.25 cup pistachio kernels, toasted, chopped",
    "50g wild rocket",
    "1 tbs chives, chopped"
