  "recipeIngredient": [
    "1 tsp vanilla",
    "500g bananas, peeled, chopped and frozen",
    "1 banana, chopped (to serve)",
    "0.33 cup cocoa",
    "2 tbs maple syrup",
    "1 cup natural yoghurt (optional)",
    "0.5 cup smooth peanut butter",
    "125g strawberries, sliced (to serve)",
    "2 tbs toasted flaked coconut (to serve)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine the frozen banana, yoghurt, peanut butter, cocoa, maple syrup and vanilla in a food processor and process until smooth and combined. Transfer to a freezer proof container and freeze until firm."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Stand in refrigerator for up to an hour to soften before serving. Scoop into serving dishes and top with extra banana, strawberries and flaked coconut. Serve immediately."
    }
