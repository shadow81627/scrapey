  "recipeIngredient": [
    "1 cup raw cashews (soaked overnight, see notes)",
    "2 tbsp olive oil",
    "1 tbsp lemon juice",
    "1 tbsp apple cider vinegar",
    "1/2 tsp salt",
    "1/4 cup fresh chives",
    "3 tbsp nutritional yeast",
    "1/4 tsp ground pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2021/08/vegan-cashew-cheese-1-of-14.jpg",
      "text": "Place all ingredients in a food processor."
    },
    {
      "@type": "HowToStep",
      "image": "https://ohmyveggies.com/wp-content/uploads/2021/08/vegan-cashew-cheese-2-of-14.jpg",
      "text": "Blend until smooth. You may need to scrape the sides a few times to remove any un-blended bits."
    },
    {
      "@type": "HowToStep",
      "text": "Adjust the salt and pepper to taste."
    },
    {
      "@type": "HowToStep",
      "text": "The resulting cheese will be very thick and spreadable—even sliceable after being refrigerated! If you’re looking for cheese with a thinner consistency, add up to 2 more tbsp of oil and water."
    }
  ],
