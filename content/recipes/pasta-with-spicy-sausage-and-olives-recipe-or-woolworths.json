  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil in a large saucepan. Add onion, garlic carrots and celery and cook until softened. Stir in wine and chicken stock and simmer for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add tomatoes and simmer for a further 40 minutes. Grill sausages, slice into 2cm pieces and stir into sauce."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook pasta in plenty of boiling salted water as per packet instructions. Drain and divide between bowls, spoon sauce over and top with parmesan."
    }
