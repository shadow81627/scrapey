  "recipeIngredient": [
    "60g unsalted butter",
    "100g wholemeal biscuits, bashed",
    "2 medium banana, lightly mashed",
    "200g caramel",
    "0.6 cup whipped cream"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Prepare 6 muffin tins by lightly buttering."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine the butter and biscuits together fully and press firmly in the base of the tins and edges."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place some of the mashed banana inside the biscuit bases."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cover with some caramel and allow to in in the fridge for 120 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once they are set, remove from the fridge and serve with whipped cream."
    }
  ],
