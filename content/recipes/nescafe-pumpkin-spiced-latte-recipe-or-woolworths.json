  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pumpkin Spice Mix: Mix cinnamon, ground ginger, cloves and nutmeg (optional) together."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Store in an airtight container."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pumpkin Spice Latte: Dissolve coffee, sugar and 1/4 tsp pumpkin spice mix in 60ml hot water and stir well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk hot milk till frothy."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sprinkle a pinch of spice mix on top to serve."
    }
