  "recipeId": "a0K0I00000ShxkBUAR",
  "recipeIngredient": [
    "0.75 cup coconut flour",
    "0.25 cup flaxseed meal",
    "2 tsp ground cinnamon",
    "1 tsp ground clove",
    "1 tsp all spice",
    "1 tsp baking soda",
    "pinch salt",
    "1.5 cup pumpkin puree",
    "3 egg",
    "2 tbs macadamia oil",
    "2 tbs honey",
    "0.5 cup nuts and seeds mix",
    "0.25 cup chocolate, chopped (optional)",
    "1 tsp ground ginger"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat the oven at 180°C (160°C, fan-forced). Line a loaf tin with sheet of baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a large bowl, mix pumpkin puree, eggs, oil and honey with a spatula."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add flour, spices, baking soda and salt into the bowl and mix until just combined. Add nuts and seeds (leave some for topping) and chocolate if using."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour the mixture into the tin and smooth the surface. Sprinkle the reserved nuts and seeds on top."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in the oven for about one hour (check if a skewer comes clean when inserted). Cool on a wire rack until cool down completely."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/tylu3rlwdibflchnjeap?wid=481&hei=640&fmt=png-alpha",
      "text": "Slice into pieces and enjoy."
    }
  ],
