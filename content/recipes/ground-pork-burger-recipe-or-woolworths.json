  "recipeId": "RGWW338165",
  "recipeIngredient": [
    "600g minced pork",
    "1 tbs parsley, chopped",
    "1 tbs oregano, chopped",
    "4 tbs mayonnaise",
    "1 egg, free range",
    "80g panko bread crumbs",
    "4 medium brioche buns, sliced in half",
    "80g iceberg lettuce, torn",
    "1 medium Roma tomato, sliced",
    "1 small red onion, peeled and sliced",
    "4 slices chilli cheese"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a bowl, mix together the pork, bread crumbs, herbs and egg."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide the mixture into 4 patties and pack tightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan to a medium heat and then add the oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the burgers for 6 minutes, flip and fry for a further 6 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lightly toast the buns and serve with the burgers, mayonnaise, salads and cheese."
    }
  ],
