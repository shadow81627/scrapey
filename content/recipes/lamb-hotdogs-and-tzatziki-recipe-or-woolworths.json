  "recipeIngredient": [
    "1 small continental cucumber",
    "1 tbs flat leaf parsley, chopped",
    "0.25 cup extra virgin olive oil",
    "2 cup Greekstyle yoghurt",
    "12 lamb and mint sausages",
    "3 tsp lemon juice",
    "12 soft hotdog rolls",
    "1 gem lettuce, leaves separated",
    "1 clove garlic, crushed"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "For tzatziki, coarsely grate cucumber and squeeze to remove excess moisture."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place yoghurt, garlic, oil, lemon juice and parsley in a bowl. Whisk well to combine. Stir in cucumber. Refrigerate until ready to serve."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Chargrill or barbecue sausages over medium heat, turning regularly, for 810 minutes or until cooked through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut the rolls open lengthways, without cutting all the way through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fill each roll with lettuce and a sausage. Finish with a dollop of tzatziki. Serve."
    }
  ],
