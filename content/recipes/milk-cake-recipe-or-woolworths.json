  "keywords": "Desserts,Cake",
  "name": "Milk Cake Recipe | Woolworths",
  "prepTime": "PT2M",
  "recipeCategory": "Desserts",
  "recipeCuisine": "Australian",
  "recipeId": "a0K0I00000Shy0aUAB",
  "recipeIngredient": [
    "1L milk",
    "2 tbs lemon, juice",
    "6 cardamom pods, ground",
    "1 cup sugar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Start by reducing the sugar and 1L of milk on a light simmer to 1/3 of the amount - keep scraping the sides down and stir to avoid the milk burning to the bottom of the saucepan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once the milk is reduced to 1/3 you need to add the lemon juice and let sit for 5 mins on the low heat. The curd will separate from the whey."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Keep cooking over low heat stirring until all whey is dissolved. Once the whey is dissolved add the cardamom pods (ground) and mix through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place into a greased container and let sit overnight to set. Cut into squares and serve."
    }
  ],
