  "recipeIngredient": [
    "250g wholemeal penne",
    "0.5 large cauliflower, cut into florets",
    "400g cans no-added-salt diced tomatoes",
    "200g cherry tomatoes, halved",
    "415g can red salmon, flaked",
    "60g baby spinach leaves",
    "1 cup basil leaves",
    "0.5 cup parmesan, finely grated",
    "100g Woolworths light ricotta",
    "120g mixed leaf salad, to serve"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Samon_pasta_bake_step_1?$foodhub-method-images$&fmt=png-alpha",
      "text": "Preheat oven to 200°C/180°C fan-forced. Grease a 10-cup capacity baking dish. Cook pasta and cauliflower in a large saucepan of boiling water for 10 minutes or until tender. Drain. Transfer to a large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Samon_pasta_bake_step_2?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add canned and cherry tomatoes, 1 cup water, salmon, spinach, 2/3 each of the basil and parmesan, and season to taste. Toss until combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Samon_pasta_bake_step_3?$foodhub-method-images$&fmt=png-alpha",
      "text": "Transfer to a baking dish. Dollop over ricotta and scatter over remaining parmesan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Samon_pasta_bake_step_4?$foodhub-method-images$&fmt=png-alpha",
      "text": "Bake for 20 minutes or until golden. Top with remaining basil leaves and serve with salad."
    }
  ],
