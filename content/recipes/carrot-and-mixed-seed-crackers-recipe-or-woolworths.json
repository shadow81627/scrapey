  "recipeId": "RGWW339582",
  "recipeIngredient": [
    "100g flaked almonds",
    "40g sunflower seeds",
    "20g pumpkin seeds",
    "35g sesame seeds",
    "15g flaxseeds",
    "200g carrots, peeled and grated",
    "3 tbs red pesto",
    "1 tbs olive oil",
    "1 tsp oregano"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat the oven to 200°C/180°C fan-forced. Blitz the almonds in a food processor to a coarse crumb and add the seeds. Pulse to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the grated carrot, pesto, 3 tablespoons of water, oil and oregano and mix well. Spread on a lined baking sheet in a thick triangle, score diagonally and bake for 15 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut into crackers along the diagonal scored lines and bake for 5 more minutes."
    }
  ],
