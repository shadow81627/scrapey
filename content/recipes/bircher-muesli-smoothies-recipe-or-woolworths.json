  "recipeIngredient": [
    "1 cup milk",
    "1 small apple, cored, chopped",
    "1 banana, chopped",
    "4 strawberries, hulled",
    "banana, apple & strawberry skewers, to serve (to serve)",
    "0.25 tsp ground cinnamon",
    "1 tbs white chia seeds",
    "2 tbs organic rolled oats",
    "0.25 cup vanilla bean yoghurt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine all the ingredients, except skewers, in a bowl. Cover and refrigerate overnight."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer mixture to a blender. Blitz until very smooth. Pour into serving glasses and serve with banana, apple & strawberry skewers."
    }
