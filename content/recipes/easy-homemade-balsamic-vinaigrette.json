  "recipeIngredient": [
    "1/3 cup olive oil ($0.55)",
    "3 Tbsp balsamic vinegar ($0.41)",
    "2 Tbsp mayonnaise ($0.17)",
    "1/2 Tbsp Dijon mustard ($0.09)",
    "1 clove garlic, minced ($0.08)",
    "1/2 tsp dried basil ($0.05)",
    "1/4 tsp salt ($0.01)",
    "freshly cracked black pepper ($0.02)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Add all the ingredients to a jar, bowl, or blender. Close the jar and shake, whisk in the bowl, or pulse in a blender until the ingredients are combined and smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Let the dressing stand 5-10 minutes for the flavors to blend, then serve or refrigerate for up to 5 days."
    }
