  "recipeIngredient": [
    "1 cup white vinegar",
    "1/2 cup molasses",
    "1/2 cup soy sauce (low sodium if youâ€™re watching your sodium intake)",
    "3 tablespoon orange juice (fresh squeezed without pulp is best)",
    "1 tablespoon brine from capers",
    "2 teaspoon sugar",
    "1 teaspoon onion powder",
    "1 teaspoon garlic powder",
    "1/2 teaspoon curry powder",
    "1/2 teaspoon black pepper",
    "1/4 teaspoon ground ginger",
    "1/8 teaspoon cinnamon",
    "1/8 teaspoon cloves"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Add all of the ingredients to a saucepan and stir to combine. Bring the mixture to a boil and reduce the heat to a simmer. Simmer for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Let cool and pour in a glass jar with a lid and refrigerate for 1 week to let the flavors marry."
    },
    {
      "@type": "HowToStep",
      "text": "Store covered in the refrigerator for up to 3 more weeks."
    }
  ],
