  "recipeIngredient": [
    "1 cup frozen peas",
    "1 tsp flatleaf parsley leaves (to garnish)",
    "3 small zucchini, thinly sliced lengthwise",
    "1 lemon, rind finely grated, juiced",
    "0.5 cup Greekstyle yoghurt",
    "8 slice sourdough bread, toasted",
    "12 tbs extra virgin olive oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat a chargrill pan over mediumhigh heat. Toss zucchini slices in a bowl with oil and lemon juice. Season."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook zucchini for 1 minute on each side. Set aside. Soak peas in hot water until thawed. Drain well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "To serve, spread yoghurt over toast. Top with zucchini, peas, lemon rind and parsley. Finish with freshly ground black pepper and serve."
    }
  ],
