  "recipeIngredient": [
    "2 medium duck breasts, no skin, bone and excess fat removed",
    "2 heads iceberg lettuce, leaves removed and washed",
    "0.5 medium cucumber, finely sliced",
    "1 tbs sesame seeds",
    "4 tbs hoisin sauce",
    "2 tbs sesame oil",
    "2 stalks scallions, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan to a medium with and add the sesame oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the duck for 10 minutes turning over halfway and then remove from the heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Using 2 forks, pull the duck meat apart into shreds."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve the lettuce leaves with sliced cucumber and torn duck. Drizzle with hoisin and garnish with sesame seeds."
    }
  ],
