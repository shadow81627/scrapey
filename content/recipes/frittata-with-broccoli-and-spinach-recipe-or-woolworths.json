  "recipeIngredient": [
    "140g chickpea flour",
    "1 tbs olive oil",
    "4 tbs soy cream",
    "1.2 cup water",
    "3 tbs nutritional yeast",
    "1 tsp garlic powder",
    "1 tsp onion powder",
    "160g spinach, washed and drained",
    "1 pinch salt",
    "1 large white onion, peeled and chopped",
    "160g broccoli, lightly chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat the oven to 190°C/170°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a 9 inch skillet or similar oven proof pan to a medium heat and then fry the broccolini and onions in the olive oil for 4 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the rest of the ingredients in a food processor and blend until smooth."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Empty blended ingredients onto the skillet with spinach and the vegetables and transfer the dish to the oven."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake the entire dish in the oven for 60 minutes before serving or until the edges and top start to brown."
    }
  ],
