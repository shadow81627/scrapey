  "recipeIngredient": [
    "1 1/2 cups whole-wheat flour or all-purpose flour",
    "1/2 cup white sugar (organic for vegan-friendly)",
    "1/2 tsp baking powder",
    "1/2 tsp baking soda",
    "1 pinch salt",
    "1 cup cottage cheese ( at 1% M.G.)",
    "1 cup apples (grated)",
    "1/4 cup oil",
    "1/4 cup milk",
    "1 1/2 tsp ground cinnamon",
    "1 egg",
    "1 tsp vanilla extract",
    "1/4 cup brown sugar",
    "1/2 cup oatmeal",
    "1/4 cup butter (melted)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat the oven to 375°F. Line a muffin tin with paper liners."
    },
    {
      "@type": "HowToStep",
      "text": "In a large bowl, combine dry ingredients—flour, sugar, baking powder, baking soda, and salt."
    },
    {
      "@type": "HowToStep",
      "text": "In another bowl, mix remaining ingredients. Pour wet mixture over dry ingredients and blend until dough is smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Pour into muffin tins."
    },
    {
      "@type": "HowToStep",
      "text": "Mix together the ingredients for the topping and sprinkle on top of the muffins."
    },
    {
      "@type": "HowToStep",
      "text": "Place muffin tin in the oven and bake for about 20 minutes."
    }
  ],
