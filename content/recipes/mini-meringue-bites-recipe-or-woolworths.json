  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat the oven to 90Â°C."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Beat the egg white until stiff peaks appear."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the vanilla, cream of tartar and mix until peaks again."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Incrementally add the sugar always beating to keep the stiff peaks."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer to a piping bag and dollop about 20 meringues lifting away from each one as you pipe to form peaks."
    },
    {
      "@type": "HowToStep",
