  "recipeIngredient": [
    "250g blackberries",
    "0.25 cup caster sugar",
    "2 lemons juice and zest",
    "2 cups Jalna Greek Plus Natural Yoghurt",
    "1 cup whipped cream",
    "3 tbs icing sugar",
    "1 tsp vanilla paste"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place berries in a small pot over medium heat with sugar and juice from one lemon."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook for a few minutes until combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove pot from heat and set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a medium bowl, whisk Jalna Greek Plus Natural Yoghurt, cream, icing sugar, vanilla and juice and zest from the second lemon."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Layer the yoghurt and blackberries in glasses."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Garnish with fresh blackberries and extra lemon zest."
    }
  ],
