  "recipeIngredient": [
    "1 cup Balsamic vinegar",
    "1/2 cup maple syrup",
    "1 cantaloupe or other melon of your choice",
    "1 200 g package mini bocconcinis",
    "fresh basil leaves"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Pour balsamic vinegar and maple syrup into a saucepan. Simmer over low heat without covering until the liquid has reduced by half (about 10 minutes). Let cool."
    },
    {
      "@type": "HowToStep",
      "text": "Cut the melon into cubes."
    },
    {
      "@type": "HowToStep",
      "text": "On 12 mini-skewers of bamboo, prick a cube of melon, a bocconcini, and basil leaf. Repeat."
    },
    {
      "@type": "HowToStep",
      "text": "Serve with syrup or plain balsamic vinegar."
    }
  ],
