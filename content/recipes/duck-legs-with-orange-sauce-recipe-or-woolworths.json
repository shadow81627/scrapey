  "recipeIngredient": [
    "1 pkt Woolworths duck legs",
    "1 mandarin, juice",
    "4 tbs select orange marmalade",
    "1 chicken oxo cube or similar",
    "3 tbs cornflour",
    "1 cup cold water",
    "1 tsp liquid honey (omit or adjust to your taste)",
    "Shake of salt & pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/xbd3ve2rdwwjbrv4ctvq?wid=1936&hei=2592&fmt=png-alpha",
      "text": "Follow the steps on the back of the pack to cook the duck."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/u74dvw0vy3sbcjkoesnw?wid=1936&hei=2592&fmt=png-alpha",
      "text": "Assemble all other ingredients."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Put all ingredients into a pan over a medium heat, stirring until it boils."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Turn the heat to low and allow the sauce to simmer until it's reduced to the consistency you require (a bit of trial and error here, more liquid or more cornflour can always be added) Stir every few minutes, scraping down the sides. It will take around 20 mins."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with vegetables of your choice."
    }
