  "recipeIngredient": [
    "4 sheet lavash bread",
    "2 avocados, peeled and sliced",
    "1 Lebanese cucumber, sliced lengthways",
    "2 cup rocket leaves",
    "4 tbs ricotta",
    "8 slice honey leg ham"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lay one sheet of lavash bread onto a chopping board and lay two slices of ham onto the bread."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place sliced avocado and cucumber on top of the ham, and spoon your choice of cheese along one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Finish with a half cup of rocket leaves or your choice of lettuce. Season with salt and pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Roll up firmly from the cheese side and cut into thirds. Repeat with remaining ingredients."
    }
  ],
