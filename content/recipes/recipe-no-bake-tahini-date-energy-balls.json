  "recipeCuisine": [],
  "recipeIngredient": [
    "1 1/2 cups pitted Medjool dates (12 to 14)",
    "1/3 cup raw cashews",
    "1/4 teaspoon kosher salt",
    "1/8 teaspoon ground cardamom",
    "3 tablespoons tahini",
    "1/4 cup unsweetened shredded coconut",
    "3 tablespoons toasted sesame seeds"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Line a large plate with parchment or wax paper; set aside."
    },
    {
      "@type": "HowToStep",
      "text": "Place the dates, cashews, salt, and cardamom in a food processor fitted with the blade attachment. Process until the dates and nuts are broken down and clump together, about 1 minute. Add the tahini and process until incorporated, about 20 seconds."
    },
    {
      "@type": "HowToStep",
      "text": "Using a spoon or cookie scoop, measure 1 tablespoon of the mixture. Roll between the palms of your hands to form a ball, then place on the plate. Repeat with the remaining mixture."
    },
    {
      "@type": "HowToStep",
      "text": "Place the shredded coconut and/or sesame seeds in a shallow bowl. One by one, roll the energy balls in the topping until fully coated. Return to the plate. Cover with plastic wrap and refrigerate at least 1 hour or up to overnight before serving."
    }
  ],
