  "recipeIngredient": [
    "200g bacon lardons",
    "1 medium carrot, peeled and chopped",
    "300g mixed mushrooms",
    "300g baby potatoes, washed and halved",
    "20g butter",
    "3 medium shallots, peeled and chopped",
    "3 cloves garlic, peeled and chopped",
    "2 tsp flour",
    "1.6 cup beef stock",
    "0.2 cup red wine"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a large pot to medium heat with the butter and fry the bacon for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the carrot, garlic, mushrooms and onion and cook for a further 2 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients to the pot and bring to a simmer to cook out for 25 minutes until the potatoes are cooked through and the sauce has thickened."
    }
  ],
