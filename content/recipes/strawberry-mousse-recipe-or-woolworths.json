  "recipeId": "7878",
  "recipeIngredient": [
    "1.5 cup cream",
    "1 punnet strawberries",
    "1 tsp gelatine powder",
    "1 tbs lemon juice",
    "2 egg whites",
    "0.5 cup caster sugar"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add gelatine to hot water, stand for 5 minutes until dissolved. Set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Beat egg whites until soft peaks form, then gradually add sugar, beating until dissolved."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a seperate bowl, whip cream until soft peaks form."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fold in egg white mixture."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add strawberries, lemon juice and gelatine and fold in lightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour into serving glasses and refrigerate until set."
    }
  ],
