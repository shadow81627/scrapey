  "recipeIngredient": [
    "2 tsp baking powder",
    "0.33 cup desiccated coconut",
    "1.25 cup plain flour",
    "2 cup seedless white grapes (approx. 300g)",
    "2 tsp honey (to serve)",
    "1 lemon, rind finely grated and juice",
    "1 cup Greek-style yoghurt",
    "0.5 cup light olive oil",
    "3 eggs, whisked",
    "0.5 cup caster sugar",
    "0.75 cup almond meal"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180Â°C. Grease and line a 22cm fluted flan pan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk yoghurt, oil, eggs, lemon rind and juice together in a bowl. Sift flour and baking powder into the bowl with yoghurt mixture. Gently stir in the almond meal, coconut and sugar."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour cake batter into pan. Press grapes into mixture. Bake for 40 minutes or until cooked when tested with a skewer. Cool for 20 minutes before removing from pan. Cut into wedges and serve drizzled with honey and, if desired, extra yoghurt."
    }
  ],
