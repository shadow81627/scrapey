  "recipeIngredient": [
    "1 cup YoPro yoghurt of choice",
    "1 cup self raising flour",
    "1 tsp bicarb of soda",
    "0.75 cup milk",
    "1 large egg",
    "2 tbs maple syrup"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk all the ingredients together"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat a small pan (16cm) over medium heat and spray with oil"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add mixture to the pan  an amount that feels right to you. Cook until small bubbles appear and then flip. Cook for another one minute, or until golden on both sides"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top with fruits, more yoghurt, or sugarfree maple syrup if desired"
    }
  ],
