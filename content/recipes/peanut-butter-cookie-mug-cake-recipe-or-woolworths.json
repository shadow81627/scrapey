  "recipeIngredient": [
    "0.25 cup self-raising flour",
    "3 tbs chocolate chips",
    "0.25 cup milk",
    "0.25 tsp vanilla extract",
    "1 tbs brown sugar",
    "3 tbs smooth peanut butter"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place all the ingredients, except chocolate chips, in a microwave- safe mug, and combine using a fork. Fold in two-thirds of the chocolate chips, then scatter over remaining chocolate chips."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Microwave on high for 1 minute 40 seconds or until cooked through. Allow to cool slightly, then serve."
    }
