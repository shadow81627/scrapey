  "recipeIngredient": [
    "650g chicken thigh fillet",
    "1 onion",
    "3 clove garlic",
    "400ml coconut cream",
    "540g bamboo shoots, slices in water",
    "100g peas",
    "2 sprig coriander (or shallots)",
    "1 tbs olive oil",
    "1 tbs thai green curry paste",
    "2 tsp fish sauce (optional)",
    "1 cup basmati rice"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut chicken thigh fillet into bite size. Cut garlic, onions, coriander / shallots into small pieces."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil, cook garlic and onions until golden then add chicken, stir well. Add curry paste, coconut milk and bamboo shoots (fish sauce optional). Stir well and simmer for 15 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook rice as per packet directions and microwave peas."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve rice, peas and chicken curry. Sprinkle coriander / shallots on top."
    }
  ],
