  "recipeIngredient": [
    "1 1/2 cups frozen blueberries",
    "2 peeled and roughly chopped medium frozen banana (about 4 ounces frozen banana)",
    "2 pitted Medjool dates",
    "2 tablespoons chia seeds",
    "1/4 to 1/2 teaspoon ground cinnamon",
    "Pinch kosher salt",
    "1 1/2 cups unsweetened milk of choice"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Add 1 1/2 cups frozen blueberries, 1 frozen banana, 2 pitted Medjool dates, 2 tablespoons chia seeds, 1/2 teaspoon ground cinnamon, pinch of salt, and 1 1/2 cups milk to a blender. Blend on high speed until smooth and creamy, about 1 minute."
    },
    {
      "@type": "HowToStep",
      "text": "Pour into 2 glasses and serve immediately."
    }
  ],
