  "recipeIngredient": [
    "400g tuna steaks",
    "1 tbs olive oil, divided",
    "400g baby potatoes, halved",
    "400g green beans, trimmed",
    "2 tomatoes, diced",
    "100g black olives, drained, pitted",
    "3 tbs capers, drained",
    "2 lemons, 1.5 juiced, 0.5 chopped into wedges",
    "1 tbs wholegrain mustard"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Brush the tuna steaks with a little oil and sear in a griddle pan on the highest heat for 20-30 seconds on each side. Rest for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bring a large saucepan of water to the boil and simmer the potatoes for 10-12 minutes, add the green beans and cool for 3 more minutes and then drain well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Tip the potatoes and green beans into a large bowl with the tomatoes, olives and capers. Pour over the oil and lemon juice and stir in the mustard."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve topped with sliced tuna and a lemon wedge."
    }
  ],
