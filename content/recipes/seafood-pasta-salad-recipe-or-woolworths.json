  "recipeIngredient": [
    "1 tbs tomato paste",
    "0.25 cup parsley, chopped",
    "4 spring onions, sliced",
    "250g cherry tomatoes",
    "2 tbs balsamic vinegar",
    "250g pasta",
    "500g seafood marinara mix",
    "12 kalamata olives",
    "2 tbs olive oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook rollini pasta following packet instructions. Transfer pasta to a large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, heat olive oil in a large frying pan and cook seafood marinara mix, in batches, until seafood changes colour. Add seafood to pasta."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Halve cherry tomatoes and kalamata olives. Add to seafood with spring onions. Toss gently to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Make a dressing by whisking 1/2 cup olive oil with white balsamic vinegar, tomato paste and chopped parsley. Toss dressing through pasta salad. Serve."
    }
  ],
