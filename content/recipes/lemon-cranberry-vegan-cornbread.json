  "recipeIngredient": [
    "4 tbsp refined coconut oil melted and slightly cooled plus extra to grease the baking dish",
    "3 flax eggs ( 1/2 cup unsweetened non-dairy milk mixed with 3 tbsp flax meal)",
    "1.5 cups medium grind cornmeal",
    "1 cup whole wheat pastry flour",
    "1 1/2 tsp baking powder",
    "1/2 tsp baking soda",
    "1 tsp salt",
    "3 tbsp Maple Syrup",
    "1- 6oz container vanilla non-dairy yogurt",
    "1/2 cup seltzer water",
    "1/2 cup dried cranberries",
    "Zest of 1 small lemon"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat oven to 350F. Lightly grease an 8x8 inch square baking dish"
    },
    {
      "@type": "HowToStep",
      "text": "In a small bowl, combine the plant-based milk and flax meal. Set aside"
    },
    {
      "@type": "HowToStep",
      "text": "In a large bowl, combine cornmeal, flour, baking powder, baking soda, and salt"
    },
    {
      "@type": "HowToStep",
      "text": "In a small bowl, whisk together yogurt, maple syrup, almond milk/flax mixture, and melted coconut oil. Pour into dry ingredients and mix until just combined."
    },
    {
      "@type": "HowToStep",
      "text": "Add seltzer water, cranberries, and lemon zest and mix until well incorporated"
    },
    {
      "@type": "HowToStep",
      "text": "Pour batter into prepared dish and bake at 350F for 50-55 minutes."
    }
  ],
