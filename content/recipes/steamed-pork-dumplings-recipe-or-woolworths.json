  "recipeIngredient": [
    "350g pork mince",
    "100g water chestnuts",
    "5 medium spring onions",
    "5cm ginger",
    "1 cloves garlic",
    "2 tbs soy sauce",
    "1 tsp rice wine vinegar",
    "1 pinch black pepper",
    "18 wonton wrappers"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the pork mince, water chestnuts, spring onions, ginger, garlic, soy sauce, vinegar and black pepper into a large bowl and mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spoon a heaped teaspoon of filling onto each wonton wrapper. Spread out slightly then gently purse the edges around the filling, leaving an opening at the top."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Steam the dumplings for 10-12 minutes until the pork is cooked and the wrappers are translucent."
    }
  ],
