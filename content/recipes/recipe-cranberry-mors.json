  "recipeCuisine": [],
  "recipeIngredient": [
    "2 cups (224 grams) cranberries",
    "5 cups (1175 milliliters) water",
    "1/3 cup (113 grams) honey, or more to taste",
    "Juice of 1/2 lemon (about 1 tablespoon, or 15 milliliters)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Rinse and pick over the cranberries, discarding any stems or leaves."
    },
    {
      "@type": "HowToStep",
      "text": "Combine the cranberries and water in a large saucepan. Bring to a boil and cook until the cranberries burst. Remove from the heat."
    },
    {
      "@type": "HowToStep",
      "text": "Using a potato masher or the back of a large wooden spoon, thoroughly mash the cranberries in the pot. Return the pot to a boil and boil for 5 minutes. Remove from the heat. Stir in the honey and lemon juice."
    },
    {
      "@type": "HowToStep",
      "text": "Cover and let cool to room temperature. Strain through a fine-mesh strainer into a container; discard the pulp, or better yet, reserve it for another use."
    },
    {
      "@type": "HowToStep",
      "text": "Cover and refrigerate, and use within 1 week. Serve chilled."
    }
  ],
