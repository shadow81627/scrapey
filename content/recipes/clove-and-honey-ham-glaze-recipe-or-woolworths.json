  "recipeIngredient": [
    "100g maple syrup",
    "50g brown sugar",
    "2 tbs wholegrain mustard",
    "3 tbs cloves, powder",
    "2 tsp ginger, powder",
    "1 tbs peppercorns"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add all ingredients to a pot on a medium/low heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook and stir constantly until the sugar has fully melted for about 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and glaze your meat numerous times while cooking."
    }
