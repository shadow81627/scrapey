  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bring a large saucepan of water to the boil and simmer the potatoes for 10-12 minutes until tender and then drain well and place back in the saucepan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the butter and garlic powder and mash until smooth."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with truffle oil swirled on top and plenty of cracked black pepper."
    }
