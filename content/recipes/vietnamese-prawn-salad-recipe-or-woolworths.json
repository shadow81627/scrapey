  "recipeIngredient": [
    "1 clove garlic, crushed",
    "2 tbs white sugar",
    "500g cooked king prawns",
    "2 tbs fish sauce",
    "2 long red chillis",
    "0.5 cup coriander leaves",
    "1 Lebanese cucumber",
    "250g grape tomatoes",
    "1 French shallot",
    "2 kaffir lime leaves",
    "2 limes, plus juice",
    "60g baby rocket",
    "2 tbs peanuts"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine fish sauce and sugar in a shallow bowl and stir to dissolve sugar. Add lime rind, juice, garlic, chilli and kaffir lime leaves. Add prawns and toss well. Cover and refrigerate for 15 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, halve cucumber lengthways and scoop out seeds with a teaspoon. Slice on an angle."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine rocket, cucumber, tomato, shallot and coriander on a platter. Add prawns and drizzle over marinade. Top with peanuts. Serve."
    }
  ],
