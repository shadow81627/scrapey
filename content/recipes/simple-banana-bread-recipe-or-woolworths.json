  "recipeIngredient": [
    "3 medium banana, peeled and mashed",
    "100g unsalted butter, keep some for the baking dish",
    "120g sugar",
    "190g self raising flour",
    "2 medium eggs, free range",
    "1 tsp bicarbonate soda",
    "1 pinch salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a mixing bowl, combine the butter with the sugar and eggs and combine fully."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the salt and baking powder and incrementally add the flour."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lastly fold in the mashed bananas."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lightly butter a loaf tin and empty the dough in and level out. Bake for 60 minutes or until cooked through."
    }
  ],
