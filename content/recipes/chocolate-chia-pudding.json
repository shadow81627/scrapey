  "recipeIngredient": [
    "1/4 cup unsweetened cocoa powder",
    "1/4 cup chia seeds",
    "6 Medjool dates, pitted",
    "1 1/2 cups unsweetened almond milk",
    "1 tsp natural vanilla extract",
    "Assorted berries or other seasonal fruit"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Combine the cocoa powder and chia seeds in a large bowl and set aside"
    },
    {
      "@type": "HowToStep",
      "text": "Combine the dates, almond milk and vanilla in a blender and blend until smooth. Pour over the cocoa chia mixture and whisk well until the cocoa has dissolved"
    },
    {
      "@type": "HowToStep",
      "text": "Divide it into 3 individual containers and refrigerate overnight, or until set. Top with your favorite berries"
    }
  ],
