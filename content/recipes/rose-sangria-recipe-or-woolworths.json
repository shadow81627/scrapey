  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine 1 cup water and sugar in a saucepan over high heat and stir until sugar dissolves. Bring to the boil, then reduce heat and add 21/2 cups pitted cherries. Simmer for 10 minutes. Remove from heat, cool completely and remove cherries using a slotted spoon."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine 1 punnet strawberries (hulled, quartered) and 1 punnet blueberries. Add 1/3 cup of the cherry syrup, 200ml pineapple juice, 2 tbs vodka and 750ml ros√©. Combine and chill."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide among 8 glasses, top up with lemonade and garnish with watermelon stars and blueberries (threaded onto swizzle sticks)."
    }
