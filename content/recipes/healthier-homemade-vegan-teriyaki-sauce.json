  "recipeCuisine": "Asian",
  "recipeIngredient": [
    "1 cup low-sodium soy sauce",
    "1/4 cup pure maple syrup (See Note)",
    "1 tablespoon balsamic vinegar",
    "1/2 teaspoon ground ginger or 3 tsp fresh grated ginger",
    "1/2 teaspoon garlic powder",
    "1 1/2 tablespoon corn starch",
    "1/4 cup water"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "In a small sauce pan, whisk the soy sauce, syrup, vinegar, ginger and garlic powder until combined."
    },
    {
      "@type": "HowToStep",
      "text": "Heat on medium-high heat until it just begins to boil, then reduce to a simmer."
    },
    {
      "@type": "HowToStep",
      "text": "Whisk the corn starch and water together in a small bowl until fully combined. Whisk into the teriyaki sauce for approximately 30 seconds or until it thickens. Remove from heat."
    }
  ],
