  "recipeIngredient": [
    "2 tsp horseradish cream",
    "1 avocado, sliced",
    "0.25 small watermelon, cut into fingers",
    "2 Lebanese cucumbers, halved lengthways",
    "1 cup mint leaves",
    "1 small red onion, thinly sliced",
    "0.33 cup extra virgin olive oil",
