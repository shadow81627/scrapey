  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk oil, balsamic vinegar and horseradish cream in a jug with sea salt and pepper. Slice cucumber into long wedges."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Arrange cucumber, watermelon, avocado, onion and olives on a platter. Top with feta, dressing and herbs. Serve."
    }
