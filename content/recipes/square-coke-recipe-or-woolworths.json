  "recipeIngredient": [
    "200ml Coca-Cola",
    "300g Watermelon",
    "4 leaves mint leaves (to garnish)",
    "1 lemon, 1/2 tbs juiced",
    "1 lime (to rim glass)",
    "1 tsp salt (to rim glass)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Chop 300g of watermelon."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Blend with 1/2 a tbsp of lemon juice until smooth then sift."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Rim a square glass with lime and salt."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fill the glass with ice, Coke and watermelon juice."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add some mint to garnish."
    }
  ],
