  "recipeIngredient": [
    "200g risotto rice, wash and drain",
    "1 tbs canola oil",
    "200g butternut squash, peeled and diced",
    "1 medium white onion, peeled and diced",
    "80g crispy onions",
    "3 cloves garlic, peeled and diced",
    "3.2 cups vegetable stock",
    "0.4 cup white wine",
    "80g vegan parmesan, grated"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a large pot to a medium heat with the canola oil and fry the butternut squash for 5 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the onions and garlic and fry for a further 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the risotto rice to the pot and fry for 3 minute and then add the wine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add half of the stock to the pot and allow to cook until it has absorbed."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the stock to the pot and continue stirring occasionally."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Allow the stock to reduce, serve garnished with vegan parmesan."
    }
  ],
