  "recipeIngredient": [
    "0.25 cup mint leaves",
    "1 lemon (cut into wedges)",
    "2 tbs whole egg mayonnaise",
    "1 tsp Dijon mustard",
    "2 slices slices wholegrain sourdough bread (toasted)",
    "2 free range eggs (soft boiled, quartered)",
    "0.25 cup dill"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine mayonnaise, mint, dill and mustard in a small bowl. Add egg and gently stir to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top toast with egg mixture. Scatter over extra mint and dill. Serve with lemon wedges."
    }
