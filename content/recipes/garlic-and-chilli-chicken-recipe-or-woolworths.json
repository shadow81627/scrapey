  "recipeIngredient": [
    "4 medium chicken breasts, chopped",
    "0.8 cup chicken stock",
    "2 tbs green curry paste",
    "4 cloves garlic, peeled and mashed",
    "1 medium green chilli, de-seeded and sliced",
    "1 medium red chilli, de-seeded and sliced",
    "800g coconut milk",
    "1 tbs canola oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a large saucepan to a medium heat and then add the oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the chicken pieces while tossing for 8 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the chillies and garlic and fry for a further 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients, bring the heat to a simmer and allow to cook for 30 minutes stirring occasionally."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and suggested serving with rice or naan breads."
    }
  ],
