  "name": "Moscow Mule Mojito",
  "prepTime": "PT5M",
  "recipeCategory": "Drinks",
  "recipeIngredient": [
    "6 fresh mint leaves",
    "1 tsp sugar",
    "2 tbsp lime juice",
    "2 tbsp vodka",
    "1-2 tbsp rum",
    "1/2 cup ginger beer",
    "1/4-1/2 cup seltzer"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place the mint leaves on the bottom of a 10 ounce glass. Add sugar and lime juice and crush the leaves using a muddler or the handle of a wooden spoon"
    },
    {
      "@type": "HowToStep",
      "text": "Add vodka and rum and top with ginger beer and seltzer. You can add a couple of ice cubes if you like it extra cold."
    }
  ],
