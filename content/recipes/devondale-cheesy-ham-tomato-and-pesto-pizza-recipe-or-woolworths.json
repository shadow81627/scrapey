  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 200c. Place pizza bases onto 2 baking trays and spread with tomato paste, leaving a 1cm border."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lay ham over tomato paste and dot cherry tomatoes around. Scatter cheese over the top and bake for 12-15 minutes or until bubbly and golden brown."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top with pesto, scatter with basil leaves and serve."
    }
