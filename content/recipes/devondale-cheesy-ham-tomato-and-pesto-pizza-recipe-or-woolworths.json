  "recipeIngredient": [
    "1 cup Devondale 3 cheese blend",
    "2 pre-made pizza bases",
    "100ml tomato paste",
    "4 Bertocchi Premium Boneless Leg Ham, torn",
    "10 cherry tomatoes",
    "2 tbs basil pesto (optional)",
    "2 tbs basil leaves (to serve)"
