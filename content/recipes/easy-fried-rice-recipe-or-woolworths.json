  "recipeIngredient": [
    "500g microwave brown rice",
    "400g green prawn meat",
    "3 cups frozen mixed vegetables, tawed, drained",
    "0.25 cup salt-reduced soy sauce",
    "4 eggs",
    "5ml olive oil spray"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/easy-stir-fried-rice-step-01?$foodhub-method-images$&fmt=png-alpha",
      "text": "Heat rice following packet directions. Meanwhile, heat a large non-stick frying pan over medium heat. Spray with oil, add prawn meat and cook for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/easy-stir-fried-rice-step-02?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add vegetables, cover and cook for a further 1 minute. Add heated rice and soy sauce and toss to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/easy-stir-fried-rice-step-03?$foodhub-method-images$&fmt=png-alpha",
      "text": "Spray a second non-stick frying pan with oil. Fry eggs for 3 minutes, or until cooked to your liking."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/easy-stir-fried-rice-step-04?$foodhub-method-images$&fmt=png-alpha",
      "text": "Spoon fried rice into shallow bowls. Top with egg, season and serve."
    }
  ],
