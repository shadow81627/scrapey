  "name": "Chargrilled Chicken Breast with a Mustard & Sage Cream Recipe | Woolworths",
  "prepTime": "PT5M",
  "recipeCategory": "Lunch,Dinner",
  "recipeCuisine": "British",
  "recipeId": "RGWW337570",
  "recipeIngredient": [
    "4 medium chicken breasts, butterfly cut",
    "1 tsp canola oil",
    "0.8 cup cream",
    "2 tsp wholegrain mustard",
    "2 tsp sage, chopped",
    "2 medium shallots, peeled and chopped",
    "2 cloves garlic, peeled and diced",
    "1 pinch salt",
    "1 pinch pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a chargrill to medium heat with the canola oil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Chargrill the butterfly cut chicken breasts for 3 minutes on each side and then remove from the heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a non stick pan to a medium heat and fry the shallots and garlic for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the other ingredients to the pan and cook while stirring for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Now add the chargrilled chicken to the pan and cook for a further 2 minutes before serving."
    }
  ],
