  "recipeIngredient": [
    "4-5 brown onions",
    "2 sweet potato",
    "0.25 piece pumpkin",
    "5 carrots",
    "1 bunch celery",
    "0.5 red cabbage",
    "0.25 green cabbage",
    "1 cup red lentils, washed & drained",
    "1 jar passata",
    "1 can diced tomatoes",
    "4 cups salt reduced vegetable stock",
    "1 tsp paprika",
    "0.25 tsp black pepper (to season)",
    "0.25 tsp dried garlic flakes (to season)",
    "0.25 tsp dried chilli flakes (optional)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/swp5nbe8mrbeqbr00czq?wid=6000&hei=4000&fmt=png-alpha",
      "text": "Prepare your vegetables to shred the cabbage, cube the pumpkin, slice the celery/carrots/sweet potato and onions. Use your food processor to cut down food prep time."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Soften the onions with a tablespoon of oil in a large pot over medium heat for a few minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the cabbage and celery to soften for a couple of minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the remaining vegetables with the passata, diced tomatoes, stock, seasonings and top up with water."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/zkl25uajqqutp71clpau?wid=4606&hei=3543&fmt=png-alpha",
      "text": "Rinse the lentils in a strainer with cold water, drain and add to the pot."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/rslp5n7ysgt5yypjwxnn?wid=4631&hei=3562&fmt=png-alpha",
      "text": "Pop on the lid, turn the heat up to high to bring to the boil then reduce to a medium heat to cook for around 25 minutes."
    }
  ],
