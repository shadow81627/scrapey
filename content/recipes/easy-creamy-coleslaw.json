  "recipeIngredient": [
    "1/3 cup mayonnaise ($0.55)",
    "1/2 Tbsp honey ($0.06)",
    "1 tsp Dijon mustard ($0.03)",
    "1 tsp red wine vinegar (or apple cider vinegar) ($0.02)",
    "1/4 tsp salt ($0.02)",
    "freshly cracked black pepper ($0.03)",
    "1/2 14 oz. bag coleslaw mix (shredded cabbage and carrots)* ($0.75)",
    "3 green onions ($0.17)"
