  "recipeIngredient": [
    "400g pearl couscous, rinsed",
    "1.6 cup vegetable stock",
    "1 small red onion, peeled and diced",
    "4 cloves garlic, peeled and diced",
    "100g lentils, rinsed",
    "2 medium chillies, deseeded and sliced",
    "1 tbs canola oil",
    "1 tbs chilli flakes"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the couscous and lentils and 400ml of the vegetable stock in a pot and bring to the boil, allow to simmer for 25 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan to a medium heat the canola oil and fry the onion, chillies and garlic for 4 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove the vegetables from the heat and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once the pearl couscous is cooked, remove from the heat and allow to cool slightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine the couscous with the rest of the ingredients before serving."
    }
  ],
