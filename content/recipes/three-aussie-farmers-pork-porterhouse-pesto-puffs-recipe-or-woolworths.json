  "recipeIngredient": [
    "2 sheet puff pastry",
    "8 bocconcini balls",
    "0.5 cup sundried tomatoes",
    "0.25 cup olive tapenade",
    "1 pkt pork porterhouse with garlic and chive easoning (400g pack)",
    "1 egg, whisked",
    "0.5 cup basil pesto"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat oven to 180°C."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice Three Aussie Farmers Pork Porterhouse into 1cm thick strips."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut puff pastry into 4 squares."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread 1 teaspoon of pesto and 1/2 teaspoon of olive tapenade into the middle of each pastry square."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place 2 sundried tomatoes and a bocconcini ball in the middle of each pastry square."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place a slice of porterhouse on the top of the bocconcini and fold the pastry in half so that it completely covers the filling. Seal sides of pastry with a fork."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Brush each pastry parcel with egg wash and place on baking tray. Cook in oven for 30 minutes until golden."
    }
  ],
