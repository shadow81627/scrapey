  "recipeIngredient": [
    "2 tbs butter",
    "2 tbs olive oil",
    "3 cloves garlic, peeled and minced",
    "1 head savoy cabbage, shredded",
    "1 pinch cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the butter and oil in a large pan medium heat and add the garlic. Cook for 30 seconds, add the cabbage and a couple of tablespoons of water and cook for 10 minutes with the lid on, stirring occasionally, until softened."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Season with cracked black pepper and serve."
    }
