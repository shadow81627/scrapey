  "recipeIngredient": [
    "1 medium pumpkin (sugar pumpkin or muscat pumpkin )"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat your oven to 200°C / 400°F."
    },
    {
      "@type": "HowToStep",
      "text": "Cut your pumpkin in half and scoop out the seeds and stringy bits."
    },
    {
      "@type": "HowToStep",
      "text": "Place the pumpkin cut-side down into a roasting pan."
    },
    {
      "@type": "HowToStep",
      "text": "Place in the oven and roast until the skin has blistered and the pumpkin flesh is very soft."
    },
    {
      "@type": "HowToStep",
      "text": "Remove from the oven and let cool."
    },
    {
      "@type": "HowToStep",
      "text": "Peel the skin away from the pumpkin flesh, and transfer in batches to a food processor."
    },
    {
      "@type": "HowToStep",
      "text": "Puree until smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Line a strainer with a cheese cloth (or similar) and strain the pumpkin over a bowl for 2-3 hours, until thick."
    },
    {
      "@type": "HowToStep",
      "text": "Transfer the puree into freezer bags in 1 or 2 cup portions."
    },
    {
      "@type": "HowToStep",
      "text": "Freeze for 3-6 months."
    }
  ],
