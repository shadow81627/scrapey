  "recipeIngredient": [
    "2 medium carrots, peeled",
    "2 medium apples, peeled, cored",
    "2 medium beetroot, raw, scrubbed and peeled",
    "100g red cabbage",
    "3 tbs red wine vinegar",
    "2 tbs Dijon mustard",
    "5 tbs olive oil",
    "300g hot smoked trout"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "With a box grater or food processor, grate the carrots, apples and beetroot. Shred the cabbage and add all the vegetables to a large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour over the vinegar, olive oil and mustard and mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve the slaw with flaked smoked trout."
    }
  ],
