  "recipeIngredient": [
    "400g plant based 'chicken', cooked and chopped",
    "300g basmati rice, cooked",
    "100g peas",
    "1 tsp mustard seed",
    "1 tsp cumin seeds",
    "2 tbs coconut oil",
    "1 small white onion, peeled and sliced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook the rice and also the plant based chicken as per packet instructions and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a wok or pan to a medium heat with the coconut oil and fry the cumin and mustard seeds with the white onion for 2 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the peas and continue to fry for a further 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the cooked rice to the wok and fry for another 1 minute and serve with the sliced plant based chicken."
    }
  ],
