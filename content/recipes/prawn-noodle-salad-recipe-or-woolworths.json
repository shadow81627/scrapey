  "recipeIngredient": [
    "500g cooked tiger prawns, peeled, deveined, tails intact",
    "1 tsp sesame oil",
    "2 tsp fish sauce",
    "0.33 cup snow pea sprouts, trimmed",
    "1 red capsicum, deseeded, thinly sliced",
    "0.33 cup coriander leaves (to garnish)",
    "100g snow peas, trimmed",
    "1 lemon, juiced",
    "0.25 cup soy sauce",
    "500g fresh egg noodles",
    "1 tbs olive oil",
    "0.25 cup slivered almonds, toasted"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook noodles according to packet instructions. Blanch snow peas in boiling water for 1-2 minutes. Drain."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "To make the dressing, whisk soy sauce, lemon juice, oils and fish sauce in a jug."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine noodles, snow peas, prawns, sprouts, capsicum and almonds in a bowl. Add dressing and toss gently. Serve garnished with coriander."
    }
  ],
