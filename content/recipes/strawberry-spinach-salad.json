  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place the spinach into a large salad bowl, then place the strawberries, pecans and avocado on top."
    },
    {
      "@type": "HowToStep",
      "text": "Drizzle the salad with dressing and toss to distribute the ingredients."
    },
    {
      "@type": "HowToStep",
      "text": "Sprinkle the salad with salt and pepper to taste."
    },
    {
      "@type": "HowToStep",
      "text": "Divide onto plates and serve."
    }
