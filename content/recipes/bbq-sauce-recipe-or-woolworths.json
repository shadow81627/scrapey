  "recipeIngredient": [
    "2 cups tomato sauce",
    "0.25 cup apple cider vinegar",
    "0.33 cup brown sugar",
    "2 tbs honey",
    "1 lemon, juiced",
    "1 tsp chilli sauce (hot/mild )"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix and combine all ingredients in a medium saucepan on low to medium heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Simmer mixture for 10 mins or until mixture thickens."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from heat and transfer to bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Allow to cool completely before storing in fridge."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Use straight away or keep in fridge until needed. Enjoy!"
    }
  ],
