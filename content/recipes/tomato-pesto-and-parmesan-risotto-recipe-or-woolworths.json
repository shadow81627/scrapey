  "recipeIngredient": [
    "5 cloves garlic, peeled, chopped",
    "30g unsalted butter",
    "1 onion, chopped",
    "200g cherry tomato, halved",
    "1L chicken stock",
    "25g parmesan, shaved",
    "1 pinch pepper (for seasoning)",
    "100g baby spinach",
    "1.33 cup arborio rice",
    "2 tbs olive oil",
    "0.66 cup prepared basil pesto",
    "1 pinch salt (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat chicken stock in a large saucepan. Keep warm at a low simmer."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the oil in a large saucepan over medium heat. Add onions and cook for 5 minutes until soft. Add garlic and cook for 1 minute."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rice and cook, stirring, for 3 minutes until pale and golden. Add 1 cup of warm stock and cook, stirring constantly, until liquid is completely absorbed."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Repeat with remaining stock, in batches, until rice is tender and stock has been completely absorbed."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add tomato, pesto and butter. Season with salt and pepper. Stir to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fold in the spinach and shaved parmesan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve."
    }
  ],
