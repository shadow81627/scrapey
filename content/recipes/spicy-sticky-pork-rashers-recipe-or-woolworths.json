  "recipeIngredient": [
    "3 cup jasmine rice, steamed (to serve)",
    "2 bunch broccolini (to serve)",
    "1 orange, rind finely grated",
    "2 green shallots, finely chopped",
    "1 tbs soy sauce",
    "0.25 tsp dried chilli flakes",
    "1 tsp ground cumin",
    "0.25 tsp ground ginger",
    "0.5 tsp smokey paprika",
    "1 tbs honey",
    "600g pork rashers",
    "1 tbs oil",
    "1 garlic clove, crushed"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine all ingredients except pork rashers in a jug and whisk until well combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Arrange pork rashers in a shallow glass dish and pour over marinade. Turn rashers to coat evenly with marinade and set aside for at least 20 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer marinade to a small saucepan and bring to the boil. Cook for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat a chargrill pan or BBQ on medium. Cook pork rashers on chargrill for about 8 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with steamed rice, broccolini and spoon over a little of the heated marinade."
    }
  ],
