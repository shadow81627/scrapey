  "recipeIngredient": [
    "2 tbs olive oil",
    "1 large onion (peeled and sliced)",
    "400g butternut squash (peeled, deseeded and chopped into cubes)",
    "400g red lentils (dried)",
    "200g carrots (peeled and chopped into cubes)",
    "4 cloves garlic (peeled and minced)",
    "1 tsp ground ginger",
    "1 tsp cumin",
    "0.5 tsp cinnamon",
    "1.2L vegetable stock (made with 2 stock cubes)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the onion, squash, lentils and carrots in the slow cooker. Pour over the oil, add the garlic and spices and stir to coat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour over the stock and cook on high for 2-3 hours, until the vegetables and lentils have softened."
    }
  ],
