  "recipeId": "FIM-2209-13",
  "recipeIngredient": [
    "0.25 cup rice wine vinegar",
    "1 tbs caster sugar",
    "1 large carrot, shredded",
    "0.5 cup Greek-style yoghurt",
    "0.5 tsp chilli powder",
    "0.5 tsp mild paprika",
    "4 large wholegrain wraps",
    "1 oak leaf lettuce, leaves separated",
    "1 Lebanese cucumber",
    "80g bean sprouts",
    "0.25 cup small mint leaves",
    "0.25 cup coriander leaves",
    "20 cooked tiger prawns, peeled, deveined, tails intact",
    "1 fresh long red chilli (optional)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Prawn_and_Chilli_aioli_wraps_step_1?$foodhub-method-images$&fmt=png-alpha",
      "text": "Combine vinegar, sugar and 1/4 cup water in a bowl. Season. Add carrot and set aside to pickle for 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Prawn_and_Chilli_aioli_wraps_step_2?$foodhub-method-images$&fmt=png-alpha",
      "text": "Combine yoghurt, chilli powder and paprika in a small bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Prawn_and_Chilli_aioli_wraps_step_3?$foodhub-method-images$&fmt=png-alpha",
      "text": "Drain carrot. Spread wraps evenly with yoghurt mixture, and top with lettuce, cucumber, sprouts, carrot, mint, coriander, prawns and fresh chilli, if using. Season with pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Prawn_and_Chilli_aioli_wraps_step_4?$foodhub-method-images$&fmt=png-alpha",
      "text": "Fold wraps to enclose filling and serve."
    }
  ],
