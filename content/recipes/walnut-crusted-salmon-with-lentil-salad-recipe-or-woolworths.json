  "recipeIngredient": [
    "1 tbs olive oil",
    "400g salmon, 4 skinless, boneless fillets",
    "50g walnuts, very finely chopped",
    "1 medium lemon, zest and juice",
    "0.5 bunch dill, finely chopped",
    "20g butter, softened",
    "400g puy lentils, drained",
    "50g rocket",
    "0.5 bunch parsley, fresh, chopped",
    "75g sundried tomatoes, oil reserved"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat the oven to 200°C/180°C fan-forced. Place the salmon fillets on a lightly oiled baking sheet. Mix the walnuts, lemon zest, dill and butter in a small bowl and spread on each salmon fillet. Roast for 15 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile toss the lentils, lemon juice, rocket, parsley, chopped sundried tomatoes and 1-2 tablespoons of the sundried tomato oil together to make a salad."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve the salmon fillets on top of the lentil salad with more lemon juice and cracked black pepper."
    }
  ],
