  "recipeCuisine": "Mediterranean",
  "recipeId": "RGWW338209",
  "recipeIngredient": [
    "400g puff pastry, thawed",
    "500g ricotta",
    "4 eggs",
    "200g spinach, washed and drained",
    "8 stalks asparagus, sliced",
    "0.2 cup milk",
    "20g butter",
    "1 pinch salt",
    "1 pinch cracked black pepper",
    "1 tbs chives, chopped",
    "1 tbs parsley, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Line a small 6-8 inch cake tin with some butter."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the puff pastry at the bottom and the edges, keeping some pastry for the top."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a bowl, crack the eggs and combine with the ricotta, spinach, parsley, chives, salt, pepper and asparagus."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Empty contents into the cake tin and cover with puff pastry, tighten the edges together. Brush with milk."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pierce the top of the pastry a few times with the fork and bake in the oven for 35 minutes before serving."
    }
  ],
