  "recipeIngredient": [
    "1 cup self raising flour",
    "2 tbs sugar",
    "1 egg",
    "1 tinned pineapple, juice",
    "50g butter, melted",
    "1 tin crushed pineapple",
    "1 scoop coconut ice cream",
    "1 dash milk"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Beat egg and sugar well together."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add flour and pineapple juice and beat well, add a dash of milk if needed to get consistency right."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat a frypan to a medium heat, with a small amount of butter in the pan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drop batter into fry pan, when it starts to bubble turn. repeat with remaining mixture."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once pancakes are cooked, top with crushed pineapple, coconut ice cream and any other toppings you like. I used blueberry maple syrup and paleo granola."
    }
  ],
