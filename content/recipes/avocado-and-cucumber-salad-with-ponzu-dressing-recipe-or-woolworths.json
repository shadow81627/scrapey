  "recipeIngredient": [
    "2 Lebanese cucumbers, roughly chopped",
    "2 Hass avocados, halved, deseeded, cut into thick wedges",
    "0.5 bunch coriander, leaves picked",
    "1 French shallot, thinly sliced",
    "2 tbs pickled sushi ginger, drained",
    "1 clove garlic, crushed",
    "1 tbs rice wine vinegar",
    "0.25 cup salt-reduced soy sauce",
    "1 tbs sesame oil",
    "1 tsp brown sugar",
    "1 lemon, juiced",
    "1 tsp sesame seeds, toasted"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "To make the ponzu dressing, combine all of the ingredients in a small bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Arrange cucumber, avocado, coriander and shallot in a serving bowl. Serve topped with ginger and drizzled with ponzu dressing."
