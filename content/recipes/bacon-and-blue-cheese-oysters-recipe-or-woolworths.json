  "recipeIngredient": [
    "24 oysters, shucked, drained and rinsed",
    "200g bacon lardons",
    "20g butter",
    "20g parsley, chopped",
    "200g panko bread crumbs",
    "80g blue cheese, crumbled",
    "2 medium lemons, sliced into wedges"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up the grill on the oven and prepare a baking tray with shaped kitchen foil to hold the oysters steady."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the oysters on the foil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a pan on a medium heat, fry the butter with the bacon lardons for 6 minutes or until cooked."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and mix with the breadcrumbs and parsley."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Equally cover the oysters with the crumb mix and broil in the grill for 3 minutes until the crumbs start to brown."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve scattered with blue cheese and lemon wedges on the side."
    }
  ],
