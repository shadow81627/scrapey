  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Stir together the broth, soy sauce, white wine vinegar, poultry seasoning, and olive oil in a small bowl or cup."
    },
    {
      "@type": "HowToStep",
      "text": "Place the tofu cubes in a shallow dish and pour the broth mixture on top. Gently stir to distribute the ingredients. Marinate the tofu for about 30 minutes."
    },
    {
      "@type": "HowToStep",
      "text": "Preheat the oven to 400Â° and line a baking sheet with parchment paper."
    },
    {
      "@type": "HowToStep",
      "text": "Arrange the tofu cubes in an even layer on the baking sheet. Bake until lightly browned, about 30 minutes, flipping halfway through."
    },
    {
      "@type": "HowToStep",
      "text": "Remove the baking sheet from the oven and let the tofu cool a bit."
    }
