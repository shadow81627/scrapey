  "recipeIngredient": [
    "80g dried banana chips, chopped",
    "100g plain flour",
    "80g brown sugar",
    "70g white chocolate chips",
    "1 pinch bicarbonate soda",
    "60g unsalted butter, room temperature",
    "1 medium egg"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a mixing bowl, combine the butter with the sugar and crack in the egg to combine fully."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Inclemently add flour while mixing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once its fully combined, mix in the rest of the ingredients and combine. Allow to sit for 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Now divide the mixture into 10-12 balls and shape into your desired biscuit shape."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in the oven for 10 minutes or until nicely browned."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Allow to cool before serving."
    }
  ],
