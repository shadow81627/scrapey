  "recipeIngredient": [
    "5 cloves garlic, finely chopped",
    "250g butter, at room temperature",
    "2 tbs continental parsley, finely chopped",
    "2 tsp capers, rinsed, finely chopped",
    "2 tsp dijon mustard",
    "0.25 cup chives, finely chopped",
    "3 anchovies, finely chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine butter, garlic, chives, parsley, anchovy, mustard, capers, salt and pepper in a bowl and mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place a sheet of baking paper on a flat surface. Spoon garlic butter along centre of baking paper, then roll in paper to make a sausage shape. Twist ends of paper to seal. Chill in fridge for 30-45 minutes or until firm and ready to serve."
    }
