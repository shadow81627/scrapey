  "recipeIngredient": [
    "0.5 onion",
    "150g red capsicum",
    "2 clove garlic",
    "200g macro textured vegetable protein",
    "1 zucchini",
    "0.5 tsp turmeric",
    "1 tbs chilli powder",
    "1 tsp cumin",
    "0.5 tbs oregano",
    "0.5 cup veggie stock",
    "400g can macro kidney beans",
    "400g can macro chickpeas",
    "1 tbs salt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook TVP as per packet."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add in onion, capsicum & garlic. Cook for 4-5 minutes on low heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add in remaining veggies and spices and cook until all vegetables are cooked."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add in veggie stock and beans & bring to boil."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lower the heat and let simmer for 20-25 minutes or until desired consistency."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Serve with rice, mash potato or potato fries (i love serving it with homemade fries). Can also add cheese or sour cream to serve."
    }
  ],
