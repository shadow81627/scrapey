  "name": "Keema Beef Curry with Peas Recipe | Woolworths",
  "prepTime": "PT10M",
  "recipeCategory": "Lunch,Dinner",
  "recipeCuisine": "Indian",
  "recipeId": "RGWW337696",
  "recipeIngredient": [
    "600g minced beef, lean",
    "2 medium white onions, finely sliced",
    "2 cloves garlic, peeled and chopped",
    "3 tbs coconut oil",
    "20g ginger, chopped",
    "3 tbs hot curry powder",
    "2 medium green chillies, sliced",
    "1 tbs tomato puree",
    "1 pinch salt, to taste",
    "1 pinch pepper, to taste"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a large pan to a medium heat with the oil and fry the onions, garlic and ginger for 10 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove the vegetables from the pan and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Now fry the mince in the pan for 15 minutes on a medium to high heat or until cooked fully through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Now add the vegetables back to pan and add all the leftover ingredients, lower the heat to medium and allow to cook while mixing for another 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and use salt and pepper to taste."
    }
  ],
