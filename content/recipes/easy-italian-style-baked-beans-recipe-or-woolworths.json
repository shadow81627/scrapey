  "recipeIngredient": [
    "2 tsp extra virgin olive oil",
    "400g cannellini beans",
    "1 tbs Italian herb mix",
    "250g Macro Organic tomato, garlic & basil chunky pasta sauce",
    "2 stalks kale, leaves removed, torn",
    "25g parmesan, finely grated",
    "0.25 bunch basil, leaves picked"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Easy-italian-style-baked-beans-step-1?$foodhub-method-images$&fmt=png-alpha",
      "text": "Heat oil in a medium frying pan over medium-high heat. Cook beans and herb mix for 1 minute or until fragrant."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Easy-italian-style-baked-beans-step-2?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add pasta sauce and bring to a boil. Reduce heat to medium and simmer for 4 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Easy-italian-style-baked-beans-step-3?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add kale and cook for 2 minutes, stirring, or until wilted."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/Easy-italian-style-baked-beans-step-4?$foodhub-method-images$&fmt=png-alpha",
      "text": "Place bean mixture in bowl. Season with pepper. Serve topped with cheese and basil."
    }
  ],
