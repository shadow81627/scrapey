  "recipeIngredient": [
    "2 tsp canola oil",
    "1 white onion, peeled and diced",
    "1 tsp paprika",
    "1 tsp sage, chopped",
    "1 tsp thyme, chopped",
    "800g geisha oysters",
    "0.2 cup sake",
    "200g sourdough bread, sliced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat canola oil In a non-stick pan on medium heat. Add onions and spices and cook for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drain the oysters and add to the pan, cooking for an additional 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add sake, stir, remove from heat and serve with slices of sourdough."
    }
  ],
