  "recipeIngredient": [
    "8 medium cooked prawns, peeled, deveined",
    "100g noodles (see tip)",
    "0.25 red capsicum, sliced",
    "1 carrot, peeled, shredded",
    "1 tsp ginger, grated",
    "1 tbs mint leaves",
    "8 sugar snap, trimmed, sliced (or snow peas)",
    "2 tsp chilli sauce",
    "30g baby spinach leaves",
    "2 tbs mayonnaise",
    "1 tsp sesame seeds, toasted"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place noodles into a heatproof bowl and cover with water. Set aside for 5 minutes to soften. Refresh in cold water. Drain."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add prawns, carrot, peas, capsicum, spinach, mint and sesame seeds. Toss gently and place into noodle boxes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine mayonnaise, chilli sauce and ginger in a small container. Add to noodle salad to serve."
    }
  ],
