  "recipeIngredient": [
    "0.8 cup passionfruit puree",
    "1 cup cream",
    "400g condensed milk, whites only needed",
    "1 tsp vanilla extract",
    "10g gelatine, soaked in water"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the gelatine in water and allow to soak for at least 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the ingredients to a blender and blend until smooth."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the gelatine from the water to the mixture and blend again until smooth."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pout the mixture into 4 serving dishes and allow to set in the fridge for 3 hours before serving."
    }
  ],
