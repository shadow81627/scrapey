  "recipeIngredient": [
    "2 tubs YoPro plain yoghurt, room temperature",
    "0.5 cup oat flour, room temperature",
    "0.5 cup almond meal, room temperature",
    "0.25 cup coconut flour, room temperature",
    "0.25 cup pure maple syrup, room temperature",
    "80g coconut oil, room temperature",
    "175g light cream cheese",
    "3 egg",
    "3 tsp lemon zest",
    "0.5 cup stevia",
    "1 tsp vanilla essence"
