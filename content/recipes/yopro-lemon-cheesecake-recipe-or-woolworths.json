  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat your oven to 160Â°C fan forced"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Grease and line a cake or spring tin with baking paper"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine oat flour, coconut flour, and almond meal with maple and melted coconut oil"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Press into base of tin and bake for 10-12 minutes until lightly brown"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove tin and cool completely"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a bowl, beat together cheese and yoghurt, and in stevia and 1 egg at a time until combined"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Grate in lemon zest and stir in vanilla"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour over base and bake for 45-50 minutes until it has a slight wobble when touched in the centre"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cool and pop in the fridge to set the another 2-3 hours"
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cut into 12 slices"
    }
