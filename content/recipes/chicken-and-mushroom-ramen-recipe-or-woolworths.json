  "recipeIngredient": [
    "2 tbs sesame oil",
    "5 spring onions (sliced, green tops reserved)",
    "5cm ginger (peeled, sliced into matchsticks)",
    "1 red chilli (sliced, divided)",
    "1 bunch coriander (fresh, stalks chopped)",
    "150g shiitake mushrooms (sliced)",
    "4 tbs miso paste",
    "4 chicken breasts (cooked, shredded)",
    "500g soba noodles (quick cook)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat the sesame oil in a large, deep pan on medium high heat, add the spring onions, ginger, half the chilli, mushrooms and coriander stalks and stir fry for 34 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk the miso paste with 1.2 litres of boiling water to dissolve in a large saucepan. Pour the miso stock over the spring onion and mushroom mix and stir in the shredded chicken."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the noodles to the pan and cook for 23 minutes until heated through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide the soup between 4 bowls, top with sliced chilli, spring onion tops and coriander leaves and serve."
    }
  ],
