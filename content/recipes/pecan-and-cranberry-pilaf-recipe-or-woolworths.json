  "recipeIngredient": [
    "1 small red onion, fnely chopped",
    "0.5 cup parsley, roughly chopped",
    "0.5 cup pecans, toasted, chopped",
    "4 cup chicken stock",
    "2 tbs olive oil",
    "0.5 cup sultanas",
    "0.5 cup cranberries",
    "2 cup brown rice"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat oil in a saucepan over medium heat. Add onion and cook, stirring, for 3 minutes or until soft."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add rice blend and cook for 1 minute. Add stock. Bring to the boil. Reduce heat and simmer, covered, for 35 minutes or until rice is tender. Drain. Cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer to a bowl. Stir in cranberries, pecan, sultanas and parsley. Serve."
    }
  ],
