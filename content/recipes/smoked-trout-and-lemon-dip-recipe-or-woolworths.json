  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the trout, creme fraiche and yoghurt to a large bowl and gently mix with a fork."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour over the lemon juice, add the zest and chopped fresh dill and chives and mix gently. Season with cracked black pepper to taste."
    }
