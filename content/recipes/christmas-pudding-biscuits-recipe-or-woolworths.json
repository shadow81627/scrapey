  "recipeIngredient": [
    "190g Woolworths rocky road mallows biscuits",
    "0.25 cup Woolworths white chocolate melts",
    "8 spearmint leaves",
    "8 jaffas"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/ggf9uqqitg3vswysiysq?$foodhub-method-images$&fmt=png-alpha",
      "text": "Melt the white chocolate melts in a bowl over boiling water."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/b9dobol13oif9byrs3zs?$foodhub-method-images$&fmt=png-alpha",
      "text": "While the chocolate is melting, cut the spearmint leaves in half."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/dpztvlyfwmncsxfgvs34?$foodhub-method-images$&fmt=png-alpha",
      "text": "Dip the top of the Rocky Road Mallow in the melted chocolate."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/nknaff6pezmd5fi4rjpi?$foodhub-method-images$&fmt=png-alpha",
      "text": "Add the spearmint leaves leaving a space between them for the Jaffa."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/xxiawqwdmngzyhax1yz7?$foodhub-method-images$&fmt=png-alpha",
      "text": "Put the Jaffa in the middle."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/swyyjntdpoohz84t8mj8?$foodhub-method-images$&fmt=png-alpha",
      "text": "Once all decorated, put in the fridge until ready to serve. They should go in the fridge for at least 10 minutes."
    }
  ],
