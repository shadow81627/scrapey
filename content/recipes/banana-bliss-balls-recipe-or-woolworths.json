  "recipeIngredient": [
    "2 small bananas, ripe",
    "2 cups rolled oats",
    "1 cup desiccated coconut",
    "20 dried dates",
    "2 tbs honey",
    "1 tbs pure unsweetened cocoa powder",
    "0.5 tsp vanilla paste",
    "0.25 dessicated coconut, for rolling"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place all ingredients (except for extra coconut) in a food processor and blend till combined. Shape portions into balls (about 1 tablespoon size) and then roll in extra desiccated coconut. Keep in the fridge till it's time to enjoy them!"
    }
