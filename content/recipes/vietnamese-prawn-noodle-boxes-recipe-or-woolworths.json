  "recipeIngredient": [
    "750g cooked tiger prawns, peeled, halved lengthways",
    "0.25 cup fish sauce",
    "3 cup bean sprouts, trimmed",
    "1 long red chilli, deseeded, sliced",
    "3 Lebanese cucumbers, thinly sliced",
    "1 bunch mint, leaves picked",
    "1 red onion, thinly sliced",
    "crispy shallots (to serve)",
    "2 tbs rice wine vinegar",
    "400g thin rice stick noodles",
    "0.33 cup roasted peanuts, chopped",
    "1 bunch thai basil, leaves picked",
    "2 tbs brown sugar",
    "0.33 cup lime juice"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Prepare noodles according to packet directions. Drain and rinse under cold running water."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Gently toss drained noodles with prawns, cucumber, bean sprouts, herbs and onion."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "To make dressing, whisk all ingredients together. Drizzle dressing over salad and toss to combine. Spoon into noodle boxes and top with peanuts and crispy shallots. Serve."
    }
