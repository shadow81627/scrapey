  "recipeIngredient": [
    "250g risoni",
    "400g grape tomatoes, halved",
    "0.5 cup extra virgin olive oil",
    "1-2 tbs balsamic glaze",
    "0.5 bunch basil, leaves picked",
    "200g cherry bocconcini, torn",
    "1 clove garlic, crushed"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook risoni as per packet instructions. Drain and set aside to cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, make the dressing. Whisk oil, balsamic glaze, garlic and season with salt and pepper in a jug."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer risoni to a bowl. Add 3/4 of the dressing and toss to combine. Add bocconcini, tomato and basil. Add remaining dressing just before serving."
    }
  ],
