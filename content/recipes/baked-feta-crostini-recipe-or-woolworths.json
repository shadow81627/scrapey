  "recipeIngredient": [
    "1 small French baguette, sliced at an angle to get at least 8 slices",
    "160g feta, crumbled",
    "100g heirloom tomato, quartered",
    "2 tbs olive oil",
    "2 tbs balsamic vinegar",
    "40g pine nuts",
    "1 tsp cracked black pepper",
    "2 tbs chives, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Equally spread the crumbled feta across the sliced baguette."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix the chopped tomatoes with the pine nuts, balsamic, oil and pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the tomato mixture on top of the feta."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the crostinis on a baking tray and bake in the oven for 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the oven and garnish with chives before serving."
    }
  ],
