  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Vinaigrette: Whisk oil, Dijon mustard, lemon juice, thyme leaves, salt and pepper in a bowl until blended."
    },
    {
      "@type": "HowToStep",
