  "name": "Banana Date and Walnut Loaf Recipe | Woolworths",
  "prepTime": "PT15M",
  "recipeCategory": "Desserts",
  "recipeCuisine": "Australian",
  "recipeId": "a0K2y000000pRHaEAM",
  "recipeIngredient": [
    "2 ripe bananas",
    "1 cup pitted dates, chopped",
    "0.75 cup chopped walnuts",
    "2 large eggs",
    "0.5 cup olive oil",
    "0.25 cup maple syrup",
    "1.5 cups whole wheat flour",
    "2 tsp baking powder",
    "1 tsp cinnamon",
    "0.75 tsp baking soda",
    "0.25 tsp salt",
    "2 tsp vanilla extract"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix flour, baking soda, baking powder, salt, cinnamon in a bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In another bowl mix mashed bananas, oil, vanilla, eggs and maple syrup."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add your dry mix slowly to your wet mix mixing well. Then combine your dates and walnuts."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer to a loaf pan (in baking paper) and cook at 180 degrees Celsius for 45 to 55 minutes until the center comes out clean with a knife. Cool and serve with butter"
    }
  ],
