  "recipeIngredient": [
    "3 large, ripe peaches, peeled and cut into chunks (about 3 cups)",
    "1/2 cup sugar",
    "Juice of 1/2 lemon",
    "2 cups plain whole milk yogurt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Put the peaches, sugar, and lemon juice in a food processor or blender and purée."
    },
    {
      "@type": "HowToStep",
      "text": "Transfer the peach mixture to a large bowl and stir in the yogurt. Chill the base in the refrigerator for at least an hour."
    },
    {
      "@type": "HowToStep",
      "text": "Freeze in an ice cream maker according to the directions. Serve immediately or transfer to a container and allow to firm up for an hour or two in the freezer."
    }
  ],
