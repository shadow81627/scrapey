  "recipeIngredient": [
    "1 ripe banana (peeled and frozen)",
    "1/2 cup frozen blueberries",
    "2 teaspoons ginger (peeled and finely grated)",
    "2 cups kale leaves (loosely packed)",
    "1 cup unsweetened almond milk",
    "1 tablespoon chia seeds",
    "1/8 teaspoon ground cinnamon",
    "2 teaspoons to 1 tablespoon raw honey"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Add all ingredients to a blender and blend until completely smooth. If necessary, add more almond milk to help your blender process the frozen fruit."
    }
