  "recipeIngredient": [
    "120g self raising flour",
    "100g cherry tomatoes",
    "50g unsalted butter (or alternative)",
    "30g cheddar cheese",
    "3 medium eggs (medium size)",
    "handful fresh basil (handful,fresh is preferable)",
    "1 tsp garlic (to your taste)",
    "1 pinch pepper",
    "50g zucchini"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat your oven to 200C."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Finely chop the tomatoes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sift the flour into a mixing bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk in the eggs and grate in the zucchini."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add all remaining ingredients and mix gently."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Grease a mini loaf mould/tin and add the mixture equally."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in the oven for around 18 minutes or until golden."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Can serve warm or cooled."
    }
  ],
