  "recipeIngredient": [
    "2 tbs MasterFoodsTM Moroccan Seasoning",
    "1 tsp MasterFoodsTM Minced Garlic",
    "2 large beetroots",
    "1 pkt halloumi",
    "2 tbs plain flour",
    "1 mango, peeled and sliced",
    "1 cup almond slivers, toasted",
    "2 tbs olive oil",
    "2 tbs lemon juice",
    "1 tsp sugar",
    "sprigs of coriander",
    "5 tsp yoghurt"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Roast whole beetroots in an oven at 180C for 20 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Coat thick pieces of halloumi in flour mixed with MasterFoodsTM Moroccan Seasoning."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pan fry the halloumi in olive oil and serve on thick slices of beetroot with slices of fresh cold mango, and the toasted almond slivers."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix olive oil, lemon juice, sugar and MasterFoodsTM Minced Garlic in a jar."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Drizzle the dressing on top with sprigs of coriander and a few dollops of yoghurt. This is a great standalone salad for summer!"
    }
  ],
