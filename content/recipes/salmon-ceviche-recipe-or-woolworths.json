  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the salmon in a non-metallic bowl and cover with the lime juice. Cover and leave in the fridge for 25 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "When ready to serve, drain the salmon, add the spring onions, celery and coriander and mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Garnish with the rocket and serve with oatcakes, if liked."
    }
