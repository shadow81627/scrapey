  "recipeIngredient": [
    "3 cups rolled oats",
    "1/3 cup canola oil",
    "1 cups almonds, (sliced or slivered)",
    "3/4 cup unsweetened shredded coconut",
    "1/4 cup maple syrup",
    "1/4 cup unsweetened cocoa powder",
    "1/4 cup organic brown sugar",
    "1 teaspoon ground cinnamon",
    "1/2 teaspoon salt",
    "1 cup raisins"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat oven to 275Â°F and line a couple of baking sheets with parchment paper."
    },
    {
      "@type": "HowToStep",
      "text": "Mix all ingredients except for the raisins in a large bowl, stirring until the mixture is uniform."
    },
    {
      "@type": "HowToStep",
      "text": "Spread the mixture in an even layer on the cookie sheets."
    },
    {
      "@type": "HowToStep",
      "text": "Bake the granola for 45 to 60 minutes, until dried out and lightly browned around the edges. Watch it carefully as it can burn quickly."
    },
    {
      "@type": "HowToStep",
      "text": "Place the baking sheets on cooling racks and let the granola cool completely."
    },
    {
      "@type": "HowToStep",
      "text": "Add the raisins and transfer the granola to an airtight container for storage."
    }
  ],
