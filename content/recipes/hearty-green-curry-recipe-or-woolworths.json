  "recipeIngredient": [
    "360g tofu, drained and cubed",
    "100g cabbage, shredded",
    "50g edamame",
    "50g snow peas",
    "50g butternut squash, diced",
    "1 large red onion, peeled and sliced",
    "1 tbs sesame oil",
    "3.2 cups coconut milk",
    "1 tsp lime juice",
    "3 medium shallots, chopped",
    "3 cloves garlic, peeled and crushed",
    "1 tbs coriander, chopped",
    "10g ginger, chopped",
    "2 stalks lemongrass, sliced",
    "1 tbs cumin, sliced",
    "1 medium green chilli, sliced",
    "2 tsp Kaffir lime leaves, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Blend all your paste ingredients until smooth and well blended and set to one side. Fry your diced tofu in a pot with the sesame oil for 3 minutes while turning."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the vegetables and fry for a further 3 minutes. Now add the coconut oil, lime juice and green curry paste and lower to heat to allow to simmer for 15 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and serve with a choice of condiments."
    }
  ],
