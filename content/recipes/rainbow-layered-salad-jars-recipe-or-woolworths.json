  "recipeIngredient": [
    "100g elbow pasta",
    "1.5 cups Greekstyle yoghurt",
    "3 tsp Dijon mustard",
    "0.5 bunch dill, finely chopped",
    "2 small Lebanese cucumbers, finely diced",
    "2 truss tomatoes, finely diced",
    "420g canned noadded-salt corn kernels, drained",
    "2 celery stalks, finely diced",
    "0.66 cup shredded tasty cheese"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook pasta in a saucepan of boiling water according to packet instructions. Drain and allow to cool completely."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place yoghurt, mustard and dill in a medium bowl. Stir until combined. Season with pepper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide yoghurt mixture among. 4 x 400mlcapacity jars, then top with pasta, cucumber, tomato, corn, celery and cheese. Serve."
    }
  ],
