  "recipeIngredient": [
    "1 cup raw cashews, (soaked in water 4 to 8 hours, drained and rinsed)",
    "2 garlic cloves",
    "2 tablespoons white miso paste",
    "2 tablespoons lemon juice",
    "1 tablespoon vegan Worcestershire sauce",
    "1 tablespoon Dijon mustard",
    "1/2 teaspoon salt, (plus more to taste (I use 1 full teaspoon)",
    "3/4 cup water, (plus more as needed)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place all ingredients into a blender or food processor bowl. (Note 1)"
    },
    {
      "@type": "HowToStep",
      "text": "Blend until smooth."
    },
    {
      "@type": "HowToStep",
      "text": "Thin with additional water to reach your desired consistency. Blend again."
    },
    {
      "@type": "HowToStep",
      "text": "Taste test and add salt to taste. Adjust any other seasonings to taste."
    }
  ],
