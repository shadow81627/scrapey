  "recipeIngredient": [
    "110g John West Sardines in Rosemary and Sea Salt, drained, oil reserved",
    "150g baby roma tomatoes, halved",
    "1 lebanese cucumber, halved lengthways and sliced",
    "1 red onion, thinly sliced",
    "0.5 red capsicum, thinly sliced",
    "0.25 cup kalamata olives",
    "50g fetta, crumbled",
    "0.5 tsp dried oregano",
    "1 tsp red wine vinegar",
    "1 pinch sugar",
    "1 loaf crusty bread (for serving)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine tomatoes, cucumber, onion, capsicum and olives in a serving bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Top with John West Sardines and fetta. Sprinkle with oregano."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine reserved sardine oil, vinegar and sugar, drizzle over salad. Serve with crusty bread."
    }
  ],
