  "recipeId": "10683",
  "recipeIngredient": [
    "800g chickpeas, rinsed, drained",
    "0.33 cup quick oats",
    "0.25 cup pickled jalapeno",
    "1 free range egg",
    "0.5 tsp sea salt flakes",
    "3 spring onions, roughly chopped",
    "2 cloves garlic, roughly chopped",
    "2 kale stalks, roughly chopped",
    "0.25 cup extra virgin olive oil",
    "4 burger buns, toasted",
    "0.25 iceberg lettuce, leaves separated",
    "2 tomatoes, sliced",
    "0.33 cup mayonnaise"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place chickpeas, oats, jalapeno, egg, onion, garlic, kale and salt flakes in a food processor and pulse, occasionally scraping down bowl, until smooth enough to shape."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Shape mixture into 4 patties. Refrigerate for 30 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat half the oil in a frying pan or barbecue flat plate on medium heat. Add 2 patties and cook for 2 minutes on each side or until crisp and golden. Drain on a plate lined paper towel. Repeat with remaining oil and patties."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sandwich patties in burger buns with lettuce, tomato and mayonnaise. Serve."
    }
  ],
