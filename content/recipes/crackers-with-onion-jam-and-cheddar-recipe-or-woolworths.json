  "recipeIngredient": [
    "200g cheese flavoured crackers",
    "200g cheddar cheese, sliced",
    "2 medium red onion, finely diced",
    "50g caster sugar",
    "2 tbs balsamic vinegar",
    "1 stick cinnamon",
    "0.4 cup red wine",
    "1 star anise",
    "1 pinch salt",
    "1 tsp cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the onions on a non-stick pan on a medium heat for 3 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rest of the the jam ingredients and allow to simmer for up to 20 minutes until the sauce begins to reduce and become sticky."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove form the heat and take the star anise and cinnamon stick away and serve with the crackers and sliced cheese."
    }
  ],
