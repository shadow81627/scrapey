  "recipeIngredient": [
    "85g quinoa, uncooked",
    "80g oats",
    "75g pecans, chopped",
    "40g chia seeds",
    "70g dried cranberries, chopped",
    "3 tbs light olive oil",
    "2 tbs maple syrup",
    "1 tsp vanilla"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 160°C/140°C fan-forced and line a large baking trays with parchment."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix all the dry ingredients (excluding the cranberries) together in a large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk the olive oil, maple syrup and vanilla together in a small bowl and pour over the dry ingredients, mixing well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread the mixture in a thin layer on the baking tray bake for 20-25 minutes until golden."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Allow the granola to cool completely, break up any large clusters, mix in the dried cranberries and serve."
    }
  ],
