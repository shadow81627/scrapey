  "recipeIngredient": [
    "1 1/2 lbs cooking apples (peeled and sliced)",
    "1/4 cup maple syrup",
    "3/4 cup coconut sugar",
    "3/4 cup gluten-free flour",
    "3/4 cup rice flour",
    "1/2 cup gluten-free oats",
    "1/4 cup peanut butter (smooth)",
    "6 tbsp vegan butter"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat the oven to 180 degrees C (350 degrees F) and lightly grease a rectangular baking dish."
    },
    {
      "@type": "HowToStep",
      "text": "Layer the apple slices in the baking dish, then drizzle evenly with the maple syrup."
    },
    {
      "@type": "HowToStep",
      "text": "In a large mixing bowl, combine the gluten-free flour, rice flour, oats, peanut butter and vegan butter, until a crumbly texture forms."
    },
    {
      "@type": "HowToStep",
      "text": "Layer the crumb on top of the apples and bake in the preheated oven for 40-45 minutes, until crispy and golden brown on top."
    },
    {
      "@type": "HowToStep",
      "text": "Serve immediately with dairy-free ice cream."
    }
  ],
