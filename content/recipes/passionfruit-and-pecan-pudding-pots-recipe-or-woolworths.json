  "recipeIngredient": [
    "4 medium passionfruit, flesh and pips only needed",
    "60g caster sugar",
    "1.6 cup double cream",
    "1 tsp vanilla extract",
    "80g dark chocolate",
    "80g pecans, lightly broken",
    "0.4 cup whipped cream"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a large saucepan, melt the sugar with the cream and vanilla for 5 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Once melted, remove from the heat and combine with the passionfruit."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour the mixture into 4 serving dishes and allow to cool for 4 hours in the fridge."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Before you are ready to serve, melt the chocolate in a saucepan and mix in the chopped pecans."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Now top the pudding pots with whipped cream and garnish with chocolate coated pecans."
    }
  ],
