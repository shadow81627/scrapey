  "recipeIngredient": [
    "3/4 cup oat flour (, gluten free if needed)",
    "3/4 cup fine grained almond flour",
    "1/4 - 1/2 teaspoon sea salt (, depending on your preference)",
    "6 tablespoons maple syrup",
    "2 tablespoons almond butter (, you want it runny and not dry (see note)",
    "2 teaspoons vanilla extract",
    "1/4 cup chocolate chips (, dairy-free )",
    "2-4 tablespoons apple sauce (, optional if you want a thinner consistency)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Combine flours and salt in a bowl and whisk to mix."
    },
    {
      "@type": "HowToStep",
      "text": "Add in the maple syrup, almond butter, and vanilla. Mix well to combine. Add some apple sauce if you want a thinner consistency."
    },
    {
      "@type": "HowToStep",
      "text": "Mix in the chocolate chips."
    },
    {
      "@type": "HowToStep",
      "text": "Store in an airtight container in the fridge and enjoy!"
    }
  ],
