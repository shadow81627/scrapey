  "recipeIngredient": [
    "3L vegetable stock (add more or less stock depending on preference)",
    "2 large potatoes, washed",
    "1 medium sweet potato",
    "0.5 medium butternut pumpkin",
    "2 large carrots",
    "2 medium zucchini's",
    "1 large brown onion",
    "0.25 cup dried barley",
    "salt (to taste)",
    "pepper (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Make stock according to packet instructions (I use Vegeta vegetable stock). Set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Chop all veggies and place in slow cooker. Add stock, salt and pepper and set on high for 4 hours."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add barley in last 45-60 mins of cooking. Serve with fresh crusty bread rolls."
    }
  ],
