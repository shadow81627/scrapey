  "recipeIngredient": [
    "15 ounce cooked chickpeas (drained and rinsed)",
    "1 avocado",
    "1 stalk of celery (finely chopped)",
    "2 tablespoon fresh squeezed lemon juice",
    "1/4 cup roasted/salted pepita or sunflower seeds",
    "3 tablespoon chopped chives",
    "1 tablespoon curry (or to taste)",
    "1 teaspoon cumin",
    "1/2 teaspoon coriander",
    "salt to taste",
    "black pepper to taste",
    "bread",
    "greens",
    "tomato slices"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Grind the chickpeas, avocado, lemon juice, pepita or sunflower seeds in a food processor and transfer to a bowl. If you don't have a food processor you can mash them with a potato masher and chop the seeds with a knife."
    },
    {
      "@type": "HowToStep",
      "text": "Add the curry, cumin, coriander, and salt & pepper to the mixture and stir until well blended."
    },
    {
      "@type": "HowToStep",
      "text": "Spread on bread and top with greens and tomato for a sandwich."
    }
  ],
