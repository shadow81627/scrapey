  "recipeCuisine": "Scandinavian",
  "recipeId": "RGWW338422",
  "recipeIngredient": [
    "200g oats",
    "50g pumpkin seeds",
    "50g walnuts, chopped",
    "25g sunflower seeds",
    "3 tbs chia seeds",
    "1 tsp cinnamon",
    "2 tbs honey",
    "100g raisins",
    "1.6 cup oat milk"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 150°C/130°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Spread the oats, nuts, cinnamon and seeds out on a large baking tray, drizzle with honey and mix well to coat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake for 15 minutes until golden, stirring halfway through cooking. Allow to cool and serve in bowls with oat milk."
    }
  ],
