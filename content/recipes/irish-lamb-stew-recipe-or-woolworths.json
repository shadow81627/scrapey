  "recipeIngredient": [
    "500g lamb, cut into chunks",
    "2 medium carrots, peeled and chopped",
    "40g pearl barley",
    "5 medium shallots, peeled and sliced",
    "1 medium leek, peeled and chopped",
    "1 ltr chicken stock",
    "1 tbs rosemary, chopped",
    "1 tsp parsley, chopped (to garnish)",
    "3 cloves garlic, peeled and chopped",
    "10 small potatoes, peeled and halved",
    "1 tbs olive oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fry the diced lamb on a pan a medium heat with the olive oil for 3 minutes while turning occasionally and then remove from the pan."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Now placed the chopped vegetables onto the pan and fry for 10 minutes with the rosemary. In a pot add the chicken stock and pearl barley and bring to a medium heat for 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the cooked vegetables and seared lamb all into the stock, cover the pot, lower the heat and allow to simmer for 30 minutes before serving with parsley to garnish."
    }
  ],
