  "recipeIngredient": [
    "70g butter, melted",
    "1 tsp ground white pepper",
    "600g snapper fillets",
    "12 medium green prawns, peeled, deveined, tails intact",
    "1 red chilli, sliced diagonally (to garnish)",
    "2 tbs parsley (to garnish)",
    "0.25 tsp cayenne pepper",
    "2 tsp garlic powder",
    "2 tbs sweet paprika",
    "2 lemons, halved",
    "1 tsp ground black pepper",
    "2 tsp onion powder",
    "3 tsp sea salt flakes",
    "1 tsp dried thyme leaves",
    "1 tsp dried oregano leaves"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine spice mixture ingredients in a bowl. Set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place fish fillets and prawns on a baking tray. Brush on both sides with butter, reserving 1 tbs. Sprinkle with half of the spice mixture (see tip)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat a barbecue hot plate on medium-high. Cook fish and prawns for 2 minutes each side or until blackened. Transfer to a platter."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add lemon halves to hot plate and cook for 2 minutes or until charred. Drizzle fish and prawns with remaining butter. Serve with charred lemon and top with parsley and chilli."
    }
  ],
