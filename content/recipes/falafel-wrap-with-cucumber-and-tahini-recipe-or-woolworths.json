  "recipeIngredient": [
    "8 falafel",
    "1 large carrot, peeled, sliced into ribbons",
    "3 tbs lemon juice, divided",
    "1 medium cucumber, sliced into ribbons",
    "1 bunch coriander, fresh, chopped",
    "150g Greek yoghurt",
    "3 tbs tahini",
    "4 wholemeal wraps"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the carrot and cucumber ribbons in a bowl, add half the lemon juice and set aside."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix the yoghurt, tahini and remaining lemon juice in a bowl. Spread on each of the wraps."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Fill the wraps with cucumber, carrot and coriander, add two falafels to each, wrap up and serve."
    }
  ],
