  "recipeIngredient": [
    "4 tbs sriracha sauce",
    "2 tbs soy sauce (reduced salt)",
    "2 tbs sesame oil",
    "4 cloves garlic (peeled and minced)",
    "2 tbs ginger (peeled and grated)",
    "1 tbs honey",
    "1 tbs lime juice",
    "1kg chicken wings",
    "2 spring onions (sliced)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk the sriracha, soy sauce, sesame oil, garlic, ginger, honey and lime juice together in a large bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Tip the wings into the bowl and toss well to coat in the marinade. Cover and set aside for 30 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Set the air-fryer to 180Â°C and cook the wings for 12-14 minutes, toss and brush with any excess marinade and cook for 12 more minutes until sticky and crisp. Cook in two batches if necessary."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sprinkle the chicken wings with sliced spring onions and serve."
    }
  ],
