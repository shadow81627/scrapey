  "recipeIngredient": [
    "200g minced beef, cooked as per packet instructions",
    "1 medium red onion, peeled and diced",
    "2 cloves garlic, peeled and mashed",
    "1 pinch cracked black pepper",
    "2 tbs sesame oil",
    "1 medium carrot, grated",
    "2 tbs soy sauce",
    "12 bao buns"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a pan to a medium heat and fry the cooked beef and vegetables with the sesame oil, seeds and spices for 5 minutes while tossing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the heat and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Warm up the bao buns in the microwave for 15 seconds."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Put the ingredients in the bao buns and serve with your choice of condiments."
    }
  ],
