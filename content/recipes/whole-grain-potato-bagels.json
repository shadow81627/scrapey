  "recipeIngredient": [
    "1 cup warm water",
    "1 packet active dry yeast (2 and 1/4 tsp)",
    "1 cup spelt flour (or whole wheat flour)",
    "1 cup rye flour",
    "1 cup spelt flour (or whole wheat flour) (plus about 1 more c)",
    "1/4 cup vital wheat gluten",
    "1 tbsp barley malt powder",
    "1 tsp salt",
    "3/4 cup mashed cooked potato (gently packed)",
    "1/2 cup plain, unsweetened non-dairy milk",
    "1/2 tsp vegetable oil",
    "water for boiling",
    "2 tbsp barley malt powder",
    "2 tbsp baking soda",
    "1/4 cup plain, unsweetened non-dairy milk",
    "everything bagel toppings",
    "other toppings of choice (use any or all): flax seeds, chia seeds, hemp seeds)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Stir together the sponge ingredients in a bowl. Cover with a damp towel and set aside for 90 minutes to 2 hours. Check to make sure the mixture has some foaminess to it - that means the yeast is working."
    }
  ],
