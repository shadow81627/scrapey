  "recipeIngredient": [
    "1 pkt instant noodles",
    "100150g cooked and shredded chicken",
    "1 clove garlic, crushed",
    "1 carrot, peeled and grated",
    "23 spring onion stalks",
    "0.25 cup corn kernels"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Peel and grate carrot. Place in a heat proof bowl. Add corn to bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice spring onion and place in bowl with carrot and corn. Crush garlic and add to bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add cooked chicken to bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Boil water for instant noodles."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Open packet of noodles and add them to the bowl with the other ingredients. Add stock sachet, that came with the noodles."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pour boiling water into the bowl, and cover with a plate, or lid for 2 minutes. Stir noodles through and serve."
    }
  ],
