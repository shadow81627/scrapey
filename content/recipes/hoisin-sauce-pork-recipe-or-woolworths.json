  "recipeIngredient": [
    "300g lean pork",
    "1 small ginger",
    "2 cloves garlic",
    "1 red onion",
    "1 tbs hoisin sauce",
    "1 tsp light soy sauce"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice some ginger and 2 cloves of garlic. Slice crosswise a medium-size red onion."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Sauteed the ginger and garlic until lightly brown."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Thinly slice the pork, then fry over medium-high heat. When brown, turn over and fry the other side until cooked. Sprinkle some salt."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Separate them to the side of the pan. Fry the sliced onions at the center of the pan at reduced heat."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "When almost caramelised, apply 1 tablespoon of hoisin sauce and stir with the onions. Then add a teaspoon of light soy sauce. (If too dry, add a bit of hot water)."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Then mix and stir the pork and onions. Lastly, garnish with spring onions."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Ready to be served with rice on the side."
    }
  ],
