  "recipeIngredient": [
    "60g cocoa",
    "250g scotch finger biscuits",
    "250g dessicated coconut",
    "395g condensed milk",
    "100g dessicated coconut (to dust on top)",
    "100ml hershey caramel sauce"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Crush scotch fingers until a mixture of fine and larger crumbs."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add cocoa, coconut and condensed milk with the biscuits."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine all ingredients until mixed."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Begin to roll into balls and break each ball apart and add a small drop of caramel sauce in the middle before rolling back into a ball."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Dust each ball with coconut."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Set in the fridge, and enjoy once cooled."
    }
  ],
