  "recipeIngredient": [
    "0.5 bunch basil leaves",
    "2 cup lettuce, shredded",
    "0.25 red onion, finely sliced",
    "2 peaches",
    "1 tbs honey mustard salad dressing",
    "1 lime, juiced",
    "1 pinch freashly ground black pepper",
    "5ml canola cooking spray",
    "3 tbs feta, crumbled",
    "1 pinch sea salt flakes (to serve)",
    "1 punnet solanato tomatoes, quartered",
    "2 tortillas",
    "400g thick-cut ham off the bone",
    "1 pinch freshly-milled black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice the tortillas into thick strips then sprinkle with lime juice and canola oil. Season generously with salt and pepper then grill until lightly toasted. Sprinkle the ham with canola oil and grill until lightly blackened."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, toss the lettuce, tomatoes, onion and basil in a bowl. Cut the peaches into wedges and mix gently into the salad with the ham and wafers. Top with feta and dressing."
    }
  ],
