  "recipeIngredient": [
    "200g vermicelli noodles",
    "4 radishes, thinly sliced",
    "1 Lebanese cucumber, deseeded, finely shredded",
    "1 small red onion, sliced",
    "30g snow pea sprouts, trimmed",
    "1 bunch coriander, leaves picked",
    "2 limes, juiced",
    "1 tbs fish sauce",
    "2 tsp caster sugar, peeled",
    "500g cooked tiger prawns"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cook noodles in a saucepan of boiling water, according to packet instructions or until tender. Drain. Place in a bowl. Add radish, cucumber, onion and sprouts. Chop half the coriander and add to the noodle mixture. Toss to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk lime juice, fish sauce and sugar in a jug. Add lime dressing to salad, top with prawns and remaining coriander leaves and serve."
    }
  ],
