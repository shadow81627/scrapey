  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bring a large saucepan of water to the boil over high heat. Add noodles and cook for 3 minutes. Add broccolini and snow peas and cook for a further 2 minutes or until vegetables are just tender. Drain well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Meanwhile, heat corn in a microwavesafe bowl on high for 2 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Divide noodles and vegetables between bowls. Add beef, and season to taste. Serve."
    }
  ],
