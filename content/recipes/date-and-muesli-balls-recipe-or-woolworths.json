  "recipeIngredient": [
    "50g Devondale unsalted butter, softened",
    "2.5 cups nut-free toasted muesli",
    "8 Medjool dates, pitted, roughly chopped",
    "2 tbs rice malt syrup",
    "1 tbs tahini"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place muesli, dates, rice malt syrup, tahini and butter in a blender or food processor. Process until well combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Using damp hands, roll one level tablespoon of mixture at a time into balls. Place on a baking tray lined with baking paper. Refrigerate for 30 minutes or until firm. Serve."
    }
  ],
