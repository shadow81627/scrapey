  "recipeIngredient": [
    "1 green apple (cut into matchsticks)",
    "0.25 small red cabbage (finely shredded)",
    "1 carrot (cut into matchsticks)",
    "0.66 cup coriander leaves",
    "2 tbs ginger",
    "3 tsp rice wine vinegar",
    "0.33 cup cashews (chopped)",
    "2 tbs pure honey",
    "2 tbs reduced-salt soy sauce",
    "1 tbs sesame seeds (toasted)",
    "2 cup jasmine rice (to serve)",
    "600g rump steak (thinly sliced)",
    "1 tbs peanut oil",
    "0.33 cup roasted sesame dressing"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine cabbage, carrot, apple, cashews, sesame seeds, 1/2 cup coriander and sesame dressing in a large bowl. Season. Toss to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place beef, ginger and oil in a bowl. Toss to coat. Heat a large frying pan or wok over high heat. Stir-fry beef for 1-2 minutes or until browned. Add soy, honey and vinegar and cook for 1 minute or until warmed through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "To serve, top rice with beef, salad and any pan juices, and scatter over remaining coriander."
    }
  ],
