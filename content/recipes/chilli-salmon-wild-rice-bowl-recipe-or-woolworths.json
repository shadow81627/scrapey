  "recipeIngredient": [
    "190g John West Salmon Tempter - Chilli",
    "250g microwave brown and wild rice",
    "1 carrot, julienned",
    "0.5 cup shelled edamame beans",
    "0.5 bunch spring onions, chopped",
    "0.25 cup fresh coriander leaves, chopped",
    "1 tbs sesame seeds, toasted",
    "1 tsp sesame oil",
    "2 tbs salt reduced soy sauce",
    "1 tbs honey",
    "1 lime, juiced"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat rice following packet directions. Set aside to cool slightly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place rice in bowl, top with John West Salmon, carrot, edamame beans, spring onion, coriander and toasted sesame seeds."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Combine remaining ingredients in a screw top jar to make a dressing. Shake well. Pour over rice bowls and serve."
    }
