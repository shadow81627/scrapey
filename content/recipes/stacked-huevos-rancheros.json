  "recipeIngredient": [
    "2 corn tortillas",
    "1/3 cup refried beans",
    "2 eggs, cooked how you like them",
    "2 tablespoons shredded cheese"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Preheat oven to 400 degrees F. If desired, warm in the tortillas in a skillet with a small amount of olive oil or butter."
    },
    {
      "@type": "HowToStep",
      "text": "Assemble your Huevos rancheros: place one corn tortilla down and spread half the refried beans and top with 1 tablespoon cheese. Add the second tortilla on top and spread with remaining refined beans, eggs and the remaining cheese."
    },
    {
      "@type": "HowToStep",
      "text": "Place on a sheet pan and bake until cheese is melted and beans are warmed through, about 10 minutes. Top with your favorite toppings and enjoy."
    }
  ],
