  "recipeIngredient": [
    "2 tbs continental parsley, torn",
    "0.5 cup extra virgin olive oil",
    "1 tsp balsamic vinegar",
    "60g baby rocket leaves",
    "18 cherries, pitted, halved",
    "120g baby spinach leaves",
    "80g goat’s cheese, crumbled",
    "1.5 tbs apple cider vinegar",
    "1.5 cup frozen broad beans, thawed",
    "0.33 cup natural sliced almonds, toasted",
    "0.5 crusty baguette, thinly sliced diagonally"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat a chargrill pan over high heat. Drizzle bread slices with 2 tbs oil and cook for 1-2 minutes on each side or until lightly charred."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Unpod beans. Combine spinach and rocket in a serving bowl. Add beans, cherries, almond and parsley."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk remaining oil and apple cider vinegar in a separate bowl to combine and season to taste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Scatter goat’s cheese over salad. Drizzle with dressing and balsamic vinegar just before serving with toasted bread."
    }
  ],
