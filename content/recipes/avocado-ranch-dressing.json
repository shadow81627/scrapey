  "name": "Avocado Ranch Dressing",
  "recipeIngredient": [
    "2 large avocados",
    "1 1/4 cup water",
    "1/4 cup fresh lime juice, or lemon",
    "1 tablespoon chopped fresh parsley",
    "1 teaspoons granulated garlic",
    "1 teaspoon onion powder",
    "1/2 teaspoon salt, plus more to taste if needed",
    "1/2 teaspoon dried dill weed",
    "1/8 teaspoon black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Add all ingredients to a blender and blend until creamy, about 1 minute."
    },
    {
      "@type": "HowToStep",
      "text": "Use as salad dressing or vegetable dip!"
    }
  ],
