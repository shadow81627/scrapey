  "recipeId": "a0K2y0000011O8BEAU",
  "recipeIngredient": [
    "175g butter",
    "0.5 tsp salt",
    "0.66 cup caster sugar",
    "0.66 cup brown sugar",
    "0.75 cup flour",
    "3 eggs",
    "50g dark chocolate",
    "0.5 cup dark cacao powder",
    "125g dark chocolate chunks"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Melt butter in a saucepan. Make sure it goes bubbly and foamy as it will give it a nutty taste."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cool slightly by adding the 50g of chocolate and mix."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add in both sugars and mix well before adding salt and eggs *add eggs one by one and mix well."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add in cacao and flour and mix until combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add in choc chunks."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in a square baking dish at 160 for 30  35 minutes."
    }
  ],
