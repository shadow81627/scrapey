  "recipeIngredient": [
    "900g pork belly",
    "2 tbs five spice powder (to taste)",
    "2 tbs salt (to taste)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/fzggg8d6epszgfv7ikcb?$foodhub-method-images$&fmt=png-alpha",
      "text": "Layer the top of the pork belly with the salt evenly."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Preheat the oven to 200°C, arrange a pan on the bottom of oven rack and fill with water. The pork belly should be roasted using the rack above it."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Wash and dry (paper towels) pork belly. Rub the meat side with the five spice powder."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/ywqksstwanbxdzfw0b5y?$foodhub-method-images$&fmt=png-alpha",
      "text": "Bake until the salt crust forms, the salt should be hardened. Check water in the pan below and add if needed.Remove the pork belly from the oven, pull off the salt crust and discard."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Place the pork belly at the top rack of the oven and roast for 1 hour. While baking, the oil will drip to the bottom pan with water."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/vlphkyuzfnuifwaiebbh?$foodhub-method-images$&fmt=png-alpha",
      "text": "Raise the heat of oven to 240°C to 250°C, place the pork belly back in the oven and roast for another 50 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/sryebbvijsdzgej1j60x?$foodhub-method-images$&fmt=png-alpha",
      "text": "Remove from oven and let set for 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/l6pechdk7rmfhufhyxpc?$foodhub-method-images$&fmt=png-alpha",
      "text": "Cut and serve with some chili sauce or hoisin sauce or eat as is."
    }
  ],
