  "recipeIngredient": [
    "1/2 ripe avocado",
    "juice of 1 lime",
    "2 tbsp. fresh cilantro (packed)",
    "1/4 tsp. salt",
    "1 1/2 tbsp. soy sauce",
    "1 tbsp. maple syrup or agave",
    "1 tsp. cumin",
    "1/2 tsp. liquid smoke",
    "1/2 tsp. ancho chile powder",
    "1/2 tsp. ground black pepper",
    "1 tbsp. vegetable oil",
    "2 garlic cloves (minced)",
    "20 oz. can young (green jackfruit, drained and pulled apart)",
    "1 green bell pepper (sliced into strips)",
    "1 onion (sliced)",
    "3-4 8- inch flour tortillas",
    "3-4 big lettuce leaves (optional, for a little extra crunch)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Place all ingredients into food processor or blender and blend until smooth. Set aside."
