  "recipeIngredient": [
    "4 tbs olive oil",
    "0.5 orange, juice and zest",
    "1 tsp maple syrup",
    "1.5 tbs red wine vinegar",
    "1 tsp Dijon mustard",
    "300g red cabbage, shredded",
    "1 red onion, peeled and finely sliced",
    "2 carrots, peeled and grated",
    "2 tbs capers",
    "400g puy lentils, drained",
    "1 zucchini, spiralised",
    "0.5 bunch parsley, fresh, chopped",
    "0.5 bunch mint, fresh, chopped"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk the oil, orange zest and juice, maple syrup, vinegar and Dijon mustard together to make a dressing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the cabbage, onions, carrots, capers, lentils, zucchini and herbs to a large bowl. Pour over the dressing, toss well and serve."
    }
