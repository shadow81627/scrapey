  "recipeIngredient": [
    "2 -15 oz cans butter beans ($2.18)",
    "1/4 bunch fresh parsley ($0.20)",
    "2 Tbsp olive oil ($0.22)",
    "1 Tbsp lemon juice ($0.05)",
    "1 clove garlic, minced ($0.08)",
    "1/2 tsp dried oregano ($0.05)",
    "Salt and pepper to taste ($0.05)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Drain the beans and rinse them with cool water in a colander. Let the excess water drain away."
    },
    {
      "@type": "HowToStep",
      "text": "Rinse and drain the parsley. Pull the parsley leaves from the stems, then chop them roughly."
    },
    {
      "@type": "HowToStep",
      "text": "Combine the rinsed beans, chopped parsley, olive oil, lemon juice, minced garlic, oregano, a pinch of salt, and some freshly cracked pepper in a bowl. Stir until everything is well mixed. Taste the beans and adjust the salt if needed. Refrigerate until ready to eat."
    }
  ],
