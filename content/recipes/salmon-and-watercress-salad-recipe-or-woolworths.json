  "recipeIngredient": [
    "3 tbs extra virgin olive oil",
    "1 orange, juice only",
    "0.5 tsp mustard",
    "0.5 tsp caster sugar",
    "75g watercress",
    "0.25 cucumber, chopped",
    "2 oranges, peeled and segmented",
    "2 tbs toasted walnuts",
    "1 avocado, peeled, stoned and sliced",
    "175g smoked salmon strips",
    "4 gluten-free pitta breads, toasted"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Whisk together the oil, orange juice, mustard and sugar in a small bowl to make a dressing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a large bowl, toss together the watercress, cucumber, oranges, walnuts and avocado."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Toss with the dressing and smoked salmon and serve with the toasted pitta breads, if liked."
    }
  ],
