  "recipeId": "a0K0I00000SiUQuUAN",
  "recipeIngredient": [
    "70g firm tofu",
    "0.25 cup woolworths tinned corn",
    "1 stalk celery",
    "1 small carrot",
    "1 small cucumber",
    "3 slices tinned beetroot",
    "1 cup mixed lettuce leaves",
    "3 mushrooms",
    "1 tsp olive oil",
    "2 tsp apple cider vinegar",
    "0.25 tsp sriracha sauce",
    "0.25 tsp english mustard",
    "0.25 tsp honey"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Cube tofu and halve mushrooms before drizzling in oil and baking in a 160 fan forced oven for 15 to 20 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Prepare salad leaves and using a peeler ribbon the carrot and cucumber."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Slice beetroot and chop celery."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Prepare dressing ingredients."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Allow tofu and mushrooms to cool slightly and then plate lettuce and add all ingredients."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add dressing last. Mix and enjoy."
    }
  ],
