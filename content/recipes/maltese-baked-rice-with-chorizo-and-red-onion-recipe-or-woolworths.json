  "recipeIngredient": [
    "400g beef mince",
    "160g chorizo, sliced",
    "1 large red onion, peeled and diced",
    "400g rice, rinsed",
    "2 eggs",
    "2 cups Bolognese sauce",
    "1.2 cup vegetable stock",
    "160g cheddar cheese, grated",
    "1 tbs canola oil",
    "1 pinch salt",
    "1 pinch cracked black pepper"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Heat up a non-stick pan to a medium heat, add the canola oil and fry the mince and chorizo for 8 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Transfer the mince to a large baking dish and add the rest of the ingredients."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Mix together well before placing in the oven to bake for 60 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Allow to cool slightly before serving."
    }
  ],
