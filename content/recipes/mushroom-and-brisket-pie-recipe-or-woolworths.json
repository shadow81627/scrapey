  "name": "Mushroom & Brisket Pie Recipe | Woolworths",
  "prepTime": "PT15M",
  "recipeCategory": "Lunch,Dinner",
  "recipeCuisine": "British",
  "recipeId": "RGWW337771",
  "recipeIngredient": [
    "500g leftover cooked beef brisket, see recipe for brisket",
    "200g mushrooms, sliced",
    "5 large potatoes, peeled and sliced",
    "1 medium carrot, peeled and diced",
    "0.5 medium leek, diced",
    "1 medium white onion, peeled and chopped",
    "3 cloves garlic, peeled and crushed",
    "0.8 cup beef stock",
    "1 tbs parsley, chopped",
    "40g butter"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a casserole dish, sauté the carrots, mushrooms, leek, onion and garlic for 4-5 minutes. Add the stock and beef brisket."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake in the oven for 30 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Remove from the oven and cover with the sliced potatoes. Melt the butter evenly over the potatoes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Bake for another 30 minutes and serve with parsley to garnish and a choice of sides."
    }
  ],
