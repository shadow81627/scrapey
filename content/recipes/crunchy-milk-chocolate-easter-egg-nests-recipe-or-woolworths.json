  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "MELT the chocolate and peanut butter together in a bowl over gently simmering water, stirring occasionally until smooth. Remove from the heat, add the noodles and stir to combine."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "USING 2 spoons make 10 noodle nests on a baking paper lined tray. Allow to set and store in an airtight container until required."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "FILL each nest with eggs before serving."
    }
