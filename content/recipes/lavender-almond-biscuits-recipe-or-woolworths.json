  "recipeCuisine": "Australian",
  "recipeId": "a0K2y00000171ItEAI",
  "recipeIngredient": [
    "100g unsalted butter",
    "90g caster sugar",
    "150g plain flour",
    "0.5 tsp baking powder",
    "1 tbs dried lavender",
    "30g flaked almonds",
    "100g white chocolate (optional)"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/oxjkehplkf4bz8nqeqtn?wid=2322&hei=4128&fmt=png-alpha",
      "text": "Beat butter and sugar in a bowl."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/naw1gtdj7nlg4nttmqin?wid=2322&hei=4128&fmt=png-alpha",
      "text": "Preheat oven to 150'C. Line oven trays with baking paper."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/dqiuilcodmeekz9uhgmy?wid=2322&hei=4128&fmt=png-alpha",
      "text": "Mix in other dry ingredients (except chocolate). Mix until well combined."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/nuvkifforafd1lddyh6x?wid=2322&hei=4128&fmt=png-alpha",
      "text": "Form teaspoon full of mix into a ball. Place on oven tray and flatten."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/wvyuxmo41drvyatxx0ln?wid=2322&hei=4128&fmt=png-alpha",
      "text": "Cook at 150'C for 20 minutes. Remove from oven and allow to cool."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "image": "https://foodhub.scene7.com/is/image/woolworthsltdprod/g3mcnuaqy8mopd2qyrcj?wid=2322&hei=4128&fmt=png-alpha",
      "text": "Optional: Melt chocolate in 30 second bursts in the microwave. Once melted, dip biscuit in the chocolate and then place on baking paper to set."
    }
  ],
