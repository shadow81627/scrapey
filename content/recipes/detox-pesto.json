  "recipeIngredient": [
    "1 bunch basil leaves",
    "1 bunch cilantro",
    "1 bunch parsley",
    "1 bunch mint leaves",
    "1/2 cup sunflower seeds",
    "2 cloves garlic",
    "1 teaspoon sea salt",
    "1/4 cup extra virgin olive oil"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "text": "Add the herbs, sunflower seeds, garlic, and sea salt to a blender or food processor. Begin to process and slowly add the olive oil. Feel free to add more olive oil until the mixture becomes a smooth paste. Cover and store in the refrigerator for at least 30 minutes before serving."
    }
  ],
