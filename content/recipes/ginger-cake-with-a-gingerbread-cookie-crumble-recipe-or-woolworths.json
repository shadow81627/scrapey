  "recipeIngredient": [
    "75g dried ginger pieces, finely diced",
    "4 tsp ginger, powder",
    "2 tsp allspice, powder",
    "150g molasses",
    "100g brown sugar",
    "100g butter, unsalted (extra for baking tin)",
    "2 eggs (medium)",
    "0.5 tsp salt",
    "100g gingerbread, broken",
    "80g whipped cream",
    "175g self raising flour"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat the oven to 180°C/160°C fan-forced. In a mixing bowl, combine the butter with the sugar and molasses for 3 minutes until its smooth."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the eggs and combine fully for another minute. Add the dried ginger and spices and incremently add the flour while mixing."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lightly butter a baking tin and empty the contents. Bake for 50 minutes or until cooked fully through."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "All to cool slightly before serving with broken gingerbread cookies and whipped cream."
    }
  ],
