  "recipeCuisine": "Mediterranean",
  "recipeId": "RGWW338224",
  "recipeIngredient": [
    "30g unsalted butter",
    "30g brown sugar",
    "1 tsp cinnamon powder",
    "2 tbs cocoa powder",
    "2 tbs dark rum",
    "1.2 cups milk",
    "800g bread, torn (white or brioche)",
    "2 eggs",
    "60g cherries",
    "50g pine nuts"
  ],
  "recipeInstructions": [
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Pre-heat your oven to 190°C/170°C fan-forced."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Lightly butter a casserole dish or large loaf tin and set to one side."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "In a mixing bowl, combine the butter, cocoa, sugar and cinnamon together."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the rum and then crack in the eggs and combine fully."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Add the milk to the mixture with the cherries, pine nuts and 2 tablespoons of the juice from the cherries."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Tear up your bread and place in your baking dish and now pour your mixture evenly over the torn bread and allow to soak for 10 minutes."
    },
    {
      "@type": "HowToStep",
      "HowToTip": {},
      "text": "Flatten your mixture and then place into the oven and bake for 60 minutes. Allow to cool slightly before serving."
    }
  ],
